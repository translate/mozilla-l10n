#~ msgstr "саясат uri-ін сәйкес келмейтін хост атынан алу мүмкін емес: %1$S"

#~ msgid "can't fetch policy uri from non-matching port: %1$S"
#~ msgstr "саясат uri-ін сәйкес келмейтін порттан алу мүмкін емес: %1$S"

#~ msgid "can't fetch policy uri from non-matching scheme: %1$S"
#~ msgstr "саясат uri-ін сәйкес келмейтін схемадан алу мүмкін емес: %1$S"

#~ msgid "Error fetching policy-uri: %1$S"
#~ msgstr "policy-uri алу қатесі: %1$S"

#~ msgid "Provided argument is not an nsIURI"
#~ msgstr "Ұсынылған аргумент nsIURI емес"

#~ msgid "Provided argument is not a string"
#~ msgstr "Ұсынылған аргумент жол емес"

#~ msgid "Can't use 'self' if self data is not provided"
#~ msgstr "self мәліметтері қолданбаса, 'self' қолдануға болмайды"

#~ msgid "can't parse a URI without a scheme: %1$S"
#~ msgstr "Схемасыз URI-ды талдау мүмкін емес: %1$S"

#~ msgid "self keyword used, but no self data specified"
#~ msgstr "self кілт сөзі қолданылған, бірақ self мәліметтері көрсетілмеген"

#~ msgid "Can't create host-only source %1$S without 'self' data"
#~ msgstr "'self' мәліметсіз host-only %1$S қайнар көзін жасауға болмайды"

#~ msgid "Can't create source %1$S without 'self' data"
#~ msgstr "'self' мәліметсіз %1$S қайнар көзін жасауға болмайды"

#~ msgid "Could not intersect %1$S with %2$S due to port problems."
#~ msgstr "Порт мәселесі салдарынан %1$S және %2$S қиылыстыру мүмкін емес."

#~ msgid "Could not intersect %1$S with %2$S due to scheme problems."
#~ msgstr "Схема мәселесі салдарынан %1$S және %2$S қиылыстыру мүмкін емес."

#~ msgid "intersecting source with undefined host: %1$S"
#~ msgstr "анықталмаған хосты бар қайнар көзінің қиылысуы: %1$S"

#~ msgid "intersecting two sources with undefined hosts: %1$S and %2$S"
#~ msgstr ""
#~ "анықталмаған хосттары бар екі қайнар көзінің қиылысуы: %1$S және %2$S"

#~ msgid "The report URI (%1$S) must be from the same eTLD+1."
#~ msgstr "Есептеме URI (%1$S) дәл сол eTLD+1 шыққан болуы керек."

#~ msgid ""
#~ "The report URI (%1$S) must use the same scheme as the originating "
#~ "document."
#~ msgstr ""
#~ "Есептеме URI (%1$S) түпнұсқа құжаты қолданатын схеманы қолдануы тиіс."

#~ msgid ""
#~ "The report URI (%1$S) must use the same port as the originating document."
#~ msgstr ""
#~ "Есептеме URI (%1$S) түпнұсқа құжаты қолданатын портты қолдануы тиіс."

#~ msgid "Directive %1$S violated"
#~ msgstr "%1$S директивасы бұзылған"

#~ msgid "Directive %1$S violated by %2$S"
#~ msgstr "%1$S директивасын %2$S бұзған"

#~ msgid "don't understand option '%1$S'.  Ignoring it."
#~ msgstr "'%1$S' опциясы түсініксіз. Оны елемеу."

#~ msgid "can't use report URI from non-matching eTLD+1: %1$S"
#~ msgstr ""
#~ "Сәйкес келмейтін eTLD+1-ден есептеме URI-ін қолдану мүмкін емес: %1$S"

#~ msgid ""
#~ "can't use report URI with different scheme from originating document: %1$S"
#~ msgstr ""
#~ "Схемасы бастапқы құжаттың схемасынан өзгеше есептеме URI-ін қолдану "
#~ "мүмкін емес: %1$S"

#~ msgid ""
#~ "can't use report URI with different port from originating document: %1$S"
#~ msgstr ""
#~ "Порты бастапқы құжаттың портынан өзгеше есептеме URI-ін қолдану мүмкін "
#~ "емес: %1$S"
