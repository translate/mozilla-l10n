#. This and the other report.* strings should be as short as possible, ideally 2-3 words.
#: report.please
msgid "Send crash report"
msgstr "Құлау жөнінде хабарламаны жіберу"

#: report.submitting
msgid "Sending report…"
msgstr "Хабарлама жіберілуде…"

#: report.submitted
msgid "Crash report sent."
msgstr "Құлау жөнінде хабарлама жіберілді."

#: report.disabled
msgid "Crash reporting disabled."
msgstr "Құлау жөнінде хабарлау сөндірілген."

#: report.failed
msgid "Submission failed."
msgstr "Жіберу сәтсіз."

#: report.unavailable
msgid "No report available."
msgstr "Қолжетерлік хабарламалар жоқ."

#: report.comment
msgid "Add a comment (comments are publicly visible)"
msgstr "Түсініктеме қосу (түсініктеме әркімге көрінеді)"

#: report.pageURL
msgid "Include the page’s URL"
msgstr "Парақ сілтемесін қосып жіберу"

#: plugin.file
msgid "File"
msgstr "Файл"

#: plugin.mimeTypes
msgid "MIME Types"
msgstr "MIME түрлері"

#: plugin.flashProtectedMode.label
msgid "Enable Adobe Flash protected mode"
msgstr "Adobe Flash қорғалған режимін іске қосу"

#: plugin.enableBlocklists.label
msgid "Block dangerous and intrusive Flash content"
msgstr "Қауіпті және зиянкес Flash құрамасын блоктау"

