#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "पछिल्लो सिङ्कः %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "पछिल्लो सिङ्कः जारी छ…"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "खाता: %S"

#: notconnected.label
msgid "Not connected"
msgstr "जडान गरिएको छैन"

#: connecting.label
msgid "Connecting…"
msgstr "जडान हुँदैछ..."

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "तपाईंको Firefox Sync खाता हटाइएको छ"

#: notificationDisconnect.button
msgid "Undo"
msgstr "पूर्वस्थिति"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 Firefox Sync को नयाँ सँस्करणसँग उपयुक्त छैन। पछिल्लो संस्करणमा अद्यावधिक गर्नुहोस्।"

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 Firefox Sync को पुरानो सँस्करणसँग उपयुक्त छैन। पछिल्लो संस्करणमा अद्यावधिक "
"गर्नुहोस्। कृपया आफ्नो अन्य कम्प्युटर(हरू) मा Firefox अद्यावधिक गर्नुहोस्।"

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox Sync"

#: sync.update.button
msgid "Learn More"
msgstr "अझ जान्नुहोस्"

#: sync.update.close
msgid "Close"
msgstr "बन्द गर्नुहोस्"

#: sync.setup.error.title
#, fuzzy
msgid "Cannot Setup Sync"
msgstr "Sync सेट अप गर्न सकिएन"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "इन्टरनेट जडान उपलब्ध छैन"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr ""

#: sync.setup.tryagain
msgid "Try again"
msgstr "फेरि प्रयास गर्नुहोस्"

#: sync.setup.manual
msgid "Manual setup"
msgstr "हाते स्थापना"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr ""

#. # LOCALIZATION NOTE (promoBanner.message.text): First part of the message displayed in a
#. # banner on about:home. The final space separates this text from the link.
#: promoBanner.message.text
msgid "Sync is brand new and easier than ever. "
msgstr ""

