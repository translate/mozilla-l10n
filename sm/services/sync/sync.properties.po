#. # %1: the user name (Ed), %2: the app name (Firefox), %3: the operating system (Android)
#: client.name2
#, fuzzy
msgid "%1$S’s %2$S on %3$S"
msgstr "%1$S's %2$S i %3$S"

#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Tu'ufa'atasia mulimuli: %S"

#. # signInToSync.description is the tooltip for the Sync buttons when Sync is
#. # not configured.
#: signInToSync.description
msgid "Sign In To Sync"
msgstr ""

#: mobile.label
msgid "Mobile Bookmarks"
msgstr "Mobile Tusi-togi"

#: error.sync.title
msgid "Error While Syncing"
msgstr "Sesē tu'ufa'atasia"

#: error.sync.description
msgid ""
"Sync encountered an error while syncing: %1$S.  Sync will automatically "
"retry this action."
msgstr ""
"Sesē i le taimi tu'ufa'atasia le: %1$S.  Sync e te mana'o toe fai 'otometi."

#: warning.sync.eol.label
msgid "Service Shutting Down"
msgstr "Ua tāpunia le au'aunaga"

#. # %1: the app name (Firefox)
#: warning.sync.eol.description
msgid ""
"Your Firefox Sync service is shutting down soon. Upgrade %1$S to keep "
"syncing."
msgstr ""
"O le 'ā tāpunia le au'aunaga tu'ufa'atasia a lau Firefox. Fa'afou le %1$S ma "
"fa'atasia."

#: error.sync.eol.label
msgid "Service Unavailable"
msgstr "E lē avanoa le au'aunaga"

#. # %1: the app name (Firefox)
#: error.sync.eol.description
msgid ""
"Your Firefox Sync service is no longer available. You need to upgrade %1$S "
"to keep syncing."
msgstr ""
"E lē avanoa lou Firefox tu'ufa'atasia. E mana'omia ona e fa'afou le %1$S ma "
"tu'ufa'atasiaina."

#: sync.eol.learnMore.label sync.eol.learnMore.accesskey
msgid "&Learn more"
msgstr "&Mālamalama atili"

#: syncnow.label
msgid "Sync Now"
msgstr "&Tu'ufa'atasia taimi nei"

#: syncing2.label
msgid "Syncing…"
msgstr ""

#~ msgid "Remote tabs are being synced…"
#~ msgstr "Remote tama'i-ufitaga e tu'ufa'atasia i le taimi nei…"

#~ msgid "Sync your other devices again to access their tabs"
#~ msgstr "Toe tu'ufa'atasia lou isi togafiti e fai lenei tama'i-ufitaga"

#~ msgid "All remote tabs are already open"
#~ msgstr "'Ua 'uma ona fai tatala le remote tama'i-ufitaga"

#~ msgid "Recent desktop tabs will be available once they sync"
#~ msgstr ""
