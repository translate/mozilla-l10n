msgstr "Dikopi & Difoldara"

#: prefPanel-offline-and-diskspace
msgid "Offline & Disk Space"
msgstr "Sebaka se Diskeng & Ha ho sa Hokelwa Inthaneteng"

#: prefPanel-diskspace
msgid "Disk Space"
msgstr "Sebaka se Diskeng"

#: prefPanel-addressing
msgid "Composition & Addressing"
msgstr "Ho hlophisa le Ho aterese"

#: prefPanel-advanced
msgid "Advanced"
msgstr "E hatetseng pele"

## LOCALIZATION NOTE (prefPanel-smtp): Don't translate "SMTP"
#: prefPanel-smtp
msgid "Outgoing Server (SMTP)"
msgstr "Seva e Tswang (SMTP)"

# account manager multiple identity support
#LOCALIZATION NOTE: accountName: %1$S 
#: identity-list-title
msgid "Identities for %1$S"
msgstr "Boitsebiso bakeng sa %1$S"

#: identity-edit-req
msgid "You must specify a valid email address for this identity."
msgstr "O lokela ho fana ka aterese ya imeile bakeng sa boitsebiso bona."

#: identity-edit-req-title
msgid "Error Creating Identity"
msgstr "Phoso ya Ho etsa Boitsebiso"

#: choosefile
msgid "Choose a file"
msgstr "Kgetha faele"

