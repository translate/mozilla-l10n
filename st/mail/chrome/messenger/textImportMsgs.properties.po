#
# The following are used by the outlook express import code to display status/error 
# and informational messages
#
# Short name of import module
## @name TEXTIMPORT_NAME
## @loc None
#: 2000
msgid "Text file (LDIF, .tab, .csv, .txt)"
msgstr "Faele ya Mongolo (LDIF, .tab, .csv, .txt)"

# Description of import module
## @name TEXTIMPORT_DESCRIPTION
## @loc None
# (pofilter) endpunc: checks whether punctuation at the end of the strings match
#: 2001
msgid "Import an address book from a text file, including:  LDIF (.ldif, .ldi), tab-delimited (.tab, .txt) or comma-separated (.csv) formats."
msgstr "Romela-kahare buka ya diaterese ho tswa faeleng ya mongolo, ho kenyeletswa:  Fomate ya LDIF (.ldif, .ldi), ya meedi ya di-tab (.tab, .txt) kapa e arohantsweng ke feelwane (.csv)."

# Description of import module
## @name TEXTIMPORT_ADDRESS_NAME
## @loc None
#: 2002
msgid "Text Address Book"
msgstr "Buka ya Diaterese ya Mongolo"

# Description
## @name TEXTIMPORT_ADDRESS_SUCCESS
## @loc None
#: 2003
msgid "Imported address book %S"
msgstr "Romela-kahare buka ya diaterese ya %S"

# Error message
## @name TEXTIMPORT_ADDRESS_BADPARAM
## @loc None
#: 2004
msgid "Bad parameter passed to import address book."
msgstr "Pharamita e fosahetseng e fetisitswe bakeng sa ho romela-kahare buka ya diaterese."

# Error message
## @name TEXTIMPORT_ADDRESS_BADSOURCEFILE
## @loc None
#: 2005
msgid "Error accessing file for address book %S."
msgstr "Phoso e hlahile bakeng sa ho fihlella buka ya diaterese ya %S."

# Error message
## @name TEXTIMPORT_ADDRESS_CONVERTERROR
## @loc None
#: 2006
msgid "Error importing address book %S, all addresses may not have been imported."
msgstr "Phoso e hlahile bakeng sa ho romela-kahare buka ya diaterese ya %S, e ka nna ya ba diaterese tsohle di rometswe-kahare."

