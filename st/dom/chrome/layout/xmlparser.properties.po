# Map Expat error codes to error strings
#: 1
msgid "out of memory"
msgstr "e feletswe ke memori"

#: 2
msgid "syntax error"
msgstr "phoso ya sebopeho-puo"

#: 3
msgid "no element found"
msgstr "ha ho elemente e fumanweng"

#: 4
msgid "not well-formed"
msgstr "ha e a bopeha hantle"

#: 5
msgid "unclosed token"
msgstr "thokene e sa kwalwang"

#: 6
msgid "partial character"
msgstr "tlhaku e sa fellang"

#: 7
msgid "mismatched tag"
msgstr "tag e fapantsweng"

#: 8
msgid "duplicate attribute"
msgstr "popeho ya duplikeiti"

#: 9
msgid "junk after document element"
msgstr "matlakala kamora elemente ya tokomane"

#: 10
msgid "illegal parameter entity reference"
msgstr "referense ya pharamita ha e molaong"

#: 11
msgid "undefined entity"
msgstr "ntho e sa hlaloswang"

#: 12
msgid "recursive entity reference"
msgstr "referense ya 'recursive' bakeng sa ntho"

#: 13
msgid "asynchronous entity"
msgstr "ntho e hlaheng ka nako e le nngwe"

#: 14
msgid "reference to invalid character number"
msgstr "referense bakeng sa nomoro ya tlhaku e fosahetseng"

#: 15
msgid "reference to binary entity"
msgstr "referense bakeng sa ntho e habedi (binary)."

#: 16
msgid "reference to external entity in attribute"
msgstr "referense bakeng sa ntho e kantle ka popeho"

#: 17
msgid "xml declaration not at start of external entity"
msgstr "phatlalatso ya xml ha e qalehong ya ntho"

#: 18
msgid "unknown encoding"
msgstr "ho khoudwa ho sa tsejweng"

#: 19
msgid "encoding specified in XML declaration is incorrect"
msgstr "ho khoudwa ho hlalositsweng ka phatlalatso ya XML ho fosahetse."

#: 20
msgid "unclosed CDATA section"
msgstr "karolo ya CDATA e sa kwalwang"

#: 21
msgid "error in processing external entity reference"
msgstr "phoso e hlahile ho sebetsweng ha referense ya ntho e kantle"

#: 22
msgid "document is not standalone"
msgstr "tokomane ha se e ikemetseng"

#: 23
msgid "unexpected parser state"
msgstr "boemo ba 'sebopeho-puo' bo sa lebellwang"

#: 24
msgid "entity declared in parameter entity"
msgstr "ntho e phatlaladitswe ka paramitha ya ntho"

#: 27
msgid "prefix not bound to a namespace"
msgstr "ketapele ha e a matahana le sebakalebitso"

#: 29
msgid "reserved prefix (xml) must not be undeclared or bound to another namespace URI"
msgstr "ketapele e beheleditsweng (xml) ha e a lokela ho phatlalatsollwa kapa ho matahangwa ho lebitsosebaka URI le leng"

#: 30
msgid "reserved prefix (xmlns) must not be declared or undeclared"
msgstr "ketapele e beheleditsweng (xmlns) ha e a lokela ho phatlalatswa kapa ho phatlalatsollwa"

#: 31
msgid "prefix must not be bound to one of the reserved namespace URIs"
msgstr "ketapele ha e a lokela ho mahangwa le le leng la lebitsosebaka URI le leng la a beheleditsweng."

# %1$S is replaced by the Expat error string, may be followed by Expected (see below)
# %2$S is replaced by URL
# %3$d is replaced by line number
# %4$d is replaced by column number
#: XMLParsingError
msgid "XML Parsing Error: %1$S\n"
"Location: %2$S\n"
"Line Number %3$d, Column %4$d:"
msgstr "XML Phoso ya sebopeho-puo: %1$S\nTulo: %2$S\nNomoro ya Laene %3$d, Kholomo %4$d:"

# %S is replaced by a tag name.
# This gets appended to the error string if the error is mismatched tag.
#: Expected
msgid ". Expected: </%S>."
msgstr ". E lebeletsweng: </%S>."

