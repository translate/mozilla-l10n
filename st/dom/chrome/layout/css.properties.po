#: MimeNotCss
msgid "The stylesheet %1$S was not loaded because its MIME type, \"%2$S\", is not \"text/css\"."
msgstr "Setaeleshiti sa %1$S ha se a laelwa hobane mofuta wa MIME wa sona, \"%2$S\", ha se \"text/css\"."

#: MimeNotCssWarn
msgid "The stylesheet %1$S was loaded as CSS even though its MIME type, \"%2$S\", is not \"text/css\"."
msgstr "Setaeleshiti sa %1$S se jarilwe e le CSS le ha e le sa mofuta wa MIME, \"%2$S\", e se \"text/css\"."

#: PEUnexpEOF
msgid "Unexpected end of file while searching for %1$S."
msgstr "Ho fela ho sa lebellwang ha faele ha e ntse e batla %1$S."

#: PEParseRuleWSOnly
msgid "Whitespace-only string given to be parsed as rule."
msgstr "Mola wa bosweu feela o ananetswe e le setlamo."

#: PEDeclDropped
msgid "Declaration dropped."
msgstr "Phatlalatso e lahlilwe."

#: PEDeclSkipped
msgid "Skipped to next declaration."
msgstr "Ho fetetswe phatlalatsong e latelang."

#: PEUnknownProperty
msgid "Unknown property '%1$S'."
msgstr "Semelo se sa tsejweng '%1$S'."

#: PEPropertyParsingError
msgid "Error in parsing value for property '%1$S'."
msgstr "Phoso ya ho ngola boleng ba semelo sa '%1$S'."

#: PEExpectEndProperty
msgid "Expected end of value for property but found '%1$S'."
msgstr "Phelo e lebeletsweng ya boleng ba semelo sa '%1$S'."

#: PESkipAtRuleEOF
msgid "end of unknown at-rule"
msgstr "pheletso ya molawana-wa o sa tsejweng"

#: PEUnknownAtRule
msgid "Unrecognized at-rule or error parsing at-rule '%1$S'."
msgstr "Molawana-wa o sa tsehahatswang kapa phoso ho kenngweng ha molawana-wa '%1$S'."

#: PECharsetRuleEOF
msgid "charset string in @charset rule"
msgstr "mola wa charset ho molawana wa @charset"

#: PECharsetRuleNotString
msgid "Expected charset string but found '%1$S'."
msgstr "E lebeletse mola wa charset empa e fumane '%1$S'."

#: PEGatherMediaEOF
msgid "end of media list in @import or @media rule"
msgstr "pheletso ya lenane la media ho @import kapa molawana wa @media"

#: PEGatherMediaNotComma
msgid "Expected ',' in media list but found '%1$S'."
msgstr "Ho ne ho lebeletswe ',' lenaneng la media empa ho fumanwe '%1$S'."

#: PEGatherMediaNotIdent
msgid "Expected identifier in media list but found '%1$S'."
msgstr "Ho ne ho lebeletswe setsebahatsi lenaneng la media empa ho fumanwe '%1$S'."

#: PEImportNotURI
msgid "Expected URI in @import rule but found '%1$S'."
msgstr "Ho ne ho lebeletswe URI molawaneng wa ho @import empa ho fumanwe '%1$S'."

#: PEImportUnexpected
msgid "Found unexpected '%1$S' within @import."
msgstr "Ho fumanwe '%1$S' e sa lebellwang kahara @import."

#: PEGroupRuleEOF
msgid "end of @media or @-moz-document rule"
msgstr "pheletso ya @media kapa molawana wa @-moz-document"

# (pofilter) functions: checks to see that function names are not translated: url() url-prefix() domain()
#: PEMozDocRuleBadFunc
msgid "Expected url(), url-prefix(), or domain() in @-moz-document rule but found '%1$S'."
msgstr "Ho ne ho lebeletswe url(), sehlongwapele sa -url(), kapa domeini ya() molawaneng wa @-moz-document empa ha fumanwa '%1$S'."

#: PEMozDocRuleNotURI
msgid "Expected URI in @-moz-document rule but found '%1$S'."
msgstr "Ho ne ho lebeletswe URI molawaneng wa @-moz-document empa ho fumanwe '%1$S'."

#: PEAtNSPrefixEOF
msgid "namespace prefix in @namespace rule"
msgstr "sehlongwapele sa lebitsosebaka molaong wa @namespace"

#: PEAtNSURIEOF
msgid "namespace URI in @namespace rule"
msgstr "lebitsosebaka URI molaong wa @namespace"

#: PEAtNSUnexpected
msgid "Unexpected token within @namespace: '%1$S'."
msgstr "Tlotlo e sa lebellwang kahara @namespace: '%1$S'."

#: PESkipDeclBraceEOF
msgid "closing } of declaration block"
msgstr "ho kwalwa } ha boloko ba phatlalatso"

#: PESkipRSBraceEOF
msgid "closing } of invalid rule set"
msgstr "ho kwalwa } ha sete ya melawana e sa dumelleheng"

#: PEBadSelectorRSIgnored
msgid "Ruleset ignored due to bad selector."
msgstr "Sete ya molao ha e a natswa ka baka la sekgethi se sebe."

#: PESelectorListExtraEOF
msgid "',' or '{'"
msgstr "',' kapa '{'"

#: PESelectorListExtra
msgid "Expected ',' or '{' but found '%1$S'."
msgstr "Ho ne ho lebeletswe ',' kapa '{' empa ho fumanwe '%1$S'."

#: PESelectorGroupNoSelector
msgid "Selector expected."
msgstr "Sekgethi se lebeletswe."

#: PESelectorGroupExtraCombinator
msgid "Dangling combinator."
msgstr "Sehokahanyi se leketlang."

#: PEIDSelNotIdent
msgid "Expected identifier for ID selector but found '%1$S'."
msgstr "Ho ne ho le lebeletsweng sekgethi sa ID empa ho fumanwe '%1$S'."

#: PEIDSelEmpty
msgid "Expected non-empty name for ID selector, but found '%1$S'."
msgstr "Ho ne ho lebeletswe lebitso le sa tshelang la ID empa ho fumanwe '%1$S'."

#: PEClassSelEOF
msgid "class name"
msgstr "lebitso la mophato"

#: PEClassSelNotIdent
msgid "Expected identifier for class selector but found '%1$S'."
msgstr "Ho ne ho lebeletswe setsebahatsi sa mophato empa ho fumanwe '%1$S'."

#: PETypeSelEOF
msgid "element type"
msgstr "mofuta wa elemente"

#: PETypeSelNotType
msgid "Expected element name or '*' but found '%1$S'."
msgstr "Ho ne ho lebeletswe lebitso la elemente kapa '*' empa ho fumanwe '%1$S'."

#: PEUnknownNamespacePrefix
msgid "Unknown namespace prefix '%1$S'."
msgstr "Sehlongwapele sa lebitsosebaka le sa tsejweng '%1$S'."

#: PEAttributeNameEOF
msgid "attribute name"
msgstr "lebitso la popeho"

#: PEAttributeNameExpected
msgid "Expected identifier for attribute name but found '%1$S'."
msgstr "Ho ne ho lebeletswe setsebahatsi kapa bakeng sa lebitso la popeho empa ho fumanwe '%1$S'."

#: PEAttributeNameOrNamespaceExpected
msgid "Expected attribute name or namespace but found '%1$S'."
msgstr "Ho ne ho lebitso la popeho kapa lebitsosebaka empa ho fumanwe '%1$S'."

#: PEAttSelNoBar
msgid "Expected '|' but found '%1$S'."
msgstr "Ho ne ho lebeletswe '|' empa ho fumanwe '%1$S'."

#: PEAttSelInnerEOF
msgid "part of attribute selector"
msgstr "karolo ya sekgethi sa popeho"

#: PEAttSelUnexpected
msgid "Unexpected token in attribute selector: '%1$S'."
msgstr "Tlotlo e sa lebellwang sekgething sa popeho: '%1$S'."

#: PEAttSelValueEOF
msgid "attribute value"
msgstr "palo ya popeho"

#: PEAttSelCloseEOF
msgid "']' to end attribute selector"
msgstr "']' ho phetha sekgethi sa popeho"

#: PEAttSelNoClose
msgid "Expected ']' to terminate attribute selector but found '%1$S'."
msgstr "Ho ne ho lebeletswe ']' ho emisa sekgethi sa popeho empa ho fumanwe '%1$S'."

#: PEAttSelBadValue
msgid "Expected identifier or string for value in attribute selector but found '%1$S'."
msgstr "Ho ne ho lebeletswe setsebahatsi kapa mola bakeng sa palo sekgething sa popeho empa ho fumanwe '%1$S'."

#: PEPseudoSelEOF
msgid "name of pseudo-class or pseudo-element"
msgstr "lebitso la sekatlelase kapa sekaelemente"

#: PEPseudoSelBadName
msgid "Expected identifier for pseudo-class or pseudo-element but found '%1$S'."
msgstr "Ho ne ho lebeletswe sekgethi sa sekatlelase kapa sekaelemente empa ho fumanwe '%1$S'."

#: PEPseudoSelNonFunc
msgid "Function token for non-function pseudo-class or pseudo-element, or the other way around, when reading '%1$S'."
msgstr "Tlotlo ya ho se sebetse ha sekatlelase kapa seka elemente, kapa ka mokgwa o fetoletsweng, ha ho balwa '%1$S'."

#: PEPseudoSelNotPE
msgid "Expected pseudo-element but found '%1$S'."
msgstr "Ho ne ho lebeletswe sekaelemente empa ho fumanwe '%1$S'."

#: PEPseudoSelDoubleNot
msgid "Negation pseudo-class can't be negated '%1$S'."
msgstr "Kganano ya sekatlelase e ke ke ya hanwa '%1$S'."

#: PEPseudoSelPEInNot
msgid "Pseudo-elements can't be negated '%1$S'."
msgstr "Dielemente tsa sekatlelase di ke se hanelwe '%1$S'."

#: PEPseudoSelNewStyleOnly
msgid "This pseudo-element must use the \"::\" form: '%1$S'."
msgstr "Sekaelemente sena se tshwanetse ho sebedisa sebopeho sa \"::\": '%1$S'."

#: PEPseudoSelTrailing
msgid "Found trailing token after pseudo-element, which must be the last part of a selector:  '%1$S'."
msgstr "Ho fumanwe tlotlo e latelang kamora sekaelemente se lokelang ho ba karolo e qetellang ya sekgethi:  '%1$S'."

#: PEPseudoSelMultiplePE
msgid "Extra pseudo-element '%1$S'."
msgstr "Sekaelemente sa tlatsetso '%1$S'."

#: PEPseudoSelUnknown
msgid "Unknown pseudo-class or pseudo-element '%1$S'."
msgstr "Sekaelemente kapa sekatlelase se sa tsejweng '%1$S'."

#: PENegationEOF
msgid "selector within negation"
msgstr "sekgethi kahara kganano"

#: PENegationBadInner
msgid "Malformed simple selector as negation pseudo-class argument '%1$S'."
msgstr "Sekgethi se bonolo se qhwadileng se hanyetsana le sekatlelase sa '%1$S'."

#: PENegationNoClose
msgid "Missing closing ')' in negation pseudo-class '%1$S'."
msgstr "Phethelo ha eyo ')' sekatlelaseng se hanyetsang sa '%1$S'."

#: PENegationBadArg
msgid "Missing argument in negation pseudo-class '%1$S'."
msgstr "Ntlhataba ha eyo sekatlelaseng se hanyetsang sa '%1$S'."

# (review) please translate: selector
#: PELangArgEOF
msgid "argument to :lang selector"
msgstr "kganyetsano ho :lang selector"

#: PELangArgNotIdent
msgid "Expected identifier for lang pseudo-class parameter but found '%1$S'."
msgstr "Tsebahatso ya sekgethapuo e ne e lebeletswe empa ho fumanwe '%1$S'."

#: PELangNoClose
msgid "Missing closing ')' in lang pseudo-class '%1$S'."
msgstr "Phethelo ha eyo ')' sekatlelaseng sa puo se hanyetsang sa '%1$S'."

#: PELangNoArg
msgid "Missing argument in lang pseudo-class '%1$S'."
msgstr "Ntlhataba ha eyo kganyetsanong ya puo sekatlelaseng sa '%1$S'."

#: PESelectorEOF
msgid "selector"
msgstr "sekgethi"

#: PEBadDeclBlockStart
msgid "Expected '{' to begin declaration block but found '%1$S'."
msgstr "Ho ne ho lebeletswe '{' ho qala boloko ba phatlalatso empa ho fumanwe '%1$S'."

#: PEColorEOF
msgid "color"
msgstr "mmala"

#: PEColorNotColor
msgid "Expected color but found '%1$S'."
msgstr "Ho ne ho lebeletswe mmala empa ho fumanwe '%1$S'."

#: PEColorComponentEOF
msgid "color component"
msgstr "karolwana ya mmala"

#: PEExpectedPercent
msgid "Expected a percentage but found '%1$S'."
msgstr "Ho ne ho lebeletswe peresente empa ho fumanwe '%1$S'."

#: PEExpectedInt
msgid "Expected an integer but found '%1$S'."
msgstr "Ho ne ho lebeletswe integer empa ho fumanwe '%1$S'."

#: PEColorBadRGBContents
msgid "Expected number or percentage in rgb() but found '%1$S'."
msgstr "Ho ne ho lebeletswe palo kapa peresente ho rgb() empa ho fumanwe '%1$S'."

#: PEColorComponentBadTerm
msgid "Expected '%2$S' but found '%1$S'."
msgstr "Ho ne ho lebeletswe '%2$S' empa ho fumanwe '%1$S'."

#: PEColorHueEOF
msgid "hue"
msgstr "mebala ya 'hue'"

#: PEExpectedComma
msgid "Expected ',' but found '%1$S'."
msgstr "Ho ne ho lebeletswe ',' empa ho fumanwe '%1$S'."

#: PEColorSaturationEOF
msgid "saturation"
msgstr "boemo ba tlalo"

#: PEColorLightnessEOF
msgid "lightness"
msgstr "ho kganya"

#: PEColorOpacityEOF
msgid "opacity in color value"
msgstr "ho rothofala ha boleng ba mmala"

#: PEExpectedNumber
msgid "Expected a number but found '%1$S'."
msgstr "Ho ne ho lebeletswe nomoro empa ho fumanwe '%1$S'."

#: PEExpectedCloseParen
msgid "Expected ')' but found '%1$S'."
msgstr "Ho ne ho lebeletswe ')' empa ho fumanwe '%1$S'."

#: PEDeclEndEOF
msgid "';' or '}' to end declaration"
msgstr "';' kapa '}' ho qetella phatlalatso"

#: PEParseDeclarationNoColon
msgid "Expected ':' but found '%1$S'."
msgstr "Ho ne ho lebeletswe ':' empa ho fumanwe'%1$S'."

#: PEParseDeclarationDeclExpected
msgid "Expected declaration but found '%1$S'."
msgstr "Ho ne ho lebeletswe phatlalatso empa ho fumanwe '%1$S'."

#: PEEndOfDeclEOF
msgid "end of declaration"
msgstr "pheletso ya phatlatso"

#: PEImportantEOF
msgid "important"
msgstr "sa bohlokwa"

#: PEExpectedImportant
msgid "Expected 'important' but found '%1$S'."
msgstr "Ho ne ho lebeletswe 'sa bohlokwa' empa ho fumanwe '%1$S'."

#: PEBadDeclEnd
msgid "Expected ';' to terminate declaration but found '%1$S'."
msgstr "Ho ne ho lebeletswe ';' kgaotso ya phatlalatso empa ho fumanwe '%1$S'."

#: PEBadDeclOrRuleEnd
msgid "Expected ';' or '}' to terminate declaration but found '%1$S'."
msgstr "Ho ne ho lebeletswe ';' kapa '}' kgaotso ya phatlalatso empa ho fumanwe '%1$S'."

#: PEInaccessibleProperty
msgid "Attempt to use inaccessible property"
msgstr "Teko ya ho sebedisa thepa e sa fihlelleheng"

#: PECommentEOF
msgid "end of comment"
msgstr "pheletso ya tlhahiso"

#: SEUnterminatedString
msgid "Found unclosed string '%1$S'."
msgstr "Ho fumanwe mola o sa kwalwang wa '%1$S'."

