#: tabsTab.label
msgid "Tabs"
msgstr "Di-tab"

#: openExternalLinksIn.label
msgid "Open links from other applications in:"
msgstr "Bula mahokela ho tswa ditshebetsong tse ding ho:"

#: newWindow.label
msgid "a new window"
msgstr "fenstere e ntjha"

#: newTabInRecentWindow.label
msgid "a new tab in the most recent window"
msgstr "tab e ntjha fenstereng ya moraorao ka ho fetisa"

#: recentWindowOrTab.label
msgid "the most recent tab/window"
msgstr "fenstere/tab ya moraorao ka ho fetisa"

#: forceNewWindowLinks.label
msgid "Force links that open new windows to open in:"
msgstr "E tlamella mahokela a bulang difenstere tse ntjha ho:"

#: sameTabOrWindow.label
msgid "the same tab/window as the link"
msgstr "fenstere/tab e tshwanang le lehokela"

#: newTab.label
msgid "a new tab"
msgstr "tab e ntjha"

#: hideTabBar.label
msgid "Hide the tab bar when only one web site is open"
msgstr "Pata mola wa tab ha setsha sa inthanete se le seng feela se butswe"

#: loadInBackground.label
msgid "Select new tabs opened from links"
msgstr "Kgetha di-tab tse ntjha tse butsweng mahokeleng"

#: loadBookmarksInBackground.label
msgid "Select new tabs opened from bookmarks or history"
msgstr "Kgetha di-tab tse ntjha tse butsweng ditshwayabukeng kapa historing"

#: warnOnClose.label
msgid "Warn when closing multiple tabs"
msgstr "Lemosa ha e kwala di-tab tse ngata"

