#: lHeader
msgid "Composer"
msgstr "Sehlophisi"

#: rHeader
msgid "Settings for authoring of new Web pages"
msgstr "Disetwa tsa bongodi ba maqephe a matjha a Webe"

#: saving
msgid "Saving"
msgstr "E a boloka"

#: AutoSaveCheck
msgid "Automatically save every"
msgstr "Boloka ka ho iketsa"

#: minText
msgid "minutes"
msgstr "metsotso "

#: exterLegend.label
msgid "External Editors"
msgstr "Bahlophisi ba Kantle"

#: htmlSource
msgid "HTML Source:"
msgstr "Mohlodi wa HTML:"

#: imageeditor
msgid "Images:"
msgstr "Ditshwantsho:"

#: chooseButton.label
msgid "Choose"
msgstr "Kgetha"

#: recentFiles.title
msgid "Recent Pages Menu"
msgstr "Menu ya Maqephe a Moraorao"

#: documentsInMenu.label
#: documentsInMenu.accesskey
msgid "Maximum &number of pages listed:"
msgstr "Palo e kahodimodimo ya &maqephe a fanweng:"

#: savingFiles.title
msgid "When Saving or Publishing Pages"
msgstr "Ha e Boloka kapa e Phatlalatsa Maqephe"

#: preserveExisting.label
#: preserveExisting.accesskey
msgid "&Preserve original source formatting"
msgstr "&Sireletsa mohlodi wa motheo wa ho fomata"

#: preserveExisting.tooltip
msgid "Preserves line breaks and page's original formatting"
msgstr "E sireletsa mapomelo a mela le ho fomatwa ha leqephe ha motheo"

#: saveAssociatedFiles.label
#: saveAssociatedFiles.accesskey
msgid "&Save images and other associated files when saving pages"
msgstr "&E boloka ditshwantsho le difaele tse ding tse amanang le tsona ha e boloka maqephe"

#: showPublishDialog.label
#: showPublishDialog.accesskey
msgid "&Always show Publish dialog when publishing pages"
msgstr "&Kamehla bontsha Phatlalatso ya puisano ha o phatlalatsa maqephe"

#: composerEditing.label
msgid "Editing"
msgstr "Ho edita"

#: maintainStructure.label
#: maintainStructure.accesskey
msgid "&Maintain table layout when inserting or deleting cells"
msgstr "&Tsweletsa pele moralo wa tafole ha o kenya kapa o phumula disele"

#: maintainStructure.tooltip
msgid "Preserves table's rectangular shape by automatically adding cells after inserting or deleting cells"
msgstr "Sireletsa sebopeho sa rekthenkele sa tafole ka ho eketsa disele ka tsela e iketsang kamora ho kenya kapa ho phumula disele"

#: useCSS.label
#: useCSS.accesskey
msgid "&Use CSS styles instead of HTML elements and attributes"
msgstr "&Sebedisa ditaele tsa CSS ho fapana le dielemente tsa HTML le dipopeho"

#: crInPCreatesNewP.label
#: crInPCreatesNewP.accesskey
msgid "&Return in a paragraph always creates a new paragraph"
msgstr "&Ho tobetsa konopo ya Kgutla serapeng kamehla ho etsa serapa se setjha"

