# Window title
#: windowTitle.label
msgid "Publish Page"
msgstr "Phatlalatsa Leqephe"

#: windowTitleSettings.label
msgid "Publish Settings"
msgstr "Phatlalatsa Disetwa"

#: publishTab.label
msgid "_: publishTab.label\n"
"Publish"
msgstr "Phatlalatsa"

#: settingsTab.label
msgid "Settings"
msgstr "Disetwa"

#: publishButton.label
msgid "_: publishButton.label\n"
"Publish"
msgstr "Phatlalatsa"

# Publish Tab Panel
#: siteList.label
msgid "_: siteList.label\n"
"Site Name:"
msgstr "Lebitso la Setsha:"

#: siteList.tooltip
msgid "Choose the site that you want to publish to"
msgstr "Kgetha setsha seo o batlang ho phatlalatsa ho sona"

#: newSiteButton.label
msgid "New Site"
msgstr "Setsha se Setjha"

#: siteDirList.label
msgid "Select or type a directory name in your publishing site:"
msgstr "Kgetha kapa o thaepe lebitso la daerektri setsheng sa hao sa ho phatlalatsa:"

#: siteDirList.tooltip
msgid "Choose or enter the name of the remote subdirectory to publish to"
msgstr "Kgetha kapa kenya lebitso la daerektri e nyane e thoko ho phatlalatsa ho yona"

#: docDirList.label
msgid "Site subdirectory for this page:"
msgstr "Daerektri e nyane ya setsha bakeng sa leqephe lena:"

#: publishImgCheckbox.label
msgid "Include images and other files"
msgstr "Kenyeletsa ditshwantsho le difaele tse ding"

#: publishImgCheckbox.tooltip
msgid "Publish images and other files referenced by this page"
msgstr "Phatlalatsa ditshwantsho le difaele tse ding tse hlalositsweng leqepheng lena"

#: sameLocationRadio.label
msgid "Use same location as page"
msgstr "Sebedisa sebaka se le seng le leqephe lena"

#: sameLocationRadio.tooltip
msgid "Publish files to the same location as the page"
msgstr "Phatlalatsa difaele sebakeng se le seng le leqephe lena"

#: useSubdirRadio.label
msgid "Use this site subdirectory:"
msgstr "Sebedisa daerektri e nyane ya setsha sena:"

#: useSubdir.tooltip
msgid "Choose or enter name of remote subdirectory where files will be published"
msgstr "Kgetha kapa kenya lebitso la daerektri e nyane e thoko moo o difaele di tla phatlalatswa"

#: chooseDirButton.label
msgid "Choose..."
msgstr "Kgetha..."

#: pageTitle.label
msgid "Page Title:"
msgstr "Thaetlele ya Leqephe:"

#: pageTitle.tooltip
msgid "Enter a title to identify the page in the window and in bookmarks"
msgstr "Kenya thaetlele ho tsebahatsa leqephe fenstereng le ho ditshwayabuka"

#: pageTitleExample.label
msgid "e.g.: &quot;My Web Page&quot;"
msgstr "e.g.: &qots;Leqephe La ka la Webe&qots;"

#: filename.label
msgid "Filename:"
msgstr "Leqephe la faele:"

#: filename.tooltip
msgid "Enter a name for this file, including '.html' for a web page"
msgstr "Kenya lebitso bakeng sa faele ena, ho kenyeletswa '.html' bakeng sa leqephe la webe"

#: filenameExample.label
msgid "e.g,: &quot;mypage.html&quot;"
msgstr "e.g,: &qots;leqephelaka.html&qots;"

#: relatedDocs.label
msgid "Other files to publish"
msgstr "Fifaele tse ding tse phatlalatswang"

#: setDefaultButton.label
msgid "Set as Default"
msgstr "Seta ho ba sa kamehla"

#: setDefSiteAndDir.label
msgid "Set as Default Site and Directory"
msgstr "Seta ho ba Setsha le Daerektri tsa Kamehla"

#: removeButton.label
msgid "Remove Site"
msgstr "Tlosa Setsha"

# Settings Tab Panel
#: publishSites.label
msgid "Publishing Sites"
msgstr "Ho Phatlalatsa Ditsha"

#: serverInfo.label
msgid "Server Information"
msgstr "Tlhahisoleseding ka seva"

#: loginInfo.label
msgid "Login Information"
msgstr "Tlhahisoleseding ka Ho kena"

#: siteName.label
msgid "_: siteName.label\n"
"Site Name:"
msgstr "Lebitso la Setsha:"

#: siteName.tooltip
msgid "A nickname that identifies this publishing site (e.g.: 'MySite')"
msgstr "Lebitso la boswaswi le tsebahatsang setsha sena sa ho phatlalatsa (jwalo ka: 'Setaele saka')"

#: siteUrl.label
msgid "Publishing address (e.g.: 'ftp://ftp.myisp.com/myusername'):"
msgstr "Aterese ya ho phatlalatsa (jwalo ka: 'ftp://ftp.myisp.com/lebitsolakalamosebedisi'):"

#: siteUrl.tooltip
msgid "The FTP:// or HTTP:// address provided by your ISP or web hosting service"
msgstr "Aterese ya FTP:// kapa HTTP:// tse fanweng ke ISP ya hao kapa ba fanang ka ditsha"

#: browseUrl.label
msgid "HTTP address of your homepage (e.g.: 'http://www.myisp.com/myusername'):"
msgstr "Aterese ya HTTP bakeng sa leqephe la kamehla la hao (jwalo ka: 'http://www.myisp.com/lebitsolakalamosebedisi'):"

#: browseUrl.tooltip
msgid "The HTTP:// address of your home directory (don't include filename)"
msgstr "Aterese ya HTTP:// ya daerektri ya hao ya lapeng (ha e kenyeletse lebitso la faele)"

#: username.label
msgid "User name:"
msgstr "Lebitso la mosebedisi:"

#: username.tooltip
msgid "The user name you use to log in to your ISP or web hosting service"
msgstr "Lebitso la mosebedisi leo o le sebedisang ho kena ho ISP ya hao kapa tshebeletso ya phano ya ditsha tsa webe"

#: password.label
msgid "Password:"
msgstr "Phasewete:"

#: password.tooltip
msgid "The password associated with your user name"
msgstr "Phasewete ena e amana le lebitso la hao la mosebedisi"

#: savePassword.label
msgid "Save Password"
msgstr "Boloka Phasewete"

#: savePassword.tooltip
msgid "Select this to save your password securely using Password Manager"
msgstr "Kgetha sena ho boloka phasewete ya hao ka polokeho o sebedisa Taolo ya Phasewete"

