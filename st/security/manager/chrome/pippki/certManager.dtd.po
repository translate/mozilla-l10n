msgstr "Ditefikeiti tsa Hao"

#: certmgr.tab.others
msgid "Other People's"
msgstr "Tsa Batho ba bang"

#: certmgr.tab.websites
msgid "Web Sites"
msgstr "Ditsha tsa Inthanete"

#: certmgr.tab.ca
msgid "Authorities"
msgstr "Bolaodi"

#: certmgr.ca.builtins
msgid "Built-in CAs"
msgstr "Di-CA tse Ahelletsweng"

#: certmgr.ca.mycas
msgid "CAs Managed By You"
msgstr "Di-CA tse Laolwang ke Wena"

#: certmgr.mine
msgid "You have certificates from these organizations that identify you:"
msgstr "O na le disetefikeiti tse tswang mekgatlong ena tse o tsebahatsang:"

#: certmgr.others
msgid "You have certificates on file that identify these people:"
msgstr "O na le disetefikeiti faeleng tse tsebahatsang batho bana:"

#: certmgr.websites
msgid "You have certificates on file that identify these web sites:"
msgstr "O na le disetefikeiti faeleng tse tsebahatsang ditsha tsena tsa inthanete:"

#: certmgr.cas
msgid "You have certificates on file that identify these certificate authorities:"
msgstr "O na le disetefikeiti faeleng tse tsebahatsang disetifikeiti tsena tsa bolaodi:"

#: certmgr.detail.general_tab.title
msgid "General"
msgstr "Tse akaretsang"

#: certmgr.detail.prettyprint_tab.title
msgid "Details"
msgstr "Dintlha"

#: certmgr.subjectinfo.label
msgid "Issued To"
msgstr "Se Fanwe Ho"

#: certmgr.issuerinfo.label
msgid "Issued By"
msgstr "Se Fanwe Ke"

#: certmgr.validity.label
msgid "Validity"
msgstr "Nepahalo"

#: certmgr.fingerprints.label
msgid "Fingerprints"
msgstr "Kgatisomenwana"

