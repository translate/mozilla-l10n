#: cmd.pause.label cmd.pause.accesskey
msgid "&Pause"
msgstr "నిలుపు"

#: cmd.resume.label cmd.resume.accesskey
msgid "&Resume"
msgstr "తిరిగికొనసాగించు"

#: cmd.cancel.label cmd.cancel.accesskey
msgid "&Cancel"
msgstr "రద్దుచేయి"

#: cmd.show.label cmd.show.accesskey
msgid "Open Containing &Folder"
