msgid "Sync"
msgstr "Sync"

#: sync.notconnected
msgid "Not connected"
msgstr "ไม่ได้เชื่อมต่อ"

#: sync.connect
msgctxt "sync.connect"
msgid "Connect"
msgstr "เชื่อมต่อ"

#: sync.connected
msgid "Connected"
msgstr "เชื่อมต่ออยู่"

#: sync.details
msgid "Details"
msgstr "รายละเอียด"

