#: loadError.label
msgid "Problem loading page"
msgstr "Zmylk při čitanju strony"

#: retry.label
msgid "Try Again"
msgstr "Hišće raz spytać"

#. Specific error messages
#: connectionFailure.title
msgid "Unable to connect"
msgstr "Zwisk móžno njeje"

#: connectionFailure.longDesc
msgctxt "connectionFailure.longDesc"
msgid "&sharedLongDesc;"
msgstr "&sharedLongDesc;"

#: deniedPortAccess.title
msgid "This address is restricted"
msgstr "Tuta adresa je wobmjezowana"

#: deniedPortAccess.longDesc
msgctxt "deniedPortAccess.longDesc"
msgid ""
msgstr " "

#: dnsNotFound.title
msgid "Server not found"
msgstr "Serwer njebu namakany"

#: dnsNotFound.longDesc
msgid ""
"<ul> \n"
"<li>Check the address for typing errors such as \n"
"<strong>ww</strong>.example.com instead of \n"
"<strong>www</strong>.example.com</li> \n"
"<li>If you are unable to load any pages, check your computer's network \n"
"connection.</li> \n"
"<li>If your computer or network is protected by a firewall or proxy, make "
"sure \n"
"that &brandShortName; is permitted to access the Web.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Přepruwujće adresu za pisanskimi zmylkami kaž \n"
"<strong>ww</strong>.přikład.de město \n"
"<strong>www</strong>.přikład.de</li> \n"
"<li>Jeli njemóžeće někotre strony čitać, skontrolujće syćowy zwisk \n"
"swojeho ličaka.</li> \n"
"<li>Jeli waš ličak abo syć so z wohnjowej murju abo proksy škita, "
"zawěsćće, \n"
"zo &brandShortName; smě na Web přistup měć.</li> \n"
"</ul>"

#: fileNotFound.title
msgid "File not found"
msgstr "Dataja njebu namakana"

#: fileNotFound.longDesc
msgid ""
"<ul> \n"
"<li>Check the file name for capitalization or other typing errors.</li> \n"
"<li>Check to see if the file was moved, renamed or deleted.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Přepruwujće datajowe mjeno za wulkopisanskimi abo hinašimi pisanskimi "
"zmylkami.</li> \n"
"<li>Přepruwujće, hač je so dataja přesunyła, přemjenowała abo wotstroniła.</"
"li> \n"
"</ul>"

#: generic.title
msgid "Oops."
msgstr "Hopla."

#: generic.longDesc
msgid "<p>&brandShortName; can't load this page for some reason.</p>"
msgstr "<p>&brandShortName; njemóže tutu stronu z někajkeje přičiny čitać.</p>"

#: malformedURI.title
msgid "The address isn't valid"
msgstr "Adresa płaćiwa njeje"

#: malformedURI.longDesc
msgid ""
"<ul> \n"
"<li>Web addresses are usually written like \n"
"<strong>http://www.example.com/</strong></li> \n"
"<li>Make sure that you're using forward slashes (i.e. \n"
"<strong>/</strong>).</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Webadresy so z wašnjom kaž \n"
"<strong>http://www.přikład.de/</strong> pisaja.</li> \n"
"<li>Zawěsćće, zo wužiwaće doprědka nachilene nakósne smužki (t.j. \n"
"<strong>/</strong>).</li> \n"
"</ul>"

#: netInterrupt.title
msgid "The connection was interrupted"
msgstr "Zwisk bu přetorhnjeny"

#: netInterrupt.longDesc
msgctxt "netInterrupt.longDesc"
msgid "&sharedLongDesc;"
msgstr "&sharedLongDesc;"

#: notCached.title
msgid "Document Expired"
msgstr "Dokument je njepłaćiwy"

#: notCached.longDesc
msgid ""
"<p>The requested document is not available in &brandShortName;'s cache.</"
"p><ul><li>As a security precaution, &brandShortName; does not automatically "
"re-request sensitive documents.</li><li>Click Try Again to re-request the "
"document from the website.</li></ul>"
msgstr ""
"<p>Požadany dokument w pufrowaku &brandShortName; k dispoziciji njesteji.</"
"p><ul><li>Jako wěstnotnu naprawu &brandShortName; delikatne dokumenty "
"awtomatisce znowa njewotwołuje.</li><li>Klikńće na Hišće raz, zo byšće "
"dokument hišće raz z websydła wotwołał.</li></ul>"

#: netOffline.title
msgid "Offline mode"
msgstr "Offline-modus"

#: netOffline.longDesc2
msgid ""
"<ul> \n"
"<li>Press \"Try Again\" to switch to online mode and reload the page.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Klikńće na \"Hišće raz\", zo byšće do online-modusa přešoł a stronu "
"znowa začitał.</li> \n"
"</ul>"

#: contentEncodingError.title
msgid "Content Encoding Error"
msgstr "Zmylk při kodowanju wobsaha"

#: contentEncodingError.longDesc
msgctxt "contentEncodingError.longDesc"
msgid ""
"<ul> \n"
"<li>Please contact the website owners to inform them of this problem.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Prošu stajće so z wobsedźerjemi websydła do zwiska, zo byšće jich wo "
"tutym problemje informował.</li> \n"
"</ul>"

#: unsafeContentType.title
msgid "Unsafe File Type"
msgstr "Njewěsty datajowy typ"

#: unsafeContentType.longDesc
msgctxt "unsafeContentType.longDesc"
msgid ""
"<ul> \n"
"<li>Please contact the website owners to inform them of this problem.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Prošu stajće so z wobsedźerjemi websydła do zwiska, zo byšće jich wo "
"tutym problemje informował.</li> \n"
"</ul>"

#: netReset.title
msgid "The connection was reset"
msgstr "Zwisk bu wróćo stajeny"

#: netReset.longDesc
msgctxt "netReset.longDesc"
msgid "&sharedLongDesc;"
msgstr "&sharedLongDesc;"

#: netTimeout.title
msgid "The connection has timed out"
msgstr "Zwisk je čas překročił"

#: netTimeout.longDesc
msgctxt "netTimeout.longDesc"
msgid "&sharedLongDesc;"
msgstr "&sharedLongDesc;"

#: unknownProtocolFound.title
msgid "The address wasn't understood"
msgstr "Adresa njebu zrozumjena"

#: unknownProtocolFound.longDesc
msgid ""
"<ul> \n"
"<li>You might need to install other software to open this address.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Snano dyrbiće druhe programy instalować, zo by so tuta adresa wočiniła.</"
"li> \n"
"</ul>"

#: proxyConnectFailure.title
msgid "The proxy server is refusing connections"
msgstr "Proksy-serwer zwiski wotpokazuje"

#: proxyConnectFailure.longDesc
msgid ""
"<ul> \n"
"<li>Check the proxy settings to make sure that they are correct.</li> \n"
"<li>Contact your network administrator to make sure the proxy server is \n"
"working.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Přepruwujće proksy-nastajenja, zo by so zawěsćiło, zo wone su korektne.</"
"li> \n"
"<li>Skontaktujće swojeho syćoweho administratora, zo by so zawěsćiło, \n"
"zo proksy-serwer dźěła.</li> \n"
"</ul>"

#: proxyResolveFailure.title
msgid "Unable to find the proxy server"
msgstr "Njemóžno proksy-serwer namakać"

#: proxyResolveFailure.longDesc
msgid ""
"<ul> \n"
"<li>Check the proxy settings to make sure that they are correct.</li> \n"
"<li>Check to make sure your computer has a working network connection.</"
"li> \n"
"<li>If your computer or network is protected by a firewall or proxy, make "
"sure \n"
"that &brandShortName; is permitted to access the Web.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Přepruwujće proksy-nastajenja, zo by so zawěsćiło, zo su korektne.</"
"li> \n"
"<li>Pruwujće, hač waš ličak ma fungowacy syćowy zwisk.</li> \n"
"<li>Jeli waš ličak abo syć so z wohnjowej murju abo proksy škita, "
"zawěsćće, \n"
"zo &brandShortName; smě na Web přistup měć.</li> \n"
"</ul>"

#: redirectLoop.title
msgid "The page isn't redirecting properly"
msgstr "Strona njesprawje posrědkuje"

#: redirectLoop.longDesc
msgid ""
"<ul> \n"
"<li>This problem can sometimes be caused by disabling or refusing to "
"accept \n"
"cookies.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Tutón problem so druhdy přeze znjemóžnjenje abo wotpokazowanje plackow \n"
"zawinuje.</li> \n"
"</ul>"

#: unknownSocketType.title
msgid "Unexpected response from server"
msgstr "Njewočakowana wotmołwa ze serwera"

#: unknownSocketType.longDesc
msgid ""
"<ul> \n"
"<li>Check to make sure your system has the Personal Security Manager \n"
"installed.</li> \n"
"<li>This might be due to a non-standard configuration on the server.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Pruwujće, hač Personal Security Manager je instalowany \n"
"na wašim systemje.</li> \n"
"<li>Přičina je snano njestandardna konfiguracija na wašim serwerje.</li> \n"
"</ul>"

#: nssFailure2.title
msgctxt "nssFailure2.title"
msgid "Secure Connection Failed"
msgstr "Wěsty zwisk njemóžno"

#: nssFailure2.longDesc2
msgid ""
"<ul> \n"
"<li>The page you are trying to view cannot be shown because the authenticity "
"of the received data could not be verified.</li> \n"
"<li>Please contact the website owners to inform them of this problem.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Strona, kotruž chceće sej wobhladać, njeda so pokazać, dokelž "
"awtentiskosć přijatych datow njeda so přepruwować.</li> \n"
"<li>Prošu stajće so z wobsedźerjemi websydła do zwiska, zo byšće jich wo "
"tutym problemje informował.</li> \n"
"</ul>"

#: nssBadCert.title
msgctxt "nssBadCert.title"
msgid "Secure Connection Failed"
msgstr "Wěsty zwisk njemóžno"

#: nssBadCert.longDesc2
msgid ""
"<ul> \n"
"<li>This could be a problem with the server's configuration, or it could "
"be \n"
"someone trying to impersonate the server.</li> \n"
"<li>If you have connected to this server successfully in the past, the error "
"may \n"
"be temporary, and you can try again later.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>To móhło problem z konfiguraciju serwera być, abo móhło być, \n"
"zo něchtó pospytuje serwer imitować.</li> \n"
"<li>Jeli sće ze serwerom w zańdźenosći wuspěšnje zwjazany był, móhł zmylk "
"snano \n"
"nachwilny być a móžeće pozdźišo hišće raz spytać.</li> \n"
"</ul>"

#: sharedLongDesc
msgid ""
"<ul> \n"
"<li>The site could be temporarily unavailable or too busy. Try again in a "
"few \n"
"moments.</li> \n"
"<li>If you are unable to load any pages, check your computer's network \n"
"connection.</li> \n"
"<li>If your computer or network is protected by a firewall or proxy, make "
"sure \n"
"that &brandShortName; is permitted to access the Web.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Sydło móhło nachwilu k dispoziciji njestać abo přećežene być. Spytajcé "
"za mało \n"
"wokomikow hišće raz.</li> \n"
"<li>Jeli njemóžeće někotre strony začitać, přepruwujće zwisk wašeje "
"ličakoweje \n"
"syće.</li> \n"
"<li>Jeli waš ličak abo syć so z wohnjowej murju abo proksy škita, "
"zawěsćće, \n"
"zo &brandShortName; smě na Web přistup měć.</li> \n"
"</ul>"

#: malwareBlocked.title
msgid "Suspected Attack Site!"
msgstr "Na nadpadowace sydło so tuka!"

#: malwareBlocked.longDesc
msgid ""
"<p>Attack sites try to install programs that steal private information, use "
"your computer to attack others, or damage your system.</p> \n"
"<p>Website owners who believe their site has been reported as an attack site "
"in error may <a href='http://www.stopbadware.org/home/reviewinfo' >request a "
"review</a>.</p>"
msgstr ""
"<p>Nadpadowace sydła pospytuja programy instalować, kotrež priwatne "
"informacije kradnu, waš ličak wužiwać, zo bychu druhe nadpadowali abo waš "
"system wobškodźeja.</p> \n"
"<p>Wobsedźerjo websydła, kotřiž wěrja, zo je so jich sydło zmylnje jako "
"nadpadowace sydło zdźěliło, móža wo <a href='http://www.stopbadware.org/home/"
"reviewinfo' >přepruwowanje prosyć</a>.</p>"

#: unwantedBlocked.title
msgid "Suspected Unwanted Software Site!"
msgstr ""

#: unwantedBlocked.longDesc
msgid ""
"<p>Unwanted software pages try to install software that can be deceptive and "
"affect your system in unexpected ways.</p>"
msgstr ""

#: phishingBlocked.title
msgid "Suspected Web Forgery!"
msgstr "Na webfalšowanje so tuka!"

#: phishingBlocked.longDesc
msgid ""
"<p>Entering any personal information on this page may result in identity "
"theft or other fraud.</p> \n"
"<p>These types of web forgeries are used in scams known as phishing attacks, "
"in which fraudulent web pages and emails are used to imitate sources you may "
"trust.</p>"
msgstr ""
"<p>Zapodawanje wosobinskich informacijow na tutej stronje móže ke kradnjenju "
"identity abo druhemu wobsudstwu wjesć.</p> \n"
"<p>Tute typy webwobsudstwow wužiwaja so w jebanstwach jako čušlace nadpady, "
"w kotrychž wobšudne webstrony a e-mejle so wužiwaja, zo bychu žórła "
"imitowali, kotrymž dowěrjeće.</p>"

#: cspBlocked.title
msgid "Blocked by Content Security Policy"
msgstr "Přez wěstotne prawidła za wobsah zablokowany"

#: cspBlocked.longDesc
msgid ""
"<p>&brandShortName; prevented this page from loading in this way because the "
"page has a content security policy that disallows it.</p>"
msgstr ""
"<p>&brandShortName; je začitanju tuteje strony na tute wašnje zadźěwał, "
"dokelž strona ma wěstotne prawidła za wobsah, kotrež to njedowoleja.</p>"

#: corruptedContentError.title
msgid "Corrupted Content Error"
msgstr "Zmylk - wobškodźeny wobsah"

#: corruptedContentError.longDesc
msgid ""
"<p>The page you are trying to view cannot be shown because an error in the "
"data transmission was detected.</p><ul><li>Please contact the website owners "
"to inform them of this problem.</li></ul>"
msgstr ""
"<p>Strona, kotruž chceće sej wobhladać, njeda so pokazać, dokelž je so zmylk "
"při přenošowanju datow namakał.</p><ul><li>Prošu stajće so z wobsedźerjemi "
"websydła do zwiska, zo byšće jich wo tutym problemje informował.</li></ul>"

#: securityOverride.linkText
msgid "Or you can add an exception…"
msgstr "Abo móžeće wuwzaće přidać…"

#: securityOverride.getMeOutOfHereButton
msgid "Get me out of here!"
msgstr "Chcu tu won!"

#: securityOverride.exceptionButtonLabel
msgid "Add Exception…"
msgstr "Wuwzaće přidać…"

#. <!-- LOCALIZATION NOTE (securityOverride.warningContent) - Do not translate the
#. contents of the <button> tags. It uses strings already defined above. The
#. button is included here (instead of netError.xhtml) because it exposes
#. functionality specific to firefox.
#: securityOverride.warningContent
msgid ""
"<p>You should not add an exception if you are using an internet connection "
"that you do not trust completely or if you are not used to seeing a warning "
"for this server.</p> \n"
" \n"
"<button id='getMeOutOfHereButton'>&securityOverride.getMeOutOfHereButton;</"
"button> \n"
"<button id='exceptionDialogButton'>&securityOverride.exceptionButtonLabel;</"
"button>"
msgstr ""
"<p>Njebyšće měł wuwzaće přidać, jeli internetny zwisk wužiwaće, kotremuž "
"cyle njedowěrjeće abo jeli njewidźiće warnowanje za tutón serwer.</p> \n"
" \n"
"<button id='getMeOutOfHereButton'>&securityOverride.getMeOutOfHereButton;</"
"button> \n"
"<button id='exceptionDialogButton'>&securityOverride.exceptionButtonLabel;</"
"button>"

#: errorReporting.title
msgid "Report this error"
msgstr "Tutón zmylk zdźělić"

#: errorReporting.longDesc
msgid ""
"Reporting the address and certificate information for <span id='hostname'></"
"span> will help us identify and block malicious sites. Thanks for helping "
"create a safer web!"
msgstr ""
"Zdźělenje adresy a certifikatowych informacijow za <span id='hostname'></"
"span> budźe nam pomhać, złowólne sydła identifikować a blokować. Wulki dźak, "
"zo pomhaće, wěsćiši web wutworić!"

#: errorReporting.automatic
msgid "Automatically report errors in the future"
msgstr "W přichodźe zmylki awtomatisce zdźělić"

#: errorReporting.learnMore
msgid "Learn more…"
msgstr "Dalše informacije…"

#: errorReporting.sending
msgid "Sending report"
msgstr "Rozprawa so sćele"

#: errorReporting.sent
msgid "Report sent"
msgstr "Rozprawa je so pósłała"

#: errorReporting.report
msgid "Report"
msgstr "Zdźělić"

#: errorReporting.tryAgain
msgid "Try again"
msgstr "Hišće raz spytać"

#: remoteXUL.title
msgid "Remote XUL"
msgstr "Zdaleny XUL"

#: remoteXUL.longDesc
msgid ""
"<p><ul><li>Please contact the website owners to inform them of this problem."
"</li></ul></p>"
msgstr ""
"<p><ul><li>Prošu stajće so z wobsedźerjemi websydła do zwiska, zo byšće jich "
"wo problemje informował.</li></ul></p>"

#: sslv3Used.title
msgid "Unable to Connect Securely"
msgstr "Wěsty zwisk móžno njeje"

#. <!-- LOCALIZATION NOTE (sslv3Used.longDesc) - Do not translate
#. "ssl_error_unsupported_version".
#: sslv3Used.longDesc
msgid "Advanced info: ssl_error_unsupported_version"
msgstr "Rozšěrjena informacija: ssl_error_unsupported_version"

#: sslv3Used.learnMore
msgid "Learn More…"
msgstr "Dalše informacije…"

#~ msgid ""
#~ "&brandShortName; cannot guarantee the safety of your data on <span "
#~ "id='hostname'></span> because it uses SSLv3, a broken security protocol."
#~ msgstr ""
#~ "&brandShortName; njemóže wěstotu wašich datow na <span id='hostname'></"
#~ "span> garantować, dokelž SSLv3 wužiwa, wobškodźeny wěstotny protokol."

#~ msgid "Advanced info: <span id='errorcode'></span>"
#~ msgstr "Rozšěrjena informacija: <span id='errorcode'></span>"

#~ msgid ""
#~ "<ul> \n"
#~ "<li>The page you are trying to view cannot be shown because the "
#~ "authenticity of the received data could not be verified.</li> \n"
#~ "<li>Please contact the website owners to inform them of this problem. "
#~ "Alternatively, use the command found in the help menu to report this "
#~ "broken site.</li> \n"
#~ "</ul>"
#~ msgstr ""
#~ "<ul> \n"
#~ "<li>Strona, kotruž chceće sej wobhladać, njeda so pokazać, dokelž "
#~ "awtentiskosć přijatych datow njeda so přepruwować.</li> \n"
#~ "<li>Prošu stajće so z wobsedźerjemi websydła do zwiska, zo byšće jich wo "
