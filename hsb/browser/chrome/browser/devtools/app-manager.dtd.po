msgid "Allow"
msgstr "Dowolić"

#: device.allowTooltip
msgid "This permission is allowed for apps of this type"
msgstr "Tute prawo je za nałoženja tutoho typa dowolene"

#: device.prompt
msgid "Prompt"
msgstr "Namołwa"

#: device.promptTooltip
msgid "This permission requires a user prompt for apps of this type"
msgstr "Tute prawo wužaduje sej wužiwarsku namołwu za nałoženja tutoho typa"

#: device.deny
msgid "Deny"
msgstr "Wotpokazać"

#: device.denyTooltip
