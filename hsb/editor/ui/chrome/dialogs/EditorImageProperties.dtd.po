#: imageLocationTab.label
msgid "Location"
msgstr "Adresa"

#: imageDimensionsTab.label
msgid "Dimensions"
msgstr "Wotměry"

#: imageAppearanceTab.label
msgid "Appearance"
msgstr "Napohlad"

#: imageLinkTab.label
msgid "Link"
msgstr "Wotkaz"
