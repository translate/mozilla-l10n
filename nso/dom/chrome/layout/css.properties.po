#: MimeNotCss
msgid "The stylesheet %1$S was not loaded because its MIME type, \"%2$S\", is not \"text/css\"."
msgstr "Mokgwa wa letlakala %1$S ga se wa bulwa ka baka la gore ya mohuta wa MIME, \"%2$S\", ga se \"text/css\"."

#: MimeNotCssWarn
msgid "The stylesheet %1$S was loaded as CSS even though its MIME type, \"%2$S\", is not \"text/css\"."
msgstr "Mokgwa wa letlakala %1$S e hlahletšwe bjalo ka CSS le ge e le gore ke ya mohuta wa MIME, \&quot;%2$S\&quot;, ga se \&quot;text/css\&quot;."

#: PEUnexpEOF
msgid "Unexpected end of file while searching for %1$S."
msgstr "Go fela ga faele go go sa lelelwago mola go nyakwa %1$S."

#: PEParseRuleWSOnly
msgid "Whitespace-only string given to be parsed as rule."
msgstr "Sekgoba se sešweu seo se filwego phase bjalo ka molao."

#: PEDeclDropped
msgid "Declaration dropped."
msgstr "Kgoeletšo e lahlilwe."

#: PEDeclSkipped
msgid "Skipped to next declaration."
msgstr "Fetetšwe go kgoeletšo ye e latelago."

#: PEUnknownProperty
msgid "Unknown property '%1$S'."
msgstr "Pharologantšho ye e sa tsebegego '%1$S'."

#: PEPropertyParsingError
msgid "Error in parsing value for property '%1$S'."
msgstr "Phošo go boleng bja phasing ya pharologantšho '%1$S'."

#: PEExpectEndProperty
msgid "Expected end of value for property but found '%1$S'."
msgstr "Mafelelo a boleng bja pharologantšho bjoo  bo letetšwego eupša go hweditšwe '%1$S'."

#: PESkipAtRuleEOF
msgid "end of unknown at-rule"
msgstr "mafelelo a at-rule ye e sa tsebegego"

#: PEUnknownAtRule
msgid "Unrecognized at-rule or error parsing at-rule '%1$S'."
msgstr "At-rule ye e sa amogelwego goba at-rule ya phasing '%1$S'."

#: PECharsetRuleEOF
msgid "charset string in @charset rule"
msgstr "thapo ya tšhasete ka go @molao wa tšhasete"

#: PECharsetRuleNotString
msgid "Expected charset string but found '%1$S'."
msgstr "Thapo ya tšhasete ye e letetšwego eupša gwa hwetšwa '%1$S'."

#: PEGatherMediaEOF
msgid "end of media list in @import or @media rule"
msgstr "mafelelo a lenaneo la media ka go @amogela go tšwa ka ntle goba @molao wa pegoditaba"

#: PEGatherMediaNotComma
msgid "Expected ',' in media list but found '%1$S'."
msgstr "Letetšwe',' ka go lenaneo la pegoditaba eupša gwa hwetšwa '%1$S'."

#: PEGatherMediaNotIdent
msgid "Expected identifier in media list but found '%1$S'."
msgstr "Sešupi se e letetšwego ka go lenaneo la pegoditaba eupša gwa hwetšwa '%1$S'."

#: PEImportNotURI
msgid "Expected URI in @import rule but found '%1$S'."
msgstr "Letetšwego URI go @molao wa se amogetšwego eupša gwa hwetšwa '%1$S'."

#: PEImportUnexpected
msgid "Found unexpected '%1$S' within @import."
msgstr "Hweditšwe tše sa letelwago '%1$S' ka gare ga @se amogetšwego."

#: PEGroupRuleEOF
msgid "end of @media or @-moz-document rule"
msgstr "mafelelo a @pegoditaba goba @-moz-molao wa tokumente"

#: PEMozDocRuleBadFunc
msgid "Expected url(), url-prefix(), or domain() in @-moz-document rule but found '%1$S'."
msgstr "Letetšwego url(), url-prefix(), goba legoro() ka go @-moz-molao wa tokumente eupša gwa hwetšwa '%1$S'."

#: PEMozDocRuleNotURI
msgid "Expected URI in @-moz-document rule but found '%1$S'."
msgstr "Letetšwego URI ka go @-moz-molao wa tokumente eupša gwa hwetšwa '%1$S'."

#: PEAtNSPrefixEOF
msgid "namespace prefix in @namespace rule"
msgstr "hlogo ya sekgobaina ka go @molao wa sekgobaina"

#: PEAtNSURIEOF
msgid "namespace URI in @namespace rule"
msgstr "sekgobaina sa URI ka go @molao wa sekgobaina"

#: PEAtNSUnexpected
msgid "Unexpected token within @namespace: '%1$S'."
msgstr "Token ye e sa letelwago ka go @sekgobaina: '%1$S'."

#: PESkipDeclBraceEOF
msgid "closing } of declaration block"
msgstr "go tswalela } ga poloko ya kgoeletšo"

#: PESkipRSBraceEOF
msgid "closing } of invalid rule set"
msgstr "tswalelo } ya sete ya molao wo o ego wa makgonthe"

#: PEBadSelectorRSIgnored
msgid "Ruleset ignored due to bad selector."
msgstr "Sete ya molao ga se e obamelwe ka lebaka la sekgethi se sebe."

#: PESelectorListExtraEOF
msgid "',' or '{'"
msgstr "\',' or '{'"

#: PESelectorListExtra
msgid "Expected ',' or '{' but found '%1$S'."
msgstr "Letetšwego ',' goba '{' eupša gwa hwetšwa '%1$S'."

#: PESelectorGroupNoSelector
msgid "Selector expected."
msgstr "Sekgethi se letetšwe."

#: PESelectorGroupExtraCombinator
msgid "Dangling combinator."
msgstr "Sehlakanyi se se lekeletšego."

#: PEIDSelNotIdent
msgid "Expected identifier for ID selector but found '%1$S'."
msgstr "Sehlaodi se emetšwego ya sekgethi sa ID eupša go hweditšwe '%1$S'."

#: PEIDSelEmpty
msgid "Expected non-empty name for ID selector, but found '%1$S'."
msgstr "Leina le le se nago selo le letetšwego la sekgethi sa ID, eupša gwa hwetšwa '%1$S'."

#: PEClassSelEOF
msgid "class name"
msgstr "leina la legoro"

#: PEClassSelNotIdent
msgid "Expected identifier for class selector but found '%1$S'."
msgstr "Sehlaodi se e letetšwego sa sekgethi sa leina eupša gwa hwetšwa '%1$S'."

#: PETypeSelEOF
msgid "element type"
msgstr "mohuta wa elemente"

#: PETypeSelNotType
msgid "Expected element name or '*' but found '%1$S'."
msgstr "Leina le le letetšwego la elemente goba '*' eupša gwa hwetšwa '%1$S'."

#: PEUnknownNamespacePrefix
msgid "Unknown namespace prefix '%1$S'."
msgstr "Hlogo ya sekgobaina se se tsebjego '%1$S'."

#: PEAttributeNameEOF
msgid "attribute name"
msgstr "leina la setho"

#: PEAttributeNameExpected
msgid "Expected identifier for attribute name but found '%1$S'."
msgstr "Sehlaodi se letetšwego sa leina la ditho eupša gwa hwetšwa '%1$S'."

#: PEAttributeNameOrNamespaceExpected
msgid "Expected attribute name or namespace but found '%1$S'."
msgstr "Leina le letetšwego la attribute goba sekgobaina eupša gwa hwetšwa '%1$S'."

#: PEAttSelNoBar
msgid "Expected '|' but found '%1$S'."
msgstr "Letetšwego '|' eupša gwa hwetšwa '%1$S'."

#: PEAttSelInnerEOF
msgid "part of attribute selector"
msgstr "karolo ya sekgethi sa attribute"

#: PEAttSelUnexpected
msgid "Unexpected token in attribute selector: '%1$S'."
msgstr "Mpho ye e sa letelwago ka go sekgethi sa setho: '%1$S'."

#: PEAttSelValueEOF
msgid "attribute value"
msgstr "boleng bja setho"

#: PEAttSelCloseEOF
msgid "']' to end attribute selector"
msgstr "\']' go fetša sekgethi sa setho"

#: PEAttSelNoClose
msgid "Expected ']' to terminate attribute selector but found '%1$S'."
msgstr "Letetšwego ']' go fediša sekgethi sa setho eupša gwa hwetšwa '%1$S'."

#: PEAttSelBadValue
msgid "Expected identifier or string for value in attribute selector but found '%1$S'."
msgstr "Sehlaodi se letetšwego goba thapo ya boleng  ka go sekgethi sa setho eupša gwa hwetšwa '%1$S'."

#: PEPseudoSelEOF
msgid "name of pseudo-class or pseudo-element"
msgstr "leina la legoro la bofora goba elemente ya bofora"

#: PEPseudoSelBadName
msgid "Expected identifier for pseudo-class or pseudo-element but found '%1$S'."
msgstr "Sehlaodi se letetšwego sa legoro la bofora goba elemente ya bofora eupša gwa hwetšwa '%1$S'."

#: PEPseudoSelNonFunc
msgid "Function token for non-function pseudo-class or pseudo-element, or the other way around, when reading '%1$S'."
msgstr "Token ya mošomo ye e sego ya mošomo ya legoro la bofora goba element ya bofora, goba ka tsela ye nngwe, ge go balwa '%1$S'."

#: PEPseudoSelNotPE
msgid "Expected pseudo-element but found '%1$S'."
msgstr "Elemente ya bofora ye e letetšwego eupša go hweditšwe '%1$S'."

#: PEPseudoSelDoubleNot
msgid "Negation pseudo-class can't be negated '%1$S'."
msgstr "Lgoro la bofora e ka se ganetšwe '%1$S'."

#: PEPseudoSelPEInNot
msgid "Pseudo-elements can't be negated '%1$S'."
msgstr "Dielemente tša bofora di ka se ganetšwe '%1$S'."

#: PEPseudoSelNewStyleOnly
msgid "This pseudo-element must use the \"::\" form: '%1$S'."
msgstr "Elemente ye ya bofora e swanetše go šomiša \&quot;::\&quot; form: '%1$S'."

#: PEPseudoSelTrailing
msgid "Found trailing token after pseudo-element, which must be the last part of a selector:  '%1$S'."
msgstr "Hweditše mpho ya morago ga elemente ya bofora, yeo e swanetšego go ba karolo ya mafelelo ya sekgethi:  '%1$S'."

#: PEPseudoSelMultiplePE
msgid "Extra pseudo-element '%1$S'."
msgstr "Elemente ya bofora ya extra '%1$S'."

#: PEPseudoSelUnknown
msgid "Unknown pseudo-class or pseudo-element '%1$S'."
msgstr "Legoro la bofora goba elemente ya bofora ye e sa tsebegego '%1$S'."

#: PENegationEOF
msgid "selector within negation"
msgstr "sekgethi ka gare ga kganetšo"

#: PENegationBadInner
msgid "Malformed simple selector as negation pseudo-class argument '%1$S'."
msgstr "Sekgethi se bonolo sa go hloka sebopego bjalo ka ngangišano ya legoro le e sego la nnete la kganetšo '%1$S'."

#: PENegationNoClose
msgid "Missing closing ')' in negation pseudo-class '%1$S'."
msgstr "Go tswalela ga go timela ')' ka go legoro la bofora la kganetšo '%1$S'."

#: PENegationBadArg
msgid "Missing argument in negation pseudo-class '%1$S'."
msgstr "Ngangišano ya go timela ka go legoro la bofora go kganetšo '%1$S'."

#: PELangArgEOF
msgid "argument to :lang selector"
msgstr "ngangišano go:sekgethi sa lang"

#: PELangArgNotIdent
msgid "Expected identifier for lang pseudo-class parameter but found '%1$S'."
msgstr "Sehlaodi sa kganetšo sa pharametha ya legoro la bofora la lang eupša gwa hwetšwa '%1$S'."

#: PELangNoClose
msgid "Missing closing ')' in lang pseudo-class '%1$S'."
msgstr "Tswalelo ya go timela ')' go legoro la bofora go lang '%1$S'."

#: PELangNoArg
msgid "Missing argument in lang pseudo-class '%1$S'."
msgstr "Ngangišano ya go timela go legoro la bofora la lang '%1$S'."

#: PESelectorEOF
msgid "selector"
msgstr "sekgethi"

#: PEBadDeclBlockStart
msgid "Expected '{' to begin declaration block but found '%1$S'."
msgstr "Letetšwe '{' go thoma poloko ya kgoeletšo eupša gwa hwetšwa '%1$S'."

#: PEColorEOF
msgid "color"
msgstr "mmala"

#: PEColorNotColor
msgid "Expected color but found '%1$S'."
msgstr "Mmala wo o letetšwego eupša gwa hwetšwa '%1$S'."

#: PEColorComponentEOF
msgid "color component"
msgstr "sehlangwa sa mmala"

#: PEExpectedPercent
msgid "Expected a percentage but found '%1$S'."
msgstr "Phesente ye e letetšwego eupša gwa hwetšwa '%1$S'."

#: PEExpectedInt
msgid "Expected an integer but found '%1$S'."
msgstr "Intheka ye e letetšwego eupša gwa hwetšwa '%1$S'."

#: PEColorBadRGBContents
msgid "Expected number or percentage in rgb() but found '%1$S'."
msgstr "Palo ye e letetšwego goba diphesente ka rgb() eupša gwa hwetšwa '%1$S'."

#: PEColorComponentBadTerm
msgid "Expected '%2$S' but found '%1$S'."
msgstr "Letetšwego '%2$S' eupša gwa hwetšwa '%1$S'."

#: PEColorHueEOF
msgid "hue"
msgstr "mogoma"

#: PEExpectedComma
msgid "Expected ',' but found '%1$S'."
msgstr "Letetšwego ',' eupša gwa hwetšwa '%1$S'."

#: PEColorSaturationEOF
msgid "saturation"
msgstr "go koloba"

#: PEColorLightnessEOF
msgid "lightness"
msgstr "bobebe"

#: PEColorOpacityEOF
msgid "opacity in color value"
msgstr "ophasiti go boleng bja mmala"

#: PEExpectedNumber
msgid "Expected a number but found '%1$S'."
msgstr "Letetšwe palo eupša gwa hwetšwa '%1$S'."

#: PEExpectedCloseParen
msgid "Expected ')' but found '%1$S'."
msgstr "Letetšwe ')' eupša gwa hwetšwa '%1$S'."

#: PEDeclEndEOF
msgid "';' or '}' to end declaration"
msgstr "\';' goba '}' go fetša kgoeletšo"

#: PEParseDeclarationNoColon
msgid "Expected ':' but found '%1$S'."
msgstr "Letetšwe ':' eupša gwa hwetšwa '%1$S'."

#: PEParseDeclarationDeclExpected
msgid "Expected declaration but found '%1$S'."
msgstr "Kgoeletšo ye e letetšwego eupša gwa hwetšwa '%1$S'."

#: PEEndOfDeclEOF
msgid "end of declaration"
msgstr "mafelelo a kgoeletšo"

#: PEImportantEOF
msgid "important"
msgstr "bohlokwa"

#: PEExpectedImportant
msgid "Expected 'important' but found '%1$S'."
msgstr "Letetšwego 'bohlokwa' eupša gwa hwetšwa '%1$S'."

#: PEBadDeclEnd
msgid "Expected ';' to terminate declaration but found '%1$S'."
msgstr "Letetšwego ';' go fediša kgoeletšo eupša gwa hwetšwa '%1$S'."

#: PEBadDeclOrRuleEnd
msgid "Expected ';' or '}' to terminate declaration but found '%1$S'."
msgstr "Letetšwego ';' goba '}' go fediša kgoeletšo eupša gwa hwetšwa '%1$S'."

#: PEInaccessibleProperty
msgid "Attempt to use inaccessible property"
msgstr "Leka go šomiša pharologanyo ye e sa fihlelelwego"

#: PECommentEOF
msgid "end of comment"
msgstr "mafelelo a ditshwayoswayo"

#: SEUnterminatedString
msgid "Found unclosed string '%1$S'."
msgstr "Go hweditšwe thapo ye e sa utullwago '%1$S'."

