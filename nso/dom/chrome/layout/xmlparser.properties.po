#. # Map Expat error codes to error strings
#: 1
msgid "out of memory"
msgstr "feletšwe ke polokelo"

#: 2
msgid "syntax error"
msgstr "phošo ya popafoko"

#: 3
msgid "no element found"
msgstr "ga go a hwetšwa karolwana"

#: 4
msgid "not well-formed"
msgstr "ga se ya hlangwa gabotse"

#: 5
msgid "unclosed token"
msgstr "sešupo se sa tswalelwago"

#: 6
msgid "partial character"
msgstr "tlhaka ya go hlaola"

#: 7
msgid "mismatched tag"
msgstr "tshwayo yeo e sa swanelago"

#: 8
msgid "duplicate attribute"
msgstr "seka sa kopi"

#: 9
msgid "junk after document element"
msgstr "matlakala ka morago ga karolwana ya tokumente"

#: 10
msgid "illegal parameter entity reference"
msgstr "tšhupetšo ya sehlongwa sa pharamethara se e sego sa semolao"

#: 11
msgid "undefined entity"
msgstr "sehlongwa se sa hlaloswago"

#: 12
msgid "recursive entity reference"
msgstr "tšhupetšo ya sehlongwa se diragalago gape"

#: 13
msgid "asynchronous entity"
msgstr "sehlongwa se sa diregego ka nako e tee"

#: 14
msgid "reference to invalid character number"
msgstr "tšhupetšo go nomoro ya tlhaka ye e sa šomego"

#: 15
msgid "reference to binary entity"
msgstr "tšhupetšo go sehlongwa sa gabedi"

#: 16
msgid "reference to external entity in attribute"
msgstr "tšhupetšo go sehlongwa sa ka ntle go tlhaolo"

#: 17
msgid "XML or text declaration not at start of entity"
msgstr "Kgoeletšo ya XML goba sengwalwa ga e mathomong a sehlongwa"

#: 18
msgid "unknown encoding"
msgstr "phetošo ye e sa tsebjego"

#: 19
msgid "encoding specified in XML declaration is incorrect"
msgstr "phetošo yeo e laeditšwego go kgoeletšo ya XML ga se ya maleba"

#: 20
msgid "unclosed CDATA section"
msgstr "karolo ya CDATA ye e sa tswalelwago"

#: 21
msgid "error in processing external entity reference"
msgstr "phošo go sepetšeng tšhupetšo ya sehlongwa sa ka ntle"

#: 22
msgid "document is not standalone"
msgstr "tokumente ga se tšhikanoši"

#: 23
msgid "unexpected parser state"
msgstr "seemo sa sefetleki seo se sa letelwago"

#: 24
msgid "entity declared in parameter entity"
msgstr "sehlongwa seo se goeleditšwego go sehlongwa sa pharamethara"

#: 27
msgid "prefix not bound to a namespace"
msgstr "hlogwana ga se ya ithekga ka sekgobaina"

#: 28
msgid "must not undeclare prefix"
msgstr "ga se ya swanela go se goeletše hlogwana"

#: 29
msgid "incomplete markup in parameter entity"
msgstr "tshwayo e sa felelago go sehlongwa sa pharamethara"

#: 30
msgid "XML declaration not well-formed"
msgstr "kgoeletšo ya XML ga ya bopša gabotse"

#: 31
msgid "text declaration not well-formed"
msgstr "kgoeletšo ya sengwalwa ga ya bopša gabotse"

#: 32
msgid "illegal character(s) in public id"
msgstr "(di)tlhaka tšeo di sego semolaong go ID yeo e lego phatlalatša"

#: 38
msgid ""
"reserved prefix (xml) must not be undeclared or bound to another namespace "
"name"
msgstr ""
"hlogwana yeo e beetšwego thoko (xml) ga ya swanela go se goeletšwe goba go "
"ithekga ka sekgobaina se sengwe"

#: 39
msgid "reserved prefix (xmlns) must not be declared or undeclared"
msgstr ""
"hlogwana yeo e beetšwego thoko (xmlns) ga ya swanela go goeletšwa goba go se "
"goeletšwe"

#: 40
msgid "prefix must not be bound to one of the reserved namespace names"
msgstr ""
"hlogwana ga ya swanela go ithekga go ye nngwe ya dikgobaina tše di beetšwego "
"thoko"

#. # %1$S is replaced by the Expat error string, may be followed by Expected (see below)
#. # %2$S is replaced by URL
#. # %3$u is replaced by line number
#. # %4$u is replaced by column number
#: XMLParsingError
msgid ""
"XML Parsing Error: %1$S\n"
"Location: %2$S\n"
"Line Number %3$u, Column %4$u:"
msgstr ""
"Phošo ya go fetleka ya XML: %1$S\n"
"Lefelo: %2$S\n"
"Nomoro ya mothaladi %3$u, Kholomo %4$u:"

#. # %S is replaced by a tag name.
#. # This gets appended to the error string if the error is mismatched tag.
#: Expected
msgid ". Expected: </%S>."
msgstr ". Letetšwego: </%S>."

#~ msgid "xml declaration not at start of external entity"
#~ msgstr "kgoeletšo ya xml ga e go mathomo a selo sa ka ntle"
