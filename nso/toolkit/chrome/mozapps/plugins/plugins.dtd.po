msgstr "Hlahlela letlakala gape"

#: reloadPlugin.post
msgid " to try again."
msgstr " gore o leke gape."

#. This and the other report.* strings should be as short as possible, ideally 2-3 words.
#: report.please
msgid "Send crash report"
msgstr "Romela pego ya go pšhatlega"

#: report.submitting
msgid "Sending report…"
msgstr "Go romela pego…"

#: report.submitted
msgid "Crash report sent."
msgstr "Pego ya go pšhatlega e rometšwe."

#: report.disabled
msgid "Crash reporting disabled."
msgstr "Pego ya go pšhatlega e šitišitšwe."

#: report.failed
msgid "Submission failed."
msgstr "Go iša go padile."

#: report.unavailable
msgid "No report available."
msgstr "Ga go na pego yeo e lego gona."
