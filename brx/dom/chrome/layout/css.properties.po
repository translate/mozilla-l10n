#: MimeNotCss
msgid ""
"The stylesheet %1$S was not loaded because its MIME type, \"%2$S\", is not "
"\"text/css\"."
msgstr ""
"स्टाइलसिट %1$S खौ ल'ड खालामाखैमोन मानोना बेनि MIME रोखोम, \"%2$S\", आ \"text/css"
"\" नङा।"

#: MimeNotCssWarn
msgid ""
"The stylesheet %1$S was loaded as CSS even though its MIME type, \"%2$S\", "
"is not \"text/css\"."
msgstr ""
"स्टाइलसिट %1$S खौ बेनि MIME रोखोम, \"%2$S\" ब्लाबो CSS महरै ल'ड खालामाखैमोन, "
"\"text/css\" नङा।"

#: PEUnexpEOF2
msgid "Unexpected end of file while searching for %1$S."
msgstr "%1$S नि थाखाय नागिरनाय समाव फाइला मिजिंथियै जोबबाय।"

#: PEParseRuleWSOnly
msgid "Whitespace-only string given to be parsed as rule."
msgstr "गुफुर-जायगा खान्थि बादि पार्स खालामनो गोनां होखानाय स्ट्रिं।"

#: PEDeclDropped
msgid "Declaration dropped."
msgstr "फोसावथायखौ गारबाय।"

#: PEDeclSkipped
msgid "Skipped to next declaration."
msgstr "उननि फोसावथायखौ बारलांबाय।"

#: PEUnknownProperty
msgid "Unknown property '%1$S'."
msgstr "मोन्थिमोनै आखुथाय '%1$S'."

#: PEValueParsingError
msgid "Error in parsing value for '%1$S'."
msgstr "'%1$S' नि थाखाय बेसेन पार्स खालामनायाव जानाय गोरोन्थि"

#: PEExpectEndValue
msgid "Expected end of value but found '%1$S'."
msgstr "मिजिंथिनाय जोबथा बेसेन नाथाय '%1$S' मोनायखै।"

#: PERuleTrailing
msgid "Expected end of rule but found '%1$S'."
msgstr "नियमनि जोबथा मिजिंथिबाय नाथाय '%1$S' मोनबाय।"

#: PESkipAtRuleEOF2
msgid "end of at-rule"
msgstr "जोबथा नियम"

#: PEUnknownAtRule
msgid "Unrecognized at-rule or error parsing at-rule '%1$S'."
msgstr "रुल-आव सिनायथि खालामै एबा रुल-आव '%1$S' पार्सिं गोरोन्थि।"

#: PECharsetRuleEOF
msgid "charset string in @charset rule"
msgstr "चारसेट रुल @ आव चारसेट स्ट्रिं"

#: PECharsetRuleNotString
msgid "Expected charset string but found '%1$S'."
msgstr "चारसेट स्ट्रिं मिजिंथिबाय नाथाय '%1$S' मोनबाय।"

#: PEGatherMediaEOF
msgid "end of media list in @import or @media rule"
msgstr "@लाबोनाय एबा @मेडिया रुल आव मेडिया फारिलाइनि जोबनाय।"

#: PEGatherMediaNotComma
msgid "Expected ',' in media list but found '%1$S'."
msgstr "',' मेडिया फारिलाइआव मिजिंथिबाय नाथाय '%1$S' मोनबाय ।"

#: PEGatherMediaNotIdent
msgid "Expected identifier in media list but found '%1$S'."
msgstr "मेडिया फारिलाइआव सिनायथि होग्रा मिजिंथिबाय नाथाय %1$S' मोनबाय।"

#: PEGatherMediaReservedMediaType
msgid "Found reserved keyword '%1$S' when looking for media type."
msgstr "मेडिया रोखोम नागिरनाय समाव थिखानाय गाहाय सोदोब '%1$S' मोनबाय।"

#: PEParseSourceSizeListEOF
msgid "length value for matched media condition"
msgstr "गोरोबनाय मेडिया थासारिनि थाखाय गोलाव बेसेन"

#: PEParseSourceSizeListNotComma
msgid "Expected ',' after value but found '%1$S'"
msgstr "बेसेननि उनाव ',' मिजिंथिबाय नाथाय '%1$S' मोनबाय"

#: PEImportNotURI
msgid "Expected URI in @import rule but found '%1$S'."
msgstr " @ लाबोनाय रुलआव URI मिजिंथिबाय नाथाय %1$S' मोनबाय।"

#: PEImportBadURI
msgid "Invalid URI in @import rule: '%1$S'."
msgstr "@import rule आव बाहायजायै URI: '%1$S'."

#: PEImportUnexpected
msgid "Found unexpected '%1$S' within @import."
msgstr "@लाबोनायनि सिङाव मिजिंथियै %1$S' मोनबाय।"

#: PEGroupRuleEOF2
msgid "end of @media, @supports or @-moz-document rule"
msgstr "@media, @supports एबा @-moz-document नियमनि जोबथा"

#: PEGroupRuleNestedAtRule
msgid "%1$S rule not allowed within @media or @-moz-document rule."
msgstr "%1$S rule आ @mediaएबा@-moz-document rule नि सिङाव गनायथि होआ।"

#: PEMozDocRuleBadFunc2
msgid ""
"Expected url(), url-prefix(), domain() or regexp() in @-moz-document rule "
"but found '%1$S'."
msgstr ""
"@-moz-document नियमआव url(), url-prefix(), domain() एबा regexp() मिजिंथिबाय, "
"नाथाय '%1$S' मोनबाय।"

#: PEMozDocRuleNotURI
msgid "Expected URI in @-moz-document rule but found '%1$S'."
msgstr "@-moz-फोरमान बिलाइ रुलआव URI मिजिंथिबाय नाथाय %1$S' मोनबाय।"

#: PEMozDocRuleNotString
msgid ""
"Expected string in @-moz-document rule regexp() function but found '%1$S'."
msgstr ""
"@-moz-document नियम regexp() खामानिआव स्ट्रिं मिजिंथिबाय नाथाय '%1$S' मोनबाय।"

#: PEMozDocRuleEOF
msgid "next URI in @-moz-document rule"
msgstr "@-moz-document नेमआव उननि URI"

#: PEAtNSPrefixEOF
msgid "namespace prefix in @namespace rule"
msgstr "@मुंजायगा रुलआव मुंजायगा सिगां दाजाबदा"

#: PEAtNSURIEOF
msgid "namespace URI in @namespace rule"
msgstr "@मुंजायगा रुलआव मुंजायगा URI "

#: PEAtNSUnexpected
msgid "Unexpected token within @namespace: '%1$S'."
msgstr "@मुंजायगा सिङाव मिजिंथियै टकेन: '%1$S'."

#: PEKeyframeNameEOF
msgid "name of @keyframes rule."
msgstr "@keyframes rule नि मुं"

#: PEKeyframeBadName
msgid "Expected identifier for name of @keyframes rule."
msgstr "@keyframes rule नि मुंनि थाखाय मिजिंथिनाय सिनायथिहोग्रा"

#: PEKeyframeBrace
msgid "Expected opening { of @keyframes rule."
msgstr "{ of @keyframes rule खेवनायखौ मिजिंथिबाय"

#: PESkipDeclBraceEOF
msgid "closing } of declaration block"
msgstr "फोसावथाय ब्लक} खौ बन्द खालाम गासिनो दङ "

#: PESkipRSBraceEOF
msgid "closing } of invalid rule set"
msgstr "बाहायजायै रुल सेटखौ बन्द खालाम}गासिनो दङ "

#: PEBadSelectorRSIgnored
msgid "Ruleset ignored due to bad selector."
msgstr "गाज्रि सायखगिरिनि थाखाय रुलसेटआ नेवसिबाय। "

#: PEBadSelectorKeyframeRuleIgnored
msgid "Keyframe rule ignored due to bad selector."
msgstr "साबि फ्रेम नेमखौ गाज्रि सायखग्रानि थाखाय नेवसिबाय।"

#: PESelectorListExtraEOF
msgid "',' or '{'"
msgstr "',' एबा '{'"

#: PESelectorListExtra
msgid "Expected ',' or '{' but found '%1$S'."
msgstr "',' एबा '{' मिजिंथिबाय नाथाय %1$S' मोनबाय।"

#: PESelectorGroupNoSelector
msgid "Selector expected."
msgstr "सायखगिरि मिजिंथिबाय"

#: PESelectorGroupExtraCombinator
msgid "Dangling combinator."
msgstr "आलायनाय जरायजाबगिरि"

#: PECounterStyleNotIdent
msgid "Expected identifier for name of @counter-style rule."
msgstr "@counter-style नियमनि मुंनि थाखाय सिनायथिग्रा मिजिंथिबाय।"

#: PECounterStyleBadName
msgid "Name of @counter-style rule can't be '%1$S'."
msgstr "@counter-style नियमनि मुंआ '%1$S' जानो हाया।"

#: PECounterStyleBadBlockStart
msgid "Expected '{' to begin @counter-style rule but found '%1$S'."
msgstr "@counter-style नियमखौ जागायनो '{' मिजिंथिबाय, नाथाय '%1$S' मोनबाय।"

#: PECounterStyleEOF
msgid "closing '}' of @counter-style block"
msgstr "@counter-style ब्लकनि बन्द खालामनाय '}'"

#: PECounterDescExpected
msgid "Expected counter descriptor but found '%1$S'."
msgstr "उल्था बेखेवग्रा मिजिंथिबाय नाथाय '%1$S' मोनबाय।"

#: PEUnknownCounterDesc
msgid "Unknown descriptor '%1$S' in @counter-style rule."
msgstr "@counter-style नियमआव मोनथिमोनै बेखेवग्रा '%1$S'।"

#: PECounterExtendsNotIdent
msgid "Expected identifier for extends system but found '%1$S'."
msgstr "फेहेरनाय सिस्टेमनि थाखाय सिनायथिग्रा मिजिंथिबाय नाथाय '%1$S' मोनबाय।"

#: PECounterASWeight
msgid ""
"Each weight in the additive-symbols descriptor must be smaller than the "
"previous weight."
msgstr ""
"दाजाब-दिन्थिसिन बेखेवग्राआव थानाय मोनफ्रोम गिलिरआ आगोलनि गिलिर निख्रुइ फिसासिन "
"जाथारनांगोन।"

#: PEClassSelEOF
msgid "class name"
msgstr "थाखोनि मुं"

#: PEClassSelNotIdent
msgid "Expected identifier for class selector but found '%1$S'."
msgstr "थाखो सायखगिरिनि थाखाय सिनाय होग्रा मिजिंथिबाय नाथाय %1$S' मोनबाय।"

#: PECoordinatePair
msgid "Expected coordinate pair but found '%1$S'."
msgstr "क'अर्डिनेट जरा मिजिंथिबाय नाथाय '%1$S' मोनबाय।"

#: PETypeSelEOF
msgid "element type"
msgstr "गुदिमुवानि रोखोम"

#: PETypeSelNotType
msgid "Expected element name or '*' but found '%1$S'."
msgstr "गुदिमुवानि मुं एबा '*' मिजिंथिबाय नाथाय %1$S' मोनबाय।"

#: PEUnknownNamespacePrefix
msgid "Unknown namespace prefix '%1$S'."
msgstr "मोन्थिमोनै मुंजायगा सिगां दाजाबदा '%1$S'।"

#: PEAttributeNameEOF
msgid "attribute name"
msgstr "एटट्रिबिउट मुं"

#: PEAttributeNameExpected
msgid "Expected identifier for attribute name but found '%1$S'."
msgstr "एटट्रिबिउट मुंनि थाखाय सिनाय होग्रा मिजिंथिबाय नाथाय %1$S' मोनबाय।"

#: PEAttributeNameOrNamespaceExpected
msgid "Expected attribute name or namespace but found '%1$S'."
msgstr "एटट्रिबिउटनि मुं एबा मुंजायगा मिजिंथिबाय नाथाय %1$S' मोनबाय।"

#: PEAttSelNoBar
msgid "Expected '|' but found '%1$S'."
msgstr "'|' मिजिंथिबाय नाथाय %1$S' मोनबाय।"

#: PEAttSelInnerEOF
msgid "part of attribute selector"
msgstr "एटट्रिबिउट सायखगिरिनि बाहागो"

#: PEAttSelUnexpected
msgid "Unexpected token in attribute selector: '%1$S'."
msgstr "एटट्रिबिउट सायखगिरिआव मिजिंथियै टकेन: '%1$S'।"

#: PEAttSelValueEOF
msgid "attribute value"
msgstr "एटट्रिबिउट बेसेन"

#: PEAttSelCloseEOF
msgid "']' to end attribute selector"
msgstr "एटट्रिबिउट सायखगिरिखौ फोजोबनो ']'"

#: PEAttSelNoClose
msgid "Expected ']' to terminate attribute selector but found '%1$S'."
msgstr "एटट्रिबिउट सायखगिरि बन्द खालामनो ']' मिजिंथिबाय नाथाय '%1$S' मोनबाय।"

#: PEAttSelBadValue
msgid ""
"Expected identifier or string for value in attribute selector but found "
"'%1$S'."
msgstr ""
"एट्‌ट्रिबिउट सायखग्राआव बेसेननि थाखाय सिनायथिग्रा एबा स्ट्रिं मिजिंथिबाय नाथाय '%1$S' "
"मोनबाय।"

#: PEPseudoSelEOF
msgid "name of pseudo-class or pseudo-element"
msgstr "नंखाय-थाखो एबा नंखाय-गुदिमुवानि मुं"

#: PEPseudoSelBadName
msgid ""
"Expected identifier for pseudo-class or pseudo-element but found '%1$S'."
msgstr ""
"नंखाय-थाखो एबा नंखाय-गुदिमुवानि थाखाय सिनायथिग्रा मिजिंथिबाय नाथाय '%1$S' मोनबाय।"

#: PEPseudoSelNonFunc
msgid ""
"Function token for non-function pseudo-class or pseudo-element, or the other "
"way around, when reading '%1$S'."
msgstr ""
"'%1$S' फरायनाय समाव खामानि-नङै नंखाय-थाखोनि थाखाय खामानि टकेन एबा गुबुन राहायै।"

#: PEPseudoSelNotPE
msgid "Expected pseudo-element but found '%1$S'."
msgstr "नंखाय-गुदिमुवा मिजिंथिबाय नाथाय '%1$S' मोनबाय।"

#: PEPseudoSelDoubleNot
msgid "Negation pseudo-class can't be negated '%1$S'."
msgstr "नेवसिनाय नंखाय-थाखोखौ नेवसिनो हाया '%1$S'।"

#: PEPseudoSelPEInNot
msgid "Pseudo-elements can't be negated '%1$S'."
msgstr "नंखाय-गुदिमुवाखौ नेवसिनो हाया '%1$S'."

#: PEPseudoSelNewStyleOnly
msgid "This pseudo-element must use the \"::\" form: '%1$S'."
msgstr "बे नंखाय-गुदिमुवा \"::\" फर्म: '%1$S' खौ बाहाय थारनांगोन"

#: PEPseudoSelEndOrUserActionPC
msgid ""
"Expected end of selector or a user action pseudo-class after pseudo-element "
"but found '%1$S'."
msgstr ""
"नंखाय-गुदिमुवानि उनाव सायखग्रा एबा बाहायग्रा हाबा नंखाय-थाखो मिजिंथिबाय नाथाय "
"'%1$S' मोनबाय।"

#: PEPseudoSelNoUserActionPC
msgid ""
"Expected end of selector after pseudo-element that does not support user "
"action pseudo-classes but found '%1$S'."
msgstr ""
"बाहायग्रा हाबा नंखाय-थाखोखौ हेफाजाब होयै नंखाय-गुदिमुवानि उनाव सायखग्रानि जोबथा "
"मिजिंथिबाय नाथाय '%1$S' मोनबाय।"

#: PEPseudoSelMultiplePE
msgid "Extra pseudo-element '%1$S'."
msgstr "बांद्राय नंखाय-गुदि मुवा '%1$S'."

#: PEPseudoSelUnknown
msgid "Unknown pseudo-class or pseudo-element '%1$S'."
msgstr "मोन्थिमोनै नंखाय-थाखो एबा नंखाय-गुदि मुवा '%1$S'।"

#: PENegationEOF
msgid "selector within negation"
msgstr "नेवसिनायनि गेजेराव सायखगिरि"

#: PENegationBadInner
msgid "Malformed simple selector as negation pseudo-class argument '%1$S'."
msgstr ""
"नेवसिनाय नंखाय-थाखो बाथ्राबान '%1$S' बादि गोरलै सायखगिरिखौ गाज्रि महर खालामबाय।"

#: PENegationNoClose
msgid "Missing closing ')' in negation pseudo-class '%1$S'."
msgstr "नेवसिनाय नंखाय-थाखो '%1$S' आव बन्द खालामनाय ')' गोमाबाय।"

#: PENegationBadArg
msgid "Missing argument in negation pseudo-class '%1$S'."
msgstr "नेवसिनाय नंखाय-थाखो '%1$S' आव बाथ्राबान गोमाबाय।"

#: PEPseudoClassArgEOF
msgid "argument to pseudo-class selector"
msgstr "नंखाय-थाखो सायखगिरिआव बाथ्राबान"

#: PEPseudoClassArgNotIdent
msgid "Expected identifier for pseudo-class parameter but found '%1$S'."
msgstr "नंखाय-थाखो पारामिटारनि थाखाय सिनाय होग्रा मिजिंथिबाय नाथाय '%1$S' मोनबाय।"

#: PEPseudoClassArgNotNth
msgid "Expected part of argument to pseudo-class but found '%1$S'."
msgstr "नंखाय-थाखोआव बाथ्राबाननि बाहागोखौ मिजिंथिबाय नाथाय '%1$S' मोनबाय।"

#: PEPseudoClassNoClose
msgid "Missing closing ')' in pseudo-class, found '%1$S' instead."
msgstr "नंखाय-थाखोआव ')' बन्द खालामनाय गोमानाय, बेनि सोलायै '%1$S' मोनबाय।"

#: PEPseudoClassNoArg
msgid "Missing argument in pseudo-class '%1$S'."
msgstr "'%1$S' नंखाय-थाखोआव गोमानाय बाथ्राबान।"

#: PEPseudoClassNotUserAction
msgid ""
"Expected end of selector or a user action pseudo-class after pseudo-element "
"but found pseudo-class '%1$S'."
msgstr ""
"नंखाय-गुदिमुवानि उनाव सायखग्रानि जोबथा एबा बाहायग्रा हाबा नंखाय-थाखो मिजिंथिबाय, "
"नाथाय नंखाय-थाखो '%1$S' मोनबाय।"

#: PESelectorEOF
msgid "selector"
msgstr "सायखगिरि"

#: PEBadDeclBlockStart
msgid "Expected '{' to begin declaration block but found '%1$S'."
msgstr "'{' फोसावथाय ब्लक जागायनो मिजिंथिबाय नाथाय '%1$S' मोनबाय।"

#: PEColorEOF
msgid "color"
msgstr "गाब"

#: PEColorNotColor
msgid "Expected color but found '%1$S'."
msgstr "गाब मिजिंथिबाय नाथाय '%1$S' मोनबाय।"

#: PEColorComponentEOF
msgid "color component"
msgstr "गाबनि कम्पनेन्ट"

#: PEExpectedPercent
msgid "Expected a percentage but found '%1$S'."
msgstr "मोनसे जौखोन्दो मिजिंथिबाय नाथाय '%1$S' मोनबाय।"

#: PEExpectedInt
msgid "Expected an integer but found '%1$S'."
msgstr "मोनसे आबुं अनजिमा मिजिंथिबाय नाथाय '%1$S' मोनबाय।"

#: PEColorBadRGBContents
msgid "Expected number or percentage in rgb() but found '%1$S'."
msgstr "rgb() आव अनजिमा एबा जौखोन्दो मिजिंथिबाय नाथाय '%1$S' मोनबाय।"

#: PEColorComponentBadTerm
msgid "Expected '%2$S' but found '%1$S'."
msgstr "'%2$S' मिजिंथिबाय नाथाय '%1$S' मोनबाय।"

#: PEColorHueEOF
msgid "hue"
msgstr "हिउ"

#: PEExpectedComma
msgid "Expected ',' but found '%1$S'."
msgstr "',' मिजिंथिबाय नाथाय '%1$S' मोनबाय।"

#: PEColorSaturationEOF
msgid "saturation"
msgstr "सेटुरेसन"

#: PEColorLightnessEOF
