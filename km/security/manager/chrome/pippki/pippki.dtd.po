#. Values for changepassword.xul
#: setPassword.title
msgid "Change Master Password"
msgstr "ផ្លាស់ប្ដូរ​ពាក្យ​សម្ងាត់​មេ"

#: setPassword.tokenName.label
msgid "Security Device"
msgstr "ឧបករណ៍​សុវត្ថិភាព"

#: setPassword.oldPassword.label
msgid "Current password:"
msgstr "ពាក្យ​សម្ងាត់​បច្ចុប្បន្ន ៖"

#: setPassword.newPassword.label
msgid "New password:"
msgstr "ពាក្យ​សម្ងាត់​ថ្មី ៖"

#: setPassword.reenterPassword.label
msgid "New password (again):"
msgstr "ពាក្យ​សម្ងាត់​ថ្មី (ម្តង​ទៀត) ៖"

#: setPassword.meter.label
msgid "Password quality meter"
msgstr "ឧបករណ៍​​វាស់​​​គុណ​ភាព​​​ពាក្យ​​​សម្ងាត់"

#. Values for resetpassword.xul
#: resetPasswordButtonLabel
msgid "Reset"
msgstr "កំណត់​ឡើង​វិញ"

#: resetPassword.title
msgid "Reset Master Password"
msgstr "កំណត់​ពាក្យ​សម្ងាត់​មេ​ឡើង​​វិញ"

#: resetPassword.text
msgid ""
"If you reset your master password, all your stored web and e-mail passwords, "
"form data, personal certificates, and private keys will be forgotten. Are "
"you sure you want to reset your master password?"
msgstr ""
"ប្រសិន​បើ​អ្នក​កំណត់​ពាក្យ​សម្ងាត់​មេ​របស់​អ្នក​ឡើង​វិញ ពាក្យសម្ងាត់​បណ្ដាញ និង​អ៊ីមែល​ ទិន្នន័យ​ទ្រង់ទ្រាយ "
"វិញ្ញាបនបត្រ​ផ្ទាល់ខ្លួន​ និង​សោ​ឯកជន​ទាំងអស់​របស់​អ្នក នឹង​ត្រូវ​បាន​បំភ្លេច​ចោល​ ។ តើ​អ្នក​ពិត​ជា​ចង់​កំណត់​ពាក្យ​"
"សម្ងាត់​មេ​របស់​អ្នក​ឡើង​វិញ​ឬ ?"

#. Values for getpassword.xul
#: getPassword.title
msgid "Enter Master Password"
msgstr "បញ្ចូល​ពាក្យ​សម្ងាត់​មេ"

#: getPassword.tokenName2.label
msgid "Security Device:"
msgstr "ឧបករណ៍​សុវត្ថិភាព ៖"

#: getPassword.password.label
msgid "Password:"
msgstr "ពាក្យ​សម្ងាត់ ៖"

#. Downloading a cert
#: downloadCert.title
msgid "Downloading Certificate"
msgstr "កំពុង​ទាញ​យក​វិញ្ញាបនបត្រ"

#: downloadCert.message1
msgid "You have been asked to trust a new Certificate Authority (CA)."
msgstr "អ្នក​ត្រូវ​បាន​ស្នើ​ឲ្យ​ជឿ​ទុកចិត្ត​លើ​​អាជ្ញាធរ​​វិញ្ញាបនបត្រ​ (CA) ថ្មី ។"

#: downloadCert.trustSSL
msgid "Trust this CA to identify websites."
msgstr "ទុកចិត្ត​លើ CA នេះ​ដើម្បី​កំណត់​អត្តសញ្ញាណ​តំបន់បណ្ដាញ ។"

#: downloadCert.trustEmail
msgid "Trust this CA to identify email users."
msgstr "ទុកចិត្ត​លើ​ CA នេះ​ដើម្បី​កំណត់​អត្តសញ្ញាណ​របស់​​អ្នក​ប្រើ​អ៊ីមែល ។"

#: downloadCert.trustObjSign
msgid "Trust this CA to identify software developers."
msgstr "ទុកចិត្ត​លើ CA នេះ​ដើម្បី​កំណត់​អត្តសញ្ញាណ​របស់​អ្នក​អភិវឌ្ឍន៍​កម្មវិធី ។"

#: downloadCert.message3
msgid ""
"Before trusting this CA for any purpose, you should examine its certificate "
"and its policy and procedures (if available)."
msgstr ""
"មុន​ពេល​ជឿ​ទុកចិត្ត​លើ CA នេះ​សម្រាប់​គោលបំណង​ណាមួយ អ្នក​គួរតែ​ត្រួតពិនិត្យមើល​វិញ្ញាបនបត្រ គោលការណ៍ និង​"
"បែបបទ​របស់​វា (ប្រសិនបើ​មាន)។"

#: downloadCert.viewCert.label
msgid "View"
msgstr "មើល"

#: downloadCert.viewCert.text
msgid "Examine CA certificate"
msgstr "ត្រួត​ពិនិត្យ​វិញ្ញាបនបត្រ​ CA"

#. Strings for the SSL client auth ask dialog
#: clientAuthAsk.title
msgid "User Identification Request"
msgstr "ស្នើ​សុំ​អត្ត​សញ្ញាណ​អ្នក​ប្រើ"

#: clientAuthAsk.message1
msgid "This site has requested that you identify yourself with a certificate:"
msgstr "តំបន់បណ្ដាញ​នេះ​បាន​ស្នើសុំ​ឲ្យ​អ្នក​បញ្ជាក់​ប្រាប់​អំពី​អត្តសញ្ញាណ​របស់​អ្នក​ជាមួយ​នឹង​វិញ្ញាបនបត្រ ៖"

#: clientAuthAsk.message2
msgid "Choose a certificate to present as identification:"
msgstr "ជ្រើស​វិញ្ញាបនបត្រ​មួយ​ ដើម្បី​បង្ហាញ​ជា​អត្តសញ្ញាណ ៖"

#: clientAuthAsk.message3
msgctxt "clientAuthAsk.message3"
msgid "Details of selected certificate:"
msgstr "សេចក្ដី​លម្អិត​នៃ​វិញ្ញាបនបត្រ​ដែល​បាន​ជ្រើស ៖ "

#. Strings for the cert picker dialog
#: certPicker.title
msgid "Select Certificate"
msgstr "ជ្រើស​​វិញ្ញាបនបត្រ"

#: certPicker.info
msgid "Certificate:"
msgstr "វិញ្ញាបនបត្រ ៖"

#: certPicker.detailsLabel
msgctxt "certPicker.detailsLabel"
msgid "Details of selected certificate:"
msgstr "សេចក្ដី​លម្អិត​នៃ​វិញ្ញាបនបត្រ​ដែល​បាន​ជ្រើស ៖ "

#: pkcs12.setpassword.title
msgid "Choose a Certificate Backup Password"
msgstr "ជ្រើស​ពាក្យសម្ងាត់​បម្រុងទុក​វិញ្ញាបនបត្រ"

#: pkcs12.setpassword.message
msgid ""
"The certificate backup password you set here protects the backup file that "
"you are about to create.  You must set this password to proceed with the "
"backup."
msgstr ""
"ពាក្យសម្ងាត់​បម្រុងទុក​ដែល​អ្នក​បាន​កំណត់​នៅ​​ទីនេះ​ ការពារ​ឯកសារ​បម្រុងទុក​ដែល​អ្នក​រៀបនឹង​បង្កើត ។  អ្នក​"
"ត្រូវតែ​កំណត់​ពាក្យសម្ងាត់​នេះ​ដើម្បី​បន្ត​ជា​មួយ​នឹង​ការ​បម្រុងទុក ។"

#: pkcs12.setpassword.label1
msgid "Certificate backup password:"
msgstr "ពាក្យសម្ងាត់​សម្រាប់​បម្រុងទុក​វិញ្ញាបនបត្រ ៖"

#: pkcs12.setpassword.label2
msgid "Certificate backup password (again):"
msgstr "ពាក្យសម្ងាត់​សម្រាប់​បម្រុងទុក​វិញ្ញាបនបត្រ (ម្ដងទៀត)៖"

#: pkcs12.setpassword.reminder
msgid ""
"Important: If you forget your certificate backup password, you will not be "
"able to restore this backup later.  Please record it in a safe location."
msgstr ""
"សំខាន់ ៖ ប្រសិនបើ​អ្នក​ភ្លេច​ពាក្យសម្ងាត់​សម្រាប់​បម្រុងទុក​វិញ្ញាបនបត្រ​របស់​អ្នក អ្នក​នឹង​មិន​អាច​ស្ដារ​ឯកសារ​"
"បម្រុង​ទុក​នេះ​នៅ​ពេល​ក្រោយ​បាន​ទេ ។  សូម​ចម្លង​វា​ទុក​នៅ​ក្នុង​ទីតាំង​សុវត្ថិភាព ។"

#: chooseToken.title
msgid "Choose Token Dialog"
msgstr "ជ្រើស​ប្រអប់​ថូខឹន"

#: chooseToken.message1
msgid "Please choose a token."
msgstr "សូម​ជ្រើស​ថូខឹន ។"

#: escrowWarn.title
msgid "Encryption Key Copy"
msgstr "ច្បាប់​ចម្លង​សម្រាប់​សោ​អ៊ិនគ្រីប"

#: escrowWarn.message1
msgid ""
"Important: This certificate authority has asked to make a backup of your "
"encryption private key."
msgstr "សំខាន់ ៖ អាជ្ញាធរ​វិញ្ញាបនបត្រ​នេះ​បាន​ស្នើសុំ​​ឲ្យ​បង្កើត​ច្បាប់​ចម្លង​សោ​ឯកជន​សម្រាប់​ការ​អ៊ិនគ្រីប ។"

#: escrowWarn.benefit1
msgid ""
"The benefit is that if you lose access to your encryption private key, you "
"can request a copy from this certificate authority."
msgstr ""
"អត្ថប្រយោជន៍​គឺថា ប្រសិនបើ​អ្នក​បាត់បង់​ការ​ចូល​ដំណើរការ​ទៅ​កាន់​សោ​ឯកជន​សម្រាប់​ការ​អ៊ិនគ្រីប អ្នក​អាច​ស្នើសុំ​"
"ច្បាប់​ចម្លង​ពី​អាជ្ញាធរ​វិញ្ញាបនបត្រ​នេះ ។"

#: escrowWarn.message2
msgid ""
"However, your encryption private key will be stored by the certificate "
"authority, and could be used to read your encrypted email or documents "
"without your permission."
msgstr ""
"ទោះបី​ជា​យ៉ាងណា សោ​ឯកជន​សម្រាប់​ការ​អ៊ិនគ្រីប​របស់​អ្នក​នឹង​ត្រូវ​បាន​រក្សាទុក​ដោយ​អាជ្ញាធរ​វិញ្ញាបនបត្រ និង​អាច​"
"ត្រូវ​បាន​ប្រើ​សម្រាប់​អាន​ឯកសារ ឬ​អ៊ីមែល​ដែល​បាន​អ៊ិនគ្រីប​ដោយ​មិន​ចាំបាច់​សុំ​ការ​អនុញ្ញាត​ពី​អ្នក ។"

#: examineCert.label examineCert.accesskey
msgid "&View Certificate"
msgstr "មើល​វិញ្ញាបនបត្រ"

#. Strings for the CreateCertInfo dialog
#: createCertInfo.title
msgid "Generating A Private Key"
msgstr "កំពុង​បង្កើត​សោ​ឯកជន"

#: createCertInfo.msg1
msgid "Key Generation in progress… This may take a few minutes…."
msgstr "ការ​បង្កើត​សោ​កំពុងតែ​ដំណើរការ… វា​អាច​ចំណាយ​ពេល​ពីរ​បី​នាទី…."

#: createCertInfo.msg2
msgid "Please wait…"
msgstr "សូម​រង់ចាំ..."

#. Strings for protectedAuth dialog
#: protectedAuth.title
msgid "Protected Token Authentication"
msgstr "បាន​ការពារ​ការ​ផ្ទៀងផ្ទាត់​ភាព​ត្រឹមត្រូវ​របស់​ថូខឹន"

#: protectedAuth.msg
msgid ""
"Please authenticate to the token. Authentication method depends on the type "
"of your token."
msgstr ""
"សូម​ផ្ទៀងផ្ទាត់​ភាព​ត្រឹមត្រូវ​ចំពោះ​ថូខឹន ។ វិធីសាស្ត្រ​ផ្ទៀងផ្ទាត់​ភាព​ត្រឹមត្រូវ​ផ្អែក​លើ​ប្រភេទ​នៃ​ថូខឹន​របស់​អ្នក ។"

#: protectedAuth.tokenName.label
msgid "Token:"
msgstr "ថូខឹន ៖"

