#: lookInMenuList.label lookInMenuList.accesskey
msgid "&Look in:"
msgstr "មើ&ល​ក្នុង ៖"

#: textInput.label textInput.accesskey
msgid "File &name:"
msgstr "&ឈ្មោះ​ឯកសារ ៖"

#: filterMenuList.label filterMenuList.accesskey
msgid "Files of &type:"
msgstr "ឯកសារ​ប្រ&ភេទ ៖"

#: name.label
msgid "Name"
msgstr "ឈ្មោះ"

#: size.label
msgid "Size"
msgstr "ទំហំ"

#: lastModified.label
msgid "Last Modified"
msgstr "បាន​កែប្រែ​ចុងក្រោយ"

#: showHiddenFiles.label showHiddenFiles.accesskey
msgid "&Show hidden files and directories"
msgstr "បង្ហាញ​ថត​ និង​ ឯកសារ​ដែល​បាន​លាក់"

#: noPermissionError.label
msgid "You do not have the permissions necessary to view this directory."
msgstr "អ្នក​មិន​មាន​សិទ្ធិ​ដែល​ចាំបាច់​ក្នុង​ការ​មើល​ថត​នេះ​ទេ ។"

#: folderUp.tooltiptext
msgid "Go up a level"
msgstr "ឡើង​លើ​មួយ​កម្រិត"

#: folderHome.tooltiptext
msgid "Go to home"
msgstr "ទៅ​កាន់​ទំព័រ​ដើម"

#: folderNew.tooltiptext
msgid "Create new directory"
msgstr "បង្កើត​ថត​ថ្មី"
