msgstr "កែប្រែ"

#: toggle.label toggle.accesskey
msgid "&Toggle"
msgstr "បិទបើក"

#: reset.label reset.accesskey
msgid "&Reset"
msgstr "កំណត់​ឡើង​វិញ"

#: new.label new.accesskey
msgid "Ne&w"
msgstr "ថ្មី"

#: string.label string.accesskey
msgid "&String"
msgstr "ខ្សែ​អក្សរ"

#: integer.label integer.accesskey
