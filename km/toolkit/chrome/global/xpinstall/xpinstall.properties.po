#. #--------------------------------------------------------------------
#. #   Install Actions
#. #--------------------------------------------------------------------
#: InstallFile
msgid "Installing: %s"
msgstr "ដំឡើង ៖ %s"

#: InstallSharedFile
msgid "Installing Shared File: %s"
msgstr "ដំឡើង​ឯកសារ​ដែល​បាន​ចែករំលែក ៖ %s"

#: ReplaceFile
msgid "Replacing: %s"
msgstr "ជំនួស ៖ %s"

#: ReplaceSharedFile
msgid "Replacing Shared File: %s"
msgstr "ជំនួស​ឯកសារ​ដែល​បាន​ចែករំលែក ៖ %s"

#: SkipFile
msgid "Skipping: %s"
msgstr "រំលង ៖ %s"

#: SkipSharedFile
msgid "Skipping Shared File: %s"
msgstr "រំលង​ឯកសារ​ដែល​បាន​ចែករំលែក ៖ %s"

#: DeleteFile
msgid "Deleting file: %s"
msgstr "លុប​ឯកសារ ៖ %s"

#: DeleteComponent
msgid "Deleting component: %s"
msgstr "លុប​សមាសភាគ ៖ %s"

#: Execute
msgid "Executing: %s"
msgstr "ប្រតិបត្តិ ៖ %s"

#: ExecuteWithArgs
msgid "Executing: %s with argument: %s"
msgstr "ប្រតិបត្តិ ៖ %s ដែល​ប្រើ​អាគុយម៉ង់ ៖ %s"

#: CopyFile
msgid "Copy File: %s to %s"
msgstr "ចម្លង​ឯកសារ ៖ %s ទៅកាន់ %s"

#: ExecuteFile
msgid "Execute File: %s"
msgstr "ប្រតិបត្តិ​ឯកសារ ៖ %s"

#: ExecuteFileWithArgs
msgid "Execute File: %s with argument: %s"
msgstr "ប្រតិបត្តិ​ឯកសារ ៖ %s ដែល​ប្រើ​អាគុយម៉ង់ ៖ %s"

#: MoveFile
msgid "Move File: %s to %s"
msgstr "ផ្លាស់ទី​ឯកសារ ៖ %s ទៅកាន់ %s"

#: RenameFile
msgid "Rename File: %s to %s"
msgstr "ប្ដូរ​ឈ្មោះ​ឯកសារ ៖ %s ទៅជា %s"

#: CreateFolder
msgid "Create Folder: %s"
msgstr "បង្កើត​ថត ៖ %s"

#: RemoveFolder
msgid "Remove Folder: %s"
msgstr "យក​ថត​ចេញ ៖ %s"

#: RenameFolder
msgid "Rename Folder: %s to %s"
msgstr "ប្ដូរ​ឈ្មោះ​ថត ៖ %s ទៅជា %s"

#: WindowsShortcut
msgid "Windows Shortcut: %s"
msgstr "ផ្លូវកាត់​វីនដូ ៖ %s"

#: MacAlias
msgid "Mac Alias: %s"
msgstr "ឈ្មោះ​ក្លែងក្លាយ​របស់ Mac ៖ %s"

#: WindowsRegisterServer
msgid "Windows Register Server: %s"
msgstr "ម៉ាស៊ីន​បម្រើ​ចុះឈ្មោះ​វីនដូ ៖ %s"

#: UnknownFileOpCommand
msgid "Unknown file operation command!"
msgstr "មិន​ស្គាល់​ពាក្យបញ្ជា​ប្រតិបត្តិ​ឯកសារ !"

#: Patch
msgid "Patching: %s"
msgstr "ប៉ះ ៖ %s"

#: Uninstall
msgid "Uninstalling: %s"
msgstr "លុប ៖ %s"

#: RegSkin
msgid "Register Skin: %s"
msgstr "ចុះឈ្មោះ​ស្បែក ៖ %s"

#: RegLocale
msgid "Register Locale: %s"
msgstr "ចុះឈ្មោះ​មូលដ្ឋាន (Locale) ៖ %s"

#: RegContent
msgid "Register Content: %s"
msgstr "ចុះឈ្មោះ​មាតិកា ៖ %s"

#: RegPackage
msgid "Register Package: %s"
msgstr "ចុះឈ្មោះ​កញ្ចប់ ៖ %s"

#. #--------------------------------------------------------------------
#. #   Dialog Messages
#. #--------------------------------------------------------------------
#: ApplyNowSkin
msgid "Use this theme"
msgstr "ប្រើ​ស្បែក​នេះ"

#: ApplyNowLocale
msgid "Use this locale"
msgstr "ប្រើ​មូលដ្ឋាន (locale) នេះ"

#: ConfirmSkin
msgid "Install the theme \"%1$S\" from %2$S?"
msgstr "ដំឡើង​ស្បែក \"%1$S\" ពី %2$S ?"

#: ConfirmLocale
msgid "Install the locale \"%1$S\" from %2$S?"
msgstr "ដំឡើង​មូលដ្ឋាន (locale) \"%1$S\" ពី %2$S ?"

#: OK
msgid "Install"
msgstr "ដំឡើង"

#: progress.queued
msgid "Queued"
msgstr "ដាក់​ជា​ជួរ"

#: progress.downloading
msgid "Downloading…"
msgstr "កំពុង​ទាញយក..."

#: progress.downloaded
msgid "Downloaded"
msgstr "បាន​ទាញ​យក"

#: progress.installing
msgid "Installing…"
msgstr "កំពុង​ដំឡើង..."

#: Unsigned
msgid "Unsigned"
msgstr "មិន​ចុះឈ្មោះ"

#. #--------------------------------------------------------------------
#. #   Miscellaneous
#. #--------------------------------------------------------------------
#: ERROR
msgid "ERROR"
msgstr "កំហុស"

#: error0
msgid "Success"
msgstr "ជោគជ័យ"

#: error999
msgid "Restart to complete"
msgstr "ចាប់ផ្ដើម​ឡើងវិញ​ដើម្បី​បញ្ចប់​ទាំងស្រុង"

#: error-202
msgid "Access denied"
msgstr "ការ​ចូល​ដំណើរការ​ត្រូវ​បាន​បដិសេធ"

#: error-203
msgid ""
"Unexpected installation error\n"
"Review the Error Console log for more details."
msgstr ""
"កំហុស​ក្នុង​ការ​ដំឡើង​ដែល​មិន​បាន​រំពឹង\n"
"ពិនិត្យ​ឡើងវិញ​នូវ​កំណត់ហេតុ កុងសូល​កំហុស សម្រាប់​ព័ត៌មាន​លម្អិត​បន្ថែម ។"

#: error-204
msgid "Install script not found"
msgstr "រក​មិន​ឃើញ​ស្គ្រីប​ដំឡើង"

#: error-207
msgid "Not a valid install package"
msgstr "មិន​មែន​ជា​កញ្ចប់​ដំឡើង​ត្រឹមត្រូវ"

#: error-208
msgid "Invalid argument"
msgstr "អាគុយម៉ង់​មិន​ត្រឹមត្រូវ"

#: error-210
msgid "User canceled"
msgstr "បាន​បោះបង់​ដោយ​អ្នកប្រើ"

#: error-214
msgid "Required file does not exist"
msgstr "មិន​មាន​ឯកសារ​ដែល​ទាមទារ"

#: error-215
msgid "Read only"
msgstr "បាន​តែ​អាន"

#: error-218
msgid "AppleSingle extraction error"
msgstr "កំហុស​ក្នុង​ការ​ស្រង់ចេញ AppleSingle"

#: error-219
msgid "Invalid path"
msgstr "ផ្លូវ​មិន​ត្រឹមត្រូវ"

#: error-225
msgid "EXTRACTION_FAILED"
msgstr "បាន​បរាជ័យ​ក្នុង​ការ​ស្រង់ចេញ"

#: error-227
msgid "Canceled"
msgstr "បាន​បោះបង់"

#: error-228
msgid "Download error"
msgstr "កំហុស​ទាញយក"

#: error-229
msgid "Script error"
msgstr "កំហុស​ស្គ្រីប"

#: error-230
msgid "Already exists"
msgstr "មាន​រួចហើយ"

#: error-235
msgid "Out of space"
msgstr "អស់​ទំហំ"

#: error-239
msgid "Chrome registration failed"
msgstr "បាន​បរាជ័យ​ក្នុង​ការ​ចុះឈ្មោះ Chrome"

#: error-240
msgid "Unfinished install"
msgstr "ការ​ដំឡើង​ដែល​មិន​បាន​បញ្ចប់"

#: error-244
msgid "Unsupported package"
msgstr "កញ្ចប់​ដែល​មិន​បាន​គាំទ្រ"

#: error-260
msgid "Signing could not be verified."
msgstr "មិន​អាច​ផ្ទៀងផ្ទាត់​ការ​ចូល ។"

#: error-261
msgid "Invalid file hash (possible download corruption)"
msgstr "សញ្ញា​ឯកសារ​មិន​ត្រឹមត្រូវ (អាច​ខូច​ឯកសារ​ទាញយក)"

#: error-262
msgid "Unknown or invalid file hash type"
msgstr "ប្រភេទ​សញ្ញា​ឯកសារ​មិន​ត្រឹមត្រូវ ឬ​មិន​ស្គាល់"

#: error-299
msgid "Out of memory"
msgstr "អស់​អង្គ​ចងចាំ"

#. # there are other error codes, either rare or obsolete,
#. # that are not worth translating at this time.
#: unknown.error
msgid "Unexpected error %S"
msgstr "កំហុស​ដែល​មិន​បាន​រំពឹង %S"
