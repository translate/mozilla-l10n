msgstr "នាទី;នាទី"

#: hours
msgid "hour;hours"
msgstr "ម៉ោង;ម៉ោង"

#: days
msgid "day;days"
msgstr "ថ្ងៃ;ថ្ងៃ"

#. # LOCALIZATION NOTE (paused): — is the "em dash" (long dash)
#: paused
msgid "Paused —  #1"
msgstr "បាន​ផ្អាក —  #1"

#: downloading
msgid "Downloading"
msgstr "កំពុង​ទាញ​យក"

#: notStarted
msgid "Not Started"
msgstr "មិន​បាន​ចាប់ផ្ដើម"

#: failed
