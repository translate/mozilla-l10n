#. # %1: the user name (Ed), %2: the app name (Firefox), %3: the operating system (Android)
#: client.name2
#, fuzzy
msgid "%1$S’s %2$S on %3$S"
msgstr "%1$S's %2$S នៅ %3$S"

#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "ធ្វើ​សមកាលកម្ម​ចុងក្រោយ ៖ %S"

#. # signInToSync.description is the tooltip for the Sync buttons when Sync is
#. # not configured.
#: signInToSync.description
msgid "Sign In To Sync"
msgstr "ចូល​ដើម្បី​ធ្វើ​សមកាលកម្ម"

#: mobile.label
msgid "Mobile Bookmarks"
msgstr "ចំណាំ​ចល័ត"

#: error.sync.title
msgid "Error While Syncing"
msgstr "កំហុស​នៅពេល​ធ្វើ​សមកាលកម្ម"

#: error.sync.description
msgid ""
"Sync encountered an error while syncing: %1$S.  Sync will automatically "
"retry this action."
msgstr ""
"ធ្វើ​សមកាលកម្ម​បាន​ជួប​ប្រទះ​កំហុស​នៅពេល​ធ្វើ​សមកាលកម្ម ៖ %1$S ។ ការ​ធ្វើ​សមកាលកម្ម​នឹង​ព្យាយាម​ធ្វើ​​ឡើង​"
"វិញ​ដោយ​ស្វ័យ​ប្រវត្តិ ។"

#: warning.sync.eol.label
msgid "Service Shutting Down"
msgstr "កំពុង​បិទ​សេវាកម្ម"

#. # %1: the app name (Firefox)
#: warning.sync.eol.description
msgid ""
"Your Firefox Sync service is shutting down soon. Upgrade %1$S to keep "
"syncing."
msgstr ""
"សេវា​សមកាលកម្ម Firefox របស់​អ្នក​នឹង​បិទ​ក្នុង​ពេល​ឆាប់ៗ​នេះ។ ធ្វើ​បច្ចុប្បន្នភាព %1$S ដើម្បី​បន្ត​"
"សមកាលកម្ម។"

#: error.sync.eol.label
msgid "Service Unavailable"
msgstr "មិន​មាន​សេវាកម្ម"

#. # %1: the app name (Firefox)
#: error.sync.eol.description
msgid ""
"Your Firefox Sync service is no longer available. You need to upgrade %1$S "
"to keep syncing."
msgstr ""
"សេវា​សមកាលកម្ម Firefox របស់​អ្នក​មិន​មាន​ទៀត​ទេ។ អ្នក​ចាំបាច់​ត្រូវតែ​ធ្វើ​បច្ចុប្បន្នភាព %1$S ដើម្បី​"
"បន្ត​សមកាលកម្ម។"

#: sync.eol.learnMore.label sync.eol.learnMore.accesskey
msgid "&Learn more"
msgstr "ស្វែងយល់​បន្ថែម"

#: syncnow.label
#, fuzzy
msgid "Sync Now"
msgstr "ធ្វើ​សមកាលកម្ម​ឥឡូវ"

#: syncing2.label
msgid "Syncing…"
msgstr "កំពុង​ធ្វើ​សមកាលកម្ម..."

#~ msgid "Remote tabs are being synced…"
#~ msgstr "ផ្ទាំង​ពីចម្ងាយ​កំពុង​ត្រូវ​បាន​ធ្វើ​សមកាលកម្ម..."

#~ msgid "Sync your other devices again to access their tabs"
#~ msgstr "ធ្វើ​សមកាលកម្ម​ឧបករណ៍​ផ្សេងទៀត​របស់​អ្នក​ដើម្បី​ចូល​ដំណើរការ​ផ្ទាំង​របស់​វា"

#~ msgid "All remote tabs are already open"
#~ msgstr "ផ្ទាំង​ពីចម្ងាយ​ទាំងអស់​ត្រូវ​បានបើក​រួច​ហើយ"

#~ msgid "Recent desktop tabs will be available once they sync"
#~ msgstr "ផ្ទាំង​ផ្ទៃតុ​បច្ចុប្បន្ន​នឹង​អាច​ប្រើ​បាននៅពេល​ពួកវា​ធ្វើ​សមកាលកម្ម"

#~ msgid "Error While Signing In"
#~ msgstr "កំហុស​នៅពេល​ចូល"

#~ msgid "Sync encountered an error while connecting: %1$S.  Please try again."
#~ msgstr "ធ្វើ​សមកាលកម្ម​បាន​ជួប​ប្រទះ​នឹង​កំហុសខណៈពេល​ដែល​តភ្ជាប់៖ %1$S ។ សូម​ព្យាយាម​ម្ដង​ទៀត ។"

#~ msgid "&Preferences…"
#~ msgstr "ចំណូល​ចិត្ត..."

#~ msgid "Error While Signing Out"
#~ msgstr "កំហុស​នៅពេល​ចេញ"

#~ msgid ""
#~ "Sync encountered an error while connecting.  It's probably ok, and you "
#~ "don't have to do anything about it."
#~ msgstr ""
#~ "ធ្វើ​សមកាលកម្ម​បាន​ជួប​ប្រទះ​នឹង​កំហុស ខណៈពេល​តភ្ជាប់ ។ វា​ប្រហែលជា​គ្មានបញ្ហា​ទេ ហើយ​អ្នក​​កុំ​ធ្វើ​អ្វី​"
#~ "ទាំងអស់​ជា​មួយវា ។"

#~ msgid ""
#~ "Sync has not been able to complete during the last %1$S days. Please "
#~ "check your network settings."
#~ msgstr ""
#~ "សម​កាល​កម្ម​មិន​អាច​បញ្ចប់​បាន​ឡើយ​អំឡុង​ពេល %1$S ថ្ងៃ​ចុង​ក្រោយ ។ សូម​ពិនិត្យ​មើល​ការ​កំណត់​បណ្ដាញ​របស់​"
#~ "អ្នក ។"

#~ msgid "Ser&ver Status"
#~ msgstr "ស្ថានភាព​ម៉ាស៊ីន​បម្រើ"

#~ msgid "You need to update Firefox Sync to continue syncing your data."
#~ msgstr ""
#~ "អ្នក​ត្រូវ​តែ​ធ្វើ​បច្ចុប្បន្ន​ភាព​សមកាលកម្ម​របស់​របស់ Firefox ដើម្បី​បន្ត​ការ​ធ្វើ​សមកាលកម្ម​របស់​អ្នក ។"

#~ msgid "&Update Firefox Sync"
#~ msgstr "ធ្វើ​បច្ចុប្បន្ន​ភាព​ការ​ធ្វើសមកាលកម្ម​របស់ Firefox"

#~ msgid "Approaching Server Quota"
#~ msgstr "ដល់​កូតា​របស់​ម៉ាស៊ីន​បម្រើ"

#~ msgid ""
#~ "You are approaching the server quota. Please review which data to sync."
#~ msgstr "អ្ន​បាន​ដល់​កូតា​របស់​ម៉ាស៊ីន​បម្រើ​ហើយ ។ សូម​ពិនិត្យ​ទិន្នន័យ​ណាមួយ​ត្រូវ​ធ្វើ​សមកាលកម្ម​ ។"

#~ msgid "Server Quota Exceeded"
#~ msgstr "បាន​លើស​កូតា​ម៉ាស៊ីន​បម្រើ"

#~ msgid ""
#~ "Sync failed because it exceeded the server quota. Please review which "
#~ "data to sync."
#~ msgstr ""
#~ "បានបរាជ័យ​ក្នុង​ការ​ធ្វើ​សមកាលកម្ម ពីព្រោះ​វា​បាន​លើស​កូតា​ម៉ាស៊ីន​បម្រើ ។ សូម​ពិនិត្យ​មើល​ទិន្នន័យ​ ដើម្បី​"
#~ "ធ្វើ​សមកាលកម្ម ។"

#~ msgid "&View Quota"
#~ msgstr "មើល​កូតា"

#~ msgid ""
#~ "The Sync server is a little busy right now, but you don't need to do "
