#~ msgstr "Ntosoɔ a etwa-to: %S"

#~ msgid "Last Update: in progress…"
#~ msgstr "Ntosoɔ a etwa-to: reguso"

#~ msgid "Connected: %S"
#~ msgstr "Nsiani: %S"

#~ msgid "Disconnected"
#~ msgstr "Ntwa-so"

#~ msgid "Connecting…"
#~ msgstr "Rekɔnɛkt…"
