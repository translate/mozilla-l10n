#~ msgstr "Ntoso no endi mu"

#~ msgid "Downloading %S…"
#~ msgstr "Retwe %S…"

#~ msgid "Paused downloading %S"
#~ msgstr "%S ntwe no retweɔn"

#~ msgid "Downloaded %S"
#~ msgstr "Atwe %S"
