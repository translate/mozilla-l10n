#~ msgstr "Skickar rapport"

#~ msgid "Report sent"
#~ msgstr "Rapport skickad"

#~ msgid "Try again"
#~ msgstr "Försök igen"

#~ msgid "Untrusted Connection"
#~ msgstr "Otillförlitlig anslutning"

