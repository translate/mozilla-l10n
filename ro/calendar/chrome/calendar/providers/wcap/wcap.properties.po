#. # args: host
#: accessingServerFailedError.text
msgid "Cannot access server %1$S!"
msgstr "Nu se poate accesa serverul %1$S!"

#: loginFailed.text
msgid "Login failed or invalid session Id."
msgstr "Autentificarea a eșuat sau id-ul de sesiune este invalid."

#: accessDenied.text
msgid "The user is denied access."
msgstr "Accesul cu acest utilizator este respins."

#. # args: host
#: noHttpsConfirmation.text
msgid ""
"Insecure login on %1$S!\n"
"Continue?"
msgstr ""
"Autentificare nesigură pe %1$S!\n"
"Continuați?"

#: noHttpsConfirmation.check.text
msgid "Don't ask again."
msgstr "Nu mai întreba data viitoare."

#: noHttpsConfirmation.label
msgid "Warning!"
msgstr "Avertisment!"

#. # args: host, prodId, serverVersion, wcapVersion
#: insufficientWcapVersionConfirmation.text
msgid ""
"Server %1$S (%2$S, v%3$S, WCAP v%4$S) doesn't support a sufficient WCAP "
"version! The required version is at least 3.0.0.\n"
"Continue?"
msgstr ""
"Serverul %1$S (%2$S, v%3$S, WCAP v%4$S) nu suportă o versiune potrivită de "
"WCAP! Versiunea necesară este minim 3.0.0.\n"
"Continuați?"

#: insufficientWcapVersionConfirmation.label
msgid "Insufficient WCAP version!"
msgstr "Versiune WCAP prea veche!"

#: loginDialog.label
msgid "Calendar Server Password Required"
msgstr "Parolă necesară pentru serverul calendarului"

#: privateItem.title.text
msgid "Private"
msgstr "Privat"

#: confidentialItem.title.text
msgid "Confidential"
msgstr "Confidențial"

#: busyItem.title.text
