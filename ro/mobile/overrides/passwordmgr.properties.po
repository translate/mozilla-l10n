#. # 1st string is the username for the login, 2nd is the login's hostname.
#. # Note that long usernames may be truncated.
#: savePassword
msgid "Save password for \"%1$S\" on %2$S?"
msgstr "Salvați parola pentru \"%1$S\" pe %2$S?"

#. # String is the login's hostname
#: savePasswordNoUser
msgid "Save password on %S?"
msgstr "Salvați parola pe %S?"

#: saveButton
msgid "Save"
msgstr "Salvare"

#: dontSaveButton
msgid "Don't save"
msgstr "Nu salva"

#. # String is the login's hostname
#: updatePassword
msgid "Update saved password for %S?"
msgstr "Actualizați parola salvată pentru %S?"

#: updatePasswordNoUser
msgid "Update saved password for this login?"
msgstr "Actualizați parola salvată pentru această autentificare?"

#: updateButton
msgid "Update"
msgstr "Actualizare"

#: dontUpdateButton
msgid "Don't update"
msgstr "Nu actualiza"

#: userSelectText
msgid "Please confirm which user you are changing the password for"
msgstr "Vă rugăm confirmați pentru ce utilizator schimbați parola"

#: passwordChangeTitle
msgid "Confirm Password Change"
msgstr "Confirmă schimbarea parolei"

#. # Strings used by PromptService.js
#: rememberPassword
msgid "Use Password Manager to remember this password."
msgstr "Folosește gestionarul de parole pentru a reține această parolă."

#: username
msgid "Username"
msgstr "Utilizator"

#: password
msgid "Password"
msgstr "Parolă"
