#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Ultima sincronizare: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "Ultima sincronizare: în desfășurare…"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Cont: %S"

#: notconnected.label
msgid "Not connected"
msgstr "Neconectat"

#: connecting.label
msgid "Connecting…"
msgstr "Conectare…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "Contul de sincronizat Firefox v-a fost șters"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Anulează"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 nu e compatibil cu ultima versiune de Sincronizare Firefox. Te rugăm "
"să actualizezi la ultima versiune."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 nu e compatibil cu versiunile mai vechi de Sincronizare Firefox. Te "
"rugăm să actualizezi Firefox pe celelalte calculatoare."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Sincronizare Firefox"

#: sync.update.button
msgid "Learn More"
msgstr "Află mai multe"

#: sync.update.close
msgid "Close"
msgstr "Închide"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "Nu s-a putut configura sincronizarea"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "Fără conexiune disponibilă la internet"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr ""
"%S nu s-a putut conecta la serviciul de sincronizare. Dorești să încerci din "
"nou?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Încearcă din nou"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Configurare manuală"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "Nicio filă de pe alte calculatoare."

#. # LOCALIZATION NOTE (promoBanner.message.text): First part of the message displayed in a
#. # banner on about:home. The final space separates this text from the link.
#: promoBanner.message.text
msgid "Sync is brand new and easier than ever. "
msgstr "Sincronizarea este complet nouă și mai ușoară ca niciodată. "

#. # LOCALIZATION NOTE (promoBanner.message.link): Second part of the message, styled as a link.
#: promoBanner.message.link
msgid "Tap here to learn more"
msgstr "Apasă aici pentru a afla mai multe"
