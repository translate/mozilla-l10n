#. Window title
#: windowTitle.label
msgid "Publish Page"
msgstr "Publicare pagină"

#: windowTitleSettings.label
msgid "Publish Settings"
msgstr "Setări de publicare"

#: publishTab.label
msgctxt "publishTab.label"
msgid "Publish"
msgstr "Publică"

#: settingsTab.label
msgid "Settings"
msgstr "Setări"

#: publishButton.label
msgctxt "publishButton.label"
msgid "Publish"
msgstr "Publică"

#. Publish Tab Panel
#: siteList.label siteList.accesskey
msgctxt "siteList.label siteList.accesskey"
msgid "Sit&e Name:"
msgstr "Num&e sait:"

#: siteList.tooltip
msgid "Choose the site that you want to publish to"
msgstr "Alegeți saitul pe care doriți să publicați"

#: newSiteButton.label newSiteButton.accesskey
msgid "&New Site"
msgstr "Sait &nou"

#: docDirList.label docDirList.accesskey
msgid "&Site subdirectory for this page:"
msgstr "&Subdirectorul acestei pagini pe sait:"

#: docDirList.tooltip
msgid "Choose or enter the name of the remote subdirectory for this page"
msgstr ""
"Alegeți sau introduceți numele directorului aflat la distanță pentru această "
"pagină"

#: publishImgCheckbox.label publishImgCheckbox.accesskey
msgid "Include images and &other files"
msgstr "&Include imaginile și celelalte fișiere"

#: publishImgCheckbox.tooltip
msgid "Publish images and other files referenced by this page"
msgstr ""
"Publică imaginile și celelalte fișiere de care are nevoie această pagină"

#: sameLocationRadio.label sameLocationRadio.accesskey
msgid "&Use same location as page"
msgstr "&Folosește aceeași locație ca și pentru pagină"

#: sameLocationRadio.tooltip
msgid "Publish files to the same location as the page"
msgstr "Publică fișierele la aceeași locație ca și pagina"

#: useSubdirRadio.label useSubdirRadio.accesskey
msgid "Use this site sub&directory:"
msgstr "Folosește acest sub&director de pe sait:"

#: useSubdirRadio.tooltip
msgid "Publish files to the selected remote subdirectory"
msgstr "Publică fișierele în directorul de la distanță selectat"

#: otherDirList.tooltip
msgid ""
"Choose or enter name of remote subdirectory where files will be published"
msgstr ""
"Alegeți sau introduceți numele unui subdirector la distanță unde se vor "
"publica fișierele"

#: pageTitle.label pageTitle.accesskey
msgid "Page &Title:"
msgstr "&Titlu pagină:"

#: pageTitle.tooltip
msgid "Enter a title to identify the page in the window and in bookmarks"
msgstr ""
"Introduceți un titlu care să identifice pagina în fereastră și în semnele de "
"carte"

#: pageTitleExample.label
msgid "e.g.: \"My Web Page\""
msgstr "ex.: „Pagina mea de web”"

#: filename.label filename.accesskey
msgid "&Filename:"
msgstr "&Nume fișier:"

#: filename.tooltip
msgid "Enter a name for this file, including '.html' for a web page"
msgstr ""
"Introduceți un nume pentru acest fișier, adăugând '.html' în cazul unei "
"pagini web"

#: filenameExample.label
msgid "e.g,: \"mypage.html\""
msgstr "ex.: „pagina mea.html”"

#: setDefaultButton.label setDefaultButton.accesskey
msgid "Set as &Default"
msgstr "&Implicit"

#: removeButton.label removeButton.accesskey
msgid "&Remove Site"
msgstr "&Elimină saitul"

#. Settings Tab Panel
#: publishSites.label
msgid "Publishing Sites"
msgstr "Saituri de publicare"

#: serverInfo.label
msgid "Server Information"
msgstr "Informații despre server"

#: loginInfo.label
msgid "Login Information"
msgstr "Informații de autentificare"

#: siteName.label siteName.accesskey
msgctxt "siteName.label siteName.accesskey"
msgid "Sit&e Name:"
msgstr "Num&e sait:"

#: siteName.tooltip
msgid "A nickname that identifies this publishing site (e.g.: 'MySite')"
msgstr ""
"Un nume scurt care identifică acest sit de publicare (ex.: „SaitulMeu”)"

#: siteUrl.label siteUrl.accesskey
msgid "Publishing &address (e.g.: 'ftp://ftp.myisp.com/myusername'):"
msgstr "&Adresa de publicare (ex.: „ftp://ftp.isp.ro/numelemeu”):"

#: siteUrl.tooltip
msgid ""
"The FTP:// or HTTP:// address provided by your ISP or web hosting service"
msgstr ""
"Adresa FTP:// sau HTTP:// furnizată de ISP sau de serviciul de găzduire web"

#: browseUrl.label browseUrl.accesskey
msgid ""
"H&TTP address of your homepage (e.g.: 'http://www.myisp.com/myusername'):"
msgstr ""
"&Adresa HTTP a paginii dumneavoastră personale (ex.: „http://www.isp.ro/"
"numelemeu”):"

#: browseUrl.tooltip
msgid "The HTTP:// address of your home directory (don't include filename)"
msgstr ""
"Adresa HTTP:// a directorului dumneavoastră personal (nu includeți nume de "
"fișier)"

#: username.label username.accesskey
msgid "&User name:"
msgstr "&Nume utilizator:"

#: username.tooltip
msgid "The user name you use to log in to your ISP or web hosting service"
msgstr ""
"Numele de utilizator cu care vă autentificați la ISP sau la serviciul de "
"găzduire web"

#: password.label password.accesskey
msgid "Pass&word:"
msgstr "&Parolă:"

#: password.tooltip
msgid "The password associated with your user name"
msgstr "Parola asociată numelui dumneavoastră de utilizator"

#: savePassword.label savePassword.accesskey
msgid "&Save Password"
msgstr "&Salvează parola"

#: savePassword.tooltip
msgid "Select this to save your password securely using Password Manager"
msgstr ""
"Bifați pentru a vă salva securizat parola cu ajutorul Gestionarului de parole"
