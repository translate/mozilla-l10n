msgid "element type"
msgstr "mofuta oa elemente"

#: PETypeSelNotType
msgid "Expected element name or '*' but found '%1$S'."
msgstr ""
"Ho ne ho lebeletsoe lebitso la elemente kapa '*' empa ho fumanoe '%1$S'."

#: PEUnknownNamespacePrefix
msgid "Unknown namespace prefix '%1$S'."
msgstr "Sehlongoapele sa lebitsosebaka le sa tsejoeng '%1$S'."

#: PEAttributeNameEOF
msgid "attribute name"
msgstr "lebitso la popeho"

#: PEAttributeNameExpected
msgid "Expected identifier for attribute name but found '%1$S'."
msgstr ""
"Ho ne ho lebeletsoe setsebahatsi kapa bakeng sa lebitso la popeho empa ho "
"fumanoe '%1$S'."

#: PEAttributeNameOrNamespaceExpected
