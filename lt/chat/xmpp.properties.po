#. # LOCALZIATION NOTE (command.*):
#. #  These are the help messages for each command.
#: command.join3
msgid ""
"%S [&lt;room&gt;[@&lt;server&gt;][/&lt;nick&gt;]] [&lt;password&gt;]: Join a "
"room, optionally providing a different server, or nickname, or the room "
"password."
msgstr ""
"%S [&lt;kambarys&gt;[@&lt;serveris&gt;][/&lt;slapyvardis&gt;]] [&lt;"
"slaptažodis&gt;]: jungtis prie kambario, galimai nurodant kitą serverį, "
"slapyvardį ar kambario slaptažodį."

#: command.part2
msgid "%S [&lt;message&gt;]: Leave the current room with an optional message."
msgstr ""
"%S [&lt;pranešimas&gt;]: išeiti iš kambario, galimai nurodant atsisveikinimo "
"pranešimą."

#: command.topic
msgid "%S [&lt;new topic&gt;]: Set this room's topic."
msgstr "%S [&lt;nauja tema&gt;]: pakeisti kambario temą."

#: command.ban
msgid ""
"%S &lt;nick&gt;[&lt;message&gt;]: Ban someone from the room. You must be a "
"room administrator to do this."
msgstr ""
"%S &lt;slapyvardis&gt;[&lt;pranešimas&gt;]: uždrausti kam nors užeiti į "
"kambarį. Jūs galite tai padaryti, jei esate kambario administratorius."

#: command.kick
msgid ""
"%S &lt;nick&gt;[&lt;message&gt;]: Remove someone from the room. You must be "
"a room moderator to do this."
msgstr ""
"%S &lt;slapyvardis&gt;[&lt;pranešimas&gt;]: pašalinti ką nors iš kambario. "
"Jūs galite tai padaryti, jei esate kambario moderatorius."

#: command.invite
msgid ""
"%S &lt;jid&gt;[&lt;message&gt;]: Invite a user to join the current room with "
"an optional message."
msgstr ""
"%S &lt;jid&gt;[&lt;pranešimas&gt;]: pakviesti vartotoją užeiti į šį kambarį, "
"galimai išsiunčiant jam pranešimą."

#: command.me
msgid "%S &lt;action to perform&gt;: Perform an action."
msgstr "%S &lt;veiksmas&gt;: atlikti veiksmą."

#: command.nick
msgid "%S &lt;new nickname&gt;: Change your nickname."
msgstr "%S &lt;naujas slapyvardis&gt;: keisti slapyvardį."

#: command.msg
msgid ""
"%S &lt;nick&gt; &lt;message&gt;: Send a private message to a participant in "
"the room."
msgstr ""
"%S &lt;slapyvardis&gt; &lt;pranešimas&gt;: nusiųsti asmeninį pranešimą "
"kolegai šiame kambaryje."

#~ msgid "Unknown error"
#~ msgstr "Nežinoma klaida"
