#. # LOCALIZATION NOTE (connection.*)
#. #   These will be displayed in the account manager in order to show the progress
#. #   of the connection.
#. #   (These will be displayed in account.connection.progress from
#. #    accounts.properties, which adds … at the end, so do not include
#. #    periods at the end of these messages.)
#: connection.initializingStream
msgid "Initializing stream"
msgstr "Inicijuojamas srautas"

#: connection.initializingEncryption
msgid "Initializing encryption"
msgstr "Inicijuojamas šifravimas"

#: connection.authenticating
msgid "Authenticating"
msgstr "Nustatoma tapatybė"

#: connection.gettingResource
msgid "Getting resource"
msgstr "Gaunamas išteklius"

#: connection.downloadingRoster
msgid "Downloading contact list"
msgstr "Parsiunčiamas adresatų sąrašas"

#. # LOCALIZATION NOTE (connection.error.*)
#. #   These will show in the account manager if an error occurs during the
#. #   connection attempt.
#: connection.error.invalidUsername
msgid "Invalid username (your username should contain an '@' character)"
msgstr "Naudotojo vardas netinkamas (jame turi būti simbolis „@“)"

#: connection.error.failedToCreateASocket
msgid "Failed to create a socket (Are you offline?)"
msgstr "Nepavyko sukurti lizdo (ar prisijungta prie tinklo?)"

#: connection.error.serverClosedConnection
msgid "The server closed the connection"
msgstr "Serveris užbaigė ryšį"

#: connection.error.resetByPeer
msgid "Connection reset by peer"
msgstr "Partnerinis kompiuteris netikėtai užbaigė ryšį"

#: connection.error.timedOut
msgid "The connection timed out"
msgstr "Ryšiui skirtas laikas baigėsi"

#: connection.error.receivedUnexpectedData
msgid "Received unexpected data"
msgstr "Gauti netikėti / netinkami duomenys"

#: connection.error.incorrectResponse
msgid "Received an incorrect response"
msgstr "Gautas blogas atsakymas"

#: connection.error.startTLSRequired
msgid "The server requires encryption but you disabled it"
msgstr "Serveris reikalauja šifruotojo ryšio, tačiau jis išjungtas nuostatose"

#: connection.error.startTLSNotSupported
msgid ""
"The server doesn't support encryption but your configuration requires it"
msgstr "Serveris nepalaiko šifruotojo ryšio, tačiau jo reikalaujama nuostatose"

#: connection.error.failedToStartTLS
msgid "Failed to start encryption"
msgstr "Nepavyko pradėti šifravimo"

#: connection.error.noAuthMec
msgid "No authentication mechanism offered by the server"
msgstr "Serveris nepasiūlė jokio būdo tapatybei nustatyti"

#: connection.error.noCompatibleAuthMec
msgid ""
"None of the authentication mechanisms offered by the server are supported"
msgstr ""
"Nė vienas iš serverio pasiūlytų tapatybės nustatymo būdų nėra palaikomas"

#: connection.error.notSendingPasswordInClear
msgid ""
"The server only supports authentication by sending the password in cleartext"
msgstr ""
"Vienintelis serverio palaikompas tapatybės nustatymo būdas – nešifruotas "
"slaptažodis"

#: connection.error.authenticationFailure
msgid "Authentication failure"
msgstr "Klaida bandant patvirtinti tapatybę"

#: connection.error.notAuthorized
msgid "Not authorized (Did you enter the wrong password?)"
msgstr "Tapatybė nepatvirtinta (gal įvedėte blogą slaptažodį?)"

#: connection.error.failedToGetAResource
msgid "Failed to get a resource"
msgstr "Nepavyko gauti ištekliaus"

#. # LOCALIZATION NOTE (conversation.error.notDelivered):
#. #   This is displayed in a conversation as an error message when a message
#. #   the user has sent wasn't delivered.
#. #   %S is replaced by the text of the message that wasn't delivered.
#: conversation.error.notDelivered
msgid "This message could not be delivered: %S"
msgstr "Šio pranešimo nepavyko pristatyti: %S"

#. #   This is displayed in a conversation as an error message when joining a MUC
#. #   fails.
#. #   %S is the name of the MUC.
#: conversation.error.joinFailed
msgid "Could not join: %S"
msgstr "Nepavyko įeiti: %S"

#: conversation.error.joinFailedNotAuthorized
msgid "Registration required: You are not authorized to join this room."
msgstr "Jums negalima patekti į šį kambarį – būtina registracija."

#: conversation.error.creationFailedNotAllowed
msgid "Access restricted: You are not allowed to create rooms."
msgstr "Ribota prieiga: jums neleidžiama kurti kambarių."

#. #   This is displayed in a conversation as an error message when remote server
#. #   is not found.
#. #   %S is the name of MUC room.
#: conversation.error.joinFailedRemoteServerNotFound
msgid ""
"Could not join the room %S as the server the room is hosted on could not be "
"reached."
msgstr ""

#. #   These are displayed in a conversation as a system error message.
#: conversation.error.remoteServerNotFound
msgid "Could not reach the recipient's server"
msgstr "Nepavyko susisiekti su gavėjo serveriu"

#: conversation.error.unknownError
msgid "Unknown error"
msgstr "Nežinoma klaida"

#. # LOCALIZATION NOTE (tooltip.*):
#. #   These are the titles of lines of information that will appear in
#. #   the tooltip showing details about a contact or conversation.
#. # LOCALIZATION NOTE (tooltip.status):
#. #   %S will be replaced by the XMPP resource identifier
#: tooltip.status
msgid "Status (%S)"
msgstr "Būsena (%S)"

#: tooltip.statusNoResource
msgid "Status"
msgstr "Būsena"

#: tooltip.subscription
msgid "Subscription"
msgstr "Prenumerata"

#. # LOCALIZATION NOTE (chatRoomField.*):
#. #   These are the name of fields displayed in the 'Join Chat' dialog
#. #   for XMPP accounts.
#. #   The _ character won't be displayed; it indicates the next
#. #   character of the string should be used as the access key for this
#. #   field.
#: chatRoomField.room
msgid "_Room"
msgstr "_Kambarys"

#: chatRoomField.server
msgid "_Server"
msgstr "_Serveris"

#: chatRoomField.nick
msgid "_Nick"
msgstr "Slapy_vardis"

#: chatRoomField.password
msgid "_Password"
msgstr "Slapta_žodis"

#. # LOCALIZATION NOTE (conversation.muc.*):
#. #   These are displayed as a system message when a chatroom invitation is
#. #   received.
#. #   %1$S is the inviter.
#. #   %2$S is the room.
#. #   %3$S is the reason which is a message provided by the person sending the
#. #   invitation.
#: conversation.muc.invitationWithReason
msgid "%1$S has invited you to join %2$S: %3$S"
msgstr "%1$S jus pakvietė į kambarį „%2$S“: %3$S"

#: conversation.muc.invitationWithoutReason
msgid "%1$S has invited you to join %2$S"
msgstr "%1$S jus pakvietė į kambarį „%2$S“"

#. # LOCALIZATION NOTE (options.*):
#. #   These are the protocol specific options shown in the account manager and
#. #   account wizard windows.
#: options.resource
msgid "Resource"
msgstr "Išteklius"

#: options.priority
msgid "Priority"
msgstr "Prioritetas"

#: options.connectionSecurity
msgid "Connection security"
msgstr "Saugusis ryšys"

#: options.connectionSecurity.requireEncryption
msgid "Require encryption"
msgstr "reikalauti šifravimo"

#: options.connectionSecurity.opportunisticTLS
msgid "Use encryption if available"
msgstr "naudoti šifravimą, jeigu pavyks"

#: options.connectionSecurity.allowUnencryptedAuth
msgid "Allow sending the password unencrypted"
msgstr "leisti siųsti nešifruotą slaptažodį"

#: options.connectServer
msgid "Server"
msgstr "Serveris"

#: options.connectPort
msgid "Port"
msgstr "Prievadas"

#: options.domain
msgid "Domain"
msgstr "Sritis"

#. # LOCALIZATION NOTE (*.protocolName)
#. #  This name is used whenever the name of the protocol is shown.
#: gtalk.protocolName
msgid "Google Talk"
msgstr "Google Talk"

#: odnoklassniki.protocolName
msgid "Odnoklassniki"
msgstr "Odnoklassniki"

#. # LOCALIZATION NOTE (gtalk.usernameHint):
#. #  This is displayed inside the accountUsernameInfoWithDescription
#. #  string defined in imAccounts.properties when the user is
#. #  configuring a Google Talk account.
#: gtalk.usernameHint
msgid "email address"
msgstr "el. pašto adresą"

#. # LOCALIZATION NOTE (odnoklassniki.usernameHint):
#. #  This is displayed inside the accountUsernameInfoWithDescription
#. #  string defined in imAccounts.properties when the user is
#. #  configuring a Odnoklassniki account.
#: odnoklassniki.usernameHint
msgid "Profile ID"
msgstr "profilio ID"
