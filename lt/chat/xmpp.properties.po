#: tooltip.email
msgid "Email"
msgstr "El. pašto adresas"

#: tooltip.birthday
msgid "Birthday"
msgstr "Gimtadienis"

#: tooltip.userName
msgid "Username"
msgstr "Naudotojo vardas"

#: tooltip.title
msgid "Title"
msgstr "Pavadinimas"

#: tooltip.organization
msgid "Organization"
