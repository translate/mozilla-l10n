msgstr "Įstaiga (O)"

#: certmgr.certdetail.ou
msgid "Organizational Unit (OU)"
msgstr "Įstaigos padalinys (OU)"

#: certmgr.certdetail.serialnumber
msgid "Serial Number"
msgstr "Numeris"

#: certmgr.certdetail.sha256fingerprint
msgid "SHA-256 Fingerprint"
msgstr "SHA-256 kontrolinis kodas"

#: certmgr.certdetail.sha1fingerprint
msgid "SHA1 Fingerprint"
msgstr "SHA1 kontrolinis kodas"

#: certmgr.begins
msgid "Begins On"
msgstr "Galioja nuo"

#: certmgr.expires
