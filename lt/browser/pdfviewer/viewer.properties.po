#: page_scale_width
msgid "Page Width"
msgstr "Priderinti prie lapo pločio"

#: page_scale_fit
msgid "Page Fit"
msgstr "Pritaikyti prie lapo dydžio"

#: page_scale_auto
msgid "Automatic Zoom"
msgstr "Automatinis mastelis"

#: page_scale_actual
msgid "Actual Size"
