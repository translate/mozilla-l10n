#: toggleToolboxF12.keytext
msgid "F12"
msgstr "F12"

#: toolboxCloseButton.tooltip
msgid "Close Developer Tools"
msgstr "Užverti programuotojų priemones"

#: toolboxOptionsButton.key
msgid "O"
msgstr "O"

#: toolboxNextTool.key
msgid "]"
msgstr "]"

