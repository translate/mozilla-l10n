#: captionMailContent.label
msgid "Mail Content"
msgstr "Laiškų intarpai"

#: acceptRemoteContent.label acceptRemoteContent.accesskey
msgid "Allow re&mote content in messages"
msgstr "&Leisti intarpus iš tinklo laiškuose"

#: remoteContentExceptions.label remoteContentExceptions.accesskey
msgid "E&xceptions…"
msgstr "&Išimtys…"

#: acceptRemoteContentInfo.label
msgid "Learn more about the privacy issues of remote content"
msgstr ""
"Sužinoti daugiau apie privatumo problemas, leidžiant intarpus iš tinklo"

#. Web Content
#: captionWebContent.label
msgid "Web Content"
msgstr "Saityno turinys"

#: keepHistory.label keepHistory.accesskey
msgid "&Remember websites and links I've visited"
msgstr "Įsi&minti lankytas svetaines ir saitus"

#: acceptCookies.label acceptCookies.accesskey
msgid "&Accept cookies from sites"
msgstr "Lei&sti įrašyti slapukus"

#: acceptThirdParty.pre.label acceptThirdParty.pre.accesskey
msgid "A&ccept third-party cookies:"
msgstr "Leis&ti trečiųjų šalių slapukus:"

#: acceptThirdParty.always.label
msgid "Always"
msgstr "visada"

#: acceptThirdParty.never.label
msgid "Never"
msgstr "niekada"

#: acceptThirdParty.visited.label
msgid "From visited"
msgstr "tik lankytų"

#: keepUntil.label keepUntil.accesskey
msgid "&Keep until:"
msgstr "Lai&kyti iki:"

#: expire.label
msgid "they expire"
msgstr "jų galiojimo laiko pabaigos"

#: close.label
msgid "I close &brandShortName;"
msgstr "„&brandShortName;“ seanso pabaigos"

#: askEachTime.label
msgid "ask me every time"
msgstr "klausti prieš priimant slapuką"

#: cookieExceptions.label cookieExceptions.accesskey
msgid "&Exceptions…"
msgstr "I&šimtys…"

#: showCookies.label showCookies.accesskey
msgid "&Show Cookies…"
msgstr "&Rodyti slapukus…"

