#. General Settings
#: submitTelemetry.label submitTelemetry.accesskey
msgid "Submit &Performance Data"
msgstr "Siųsti &našumo ataskaitas"

#: telemetryInfoLink.label
msgid "More information about Telemetry"
msgstr "Išsamėsnė informacija apie telemetriją"

#: enableGlodaSearch.label enableGlodaSearch.accesskey
msgid "&Enable Global Search and Indexer"
msgstr "Įjun&gti globalią laiškų paiešką ir indeksavimą"

#: allowHWAccel.label allowHWAccel.accesskey
msgid "Use &hardware acceleration when available"
msgstr ""

#: scrolling.label
msgid "Scrolling"
msgstr "Slinkimas"

#: useAutoScroll.label useAutoScroll.accesskey
msgid "&Use autoscrolling"
msgstr "Slinkti &automatiškai"

#: useSmoothScrolling.label useSmoothScrolling.accesskey
msgid "Use s&mooth scrolling"
msgstr "Slink&ti tolygiai"

#: systemIntegration.label
msgid "System Integration"
msgstr "Integracija su sistema"

#: alwaysCheckDefault.label alwaysCheckDefault.accesskey
msgid ""
"&Always check to see if &brandShortName; is the default mail client on "
"startup"
msgstr "Paleidžiant „&brandShortName;“ tikrinti, ar ji yra n&umatytoji"

#: searchIntegration.label searchIntegration.accesskey
msgid "Allow &searchIntegration.engineName; to &search messages"
msgstr "&Leisti „&searchIntegration.engineName;“ ieškoti laiškų"

#: checkDefaultsNow.label checkDefaultsNow.accesskey
msgid "Check &Now…"
msgstr "Tikrinti &dabar…"

#: configEditDesc.label
msgid "Advanced Configuration"
msgstr "Papildoma sąranka (patyrusiems naudotojams)."

#: configEdit.label configEdit.accesskey
msgid "&Config Editor…"
msgstr "&Visų parametrų sąrašas…"

#: returnReceiptsInfo.label
msgid "Determine how &brandShortName; handles return receipts"
msgstr "Parinkite pažymų apie pristatytus laiškus nuostatas"

#: showReturnReceipts.label showReturnReceipts.accesskey
msgid "&Return Receipts…"
msgstr "&Laiškų pristatymo pažymos…"

#. Display and Reading Settings
#: reading.caption
msgid "Reading"
msgstr "Skaitymas"

#: display.caption
msgid "Display"
msgstr "Rodymas"

#: showCondensedAddresses.label showCondensedAddresses.accesskey
msgid "&Show only display name for people in my address book"
msgstr ""
"Jei &asmens duomenys įrašyti į adresų knygą, tai rodyti tik jo asmenvardį "
"(be adreso)"

#: autoMarkAsRead.label autoMarkAsRead.accesskey
msgid "&Automatically mark messages as read"
msgstr "Atvertą laišką pa&žymėti kaip skaitytą"

#: markAsReadNoDelay.label markAsReadNoDelay.accesskey
msgid "Immediately &on display"
msgstr "&iškart"

#. This will concatenate to
#. "After displaying for [___] seconds",
#. using (markAsReadDelay.label) and a number (secondsLabel.label).
#: markAsReadDelay.label markAsReadDelay.accesskey
msgid "After &displaying for"
msgstr "&praėjus"

#: secondsLabel.label
msgid "seconds"
msgstr "sek."

#: openMsgIn.label
msgid "Open messages in:"
msgstr "Atveriamą laišką rodyti:"

#: openMsgInNewTab.label openMsgInNewTab.accesskey
msgid "A new &tab"
msgstr "naujoje &kortelėje"

#: reuseExpRadio0.label reuseExpRadio0.accesskey
msgid "A &new message window"
msgstr "&naujame lange"

#: reuseExpRadio1.label reuseExpRadio1.accesskey
msgid "An &existing message window"
msgstr "&tame pačiame (atvertame) lange"

#: closeMsgOnMoveOrDelete.label closeMsgOnMoveOrDelete.accesskey
msgid "&Close message window/tab on move or delete"
msgstr "Perkėlus ar pašalinus laišką, už&verti langą ar kortelę su juo"

#. Update
#: updateApp.label
msgid "&brandShortName; updates:"
msgstr "Programos „&brandShortName;“ atnaujinimas:"

#: updateAuto.label updateAuto.accesskey
msgid "&Automatically install updates (recommended: improved security)"
msgstr "&Automatiškai įdiegti naujinimus (rekomenduojama – didesnis saugumas)"

#: updateCheck.label updateCheck.accesskey
msgid "&Check for updates, but let me choose whether to install them"
msgstr "&Tikrinti, ar yra naujinimų, bet atsiklausti prieš juos įdiegiant"

#: updateManual.label updateManual.accesskey
msgid "&Never check for updates (not recommended: security risk)"
msgstr ""
"&Netikrinti, ar yra naujinimų (nerekomenduojama – galimos saugumo problemos)"

#: updateAutoAddonWarn.label updateAutoAddonWarn.accesskey
msgid "&Warn me if this will disable any of my add-ons"
msgstr "Į&spėti, jei naujinimas nesuderinamas su įdiegtais priedais"

#: updateHistory.label updateHistory.accesskey
msgid "Show U&pdate History"
msgstr "&Rodyti atnaujinimo žurnalą"

#: useService.label useService.accesskey
msgid "Use a &background service to install updates"
msgstr "Naujinimams įdiegti naudoti &fone veikiančią tarnybą"

#. Networking and Disk Space
#: showSettings.label showSettings.accesskey
msgid "&Settings…"
msgstr "&Nuostatos…"

#: proxiesConfigure.label
msgid "Configure how &brandShortName; connects to the Internet"
msgstr "Parinkite „&brandShortName;“ ryšio su internetu nuostatas"

#: connectionsInfo.caption
msgid "Connection"
msgstr "Ryšys"

#: offlineInfo.caption
msgid "Offline"
msgstr "Atsijungimas nuo tinklo"

#: offlineInfo.label
msgid "Configure offline settings"
msgstr "Parinkite darbo neprisijungus prie tinklo nuostatas"

#: showOffline.label showOffline.accesskey
msgid "&Offline…"
msgstr "&Darbas neprisijungus prie tinklo…"

#: Diskspace
msgid "Disk Space"
msgstr "Disko atmintis"

#: offlineCompactFolders.label offlineCompactFolders.accesskey
msgid "Comp&act all folders when it will save over"
msgstr ""
"&Suglaudinti visus aplankus, jei sumoje tai padės sutaupyti daugiau kaip"

#: offlineCompactFoldersMB.label
msgid "MB in total"
msgstr "MB"

#. LOCALIZATION NOTE:
#. The entities useCacheBefore.label and useCacheAfter.label appear on a single
#. line in preferences as follows:
#. &useCacheBefore.label  [ textbox for cache size in MB ]   &useCacheAfter.label;
#: useCacheBefore.label useCacheBefore.accesskey
msgid "&Use up to"
msgstr "&Podėliui skirti iki"

#: useCacheAfter.label
msgid "MB of space for the cache"
msgstr "MB disko atminties"

#: clearCacheNow.label clearCacheNow.accesskey
msgid "&Clear Now"
msgstr "Iš&valyti dabar"

#. Certificates
#: certSelection.description
msgid "When a server requests my personal certificate:"
msgstr "Serveriui paprašius mano liudijimo:"

#: certs.auto
msgid "Select one automatically"
msgstr "parinkti jį automatiškai"

#: certs.auto.accesskey
msgid "S"
msgstr "a"

#: certs.ask
msgid "Ask me every time"
msgstr "visada klausti"

#: certs.ask.accesskey
msgid "A"
msgstr "k"

#: enableOCSP.label enableOCSP.accesskey
msgid ""
"&Query OCSP responder servers to confirm the current validity of certificates"
msgstr "Tikrinti liudijimų galiojimą, užklausiant &OCSP atsakiklių "

#: viewCertificates.label viewCertificates.accesskey
msgid "View &Certificates"
msgstr "&Liudijimų peržiūra"

#: viewSecurityDevices.label viewSecurityDevices.accesskey
msgid "Security &Devices"
msgstr "&Saugumo priemonės"

