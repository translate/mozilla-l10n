#. line in preferences as follows:
#. &useCacheBefore.label  [ textbox for cache size in MB ]   &useCacheAfter.label;
#: useCacheBefore.label useCacheBefore.accesskey
msgid "&Use up to"
msgstr "&Podėliui skirti iki"

#: useCacheAfter.label
msgid "MB of space for the cache"
msgstr "MB disko atminties"

#: clearCacheNow.label clearCacheNow.accesskey
msgid "&Clear Now"
msgstr "Iš&valyti dabar"

#. Certificates
#: certSelection.description
msgid "When a server requests my personal certificate:"
msgstr "Serveriui paprašius mano liudijimo:"

#: certs.auto
msgid "Select one automatically"
msgstr "parinkti jį automatiškai"

#: certs.auto.accesskey
msgid "S"
msgstr "a"

#: certs.ask
msgid "Ask me every time"
msgstr "visada klausti"

#: certs.ask.accesskey
msgid "A"
msgstr "k"

#: enableOCSP.label enableOCSP.accesskey
msgid ""
"&Query OCSP responder servers to confirm the current validity of certificates"
msgstr "Tikrinti liudijimų galiojimą, užklausiant &OCSP atsakiklių "

#: viewCertificates.label viewCertificates.accesskey
msgid "View &Certificates"
msgstr "&Liudijimų peržiūra"

#: viewSecurityDevices.label viewSecurityDevices.accesskey
msgid "Security &Devices"
msgstr "&Saugumo priemonės"

