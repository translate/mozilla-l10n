msgstr "Pabandykite "

#: reloadPlugin.middle
msgid "Reload the page"
msgstr "įkelti tinklalapį iš naujo"

#: reloadPlugin.post
msgid " to try again."
msgstr "."

#. This and the other report.* strings should be as short as possible, ideally 2-3 words.
#: report.please
msgid "Send crash report"
msgstr "Išsiųsti strigties pranešimą"

#: report.submitting
msgid "Sending report…"
msgstr "Pranešimas siunčiamas…"

#: report.submitted
msgid "Crash report sent."
msgstr "Strigties pranešimas išsiųstas."

#: report.disabled
msgid "Crash reporting disabled."
msgstr "Strigčių pranešimai išjungti."

#: report.failed
msgid "Submission failed."
msgstr "Išsiųsti nepavyko."

#: report.unavailable
msgid "No report available."
msgstr "Strigties pranešimo nėra."

#: report.comment
msgid "Add a comment (comments are publicly visible)"
msgstr "Pridėti komentarą (komentarai yra vieši)"

#: report.pageURL
msgid "Include the page's URL"
msgstr "Įtraukti tinklalapio adresą"

#: plugin.file
msgid "File"
msgstr "Failas"

#: plugin.mimeTypes
msgid "MIME Types"
msgstr "MIME tipai"

