#. # note this section of the code may require some tinkering in other languages =(
#. # format in dialog: site [can/cannot] set cookies
#: can
msgid "website can set cookies"
msgstr "gali įrašyti slapukus"

#: cannot
msgid "website cannot set cookies"
msgstr "negali įrašyti slapukų"

#: canSession
msgid "website can set session cookies"
msgstr "gali įrašyti slapukus vienam seansui"

#: domain
msgid "Domain for which this cookie applies:"
msgstr "Slapukas taikomas sričiai:"

#: host
msgid "Server which set the cookie:"
msgstr "Serveris, kuris įdeda slapukus:"

#: hostColon
msgid "Host:"
msgstr "Pagrindinis kompiuteris:"

#: domainColon
msgid "Domain:"
msgstr "Sritis:"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "Tik šifruotasis ryšys"

#: forAnyConnection
msgid "Any type of connection"
msgstr "Bet koks ryšys"

#: expireAtEndOfSession
msgid "At end of session"
msgstr "Seanso pabaigoje"

#: deleteAllCookies
msgid "Are you sure you want to delete all the cookies?"
msgstr "Ar tikrai pašalinti visus slapukus?"

#: deleteAllCookiesTitle
msgid "Remove All Cookies"
msgstr "Visų slapukų šalinimas"

#: deleteAllCookiesYes
msgctxt "deleteAllCookiesYes"
msgid "&Remove"
msgstr "&Pašalinti"

#: deleteAllCookiesSites
msgid "Are you sure you want to delete all of the cookie websites?"
msgstr "Ar tikrai pašalinti visų slapukų svetainių aprašus?"

#: deleteAllSitesTitle
msgid "Remove All Websites"
msgstr "Visų svetainių aprašų šalinimas"

#: deleteAllSitesYes
msgctxt "deleteAllSitesYes"
msgid "&Remove"
msgstr "&Pašalinti"

#: deleteSelectedCookies
msgid "Are you sure you want to delete the selected cookies?"
msgstr "Ar tikrai pašalinti pažymėtus slapukus?"

#: deleteSelectedCookiesTitle
msgid "Remove Selected Cookies"
msgstr "Pažymėtų slapukų šalinimas"

#: deleteSelectedCookiesYes
msgctxt "deleteSelectedCookiesYes"
msgid "&Remove"
msgstr "&Pašalinti"

#: deleteSelectedCookiesSites
msgid "Are you sure you want to delete the selected cookie websites?"
msgstr "Ar tikrai pašalinti pažymėtų svetainių slapukus?"

#: deleteSelectedSitesTitle
msgid "Remove Selected Websites"
msgstr "Pažymėtų svetainių slapukų šalinimas"

#: deleteSelectedSitesYes
msgctxt "deleteSelectedSitesYes"
msgid "&Remove"
msgstr "&Pašalinti"
