#: tabHeader.label
msgid "Tabbed Browsing"
msgstr "Kortelės"

#: tabDisplay.label
msgid "Tab Display"
msgstr "Kortelių juosta"

#: autoHide.label autoHide.accesskey
msgid "Hi&de the tab bar when only one tab is open"
msgstr "&Nerodyti kortelių juostos, kai atverta tik viena kortelė"

#: background.label background.accesskey
msgid "&Switch to new tabs opened from links"
msgstr "Atveriant &saitą naujoje kortelėje, įkelti ją į pirmąjį planą"

#: warnOnClose.label warnOnClose.accesskey
msgid "&Warn me when closing a window with multiple tabs"
msgstr "Įsp&ėti prieš užveriant langą, kuriame yra atvertų kortelių"

#: relatedAfterCurrent.label relatedAfterCurrent.accesskey
msgid "&Open related tabs after current tab"
msgstr "Susijusias korteles atverti &greta esamos"

#: loadGroup.label
msgid "When opening a bookmark group"
msgstr "Atveriant į adresyną įrašytą tinklalapių grupę:"

#: loadGroupAppend.label loadGroupAppend.accesskey
msgid "&Add tabs"
msgstr "Su&kurti naujas korteles"

#: loadGroupReplace.label loadGroupReplace.accesskey
msgid "&Replace existing tabs"
msgstr "&Pakeisti esamas"

#: openTabs.label
msgid "Open tabs instead of windows for"
msgstr "Vietoj naujo naršyklės lango atverti kortelę"
