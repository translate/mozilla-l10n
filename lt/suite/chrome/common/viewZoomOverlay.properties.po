#. # zoom submenu
#. #
#. # LOCALIZATION NOTE: don't translate %zoom% in any property
#. #                    don't use digits "0"-"9" for accesskeys
#. # LOCALIZATION NOTE (fullZoom,textZoom): are never available at the same time
#: fullZoom.label fullZoom.accesskey
msgid "&Zoom (%zoom% %)"
msgstr "&Mastelis (%zoom%%)"

#: textZoom.label textZoom.accesskey
msgid "Text &Zoom (%zoom% %)"
msgstr "Teksto &mastelis (%zoom%%)"

#. # labels and accesskeys to emphasize the 100 % and 200 % entries
#: zoom.100.label zoom.100.accesskey
msgid "100 % (Original Si&ze)"
msgstr "100% (&originalus dydis)"

#: zoom.200.label zoom.200.accesskey
msgid "200 % (&Double Size)"
msgstr "200% (&dvigubas dydis)"

#. # label pattern for remaining values, accesskeys are assigned dynamically
#: zoom.value.label
msgid "%zoom% %"
msgstr "%zoom%%"

#: zoom.other.label zoom.other.accesskey
msgid "&Other (%zoom% %) …"
msgstr "&Kitas (%zoom%%)…"
