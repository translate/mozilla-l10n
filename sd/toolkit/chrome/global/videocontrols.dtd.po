msgid "Play"
msgstr "هلايو"

#: playButton.pauseLabel
msgid "Pause"
msgstr "ساهي "

#: muteButton.muteLabel
msgid "Mute"
msgstr "مؤن بڻايو"

#: muteButton.unmuteLabel
msgid "Unmute"
msgstr "مؤن رد ڪريو"

#: fullscreenButton.enterfullscreenlabel
msgid "Full Screen"
msgstr "سموري اِسڪرن"

#: fullscreenButton.exitfullscreenlabel
msgid "Exit Full Screen"
msgstr "سموري اِسڪرن ٻاهر ڪڍي ڇڏيو"

#: castingButton.castingLabel
msgid "Cast to Screen"
msgstr ""

#: stats.media
msgid "Media"
msgstr "ميڊيا"

#. Do not translate this.  Only change the numeric values if you need this dialogue box to appear bigger
#: stats.size
msgid "Size"
msgstr "آڪار"

#: stats.activity
msgid "Activity"
msgstr "مشغولي"

#: stats.activityPaused
msgid "Paused"
msgstr "ساهي پٽي ويئي"

#: stats.activityPlaying
msgid "Playing"
msgstr "هلائي پيو"

#: stats.activityEnded
msgid "Ended"
msgstr "پورو ٿيل"

#: stats.activitySeeking
msgid "(seeking)"
msgstr "(گُهري پيو)"

#: stats.volume
msgid "Volume"
msgstr "واليوم"

#: stats.framesParsed
msgid "Frames parsed"
msgstr "فريمن جو ڇيد ڪيو ويو"

#: stats.framesDecoded
msgid "Frames decoded"
msgstr "فريم ڊيڪوڊ ڪيا ويا"

#: stats.framesPresented
msgid "Frames presented"
msgstr "فريم پيش ڪيا ويا"

#: stats.framesPainted
msgid "Frames painted"
msgstr "فريم چٽاياويا"

#: error.aborted
msgid "Video loading stopped."
msgstr "ويڊيو لوڊ ڪرڻ روڪيو ويو۔"

#: error.network
msgid "Video playback aborted due to a network error."
msgstr "نيٽ ورڪ جي چوڪ سبب ويڊيو جي پس منظر جو آواز شڦل ڪيو ويو۔"

#: error.decode
msgid "Video can't be played because the file is corrupt."
msgstr "ويڊيو نٿو هلائي سگهجي ڇو تە فائل بگڙيل آهي۔"

#: error.srcNotSupported
msgid "Video format or MIME type is not supported."
msgstr "ويڊيو رچنا يا MIME قسم کي سمرٿن نٿو ڏنو وڃي۔"

#: error.noSource2
msgid "No video with supported format and MIME type found."
msgstr "سمرٿن ڪيل رچنا سان ويڊيو ناهي ۽  MIME قسم جو پڻو لڳو۔"

#: error.generic
msgid "Video playback aborted due to an unknown error."
msgstr "نامعلوم چوڪ سبب ويڊيو جي رڪارڊ ڪيل سامگري هلائڻ نشڦل ڪئي ويئي۔"

#. the #1 string is the current
#. media position, and the #2 string is the total duration. For example, when at
#. the 5 minute mark in a 6 hour long video, #1 would be "5:00" and #2 would be
#. "6:00:00", result string would be "5:00 of 6:00:00 elapsed".
#: scrubberScale.nameFormat
msgid "#1 of #2 elapsed"
msgstr "#2 جو #1  گذري ويل"
