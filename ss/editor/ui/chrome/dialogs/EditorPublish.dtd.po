# Window title
#: windowTitle.label
msgid "Publish Page"
msgstr "Likhasi lekuShicilela"

#: windowTitleSettings.label
msgid "Publish Settings"
msgstr "Inhlelembiso yekuShicilela"

#: publishTab.label
msgid "_: publishTab.label\n"
"Publish"
msgstr "Shicilela"

#: settingsTab.label
msgid "Settings"
msgstr "Inhlelembiso"

#: publishButton.label
msgid "_: publishButton.label\n"
"Publish"
msgstr "Shicilela"

# Publish Tab Panel
#: siteList.label
msgid "_: siteList.label\n"
"Site Name:"
msgstr "Libito leliSayithi:"

#: siteList.tooltip
msgid "Choose the site that you want to publish to"
msgstr "Khetsa lisayithi lofuna kushicilela kulo"

#: newSiteButton.label
msgid "New Site"
msgstr "LiSayithi leLisha"

#: siteDirList.label
msgid "Select or type a directory name in your publishing site:"
msgstr "Khetsa noma uthayiphe libito lenkhomba kulelisayithi lakho lekushicilela:"

#: siteDirList.tooltip
msgid "Choose or enter the name of the remote subdirectory to publish to"
msgstr "Khetsa noma ufake libito lenkhombanyana lekhashane lotoshicilela kuyo"

#: docDirList.label
msgid "Site subdirectory for this page:"
msgstr "Inkhombanyana yalelikhasi kulelisayithi:"

#: publishImgCheckbox.label
msgid "Include images and other files"
msgstr "Hlanganisa titfombe nalamanye emafayela"

#: publishImgCheckbox.tooltip
msgid "Publish images and other files referenced by this page"
msgstr "Shicilela titfombe nalamanye emafayela labhalwe njengemnyombo kulelikhasi"

#: sameLocationRadio.label
msgid "Use same location as page"
msgstr "Sebentisa yona lendzawo njengelikhasi"

#: sameLocationRadio.tooltip
msgid "Publish files to the same location as the page"
msgstr "Shiclela emafayela kuyona lendzawo njengelikhasi"

#: useSubdirRadio.label
msgid "Use this site subdirectory:"
msgstr "Sebentisa lelisayithi njengenkhombanyana:"

#: useSubdir.tooltip
msgid "Choose or enter name of remote subdirectory where files will be published"
msgstr "Khetsa noma ufake libito lenkhombanyana lekhashane lapho emafayela atawushicilelwa khona"

#: chooseDirButton.label
msgid "Choose..."
msgstr "Khetsa..."

#: pageTitle.label
msgid "Page Title:"
msgstr "Sihloko selikhasi:"

#: pageTitle.tooltip
msgid "Enter a title to identify the page in the window and in bookmarks"
msgstr "Faka sihloko kute utfole lelikhsi kulelifasitelo nasemagameni emiculu"

#: pageTitleExample.label
msgid "e.g.: &quot;My Web Page&quot;"
msgstr "sibonelo.: &quot;iWebupheji&quot;"

#: filename.label
msgid "Filename:"
msgstr "Libitofayela:"

#: filename.tooltip
msgid "Enter a name for this file, including '.html' for a web page"
msgstr "Faka libito lalelifayela, ufake phakatsi ne '.html' yelikhasi lewebhuf"

#: filenameExample.label
msgid "e.g,: &quot;mypage.html&quot;"
msgstr "sibonelo.: &quot;lipheji_lami.html&quot;"

#: relatedDocs.label
msgid "Other files to publish"
msgstr "Lamanye emafayela lotowashicilela"

#: setDefaultButton.label
msgid "Set as Default"
msgstr "Kwente kube yiDifolthi"

#: setDefSiteAndDir.label
msgid "Set as Default Site and Directory"
msgstr "Kwente kube yiDifolthi yelisayithi nenkhomba"

#: removeButton.label
msgid "Remove Site"
msgstr "Khweshisa lisayithi"

# Settings Tab Panel
#: publishSites.label
msgid "Publishing Sites"
msgstr "Ishicilela emaSayithi"

#: serverInfo.label
msgid "Server Information"
msgstr "Imininingwane yeSilondvoloti"

#: loginInfo.label
msgid "Login Information"
msgstr "Imininingwane yekuNgena"

#: siteName.label
msgid "_: siteName.label\n"
"Site Name:"
msgstr "Libito leliSayithi:"

#: siteName.tooltip
msgid "A nickname that identifies this publishing site (e.g.: 'MySite')"
msgstr "Libitombumbulu lelikhetsa lisayithi lekushicilela (sibonelo.: 'liSayithi Lami')"

#: siteUrl.label
msgid "Publishing address (e.g.: 'ftp://ftp.myisp.com/myusername'):"
msgstr "Likheli lekushicilela (sibonelo.: 'ftp://ftp.myisp.com/libitomsebentilami'):"

#: siteUrl.tooltip
msgid "The FTP:// or HTTP:// address provided by your ISP or web hosting service"
msgstr "Le FTP:// noma HTTP:// likheli loliniketwa yi ISP yakho noma loniketa lusito lwewebhu"

#: browseUrl.label
msgid "HTTP address of your homepage (e.g.: 'http://www.myisp.com/myusername'):"
msgstr "HTTP likheli lelikhayapheji lakho (sibonelo.: 'http://www.myisp.com/libitomsebentilami'):"

#: browseUrl.tooltip
msgid "The HTTP:// address of your home directory (don't include filename)"
msgstr "Le HTTP:// likheli lenkhomba yelikhaya lakho (ungalifaki libitofayela)"

#: username.label
msgid "User name:"
msgstr "Libito lemsebentisi:"

#: username.tooltip
msgid "The user name you use to log in to your ISP or web hosting service"
msgstr "Lelibito lemsebentisi ulisebentisa kungena ku ISP yakho noma loniketa lusito lwewebhu"

#: password.label
msgid "Password:"
msgstr "LeSikhiya:"

#: password.tooltip
msgid "The password associated with your user name"
msgstr "LeSikhiya sisebentisana nelibito lakho lemsebenisi"

#: savePassword.label
msgid "Save Password"
msgstr "Londvolota leSikhiya"

#: savePassword.tooltip
msgid "Select this to save your password securely using Password Manager"
msgstr "Khetsa loku kulondvolota sikhiya sakho ngalokuphephile usebentisa Menenja weSikhiya"

