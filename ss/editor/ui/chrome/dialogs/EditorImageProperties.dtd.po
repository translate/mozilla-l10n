# (pofilter) accelerators: accelerator & is missing from translation
#: altText.label
#: altText.accessKey
msgid "&Alternate text:"
msgstr "Umbhalo wekuntjintjisa:"

#: altTextEditField.tooltip
msgid "Type text to display in place of the image"
msgstr "Thayipha umbhalo utewukhombisa endzaweni yesitfombe"

# (pofilter) accelerators: accelerator & is missing from translation
#: noAltText.label
#: noAltText.accessKey
msgid "&Don't use alternate text"
