#: type-4
msgid "Theme"
msgstr "Ingcikitsi"

#: type-2
msgid "Extension"
msgstr "Sichumanisi"

#: incompatibleTitle
msgid "Incompatible %S"
msgstr "Lokungahambelani na %S"

#: incompatibleMsg
msgid "%S %S could not be installed because it is not compatible with %S %S. (%S %S will only work with %S versions from %S to %S)"
msgstr "%S %S akakwati kwakheleka ngobe akuvumelani na %S %S. (%S %S utawusebenta kuphela nga %S version kusuka ku %S ku %S)"

#: incompatibleMsgSingleAppVersion
msgid "%S %S could not be installed because it is not compatible with %S %S. (%S %S will only work with %S %S)"
msgstr "%S %S akakwati kwakheleka ngobe akuvumelani na %S %S. (%S %S utawusebenta kuphela nga %S version kusuka ku %S ku %S)"

#: incompatibleMessageNoApp
msgid "%S %S could not be installed because it is not compatible with %S."
msgstr "%S %S akakwati kwakheleka ngobe akuvumelani na %S."

#: incompatibleOlder
msgid "versions 0.8 or older."
msgstr "lihumusho 0.8 nome lendzala kakhulu."

#: incompatibleThemeName
msgid "this Theme"
msgstr "lengcikitsi"

#: incompatibleExtension
msgid "Disabled - not compatible with %S %S"
msgstr "Khinyabetekile - ayivumelani na %S %S"

#: invalidGUIDMessage
msgid "\"%S\" could not be installed because of an error in its Install Manifest (\"%S\" is not a valid GUID). Please contact the author of this item about the problem."
msgstr "\"%S\" akakheleki ngenca yengoti kubonakala kwayo nayifakwako (\"%S\" ayikho validi GUID). Uyacelwa kutsi utsintse umbhali waleliphuzu ngenkinga."

#: invalidVersionMessage
msgid "\"%S\" could not be installed because of an error in its Install Manifest (\"%S\" is not a valid Version String). Please contact the author of this item about the problem."
msgstr "\"%S\" akakheleki ngenca yengoti kubonakala kwayo nayifakwako (\"%S\" ayisiyo intsambo levalidi). Uyacelwa kutsi utsintse umbhali waleliphuzu ngenkinga."

#: incompatiblePlatformMessage
msgid "\"%S\" could not be installed because it is not compatible with your %S build type (%S). Please contact the author of this item about the problem."
msgstr "\"%S\" akakheleki ngobe ayivumelani na %S build type (%S). Uyacelwa kutsi utsintse umbhali waleliphuzu ngenkinga."

#: missingFileTitle
msgid "Missing File"
msgstr "Emafayela lalahlekile"

#: missingFileMessage
msgid "%S could not load this item because the file %S was missing."
msgstr "%S akakwati kulayisha leliphuzu ngobe lifayela %S belingatfolakali."

#: missingFileConsoleMessage
msgid "Failed to install from %S because %S was not provided at the top level of the jar/xpi file."
msgstr "Akakheleki kusuka ku %S ngobe %S abengakafakwa ezingeni lelisetulu kwa jar/xpi lifayela."

#: malformedMessage
msgid "%S could not install this item because \"%S\" (provided by the item) is not well-formed or does not exist. Please contact the author about this problem."
msgstr "%S akakwati kwakheleka kusuka ku %S ngobe \"%S\" (leliphuma kuleliphuzu) alikakheki kahle nome alikho vele. Uyacelwa kutsi utsintsane nembhali ngalenkinga."

#: malformedTitle
msgid "Malformed File"
msgstr "Lifayela lelingakakheki kahle"

#: malformedRegistrationTitle
msgid "Chrome Registration Failed"
msgstr "Kwehlulekile kubhalisa ikhromu"

#: malformedRegistrationMessage
msgid "%S could not install this item because of a failure in Chrome Registration. Please contact the author about this problem."
msgstr "%S akakwati kwakheleka leliphuzu ngenca yekwehluleka kwekubhaliswa kwekhromu. Uyacelwa kutsi utsintsane nembhali ngalenkinga."

#: errorInstallTitle
msgid "Error"
msgstr "Ingoti"

#: errorInstallMsg
msgid "%S could not install the file at \n"
"\n"
"%S\n"
"\n"
"because: %S"
msgstr "%S akakwati kufaka lelifayela ku \n\n%S\n\nngobe: %S"

