msgstr "Libitolalovakashelwe lelingenisiwe alikho emtsetfweni"

#### Master Password
#: password_not_set
msgid "(not set)"
msgstr "(akukahlelwa)"

#: failed_pw_change
msgid "Unable to change Master Password."
msgstr "Kumatima kugucula libitophawu lelikhulu."

#: incorrect_pw
msgid "You did not enter the correct current Master Password. Please try again."
msgstr "Awukalingenisi libitophawu lelikhulu lanyalo lelingilo"

#: pw_change_ok
msgid "Master Password successfully changed."
msgstr "Libitophawu lelikhulu liguculwe kahle"

#: pw_erased_ok
msgid "You have deleted your Master Password."
msgstr "Ucishe libitophawu lakho lelikhulu."

#: pw_not_wanted
msgid "Warning! You have decided not to use a Master Password."
msgstr "Secwayiso:Ukhetse kungasebentisa libitophawu lelikhulu."

#: pw_empty_warning
msgid "Your stored web and email passwords, form data, and private keys will not be protected."
msgstr "Ugcine iwebhu nemabitophawu e-imeyili.kwakha lwati, netikhiya tangasese atinovikeleka."

#: pw_change2empty_in_fips_mode
msgid "You are currently in FIPS mode. FIPS requires a non-empty Master Password."
msgstr "Kwanyalo usendleleni ye-FIPS.I_FIPS idzinga  libitophawu lelikhulu leligcwele."

#: pw_change_success_title
msgid "Password Change Succeeded"
msgstr "Kuguculwa kwelibitophau kuphumelele."

#: pw_change_failed_title
msgid "Password Change Failed"
msgstr "Kuguculwa kwelibitophawu kuhlulekile"

