#: tabsTab.label
msgid "Tabs"
msgstr "Emathebhu"

#: openExternalLinksIn.label
msgid "Open links from other applications in:"
msgstr "Vula luchumano kuletinye ticelo letiku:"

#: newWindow.label
msgid "a new window"
msgstr "Liwindo lelisha"

#: newTabInRecentWindow.label
msgid "a new tab in the most recent window"
msgstr "Ithebhu lensha kuwindo lesandza kuvulwa"

#: recentWindowOrTab.label
msgid "the most recent tab/window"
msgstr "Liwindo/ithebhu lesandza kuvulwa"

#: forceNewWindowLinks.label
msgid "Force links that open new windows to open in:"
msgstr "Cindzetela tichumanisi letivula emawindo lamasha kutsi tivule ekahatsi"

#: sameTabOrWindow.label
msgid "the same tab/window as the link"
msgstr "Yona lethebhu/liwindo njengeluchumano"

#: newTab.label
msgid "a new tab"
msgstr "Ithebhu lensha "

#: hideTabBar.label
msgid "Hide the tab bar when only one web site is open"
msgstr "Fihla ithebhu-bha nangabe yinye kuphela iwebsayithi levulekile"

#: loadInBackground.label
msgid "Select new tabs opened from links"
msgstr "Khetsa emathebhu lamasha kuletichumanisi"

#: loadBookmarksInBackground.label
msgid "Select new tabs opened from bookmarks or history"
msgstr "Khetsa emathebhu lamasha kuletibekisi noma emlandvweni"

#: warnOnClose.label
msgid "Warn when closing multiple tabs"
msgstr "Yecwayisa nawutawuvala emathebhu lamanyenti"

