#: 1
msgid "Parsing an XSLT stylesheet failed."
msgstr "Kwakha inshokutsi yesitayelashidi XSLT yehlulekile."

#: 2
msgid "Parsing an XPath expression failed."
msgstr "Kwakha inshokutsi yekubeka luvo la XPath kuhlulekile."

#: 4
msgid "XSLT transformation failed."
msgstr "Ingucuko ya XSLT yehlulekile."

#: 5
msgid "Invalid XSLT/XPath function."
msgstr "Lengasebenti XSLT/XPath umsebenti."

#: 6
msgid "XSLT Stylesheet (possibly) contains a recursion."
msgstr "Sitayelashidi XSLT (lokufanele) kuba nekuphindzaphindza."

#: 7
msgid "Attribute value illegal in XSLT 1.0."
msgstr "Lokwengetiwe akukho emtsetfweni ku XSLT 1.0."

#: 8
msgid "An XPath expression was expected to return a NodeSet."
msgstr "An XPath lokuboniswako bekufanele kutsi kujike NodeSet."

#: 9
msgid "XSLT transformation was terminated by <xsl:message>."
msgstr "Ingucuko ya XSLT icedvwe ngu <xsl:message>,"

#: 10
msgid "A network error occured loading an XSLT stylesheet:"
msgstr "Liphutsa lekuchumana livelile nalilayisha sitayelashidi XSLT:"

#: 11
msgid "An XSLT stylesheet does not have an XML mimetype:"
msgstr "Sitayelashidi XSLT asinalo luhlobo XML lekumayima:"

#: 12
msgid "An XSLT stylesheet directly or indirectly imports or includes itself:"
msgstr "Sitayelashidi XSLT ungenisangco noma nsombo noma siyatifaka:"

#: 13
msgid "An XPath function was called with the wrong number of arguments."
msgstr "An XPath umsebenti wabito ngenamba lengesiyo yetimphikiswano."

#: 14
msgid "An unknown XPath extension function was called."
msgstr "Indlela yemsebenti losingetelelo longatiwa XPath wabitwa."

#: 15
msgid "XPath parse failure: ')' expected:"
msgstr "XPath kuhluleka kwe parse: ')' lokulindzelekile:"

#: 16
msgid "XPath parse failure: invalid axis:"
msgstr "XPath kuhluleka kwe parse: ema axis lokungasiwo:"

#: 17
msgid "XPath parse failure: Name or Nodetype test expected:"
msgstr "XPath kuhluleka kwe parse: Libito nome iNodetype luhlolo lolulindzelwe:"

#: 18
msgid "XPath parse failure: ']' expected:"
msgstr "XPath kuhluleka kwe parse: ']' kulindzelekile:"

#: 19
msgid "XPath parse failure: invalid variable name:"
msgstr "XPath kuhluleka kwe parse: Libito lelingasebenti lelihlangahlangene:"

#: 20
msgid "XPath parse failure: unexpected end of expression:"
msgstr "XPath kuhluleka kwe parse: unexpected kuphela kwenkhulumiswano:"

#: 21
msgid "XPath parse failure: operator expected:"
msgstr "XPath kuhluleka kwe parse: kulindzeleke lephetse:"

#: 22
msgid "XPath parse failure: unclosed literal:"
msgstr "XPath kuhluleka kwe parse: lengakavaleki sanhlobo:"

#: 23
msgid "XPath parse failure: ':' unexpected:"
msgstr "XPath kuhluleka kwe parse: ':' lengakalindzeleki:"

# (review) do not translate: not()
#: 24
msgid "XPath parse failure: '!' unexpected, negation is not():"
msgstr "XPath kuhluleka kwe parse: '!' inegation lengakalindzeleki ():"

#: 25
msgid "XPath parse failure: illegal character found:"
msgstr "XPath kuhluleka kwe parse: kutfolakele luhlavu lolungekho emtsetfweni:"

#: 26
msgid "XPath parse failure: binary operator expected:"
msgstr "XPath kuhluleka kwe parse: kulindzeleke binarary lephetse:"

#: 27
msgid "An XSLT stylesheet load was blocked for security reasons."
msgstr "Kulayisha sitayelashidi XSLT kwavimbeleka ngetizatfu tekuvikeleka."

#: LoadingError
msgid "Error loading stylesheet: %S"
msgstr "Liphutsa lekulayisha sitayelashidi: %S"

#: TransformError
msgid "Error during XSLT transformation: %S"
msgstr "Liphutsa ngesikhatsi kugucuka XSLT: %S"
