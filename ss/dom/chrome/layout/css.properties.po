#: MimeNotCss
msgid "The stylesheet %1$S was not loaded because its MIME type, \"%2$S\", is not \"text/css\"."
msgstr "Lishithi lesitayela %1$S beyingakalayisheki ngobe inhlobo yayo iMIME, \"%2$S\", ayisiyo i \"text/css\"."

#: MimeNotCssWarn
msgid "The stylesheet %1$S was loaded as CSS even though its MIME type, \"%2$S\", is not \"text/css\"."
msgstr "Lishithi lesitayela %1$S lilayisheke njenge CSS nome-ke inhlobo yayo iyi MIME, \"%2$S\", ayisiyo i \"text/css\"."

#: PEUnexpEOF
msgid "Unexpected end of file while searching for %1$S."
msgstr "Kube nekuncamuka kwelifayela ngesikhatsi isaphenya I%1$S."

#: PEParseRuleWSOnly
msgid "Whitespace-only string given to be parsed as rule."
msgstr "Sikhala lesimhlophe kuphela sentsambo leniketwe kutsi ihlahlelwe njengesimiso."

#: PEDeclDropped
msgid "Declaration dropped."
msgstr "Simemetelo siyekelwe."

#: PEDeclSkipped
msgid "Skipped to next declaration."
msgstr "Sigalajiselwe kulokulandzelako simemetelo."

#: PEUnknownProperty
msgid "Unknown property '%1$S'."
msgstr "Into Lengatiwa ngu le '%1$S'."

#: PEPropertyParsingError
msgid "Error in parsing value for property '%1$S'."
msgstr "Kube nengoti nakuhlahlelwa licabiso lemphahla lengu '%1$S'."

#: PEExpectEndProperty
msgid "Expected end of value for property but found '%1$S'."
msgstr "Kubhekeke kugcina kwe licabiso lalemphahla kovwa kutfolakele '%1$S'."

# (review) can we lose the lonly dash at the end please
#: PESkipAtRuleEOF
msgid "end of unknown at-rule"
msgstr "kuphela kwalokungatiwa esimisweni -"

#: PEUnknownAtRule
msgid "Unrecognized at-rule or error parsing at-rule '%1$S'."
msgstr "Kuyaphikwa esimisweni- nome ingoti ekuhlahleleni esimesweni- '%1$S'."

#: PECharsetRuleEOF
msgid "charset string in @charset rule"
msgstr "intsambo icharset ku @charset simiso"

#: PECharsetRuleNotString
msgid "Expected charset string but found '%1$S'."
msgstr "Kubhekeke intsambo ye-charset kodvwa kutfolakele i '%1$S'."

# (review) do not translate: @media
#: PEGatherMediaEOF
msgid "end of media list in @import or @media rule"
msgstr "kuphela kweluhlu lwemidiya ku @import nome @midiaya yemtsetfo"

#: PEGatherMediaNotComma
msgid "Expected ',' in media list but found '%1$S'."
msgstr "Kubhekeke ',' kuluhlu lwemidiya kodvwa kutfolakele I '%1$S'."

#: PEGatherMediaNotIdent
msgid "Expected identifier in media list but found '%1$S'."
msgstr "Kubhekeke umatisi kuluhlu lwemidiya kodvw kutfolakele I '%1$S'."

#: PEImportNotURI
msgid "Expected URI in @import rule but found '%1$S'."
msgstr "Kubhekeke URI ku @import simiso kodvw kutfolakele I '%1$S'."

#: PEImportUnexpected
msgid "Found unexpected '%1$S' within @import."
msgstr "Kutfolakele kungakabhekwa '%1$S' ngekhatsi @import."

# (review) do not translate: @media
#: PEGroupRuleEOF
msgid "end of @media or @-moz-document rule"
msgstr "kuphela kwe @mediya nome @-moz-document rule"

#: PEMozDocRuleBadFunc
msgid "Expected url(), url-prefix(), or domain() in @-moz-document rule but found '%1$S'."
msgstr "Kubhekeke url (), url-prefix(), nome domain() ku @-moz-document rule kodvwa kutfolakele '%1$S'."

#: PEMozDocRuleNotURI
msgid "Expected URI in @-moz-document rule but found '%1$S'."
msgstr "Kubhekeke URI ku @-moz-document rule kodvwa kwatfolakala '%1$S'."

#: PEAtNSPrefixEOF
msgid "namespace prefix in @namespace rule"
msgstr "libitosikhala seprifiksi ku @namespace rule"

#: PEAtNSURIEOF
msgid "namespace URI in @namespace rule"
msgstr "libitosikhala URI ku @namespace rule"

#: PEAtNSUnexpected
msgid "Unexpected token within @namespace: '%1$S'."
msgstr "Lesingakalindzeleki sibongo ekhatsi ku @namespace: '%1$S'."

#: PESkipDeclBraceEOF
msgid "closing } of declaration block"
msgstr "kuvala } kwekumiswa kwesimemetelo"

#: PESkipRSBraceEOF
msgid "closing } of invalid rule set"
msgstr "kuvala } kwesimiso lesibekiwe lesingasebenti"

#: PEBadSelectorRSIgnored
msgid "Ruleset ignored due to bad selector."
msgstr "Itsalaliselwe imitsetfo lebekiwe ngenca yekukhetsa lokubi."

#: PESelectorListExtraEOF
msgid "',' or '{'"
msgstr "',' nome '{'"

# (pofilter) brackets: translation is missing '{'
#: PESelectorListExtra
msgid "Expected ',' or '{' but found '%1$S'."
msgstr "Kubhekwe ',' kodvwa kwatfolakala '%1$S'."

#: PESelectorGroupNoSelector
msgid "Selector expected."
msgstr "Kubhekeke umphawuli."

#: PESelectorGroupExtraCombinator
msgid "Dangling combinator."
msgstr "Sibumbanisi lesilengako."

#: PEIDSelNotIdent
msgid "Expected identifier for ID selector but found '%1$S'."
msgstr "Kubhekeke Umatisi lophawula ID kodvwa kwatfolakala '%1$S'."

#: PEIDSelEmpty
msgid "Expected non-empty name for ID selector, but found '%1$S'."
msgstr "Kubhekeke libito lelinelutfo le lalophawula ID, kodvwa kwatfolakala '%1$S'."

#: PEClassSelEOF
msgid "class name"
msgstr "libito leliklilasi"

#: PEClassSelNotIdent
msgid "Expected identifier for class selector but found '%1$S'."
msgstr "Kubhekeke matisi walophawula liklilasi kodvwa kwatfolakala '%1$S'."

#: PETypeSelEOF
msgid "element type"
msgstr "incenye yenhlobo"

#: PETypeSelNotType
msgid "Expected element name or '*' but found '%1$S'."
msgstr "Kubhekeke incenye yelibito nome '*' kodvwa kwatfolakala '%1$S'."

#: PEUnknownNamespacePrefix
msgid "Unknown namespace prefix '%1$S'."
msgstr "Libitosikhala Lelingatiwa neprefiksi '%1$S'."

#: PEAttributeNameEOF
msgid "attribute name"
msgstr "yetsa libito"

#: PEAttributeNameExpected
msgid "Expected identifier for attribute name but found '%1$S'."
msgstr "Kubhekeke matisi wekwetsa libito kodvwa kwatfolakala '%1$S'."

#: PEAttributeNameOrNamespaceExpected
msgid "Expected attribute name or namespace but found '%1$S'."
msgstr "Kubhekeke kwetsiwe libito nome libitosikhala kodvwa kwatfolakala '%1$S'."

#: PEAttSelNoBar
msgid "Expected '|' but found '%1$S'."
msgstr "Kubhekeke '|' kodvwa kwatfolakala '%1$S'."

#: PEAttSelInnerEOF
msgid "part of attribute selector"
msgstr "incenye yekwetsa siphawuli"

#: PEAttSelUnexpected
msgid "Unexpected token in attribute selector: '%1$S'."
msgstr "Kubhekeke sibongwana nje sekwetsa siphawuli: '%1$S'."

#: PEAttSelValueEOF
msgid "attribute value"
msgstr "yetsa licabiso"

#: PEAttSelCloseEOF
msgid "']' to end attribute selector"
msgstr "']' kutewuphela kwetsa siphawuli"

#: PEAttSelNoClose
msgid "Expected ']' to terminate attribute selector but found '%1$S'."
msgstr "Kubhekeke ']' itewucedza kwetsa siphawuli kodvwa kwatfolakala '%1$S'."

#: PEAttSelBadValue
msgid "Expected identifier or string for value in attribute selector but found '%1$S'."
msgstr "Kubhekeke matisi nome intsambo yelicabiso ekwetseni siphawuli kodvwa kwatfolakala '%1$S'."

#: PEPseudoSelEOF
msgid "name of pseudo-class or pseudo-element"
msgstr "libito leliklilasimbumbulu nome incenyembumbulu"

#: PEPseudoSelBadName
msgid "Expected identifier for pseudo-class or pseudo-element but found '%1$S'."
msgstr "Kubhekeke matisi weliklilasimbumbulu nome lilungambumbulukodvwa kwatfolakala '%1$S'."

#: PEPseudoSelNonFunc
msgid "Function token for non-function pseudo-class or pseudo-element, or the other way around, when reading '%1$S'."
msgstr "Siboniso lesisebentako seliklilasimbumbulu nome lilungambumbulu, nome lenye nje indlela, nakufundvwa '%1$S'."

#: PEPseudoSelNotPE
msgid "Expected pseudo-element but found '%1$S'."
msgstr "Kubhekeke lilungambumbulu kodvwa kwatfolakala '%1$S'."

#: PEPseudoSelDoubleNot
msgid "Negation pseudo-class can't be negated '%1$S'."
msgstr "Kunyembenya liklilasimbumbulu ngeke kunyembenye '%1$S'."

#: PEPseudoSelPEInNot
msgid "Pseudo-elements can't be negated '%1$S'."
msgstr "Emalungambumbulu ngeke anyembenye '%1$S'."

#: PEPseudoSelNewStyleOnly
msgid "This pseudo-element must use the \"::\" form: '%1$S'."
msgstr "Lelilungambumbulu kufanele lisebentise i \"::\" simo: '%1$S'."

#: PEPseudoSelTrailing
msgid "Found trailing token after pseudo-element, which must be the last part of a selector:  '%1$S'."
msgstr "Kutfolakele kulandzela sibonisokubonga emva kwelilungambumbulu, lekufanele libe lekugcina lesiphawuli:  '%1$S'."

#: PEPseudoSelMultiplePE
msgid "Extra pseudo-element '%1$S'."
msgstr "Lilungambumbulu lelingetulu '%1$S'."

#: PEPseudoSelUnknown
msgid "Unknown pseudo-class or pseudo-element '%1$S'."
msgstr "Liklilasimbumbulu lelingatiwa nome lilungambumbulu '%1$S'."

#: PENegationEOF
msgid "selector within negation"
msgstr "siphawuli emkhatsini wekunyembenya"

#: PENegationBadInner
msgid "Malformed simple selector as negation pseudo-class argument '%1$S'."
msgstr "Siphawuli lesilula lesakheke kabi njengemphikiswane yekunyembenya liklilasimbumbulu ' %1$S'."

#: PENegationNoClose
msgid "Missing closing ')' in negation pseudo-class '%1$S'."
msgstr "Kute kuvaleka ')' ekunyembenyeni liklilasimbumbulu '%1$S'."

#: PENegationBadArg
msgid "Missing argument in negation pseudo-class '%1$S'."
msgstr "Kute kuphikisana ekunyembenyeni liklilasimbumbulu '%1$S'."

# (review) do not translate: :lang
#: PELangArgEOF
msgid "argument to :lang selector"
msgstr "kuphikisana ku : eluhlangotsini lwetilimi"

#: PELangArgNotIdent
msgid "Expected identifier for lang pseudo-class parameter but found '%1$S'."
msgstr "Kubhekeke matisi wepharamitha yelulwimi lweliklilasimbumbulu kodvwa kwatfolakala '%1$S'."

#: PELangNoClose
msgid "Missing closing ')' in lang pseudo-class '%1$S'."
msgstr "Kute kuvaleka ')' ekunyembenyeni liklilasimbumbulu '%1$S'."

#: PELangNoArg
msgid "Missing argument in lang pseudo-class '%1$S'."
msgstr "Kute kuphikisana elulwimini lweliklilasimbumbulu '%1$S'."

#: PESelectorEOF
msgid "selector"
msgstr "siphawuli"

#: PEBadDeclBlockStart
msgid "Expected '{' to begin declaration block but found '%1$S'."
msgstr "Kubhekeke '{' kucalisa kuvinjwa kwesimemetelo kodvwa kwatfolakala '%1$S'."

#: PEColorEOF
msgid "color"
msgstr "umbala"

#: PEColorNotColor
msgid "Expected color but found '%1$S'."
msgstr "Kubhekeke umbala kodvwa kwatfolakala '%1$S'."

#: PEColorComponentEOF
msgid "color component"
msgstr "kwakheka kwemmbala"

#: PEExpectedPercent
msgid "Expected a percentage but found '%1$S'."
msgstr "Kubhekeke iphesenteji kodvwa kwatfolakala '%1$S'."

#: PEExpectedInt
msgid "Expected an integer but found '%1$S'."
msgstr "Kubhekeke sibalo kodvwa kwatfolakala ' %1$S'."

#: PEColorBadRGBContents
msgid "Expected number or percentage in rgb() but found '%1$S'."
msgstr "Kubhekeke inombolo nome iphesenteji ku rgb() kodvwa kwatfolakala '%1$S'."

#: PEColorComponentBadTerm
msgid "Expected '%2$S' but found '%1$S'."
msgstr "Kubhekeke ' %2$S' kodvwa kwatfolakala ' %1$S'."

#: PEColorHueEOF
msgid "hue"
msgstr "hyu"

#: PEExpectedComma
msgid "Expected ',' but found '%1$S'."
msgstr "Kubhekeke ',' kodvwa kwatfolakala ' %1$S'."

#: PEColorSaturationEOF
msgid "saturation"
msgstr "kwenela"

#: PEColorLightnessEOF
msgid "lightness"
msgstr "bulula"

#: PEColorOpacityEOF
msgid "opacity in color value"
msgstr "kufihlakala kwelicabiso lembala"

#: PEExpectedNumber
msgid "Expected a number but found '%1$S'."
msgstr "Kubhekeke inombolo kodvwa kwatfolakala '%1$S'."

#: PEExpectedCloseParen
msgid "Expected ')' but found '%1$S'."
msgstr "Kubhekeke ')' kodvwa kwatfolakala '%1$S'."

#: PEDeclEndEOF
msgid "';' or '}' to end declaration"
msgstr "';' nome '}' kucedza simemetelo"

#: PEParseDeclarationNoColon
msgid "Expected ':' but found '%1$S'."
msgstr "Kubhekeke ':' kodvwa kwatfolakala '%1$S'."

#: PEParseDeclarationDeclExpected
msgid "Expected declaration but found '%1$S'."
msgstr "Kubhekeke simemetelo kodvwa kwatfolakala '%1$S'."

#: PEEndOfDeclEOF
msgid "end of declaration"
msgstr "kuphela kwesimemetelo"

#: PEImportantEOF
msgid "important"
msgstr "kumcoka"

#: PEExpectedImportant
msgid "Expected 'important' but found '%1$S'."
msgstr "Kubhekeke 'lokumcoka' kodvwa kwatfolakala '%1$S'."

#: PEBadDeclEnd
msgid "Expected ';' to terminate declaration but found '%1$S'."
msgstr "Kubekeke ';' kutewucedza simemetelo kodvwa kwatfolakala '%1$S'."

#: PEBadDeclOrRuleEnd
msgid "Expected ';' or '}' to terminate declaration but found '%1$S'."
msgstr "Kubhekeke ';' nome '}' kutewucedza simemetelo kodvwa kwatfolakala '%1$S'."

#: PEInaccessibleProperty
msgid "Attempt to use inaccessible property"
msgstr "Umetamo wekusebentisa imphahla lekungangeneki kuyo"

#: PECommentEOF
msgid "end of comment"
msgstr "siphelo sekhomenti"

#: SEUnterminatedString
msgid "Found unclosed string '%1$S'."
msgstr "Kutfolakele intsambo lengakavalwa '%1$S'."

