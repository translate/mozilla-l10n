# SSL2 Ciphers
#: cipher.ssl2.rc4_128
msgid "_: cipher.ssl2.rc4_128\n"
"128-bit RC4 encryption with RSA and an MD5 MAC"
msgstr "128-bit RC4 ingucuko ne RSA ne MD5 MAC"

#: cipher.ssl2.rc2_128
msgid "128-bit RC2 encryption with RSA and an MD5 MAC"
msgstr "128-bit RC2 ingucuko ne RSA ne MD5 MAC"

#: cipher.ssl2.des_ede3_192
msgid "168-bit Triple DES encryption with RSA and MD5 MAC "
msgstr "168-bit TripleDEC  ingucuko ne RSA ne MD5 MAC "

#: cipher.ssl2.des_64
msgid "56-bit DES encryption with RSA and an MD5 MAC"
msgstr "40-bit RC4 ingucuko ne RSA ne MD5 MAC (tfumela)"

#: cipher.ssl2.rc4_40
msgid "_: cipher.ssl2.rc4_40\n"
"40-bit RC4 encryption with RSA and an MD5 MAC (export)"
msgstr "40-bit RC4 ingucuko ne RSA ne MD5 MAC (tfumela))"

#: cipher.ssl2.rc2_40
msgid "_: cipher.ssl2.rc2_40\n"
"40-bit RC2 encryption with RSA and an MD5 MAC (export)"
msgstr "40-bit RC2 ingucuko ne RSA ne MD5 MAC (tfumela)"

# SSL3/TLS ciphers
#: cipher.ssl3.rsa_rc4_128_md5
msgid "_: cipher.ssl3.rsa_rc4_128_md5\n"
"128-bit RC4 encryption with RSA and an MD5 MAC"
msgstr "128-bit RC4 ingucuko RSA ne SHA1 MAC"

#: cipher.ssl3.rsa_rc4_128_sha
msgid "128-bit RC4 encryption with RSA and a SHA1 MAC"
msgstr "128-bit RC4 ingucuko ne RSA ne SHA1 MAC"

#: cipher.ssl3.rsa_fips_des_ede3_sha
msgid "168-bit Triple DES with RSA and a SHA1 MAC (FIPS)"
msgstr "168-bit Triple DES ne RSA ne SHA1 MAC (FIPS)"

#: cipher.ssl3.rsa_des_ede3_sha
msgid "168-bit Triple DES with RSA and a SHA1 MAC"
msgstr "168-bit Triple DES ne RSA ne SHA1 MAC"

#: cipher.ssl3.rsa_fips_des_sha
msgid "56-bit DES encryption with RSA and a SHA1 MAC (FIPS)"
msgstr "56-bit DES ingucuko ne RSA neSHA1 MAC (FIPS)"

#: cipher.ssl3.rsa_des_sha
msgid "56-bit DES encryption with RSA and a SHA1 MAC"
msgstr "56-bit DES ingucuko ne RSA ne SHA1 MAC"

#: cipher.ssl3.rsa_1024_rc4_56_sha
msgid "56-bit RC4 encryption with RSA and a SHA1 MAC (export)"
msgstr "56-bit RC4 ingucuko RSA ne SHA1 MAC (tfumela)"

#: cipher.ssl3.rsa_1024_des_cbc_sha
msgid "56-bit DES encryption with RSA and a SHA1 MAC (export)"
msgstr "56-bit DES ingucuko ne RSA ne SHA1 MAC (tfumela)"

#: cipher.ssl3.rsa_rc4_40_md5
msgid "_: cipher.ssl3.rsa_rc4_40_md5\n"
"40-bit RC4 encryption with RSA and an MD5 MAC (export)"
msgstr "40-bit RC4 ingucuko ne RSA kanye ne MD5 MAC (tfumela)"

#: cipher.ssl3.rsa_rc2_40_md5
msgid "_: cipher.ssl3.rsa_rc2_40_md5\n"
"40-bit RC2 encryption with RSA and an MD5 MAC (export)"
msgstr "40-bit RC2 ingucuko ne RSA kanye ne MD5 MAC (tfumela)"

# Extra SSL3/TLS ciphers
