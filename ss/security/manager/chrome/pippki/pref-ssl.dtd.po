#: warn.description
msgid "&brandShortName; can alert you to the security status of the web page you are viewing. Set &brandShortName; to show a warning and ask permission before:"
msgstr "Libitophawulelifisha,lingakwenta ucaphelisa ngesimo sekuvikeleka selikhasi lewebhu lolibukako.Hlela libitophawulelifisha  kukhombisa kucaphela ,ucela lusito ngembi kwekulinga"

#: warn.enteringsecure
msgid "Loading a page that supports encryption"
msgstr "Layisha likhasi lelisekela ingucuko lefihliwe"

#: warn.enteringweak
msgid "Loading a page that uses low-grade encryption"
msgstr "Layisha likhasi lelisebentisa lizinga leliphasi lngucuko lefihliwe"

#: warn.insecurepost
msgid "Sending form data from an unencrypted page to an unencrypted page"
msgstr "Kutfumela lifomu lelwati kusuka likhatsi leliguculwe ngendlela lefihliwe kulikhasi lelingakaguculwa"

#: warn.leavingsecure
msgid "Leaving a page that supports encryption"
msgstr "Kuyekela likhasi lelisekela ingucuko"

#: warn.secureredirect
msgid "Redirecting from one encrypted page to another"
msgstr "Kuphindza ubuyisele emuva kusuka kulikhasi leliguculwe ngendlela lefihliwe kuya kulenye"

#: warn.secureredirecttoinsecure
msgid "Redirecting from an encrypted page to an unencrypted page"
msgstr "Kuphindza ubuyisele emuva likhasi leliguculwe ngalokufihliwe kulikhasi lelingakaguculwa ngendlela lefihlile"

#: warn.viewmixed
msgid "Viewing a page with an encrypted/unencrypted mix"
msgstr "Kubuka likhasi ngendlela yekugucula lefihliwe/noma lengakaguculwa"

#: certselect.description
msgid "Decide how &brandShortName; selects a security certificate to present to web sites that require one:"
msgstr "Khetsa kutsi libitophawulelifisha  lisikhetsa njani sitifiketi lesivikelekile kusitfumela kumasayithi ewebhu ladzinga linye"

#: certselect.auto
msgid "Select Automatically"
msgstr "Kuyatikhetsela"

#: certselect.ask
msgid "Ask Every Time"
msgstr "Buta ngasosonkhe sikhatsi"

