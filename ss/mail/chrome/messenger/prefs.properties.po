#: prefPanel-diskspace
msgid "Disk Space"
msgstr "Indzawo yediski"

#: prefPanel-addressing
msgid "Composition & Addressing"
msgstr "Kwakheka ne kulungisa"

#: prefPanel-advanced
msgid "Advanced"
msgstr "Lokutfutfukile"

#: prefPanel-smtp
msgid "Outgoing Server (SMTP)"
msgstr "Silulu lesiphumako (SMTP)"

#: identity-list-title
msgid "Identities for %1$S"
msgstr "Timphawu ta %1$S"

#: identity-edit-req
msgid "You must specify a valid email address for this identity."
msgstr "Kufanele uchaze likheli le-imeyili lesemtsetfweni kulokuchaza."

#: identity-edit-req-title
msgid "Error Creating Identity"
msgstr "Liphutsa ekwakheni bunguwe"

#: choosefile
msgid "Choose a file"
msgstr "Khetsa ifayela"
