#: pane.title
msgid "Return Receipts"
msgstr "Buyisela Emarisithi"

#: useGlobalPrefs.label
#: useGlobalPrefs.accesskey
msgid "&Use my global return receipt preferences for this account"
msgstr "&Sebentisa kuncomeka kwemarisithi ami labuyile emhlaba kule-akhawunti"

#: useCustomPrefs.label
#: useCustomPrefs.accesskey
msgid "&Customize return receipts for this account"
msgstr "&Yetayeta labuyile emarisithi kule-akhawunti"

#: requestReceipt.label
#: requestReceipt.accesskey
msgid "&When sending messages, always request a return receipt"
msgstr "&Nawutfumela imilayeto, uboyicela njalo nje irisithi lebuyako"

#: receiptArrive.label
msgid "When a receipt arrives:"
msgstr "Nakufika irisithi:"

#: leaveIt.label
#: leaveIt.accesskey
msgid "Leave it in my &Inbox"
msgstr "Yiyekele ekhatsi &ku-inbhoksi yami"

# LOCALIZATION NOTE moveToSent.label Translate: 'Sent' according to Netscape glossary
#: moveToSent.label
#: moveToSent.accesskey
msgid "&Move it to my &quot;Sent&quot; folder"
msgstr "&Yihambise iye kuFolda yami &quot;Tfumela&quot;"

#: requestMDN.label
msgid "When I receive a request for a return receipt:"
msgstr "Nangitfola sicelo serisithi lebuyako:"

#: returnSome.label
#: returnSome.accesskey
msgid "Allow r&eturn receipts for some messages"
msgstr "Vumela emarisithi la&buyako kuleminye imilayeto"

#: never.label
#: never.accesskey
msgid "&Never send a return receipt"
msgstr "&Ungaze uyitfumele irisithi lebuyako"

#: notInToCc.label
#: notInToCc.accesskey
msgid "If I'm not in the &To or Cc of the message:"
msgstr "Nangingekho ekhatsi &Ku nomeCc wemlayeto:"

#: outsideDomain.label
#: outsideDomain.accesskey
msgid "If the &sender is outside my domain:"
msgstr "Nangabe&lona loyitfumele angephandle kwendzawo yami:"

#: otherCases.label
#: otherCases.accesskey
msgid "In all &other cases:"
msgstr "Kuto &tonkhe letinye tindzaba:"

#: askMe.label
msgid "Ask me"
msgstr "Ngibute"

#: alwaysSend.label
msgid "Always send"
msgstr "Tfumela njalo nje"

#: neverSend.label
msgid "Never send"
msgstr "Ungacali utfumele"
