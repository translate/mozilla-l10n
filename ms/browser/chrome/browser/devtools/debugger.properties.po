#: enumerableTooltip
msgid "enumerable"
msgstr "boleh diangkakan"

#: writableTooltip
msgid "writable"
msgstr "boleh tulis"

#: frozenTooltip
msgid "frozen"
msgstr "dibekukan"

#: sealedTooltip
msgid "sealed"
msgstr "dimeterai"

#: extensibleTooltip
msgid "extensible"
msgstr "boleh perluas"

#: overriddenTooltip
msgid "overridden"
msgstr "dibatal"

#: WebIDLTooltip
msgid "WebIDL"
msgstr "WebIDL"

