#~ msgstr "Tema za ovu sobu nije određena."

#~ msgid "Username"
#~ msgstr "Korisničko ime"

#~ msgid "Account"
#~ msgstr "Račun"

#~ msgid "A"
#~ msgstr "D"

#~ msgid "D"
#~ msgstr "O"

#~ msgid "Two Weeks Ago"
#~ msgstr "Prije dva tjedna"
