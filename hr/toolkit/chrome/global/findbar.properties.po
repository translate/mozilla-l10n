#. # strings used by the Find bar, split from browser.properties
#: NotFound
msgid "Phrase not found"
msgstr "Izraz nije pronađen"

#: WrappedToTop
msgid "Reached end of page, continued from top"
msgstr "Dosegnut kraj stranice, nastavak s vrha"

#: WrappedToBottom
msgid "Reached top of page, continued from bottom"
msgstr "Dosegnut početak stranice, nastavak s kraja"

#: NormalFind
msgid "Find in page"
msgstr "Pronađi na stranici"

#: FastFind
msgid "Quick find"
msgstr "Brza pretraga"

#: FastFindLinks
msgid "Quick find (links only)"
msgstr "Brza pretraga (samo poveznice)"

#: CaseSensitive
msgid "(Case sensitive)"
msgstr "(Razlikovanje malih i velikih slova)"

#. # LOCALIZATION NOTE (FoundMatches): Semicolon-separated list of plural forms.
