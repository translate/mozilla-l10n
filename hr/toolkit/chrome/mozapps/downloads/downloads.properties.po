#. # example: 1m; 11h
#: timePair3
#, fuzzy
msgid "%1$S%2$S"
msgstr "%1$S %2$S"

#. # LOCALIZATION NOTE (timeLeftSingle3): %1$S time left
#. # example: 1m left; 11h left
#: timeLeftSingle3
msgid "%1$S left"
msgstr ""

