#: tooltip.nickname
msgid "Nickname"
msgstr "Nadimak"

#: tooltip.email
msgid "Email"
msgstr "E-pošta"

#: tooltip.birthday
msgid "Birthday"
msgstr "Rođendan"

#: tooltip.userName
msgid "Username"
msgstr "Korisničko ime"

#: tooltip.title
msgid "Title"
msgstr "Titula"

#: tooltip.organization
msgid "Organization"
msgstr "Organizacija"

#: tooltip.locality
msgid "Locality"
