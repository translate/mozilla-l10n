#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Nuu nɨ´ɨ-ka sincronización: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "Nuu nɨ´ɨ-ka sincronización: a jíka..."

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Cuenta: %S"

#: notconnected.label
msgid "Not connected"
msgstr "Ntu yɨ´ɨ"

#: connecting.label
msgid "Connecting…"
msgstr "Nakivɨ..."

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "Cuenta Firefox Sync nkuvi náxitá"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Niko yata"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 ntu compatible jii nuu nɨ´ɨ versión Firefox Sync. Kuamani-ininu, saa "
"actualizar-nu versión nuu nɨ´ɨ."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 Ntu compatible jii daa version tu̱´ú Firefox Sync. Kuamani-ininu, saa "
"actualizar Firefox Sync nuu daa inka ka̱a̱ kuminu."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox Sync"

#: sync.update.button
msgid "Learn More"
msgstr "Ka´vi saa kúninu-ka"

#: sync.update.close
msgid "Close"
msgstr "Nakasɨ"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "Ntu nkuvi saa configurar Sync"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "Naa íyo ka̱a̱ nántu´un ne jíka"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S ntu nkuvi kivɨ Sync. ¿A kuvinu nakivɨ tuku?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Nakivɨ tuku"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Configuración na̱´á"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "Na íyo pestañas daa inka-ka ka̱a̱ tee nɨnuu. "

#. # LOCALIZATION NOTE (promoBanner.message.text): First part of the message displayed in a
#. # banner on about:home. The final space separates this text from the link.
#: promoBanner.message.text
msgid "Sync is brand new and easier than ever. "
msgstr "Sync nnuvi jíía jee ntu víjí ni´i-oma."

#. # LOCALIZATION NOTE (promoBanner.message.link): Second part of the message, styled as a link.
#: promoBanner.message.link
msgid "Tap here to learn more"
msgstr "Kuaxin ya´a saa kúninu-ka"
