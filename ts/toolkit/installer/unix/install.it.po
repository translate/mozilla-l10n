#------------------------------------------------------------------------------
#: -601
msgid "Out of memory"
msgstr "A swi hlayisekangi"

#: -602
msgid "Invalid param"
msgstr "Pharamu yo hoxeka"

#: -603
msgid "Invalid member variable"
msgstr "Vharyyebulu ya xirho yo hoxeka"

#: -604
msgid "Invalid pointer"
msgstr "Poyintara yo hoxeka"

#: -605
msgid "Parse key has no value"
msgstr "Khiya ra phasi a ri na nkoka"

#: -606
msgid "Failed to read readme"
msgstr "Swi tsandzile ku hlaya"

#: -607
msgid "Failed to read license"
msgstr "Swi tsandzile ku hlaya layisense"

#: -608
msgid "Out of bounds in Component/Setup Type list"
msgstr "A swi kumeki hi swiyenge/Lulamisa nongonoko wo thayipa"

#: -609
msgid "Mismatched ref counts"
msgstr "Nhlayelo wa rhefe wo ka wu nag yelani"

#: -610
msgid "No components in the INI file"
msgstr "Ku hava swiyenge eka fayili ya INI"

#: -611
msgid "No setup types in the INI file"
msgstr "Ku hava tinxaka to lulamisa eka fayili ya INI"

#: -612
msgid "URL at this index already exists"
msgstr "URL eka indekisi leyi se yi kona"

#: -613
msgid "Couldn't create the directory"
msgstr "A swi kotekangi ku tumbuluxa dayirekithiri"

#: -614
msgid "FTP URL malformed"
msgstr "FTP URL a yi tumbuluxekangi"

#: -615
msgid "Download failed"
msgstr "Swi tsandzile ku panga"

#: -616
msgid "Extraction of XPCOM failed"
msgstr "Swi tsandzile ku kuma XPCOM"

#: -617
msgid "Failed to fork a process"
msgstr "Swi tsandzile ku kuma ntirhiso"

#: -618
msgid "Couldn't open xpistub library"
msgstr "Swi tsandzile ku pfula layiburari ya xpistub"

#: -619
msgid "Couldn't get symbol in library"
msgstr "Swi tsandzile ku kuma simbhulu elayiburari"

#: -620
msgid "A xpistub call failed"
msgstr "Swi tsandzile ku kuma xpistub"

#: -621
msgid "An Installer module %s (.xpi) failed to install"
msgstr "Modlulu wo nghenisa wa %s (.xpi) wu tsandzekile ku nghenisa"

#: -622
msgid "Copy of an installer module (.xpi) failed"
msgstr "Ku kopa modlulu wo nghenisa wa (.xpi) wu tsandzile"

#: -623
msgid "Destination directory doesn't exist"
msgstr "Dayirekithiri ya mpfhuka a yi kona"

#: -624
msgid "Can't make destination directory.  Please try another directory."
msgstr "A swi koteki ku tumbuluxa dayirekithiri ya mpfhuka.  Ringeta nakambe dayirekithiri yin'wana."

#: -625
msgid "A previous installation exists."
msgstr "Ku nghenisa ka nkarhi lowu nga hundza se ku kona."

#: -626
msgid "Insufficient permission"
msgstr "Ku hava mpfumelelo wo ringana"

#: -627
msgid "Insufficient disk space"
msgstr "Ku hava ndhawu yo ringana ya disiki"

#: -628
msgid "Multiple CRC Failure"
msgstr "CRC-nyingi yi tsandzekile"

