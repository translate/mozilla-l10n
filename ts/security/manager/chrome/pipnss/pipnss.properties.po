msgstr "(Nhalngano a wu tiveki)"

#: AVATemplate
msgid "%S = %S"
msgstr "%S = %S"

#: SSLDisabled
msgid "You cannot connect to %S because SSL is disabled."
msgstr "U nge hlanganisi %S hikuva SSL a yi tirhi."

#: SSL2Disabled
msgid "You cannot connect to %S because SSL version 2 is disabled."
msgstr "U nge hlanganisi %S hikuva vhexini 2 ya SSL a yi tirhi."

#: SSLNoMatchingCiphers
msgid "%S and %S cannot communicate securely because they have no common encryption algorithms."
msgstr "%S ya %S a yi swi koti ku khomunikheta hi nhlayiseko hikuva ku na ntsariso wa algorithm wo fana."

#: UsersCertRevoked
msgid "Could not establish an encrypted connection with %S because your certificate has been revoked."
msgstr "A swi kotekangi ku kuma nhlanganiso wa ntsariso na %S hikuva xitifiketi xa wena xi thleriseriwile endzhaku."

#: UsersCertExpired
msgid "Could not establish an encrypted connection with %S because your certificate is expired."
msgstr "A swi kotekangi ku kuma nhlanganiso wa ntsariso na %S hikuva xitifiketi xa wena xi hundzeriwile hi nkarhi."

#: UsersCertRejected
msgid "Could not establish an encrypted connection because your certificate was rejected by %S. Error Code: %S."
msgstr "A swi kotekangi ku kuma nhlanganiso wa ntsariso na %S hikuva xitifiketi xa wena xi ariwile hi %S. Khodi ya Xihoxo: %S."

#: BadMac
msgid "%S received a message with incorrect Message Authentication Code. If the error occurs frequently, contact the website administrator."
msgstr "%S yi kumile hungu ro hoxeka ra Khodi ya Mpfumelelo wa Hungu. Loko xihoxo xi humelela minkarhi hinkwayo, tihlanganise na mulawuri wa sayiti ra web."

#: PeerResetConnection
msgid "%S has closed the connection. Error Code: %S"
msgstr "%S yi pfarile nhlanganiso. Khodi ya Xihoxo: %S"

#: HostResetConnection
msgid "The current transfer has been cancelled."
msgstr "Ku hundzisela ka sweswi ku khanseriwile."

#: BadPassword
msgid "An incorrect password was provided."
msgstr "Ku nyikiwile phasiwedi yo hoxeka."

#: BadDatabase
msgid "There is a problem with your certificate database [Error Code: %S]."
msgstr "Ku na xiphiqo mayelana na dathabayisi ya xitifiketi xa wena [Xihoxo Code: %S]."

#: BadServer
msgid "%S has sent an incorrect or unexpected message. Error Code: %S"
msgstr "%S yi rhumerile hungu ro hoxeka kumbe ro ka ri nga languteriwangi. Khodi ya Xihoxo: %S"

#: BadClient
msgid "%S has received an incorrect or unexpected message. Error Code: %S"
msgstr "%S yi kumile hungu ro hoxeka kumbe ro ka ri nga languteriwangi. Khodi ya Xihoxo: %S"

#: HostReusedIssuerSerial
msgid "You have received an invalid certificate.  Please contact the server administrator or email correspondent and give them the following information:\n"
"\n"
"Your certificate contains the same serial number as another certificate issued by the certificate authority.  Please get a new certificate containing a unique serial number."
msgstr "U kumile xitifiketi xo hoxeka.  Tihlanganise na mulawuri wa sevha kumbe matsalana wa imeyili u va nyika mahungu lama landzelaka:\n\nXitifiketi xa wena xi na nomboro ya siriyeli yo fana na ya xitifiketi xin'wana lexi nyikiweke hi mpfumelelo wa xitifiketi.  Kuma xin'wana xitifiketi lexi nga na nomboro ya siriyeli yo hambana."

#: SSLGenericError
msgid "Error establishing an encrypted connection to %S. Error Code: %S."
msgstr "Xihoxo eka ku ringeta ku tumbuluxa nhlanganiso wa ntsariso eka %S. Khodi ya Xihoxo: %S."

#: TimeOut
msgid "Connection request timed out."
msgstr "Xikombelo xa nhlanganiso xi hundzeriwile hi nkarhi."

#: PeersCertUntrusted
msgid "Could not establish an encrypted connection because certificate presented by %S is not trusted."
msgstr "A swi kotekangi ku tumbuluxa nhlanganiso wa ntsariso hikuva xitifiketi lexi tisiweke hi %S a xi tshembekangi."

#: PeersCertRevoked
msgid "Could not establish an encrypted connection because certificate presented by %S has been revoked."
msgstr "A swi kotekangi ku tumbuluxa nhlanganiso wa ntsariso hikuva xitifiketi lexi tisiweke hi %S xi thleriseriwile endzhaku."

#: PeersCertExpired
msgid "Could not establish an encrypted connection because certificate presented by %S has expired."
msgstr "A swi kotekangi ku tumbuluxa nhlanganiso wa ntsariso hikuva xitifiketi lexi tisiweke hi %S xi hundzeriwile hi nkarhi."

#: PeersCertWrongDomain
msgid "Could not establish an encrypted connection because certificate presented by %S is for a different domain."
msgstr "A swi kotekangi ku tumbuluxa nhlanganiso wa ntsariso hikuva xitifiketi lexi tisiweke hi %S i xa ndhawu yin'wana yo hambana."

#: PeersCertBadSignature
msgid "Could not establish an encrypted connection because certificate presented by %S has an invalid signature."
msgstr "A swi kotekangi ku tumbuluxa nhlanganiso wa ntsariso hikuva xitifiketi lexi tisiweke hi %S xi na nsayino wo hoxeka."

#: PeersCertNoGood
msgid "Could not establish an encrypted connection because certificate presented by %S is invalid or corrupted. Error Code: %S"
msgstr "A swi kotekangi ku tumbuluxa nhlanganiso wa ntsariso hikuva xitifiketi lexi tisiweke hi %S xi hoxekile kumbe ku onhaka. Khodi ya Xihoxo: %S"

#: CRLExpired
msgid "Certificate Revocation List (CRL) from the CA certifying %S is past its Next Update date. Please update the CRL."
msgstr "Nongonoko wo thlerisela xitifiketi (CRL) ku suka eka CA yo tiyisisa %S xi hundzeriwile hi siku Leri Landzelaka ro Antswisa. Antswisa CRL."

#: CRLNotYetValid
msgid "Certificate Revocation List (CRL) from the CA certifying %S is not yet valid. Please check your system clock."
msgstr "Nongonoko wo thlerisela xitifiketi (CRL) ku suka eka CA yo tiyisisa %S a xi se va na ntiyiso. Langua wachi ya sisiteme ya wena."

#: CRLSigNotValid
msgid "Certificate Revocation List (CRL) from the CA certifying %S has an invalid digital signature."
msgstr "Nongonoko wo thlerisela xitifiketi (CRL) ku suka eka CA yo tiyisisa %S wu na nsayino wo ka wu nag tiyangi wa digithali."

#: CRLSNotValid
msgid "Certificate Revocation List (CRL) from the CA certifying %S is not valid."
msgstr "Nongonoko wo thlerisela xitifiketi (CRL) ku suka eka CA yo tiyisisa %S a wu na ntiyiso."

#: OCSPMalformedRequest
msgid "Error trying to validate certificate from %S using OCSP - malformed request."
msgstr "Xihoxo eka ku ringeta ku tiyisisa xitifiketi ku suka eka %S hi ku tirhisa OCSP - xikombelo xo ka xi nga endliwangi kahle."

#: OCSPRequestNeedsSig
msgid "Error trying to validate certificate from %S using OCSP - request needs signature."
msgstr "Xihoxo eka ku ringeta ku tiyisisa xitifiketi ku suka eka %S hi ku tirhisa OCSP - xikombelo xi lava nsayino."

#: OCSPUnauthorizedReq
msgid "Error trying to validate certificate from %S using OCSP - unauthorized request."
msgstr "Xihoxo eka ku ringeta ku tiyisisa xitifiketi ku suka eka %S hi ku tirhisa OCSP - xikombelo xo kala mpfumelelo."

#: OCSPServerError
msgid "Error trying to validate certificate from %S using OCSP - server error."
msgstr "Xihoxo eka ku ringeta ku tiyisisa xitifiketi ku suka eka %S hi ku tirhisa OCSP - xihoxo xa sevha."

#: OCSPTryServerLater
msgid "Error trying to validate certificate from %S using OCSP - server is busy. Please try again later."
msgstr "Xihoxo eka ku ringeta ku tiyisisa xitifiketi ku suka eka %S hi ku tirhisa OCSP - sevha yi le ku tirheni. Ringeta nakambe eka nkarhi lowu taka."

#: OCSPFutureResponse
msgid "Error trying to validate certificate from %S using OCSP - response contains a date which is in the future."
msgstr "Xihoxo eka ku ringeta ku tiyisisa xitifiketi ku suka eka %S hi ku tirhisa OCSP - Nhalmulo yi na siku leri nga eka nkarhi lowu taka."

#: OCSPOldResponse
msgid "Error trying to validate certificate from %S using OCSP - old response."
msgstr "Xihoxo eka ku ringeta ku tiyisisa xitifiketi ku suka eka %S hi ku tirhisa OCSP - nhlamulo ya khale."

#: OCSPCorruptedResponse
msgid "Error trying to validate certificate from %S using OCSP - corrupted or unknown response. Error Code: %S."
msgstr "Xihoxo eka ku ringeta ku tiyisisa xitifiketi ku suka eka %S hi ku tirhisa OCSP - nhlamulo yo hoxeka kumbe yo ka yi nga tiveki. Khodi ya xihoxo: %S."

#: OCSPUnauthorizedResponse
msgid "Error trying to validate certificate from %S using OCSP - unauthorized response."
msgstr "Xihoxo eka ku ringeta ku tiyisisa xitifiketi ku suka eka %S hi ku tirhisa OCSP - Nhlamulo yo ka yi nga pfumeleriwangi."

#: OCSPUnknownCert
msgid "Error trying to validate certificate from %S using OCSP - unknown certificate."
msgstr "Xihoxo eka ku ringeta ku tiyisisa xitifiketi ku suka eka %S hi ku tirhisa OCSP - Xitifiketi xi ka xi nga tiveki."

#: OCSPNoDefaultResponder
msgid "Error trying to validate certificate from %S using OCSP - no default responder specified."
msgstr "Xihoxo eka ku ringeta ku tiyisisa xitifiketi ku suka eka %S hi ku tirhisa OCSP - ku hava muhlamuri wo hoxeka loyi a kombisiweke."

#: OCSPDirLookup
msgid "Error trying to validate certificate from %S using OCSP - directory lookup error."
msgstr "Xihoxo eka ku ringeta ku tiyisisa xitifiketi ku suka eka %S hi ku tirhisa OCSP - xihoxo xa ku languta dayirekitheri."

