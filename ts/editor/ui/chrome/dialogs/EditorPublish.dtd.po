# Window title
#: windowTitle.label
msgid "Publish Page"
msgstr "Kandziyisa Pheji"

#: windowTitleSettings.label
msgid "Publish Settings"
msgstr "Kandziyisa Ndzulamiso"

#: publishTab.label
msgid "_: publishTab.label\n"
"Publish"
msgstr "Kandziyisa"

#: settingsTab.label
msgid "Settings"
msgstr "Ndzulamiso"

#: publishButton.label
msgid "_: publishButton.label\n"
"Publish"
msgstr "Kandziyisa"

# Publish Tab Panel
#: siteList.label
msgid "_: siteList.label\n"
"Site Name:"
msgstr "Vito ra Sayiti:"

#: siteList.tooltip
msgid "Choose the site that you want to publish to"
msgstr "Hlawula sayiti leri u lavaka ku kandziyisa eka rona"

#: newSiteButton.label
msgid "New Site"
msgstr "Sayiti rintshwa"

#: siteDirList.label
msgid "Select or type a directory name in your publishing site:"
msgstr "Hlawula kumbe thayipa vito ra dayirekitheri eka sayiti ra wena ro kandziyisa:"

#: siteDirList.tooltip
msgid "Choose or enter the name of the remote subdirectory to publish to"
msgstr "Hlawula kumbe nghenisa vito ra rhimoti ra dayirekhiterintsongo ro kandziyisa"

#: docDirList.label
msgid "Site subdirectory for this page:"
msgstr "Sayiti ra dayirekitherintsongo ra pheji leri:"

#: publishImgCheckbox.label
msgid "Include images and other files"
msgstr "Katsa swifaniso na tifayili tin'wana"

#: publishImgCheckbox.tooltip
msgid "Publish images and other files referenced by this page"
msgstr "Kandziyisa swifaniso na tifayili tin'wana leswi kombisiweke eka pheji leri"

#: sameLocationRadio.label
msgid "Use same location as page"
msgstr "Tirhisa ndhawu yin'we tani hi pheji"

#: sameLocationRadio.tooltip
msgid "Publish files to the same location as the page"
msgstr "Kandziyisa tifayili eka ndhawu yin'we tani hi pheji"

#: useSubdirRadio.label
msgid "Use this site subdirectory:"
msgstr "Tirhisa dayirekitherintsongo ya sayiti leri :"

#: useSubdir.tooltip
msgid "Choose or enter name of remote subdirectory where files will be published"
msgstr "Hlawula kumbe nghenisa vito ra rhimoti ra dayirekhiterintsongo laha tifayili ti nga ta kandziyisiwa"

#: chooseDirButton.label
msgid "Choose..."
msgstr "Hlawulaâ€¦"

#: pageTitle.label
msgid "Page Title:"
msgstr "Nhlokomhaka ya Pheji"

#: pageTitle.tooltip
msgid "Enter a title to identify the page in the window and in bookmarks"
msgstr "Nghenisa nhlokomhaka ku kuma pheji eka fasitere ni le ka timakara ta buku"

#: pageTitleExample.label
msgid "e.g.: &quot;My Web Page&quot;"
msgstr "xik.,: &quot;Pheji ra Mina ra Webe&quot;"

#: filename.label
msgid "Filename:"
msgstr "Vito ra Fayili:"

#: filename.tooltip
msgid "Enter a name for this file, including '.html' for a web page"
msgstr "Tsala vito ra fayili leri, ku katsa 'html' ya pheji leri ra webe"

#: filenameExample.label
msgid "e.g,: &quot;mypage.html&quot;"
msgstr "xik.,: &quot;pheji ra mina.html&quot;"

#: relatedDocs.label
msgid "Other files to publish"
msgstr "Tifayili tin'wana to kandziyisa"

#: setDefaultButton.label
msgid "Set as Default"
msgstr "Lulamisa tani hi xihoxo"

#: setDefSiteAndDir.label
msgid "Set as Default Site and Directory"
msgstr "Lulamisa tani hi xihoxo xa Sayiti na Dayirekitheri"

#: removeButton.label
msgid "Remove Site"
msgstr "Susa Sayiti"

# Settings Tab Panel
#: publishSites.label
msgid "Publishing Sites"
msgstr "Masayiti yo Kandziyisa"

#: serverInfo.label
msgid "Server Information"
msgstr "Mahungu ya Sevha"

#: loginInfo.label
msgid "Login Information"
msgstr "Mahungu ya Logini"

#: siteName.label
msgid "_: siteName.label\n"
"Site Name:"
msgstr "Vito ra Sayiti:"

#: siteName.tooltip
msgid "A nickname that identifies this publishing site (e.g.: 'MySite')"
msgstr "Vito ro duvulela leri tirhisiwaka ku kuma sayiti leri ro kandziyisa (xik.: 'Sayiti ra Mina') "

#: siteUrl.label
msgid "Publishing address (e.g.: 'ftp://ftp.myisp.com/myusername'):"
msgstr "Tiadirese to Kandziyisa (xik.: 'ftp://ftp.myisp.com/vito ra mina ro tirhisa'):"

#: siteUrl.tooltip
msgid "The FTP:// or HTTP:// address provided by your ISP or web hosting service"
msgstr "FTP:// kumbe HTTP:// adirese leyi nyikiweke hi ISPya wena kumbe vutirheli byo hlayisa webe."

#: browseUrl.label
msgid "HTTP address of your homepage (e.g.: 'http://www.myisp.com/myusername'):"
msgstr "HTTP:// adirese ya dayireketiri ya wena ya le kaya (u nga katsi vito ra fayili)"

#: browseUrl.tooltip
msgid "The HTTP:// address of your home directory (don't include filename)"
msgstr "HTTP:// adirese ya dayireketiri ya wena ya le kaya (u nga katsi vito ra fayili)"

#: username.label
msgid "User name:"
msgstr "Vito ra Mutirhisi:"

#: username.tooltip
msgid "The user name you use to log in to your ISP or web hosting service"
msgstr "Vito ra mutirhisi leri u ri tirhisaka loga eka ISP kumbe vutirheli byo hlayisa webe"

#: password.label
msgid "Password:"
msgstr "Phasiwedi:"

#: password.tooltip
msgid "The password associated with your user name"
msgstr "Phasiwedi leyi yelanaka na Vito ra mutirhisi"

#: savePassword.label
msgid "Save Password"
msgstr "Hlayisa Phasiwedi"

#: savePassword.tooltip
msgid "Select this to save your password securely using Password Manager"
msgstr "Hlawula leswi ku hlayisa phasiwedi ya wena kahle hi ku tirhisa Mininjhere wa Phasiwedi"

