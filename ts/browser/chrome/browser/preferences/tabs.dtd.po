#: tabsTab.label
msgid "Tabs"
msgstr "Tithebe"

#: openExternalLinksIn.label
msgid "Open links from other applications in:"
msgstr "Pfula tilinki ta tiapulikhexini tin'wana eka:"

#: newWindow.label
msgid "a new window"
msgstr "fasitere rintshwa"

#: newTabInRecentWindow.label
msgid "a new tab in the most recent window"
msgstr "thebe yintshwa eka fasitere rintshwa"

#: recentWindowOrTab.label
msgid "the most recent tab/window"
msgstr "thebe/fasitere ra sweswi"

#: forceNewWindowLinks.label
msgid "Force links that open new windows to open in:"
msgstr "Endla leswaku tilinki leti pfulaka mafasitere mantshwa ti pfula hi:"

#: sameTabOrWindow.label
msgid "the same tab/window as the link"
msgstr "thebe/fasitere ro fana na linki"

#: newTab.label
msgid "a new tab"
msgstr "thebe yintshwa"

#: hideTabBar.label
msgid "Hide the tab bar when only one web site is open"
msgstr "Tumbeta bara ya thebe loko ku pfuriwile ntsena sayiti ra web rin'we"

#: loadInBackground.label
msgid "Select new tabs opened from links"
msgstr "Hlawula tithebe tintshwa leti pfuriweke eka tilinki"

#: loadBookmarksInBackground.label
msgid "Select new tabs opened from bookmarks or history"
msgstr "Hlawula tithebe tintshwa leti pfuriweke eka timakar ata tibuku kumbe matimu"

#: warnOnClose.label
msgid "Warn when closing multiple tabs"
msgstr "Tsundzuxa loko u pfala tithebe-nyingi"

