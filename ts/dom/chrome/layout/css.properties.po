#: MimeNotCss
msgid "The stylesheet %1$S was not loaded because its MIME type, \"%2$S\", is not \"text/css\"."
msgstr "Xitayili xa phepha xa %1$S a xi pangiwangi hikuva muxaka wa xona wa MIME, \"%2$S\", a hi \"text/css\"."

#: MimeNotCssWarn
msgid "The stylesheet %1$S was loaded as CSS even though its MIME type, \"%2$S\", is not \"text/css\"."
msgstr "Xitayili xa phepha xa %1$S xi pangiwile hambileswi muxaka CSS wa xona wa MIME, \"%2$S\", a hi \"text/css\"."

#: PEUnexpEOF
msgid "Unexpected end of file while searching for %1$S."
msgstr "Fayili pfarile swi nag languteriwangi loko ku sechiwa %1$S."

#: PEParseRuleWSOnly
msgid "Whitespace-only string given to be parsed as rule."
msgstr "Ku fanele ku pfumeleriwa ntsena ndhawu ya swo basa."

#: PEDeclDropped
msgid "Declaration dropped."
msgstr "Leswi bohiweke swi tshikiwile."

#: PEDeclSkipped
msgid "Skipped to next declaration."
msgstr "Tlulela eka leswi bohiweke leswi landzelaka."

#: PEUnknownProperty
msgid "Unknown property '%1$S'."
msgstr "Swihlawulekisi leswi nga tivekiki swa '%1$S'."

#: PEPropertyParsingError
msgid "Error in parsing value for property '%1$S'."
msgstr "Xihoxo eka nkoka wa xihlawulekisi xa '%1$S'."

#: PEExpectEndProperty
msgid "Expected end of value for property but found '%1$S'."
msgstr "Ku languteriwile makumu ya xihlawulekisi kambe ku kumekile '%1$S'."

#: PESkipAtRuleEOF
msgid "end of unknown at-rule"
msgstr "makumu ya nawu lowu nga tivekiki wa at"

#: PEUnknownAtRule
msgid "Unrecognized at-rule or error parsing at-rule '%1$S'."
msgstr "Nawu wa at wo ka wu nga tiveki kumbe xihoxo xa nawu wa at wa '%1$S'."

#: PECharsetRuleEOF
msgid "charset string in @charset rule"
msgstr "chaseta ntambhu eka nawu wa @charset"

#: PECharsetRuleNotString
msgid "Expected charset string but found '%1$S'."
msgstr "Ku languteriwile ntambhu ya chasete kambe ku kumekile '%1$S'."

#: PEGatherMediaEOF
msgid "end of media list in @import or @media rule"
msgstr "makumu ya nongonoko wa midiya eka @import kume nawu wa @media"

#: PEGatherMediaNotComma
msgid "Expected ',' in media list but found '%1$S'."
msgstr "Ku languteriwile ',' eka nongonoko wa midiya kambe ku kumekile '%1$S'."

#: PEGatherMediaNotIdent
msgid "Expected identifier in media list but found '%1$S'."
msgstr "Ku languteriwile eka nongonoko wa midiya kambe ku kumekile '%1$S'."

#: PEImportNotURI
msgid "Expected URI in @import rule but found '%1$S'."
msgstr "URI leyi languteriweke eka milawu ya @import kambe ku kumekile '%1$S'."

#: PEImportUnexpected
msgid "Found unexpected '%1$S' within @import."
msgstr "Ku kumiwile leswi nga languteriwangiki swa '%1$S' eka @import."

#: PEGroupRuleEOF
msgid "end of @media or @-moz-document rule"
msgstr "emakumu ka @media kumbe e ya @-moz-document"

#: PEMozDocRuleBadFunc
msgid "Expected url(), url-prefix(), or domain() in @-moz-document rule but found '%1$S'."
msgstr "url() leyi languteriweke, url-prefix(), kumbe domain() eka dokhumente ya milawu @-moz-document kambe ku kumekile '%1$S'."

#: PEMozDocRuleNotURI
msgid "Expected URI in @-moz-document rule but found '%1$S'."
msgstr "URI leyi lang uteriweke eka @-moz-document ya milawu kambe ku kumekile '%1$S'."

#: PEAtNSPrefixEOF
msgid "namespace prefix in @namespace rule"
msgstr "ndhawu ya vito ya xirhangi xa @namespace ra ndhawu"

#: PEAtNSURIEOF
msgid "namespace URI in @namespace rule"
msgstr "ndhawu ya vito ya URI ya @namespace ra ndhawu"

#: PEAtNSUnexpected
msgid "Unexpected token within @namespace: '%1$S'."
msgstr "Thokene leyi nga languteriwangiki eka @namespace ra ndhawu '%1$S'."

#: PESkipDeclBraceEOF
msgid "closing } of declaration block"
msgstr "} ku pfala buloko ya leswi bohiweke"

#: PESkipRSBraceEOF
msgid "closing } of invalid rule set"
msgstr "} ku pfala sete ya milawu leyi nga tirhiki"

#: PEBadSelectorRSIgnored
msgid "Ruleset ignored due to bad selector."
msgstr "Milawu ya ku lulamisa yi tshikiwile hikwalaho ka nhlawulo wo hoxeka."

#: PESelectorListExtraEOF
msgid "',' or '{'"
msgstr "',' kumbe '{'"

#: PESelectorListExtra
msgid "Expected ',' or '{' but found '%1$S'."
msgstr "Ku languteriwile kumbe ',' kumbe '{' ku kumekile '%1$S'."

#: PESelectorGroupNoSelector
msgid "Selector expected."
msgstr "Ku languteriwile xihlawuri."

#: PESelectorGroupExtraCombinator
msgid "Dangling combinator."
msgstr "Xihlanganisi xa dangling."

#: PEIDSelNotIdent
msgid "Expected identifier for ID selector but found '%1$S'."
msgstr "Ku languteriwle xikombi xa xihlawuri xa ID kambe ku kumekile '%1$S'."

#: PEIDSelEmpty
msgid "Expected non-empty name for ID selector, but found '%1$S'."
msgstr "Ku languteriwile vito leri nga hava nchumu kumbe xihlawuri xa ID kambe ku kumekile '%1$S'."

#: PEClassSelEOF
msgid "class name"
msgstr "vito ra tlilasi"

#: PEClassSelNotIdent
msgid "Expected identifier for class selector but found '%1$S'."
msgstr "Ku languteriwile xikombi xa nhlawulo wa tlilasi kambe ku kuimekile '%1$S'."

#: PETypeSelEOF
msgid "element type"
msgstr "muxaka wa elemente"

#: PETypeSelNotType
msgid "Expected element name or '*' but found '%1$S'."
msgstr "Ku languteriwile vito ra elemente kambe ku '*' kumekile '%1$S'."

#: PEUnknownNamespacePrefix
msgid "Unknown namespace prefix '%1$S'."
msgstr "Xirhangi xa ndhawu ya vito lexi nga tivekiki xa '%1$S'."

#: PEAttributeNameEOF
msgid "attribute name"
msgstr "vito leri tirhisiwaka"

#: PEAttributeNameExpected
msgid "Expected identifier for attribute name but found '%1$S'."
msgstr "Ku languteriwile xikombi xa vito leri tirhisiwaka kambe ku kumekile '%1$S'."

#: PEAttributeNameOrNamespaceExpected
msgid "Expected attribute name or namespace but found '%1$S'."
msgstr "Ku languteriwile vito leri tirhisiwaka kumbe ndhawu ya vito kambe ku kumekile '%1$S'."

#: PEAttSelNoBar
msgid "Expected '|' but found '%1$S'."
msgstr "Ku languteriwile '|' kambe ku kumekile '%1$S'."

#: PEAttSelInnerEOF
msgid "part of attribute selector"
msgstr "xiphemu xa xihlawuri xa vito leri tirhisiwaka"

#: PEAttSelUnexpected
msgid "Unexpected token in attribute selector: '%1$S'."
msgstr "Thokeni leyi nga languteriwangiki ya vito leri tirhisiwaka: '%1$S'."

#: PEAttSelValueEOF
msgid "attribute value"
msgstr "nkoka wa leswi tirhisiwaka"

#: PEAttSelCloseEOF
msgid "']' to end attribute selector"
msgstr "']' ku hetisa xihlawuri xa leswi tirhisiwaka"

#: PEAttSelNoClose
msgid "Expected ']' to terminate attribute selector but found '%1$S'."
msgstr "']' Ku languteriwile ku hetisa xihlawuri xa leswi tirhisiwaka kambe ku kumekile '%1$S'."

#: PEAttSelBadValue
msgid "Expected identifier or string for value in attribute selector but found '%1$S'."
msgstr "Ku languteriwile xikombi kumbe ntambhu ya nkoka wa xihlawuri xa leswi tirhisiwaka kambe ku kumekile '%1$S'."

#: PEPseudoSelEOF
msgid "name of pseudo-class or pseudo-element"
msgstr "vito ra pseudo-tlilasi kumbe pseudo-elemente"

#: PEPseudoSelBadName
msgid "Expected identifier for pseudo-class or pseudo-element but found '%1$S'."
msgstr "Ku languteriwile xikombi xa pseudo-tlilasi kumbe pseudo-elemente kambe ku kumekile '%1$S'."

#: PEPseudoSelNonFunc
msgid "Function token for non-function pseudo-class or pseudo-element, or the other way around, when reading '%1$S'."
msgstr "Leswi nga tirhiki swa pseudo-tlilasi kumbe pseudo-elemente swi tekiwile, kumbe thlelo lerin'wana, loko ku hlayiwa '%1$S'."

#: PEPseudoSelNotPE
msgid "Expected pseudo-element but found '%1$S'."
msgstr "Ku languteriwile pseudo-elemente kambe ku kumekile '%1$S'."

#: PEPseudoSelDoubleNot
msgid "Negation pseudo-class can't be negated '%1$S'."
msgstr "Pseudo-tlilasi a yi aleriwi '%1$S'."

#: PEPseudoSelPEInNot
msgid "Pseudo-elements can't be negated '%1$S'."
msgstr "Pseudo-elemente a yi aleriwi '%1$S'."

#: PEPseudoSelNewStyleOnly
msgid "This pseudo-element must use the \"::\" form: '%1$S'."
msgstr "Pseudo-elemente yi fanele ku tirhisa fomo ya \"::\": '%1$S'."

#: PEPseudoSelTrailing
msgid "Found trailing token after pseudo-element, which must be the last part of a selector:  '%1$S'."
msgstr "Ku kumekile thokeni leyi nga sala eka pseudo-elemente, leyi nga va ka xiphemu xo hetelela xa xihlawuri:  '%1$S'."

#: PEPseudoSelMultiplePE
msgid "Extra pseudo-element '%1$S'."
msgstr "Pseudo-elemente yo engetela '%1$S'."

#: PEPseudoSelUnknown
msgid "Unknown pseudo-class or pseudo-element '%1$S'."
msgstr "Pseudo-tlilasi kumbe pseudo-elemente yo ka yi nga tiveki ya '%1$S'."

#: PENegationEOF
msgid "selector within negation"
msgstr "xihlawuri leri aleriweke"

#: PENegationBadInner
msgid "Malformed simple selector as negation pseudo-class argument '%1$S'."
msgstr "Xihlawuri xo olova lexi aleriweke xa mbulavulo wa pseudo-tlilasi xa '%1$S'."

#: PENegationNoClose
msgid "Missing closing ')' in negation pseudo-class '%1$S'."
msgstr "')' Ku kayivela ka ku pfala loku aleriweke wa pseudo-tlilasi '%1$S'."

#: PENegationBadArg
msgid "Missing argument in negation pseudo-class '%1$S'."
msgstr "Ku kayivela ka mbulavulo lowu aleriweke wa pseudo-tlilasi '%1$S'."

#: PELangArgEOF
msgid "argument to :lang selector"
msgstr "mbulavulo wa :lang xa xihlawuri"

#: PELangArgNotIdent
msgid "Expected identifier for lang pseudo-class parameter but found '%1$S'."
msgstr "Ku languteriwile pharamitara ya lang pseudo-tlilasi kambe ku kumekile '%1$S'."

#: PELangNoClose
msgid "Missing closing ')' in lang pseudo-class '%1$S'."
msgstr "')' ku kayivela ku pfala eka pseudo-tlilasi ya lang ya '%1$S'."

#: PELangNoArg
msgid "Missing argument in lang pseudo-class '%1$S'."
msgstr "Ku kayivela mbulavulo eka pseudo-tlilasi ya lang ya '%1$S'."

#: PESelectorEOF
msgid "selector"
msgstr "xihlawuri"

#: PEBadDeclBlockStart
msgid "Expected '{' to begin declaration block but found '%1$S'."
msgstr "'{' ku languteriwile ku sungula buloko ya leswi boheweke kambe ku kumekile '%1$S'."

#: PEColorEOF
msgid "color"
msgstr "muhlovo"

#: PEColorNotColor
msgid "Expected color but found '%1$S'."
msgstr "Ku languteriwile muhlovo kambe ku kumekile '%1$S'."

#: PEColorComponentEOF
msgid "color component"
msgstr "xiyenge xa muhlovo"

#: PEExpectedPercent
msgid "Expected a percentage but found '%1$S'."
msgstr "Ku languteriwile phesente kambe ku kumekile '%1$S'."

#: PEExpectedInt
msgid "Expected an integer but found '%1$S'."
msgstr "Ku languteriwile inthenja kambe ku kumekile '%1$S'."

#: PEColorBadRGBContents
msgid "Expected number or percentage in rgb() but found '%1$S'."
msgstr "Ku languteriwile nomboro kumbe phesente eka rgb() kambe ku kumekile '%1$S'."

#: PEColorComponentBadTerm
msgid "Expected '%2$S' but found '%1$S'."
msgstr "Ku languteriwile '%2$S' kambe ku kumekile '%1$S'."

#: PEColorHueEOF
msgid "hue"
msgstr "mavala"

#: PEExpectedComma
msgid "Expected ',' but found '%1$S'."
msgstr "',' languteriwile kambe ku kumekile '%1$S'."

#: PEColorSaturationEOF
msgid "saturation"
msgstr "ntalo"

#: PEColorLightnessEOF
msgid "lightness"
msgstr "ku vonakala"

#: PEColorOpacityEOF
msgid "opacity in color value"
msgstr "xiyimo xa nkoka wa muhlovo"

#: PEExpectedNumber
msgid "Expected a number but found '%1$S'."
msgstr "Ku languteriwile nomboro kambe ku kumekile '%1$S'."

#: PEExpectedCloseParen
msgid "Expected ')' but found '%1$S'."
msgstr "')' ku languteriwile kambe ku kumekile '%1$S'."

#: PEDeclEndEOF
msgid "';' or '}' to end declaration"
msgstr "';' kumbe '} ku hetisa leswi bohiweke"

#: PEParseDeclarationNoColon
msgid "Expected ':' but found '%1$S'."
msgstr "':' Ku languteriwile kambe ku kumekile '%1$S'."

#: PEParseDeclarationDeclExpected
msgid "Expected declaration but found '%1$S'."
msgstr "Ku languteriwile leswi bohiweke kambe ku kumekile '%1$S'."

#: PEEndOfDeclEOF
msgid "end of declaration"
msgstr "emakumu ka leswi bohiweke"

#: PEImportantEOF
msgid "important"
msgstr "swa nkoka"

#: PEExpectedImportant
msgid "Expected 'important' but found '%1$S'."
msgstr "Ku languteriwile 'swa nkoka' kambe ku kumekile '%1$S'."

#: PEBadDeclEnd
msgid "Expected ';' to terminate declaration but found '%1$S'."
msgstr "':' Ku languteriwile ku herisa leswi bohiweke kambe ku kumekile '%1$S'."

#: PEBadDeclOrRuleEnd
msgid "Expected ';' or '}' to terminate declaration but found '%1$S'."
msgstr "Ku languteriwile ';' kumbe '}' ku herisa leswi bohiweke kambe ku kumekile '%1$S'."

#: PEInaccessibleProperty
msgid "Attempt to use inaccessible property"
msgstr "Xitirhisiwa lexi nga fikelelekiki"

#: PECommentEOF
msgid "end of comment"
msgstr "emakumu ka mbulavulo"

#: SEUnterminatedString
msgid "Found unclosed string '%1$S'."
msgstr "Ku kumekile ntambhu leyi nga pfariwakingi '%1$S'."
