# Map Expat error codes to error strings
#: 1
msgid "out of memory"
msgstr "ku hava memori"

#: 2
msgid "syntax error"
msgstr "xihoxo xa vulongoloxa-marito"

#: 3
msgid "no element found"
msgstr "ku hava elemente yi nga kumeka"

#: 4
msgid "not well-formed"
msgstr "a swi tumbuluxiwangi kahle"

#: 5
msgid "unclosed token"
msgstr "thokeni yo ka yi nga pfariwangi"

#: 6
msgid "partial character"
msgstr "xihlawulekisinyana"

#: 7
msgid "mismatched tag"
msgstr "thege yo ka yi nga yelani"

#: 8
msgid "duplicate attribute"
msgstr "xihlawulekisi lexi vuyeleriweke"

#: 9
msgid "junk after document element"
msgstr "janki endzhaku ka elemente ya dokumente"

#: 10
msgid "illegal parameter entity reference"
msgstr "pharamitha yo ka yi nga ri yona ya nkombiso wa ndhawu"

#: 11
msgid "undefined entity"
msgstr "ndhawu yo ka yi nag boxiwangi"

#: 12
msgid "recursive entity reference"
msgstr "nthleriselo wa nkombiso wa ndhawu"

#: 13
msgid "asynchronous entity"
msgstr "ndhawu ya minkarhi yo hambana"

#: 14
msgid "reference to invalid character number"
msgstr "nkombiso wa nomboro ya xihlawulekisi xo hoxeka"

#: 15
msgid "reference to binary entity"
msgstr "nkombiso wa ndhawu ya bayinari"

#: 16
msgid "reference to external entity in attribute"
msgstr "nkombiso wa ndhawu ya le handle ya xihlawulekisi"

#: 17
msgid "xml declaration not at start of external entity"
msgstr "nhlamuselo ya xml a hi masungulo ya ndhawu ya le handle."

#: 18
msgid "unknown encoding"
msgstr "enikhodini yo ka yi nga tiveki"

#: 19
msgid "encoding specified in XML declaration is incorrect"
msgstr "Enikhodini leyi boxiwile eka nhlamuselo ya XML yi hoxekile"

#: 20
msgid "unclosed CDATA section"
msgstr "xiyenge xa CDATHA xo ka xi nga pfariwangi"

#: 21
msgid "error in processing external entity reference"
msgstr "ku na xihoxo eka ku hlela xiyenge xo hetiseka xa le handle"

#: 22
msgid "document is not standalone"
msgstr "dokhumente a yi swi koti ku yima yi ri yoxe"

#: 23
msgid "unexpected parser state"
msgstr "xiyimo xa phasa xo ka xi nga languteriwangi"

#: 24
msgid "entity declared in parameter entity"
msgstr "ku hetiseka ku kombisiwile eka pharamitha"

#: 27
msgid "prefix not bound to a namespace"
msgstr "xirhangi a xi nembenetiwangi eka ndhawu ya vito"

#: 29
msgid "reserved prefix (xml) must not be undeclared or bound to another namespace URI"
msgstr "xirhangi xa rhisevhe xa (xml) a xi fanelangi ku boxiwa kumbe ku nembenetiwa eka ndhawu ya vito rin'wana URL"

#: 30
msgid "reserved prefix (xmlns) must not be declared or undeclared"
msgstr "xirhangi xa rhisevhe xa (xml) a xi fanelangi ku boxiwa kumbe ku nembenetiwa"

#: 31
msgid "prefix must not be bound to one of the reserved namespace URIs"
msgstr "xirhangi a xi fanelangi ku nembenetiwa eka yin'wana ya tindhawu ta vito ra rhisevhe ra ti-URI"

# %1$S is replaced by the Expat error string, may be followed by Expected (see below)
# %2$S is replaced by URL
# %3$d is replaced by line number
# %4$d is replaced by column number
#: XMLParsingError
msgid "XML Parsing Error: %1$S\n"
"Location: %2$S\n"
"Line Number %3$d, Column %4$d:"
msgstr "Xihoxo xa ku Phasa xa XML. %1$S/n Ndhawu: %2$S\n Nomboro ya layini %3$d, kholomo %4$d:"

# %S is replaced by a tag name.
# This gets appended to the error string if the error is mismatched tag.
#: Expected
msgid ". Expected: </%S>."
msgstr ". Leswi Languteriweke: </%S>."

