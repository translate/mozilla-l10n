msgstr "Mpfumelelo wo hoxeka"

## @name INSUFFICIENT_ACCESS
## @loc none
#: 50
msgid "Insufficient access"
msgstr "Ku nghena ko kayivela"

## @name BUSY
## @loc none
#: 51
msgid "The LDAP server is busy"
msgstr "LDAP sevha yi le ku tirheni"

## @name UNAVAILABLE
## @loc none
#: 52
msgid "LDAP server is unavailable"
msgstr "LDAP sevha a yi kumeki"

## @name UNWILLING_TO_PERFORM
## @loc none
#: 53
msgid "LDAP server is unwilling to perform"
msgstr "LDAP sevha a yi swi lavi ku tirha"

## @name LOOP_DETECT
## @loc none
#: 54
