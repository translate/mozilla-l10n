#: pane.title
msgid "Return Receipts"
msgstr "Tirhasiti leti vuyisiweke"

#: useGlobalPrefs.label
#: useGlobalPrefs.accesskey
msgid "&Use my global return receipt preferences for this account"
msgstr "&Tirhisa nhlawulo wa rhasiti ra mina leri vuyisiweke ra gilobali eka akhawunti leyi"

#: useCustomPrefs.label
#: useCustomPrefs.accesskey
msgid "&Customize return receipts for this account"
msgstr "&Hlayisa tirhasiti leti vuyisiweke eka akhawunti leyi"

#: requestReceipt.label
#: requestReceipt.accesskey
msgid "&When sending messages, always request a return receipt"
msgstr "&Minkarhi hinkwayo loko u rhumela mahungu, kombela rhasiti ra ku vuyisa"

#: receiptArrive.label
msgid "When a receipt arrives:"
msgstr "Loko rhasiti ri fika:"

# (pofilter) accelerators: accelerator & is missing from translation
#: leaveIt.label
#: leaveIt.accesskey
msgid "Leave it in my &Inbox"
msgstr "Ri siye eka bokisi ra mina ro amukela"

# LOCALIZATION NOTE moveToSent.label Translate: 'Sent' according to Netscape glossary
#: moveToSent.label
#: moveToSent.accesskey
msgid "&Move it to my &quot;Sent&quot; folder"
msgstr "&Ri yisa eka &quot;Rhumela&quot; Folidara"

#: requestMDN.label
msgid "When I receive a request for a return receipt:"
msgstr "Loko ndzi kuma xikombelo xa rhasiti leri vuyisiweke:"

# (pofilter) accelerators: accelerator & is missing from translation
#: returnSome.label
#: returnSome.accesskey
msgid "Allow r&eturn receipts for some messages"
msgstr "Pfumelela tirhasiti leti vuyisiweke ta mahungu man'wana"

#: never.label
#: never.accesskey
msgid "&Never send a return receipt"
msgstr "&U nga rhumeli rhasiti leri vuyisiweke"

# (pofilter) accelerators: accelerator & is missing from translation
#: notInToCc.label
#: notInToCc.accesskey
msgid "If I'm not in the &To or Cc of the message:"
msgstr "Loko ndzi nga ri & Eka kumbe Cc ya hungu:"

# (pofilter) accelerators: accelerator & is missing from translation
#: outsideDomain.label
#: outsideDomain.accesskey
msgid "If the &sender is outside my domain:"
msgstr "Loko murhumeri a nga ri kona laha ndzi nga kona:"

#: otherCases.label
#: otherCases.accesskey
msgid "In all &other cases:"
msgstr "Eka hinkwaswo &na timhaka tin'wana:"

#: askMe.label
msgid "Ask me"
msgstr "Ndzi vutise"

#: alwaysSend.label
msgid "Always send"
msgstr "Rhumela minkarhi hinkwayo"

#: neverSend.label
msgid "Never send"
msgstr "U nga rhumeli"

