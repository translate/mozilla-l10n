#Error strings
#: ImportAlreadyInProgress
msgid "An import operation is currently in progress. Try again when the current import has finished."
msgstr "Ku le ku rhumeriweni sweswi. Ringeta nakambe endzhaku kaloko ku rhumela swi hundzile"

#Error strings for settings import
#: ImportSettingsBadModule
msgid "Unable to load settings module"
msgstr "A swi koteki ku panga modlulu ya ndzulamiso"

#: ImportSettingsNotFound
msgid "Unable to find settings.  Check to make sure the application is installed on this machine."
msgstr "A swi koteki ku kuma ndzulamiso. Vona leswaku apulikhexini yi nghenisiwile eka muchini."

#: ImportSettingsFailed
msgid "An error occurred while importing settings.  Some, or all, of the settings may not have been imported."
msgstr "Ku humelerile xihoxo loko ku rhumeriwa ndzulamiso. Ndzulamiso hinkwawo kumbe wun'wana a wu rhumeriwangi."

# LOCALIZATION NOTE : Do not translate the word "%S" below.
#: ImportSettingsSuccess
msgid "Settings were imported from %S"
msgstr "Ndzulamiso wu kumiwile eka %S"

#Error string for mail import
#: ImportMailBadModule
msgid "Unable to load mail import module"
msgstr "Swi tsandzile ku panga modlulu wo rhumeriwa wa mapapila"

#: ImportMailNotFound
msgid "Unable to find mail to import.  Check to make sure the mail application is correctly installed on this machine."
msgstr "Swi tsandzile ku kuma mapapila yo ma rhumela"

# LOCALIZATION NOTE : Do not translate the word "%S" below.
#: ImportMailFailed
msgid "An error occurred importing mail from %S"
msgstr "Ku humelerile xihoxo loko ku rhumeriwa mapapila ku suka eka %S"

# LOCALIZATION NOTE : Do not translate the word "%S" below.
#: ImportMailSuccess
msgid "Mail was successfully imported from %S"
msgstr "Mapapila ya rhumeriwile kahle ku suka eka %S"

# Error string for address import
#: ImportAddressBadModule
msgid "Unable to load address book import module."
msgstr "A swi koteki ku panga modlulu wo rhumela wa buku ya tiadirese"

#: ImportAddressNotFound
msgid "Unable to find any address books to import.  Check to make sure the selected application or format is correctly installed on this machine."
msgstr "A swi koteki ku kuma tibuku ta tiadirese to ti rhumela. Vona leswaku apulikhexini leyi hlawuriweke yi nghenisiwile kahle eka muchini lowu."

# LOCALIZATION NOTE : Do not translate the word "%S" below.
#: ImportAddressFailed
msgid "An error occurred importing addresses from %S."
msgstr "Ku humelerile xihoxo loko ku rhumeriwa tiadirese ku suka eka %S"

# LOCALIZATION NOTE : Do not translate the word "%S" below.
#: ImportAddressSuccess
msgid "Addresses successfully imported from %S."
msgstr "Tiadirese ti rhumeriwile kahle ku suka eka %S"

