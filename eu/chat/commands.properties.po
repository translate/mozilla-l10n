#. # LOCALIZATION NOTE (commands):
#. #  %S is a comma separated list of command names.
#: commands
msgid ""
"Commands: %S.\n"
"Use /help &lt;command&gt; for more information."
msgstr ""
"Komandoak: %S.\n"
"Erabili /help &lt;komandoa&gt; informazio gehiagorako."

#. # LOCALIZATION NOTE (noCommand, noHelp):
#. #  %S is the command name the user typed.
#: noCommand
msgid "No '%S' command."
msgstr "Ez dago '%S' komandorik."

#: noHelp
msgid "No help message for the '%S' command, sorry!"
msgstr "Ez dago laguntza-mezurik '%S' komandoarentzat, sentitzen dugu!"

#: sayHelpString
msgid "say &lt;message&gt;: send a message without processing commands."
msgstr "say &lt;mezua&gt;: bidali mezua komandorik prozesatu gabe."

#: rawHelpString
msgid "raw &lt;message&gt;: send a message without escaping HTML entities."
msgstr "raw &lt;mezua&gt;: bidali mezua HTML entitateak ihesi gabe."

#: helpHelpString
msgid ""
"help &lt;name&gt;: show the help message for the &lt;name&gt; command, or "
"the list of possible commands when used without parameter."
msgstr ""
"help &lt;izena&gt;: erakutsi &lt;izena&gt; komandoaren laguntza-mezua, edo "
"parametrorik gabe erabiliz gero, komando erabilgarrien zerrenda."

#. # LOCALIZATION NOTE (statusCommand):
#. #  %1$S is replaced with a status command name
#. #   (one of "back", "away", "busy", "dnd", or "offline").
#. #  %2$S is replaced with the localized version of that status type
#. #   (one of the 5 strings below).
#: statusCommand
msgid ""
"%1$S &lt;status message&gt;: set the status to %2$S with an optional status "
"message."
msgstr ""
"%1$S &lt;egoera-mezua&gt;: ezarri egoera %2$S gisa aukerazko egoera-"
"mezuarekin."

#: back
msgid "available"
msgstr "erabilgarri"

#: away
msgid "away"
msgstr "kanpoan"

#: busy
msgctxt "busy"
msgid "unavailable"
msgstr "ez dago erabilgarri"

#: dnd
msgctxt "dnd"
msgid "unavailable"
msgstr "ez dago erabilgarri"

#: offline
msgid "offline"
msgstr "lineaz kanpo"
