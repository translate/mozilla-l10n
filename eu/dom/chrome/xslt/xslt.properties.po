#: 1
msgid "Parsing an XSLT stylesheet failed."
msgstr "XSLT estilo-orria analizatzeak huts egin du."

#: 2
msgid "Parsing an XPath expression failed."
msgstr "XPath adierazpena analizatzeak huts egin du."

#: 3
msgctxt "3"
msgid ""
msgstr ""

#: 4
msgid "XSLT transformation failed."
msgstr "XSLT eraldaketak huts egin du."

#: 5
msgid "Invalid XSLT/XPath function."
msgstr "XSLT/XPath funtzio baliogabea."

#: 6
msgid "XSLT Stylesheet (possibly) contains a recursion."
msgstr "XSLT estilo-orriak (ziurrenik) errekurtsioa du."

#: 7
msgid "Attribute value illegal in XSLT 1.0."
msgstr "XSLT 1.0ko atributu-balio ez-legala"

#: 8
msgid "An XPath expression was expected to return a NodeSet."
msgstr "XPath adierazpena espero zen Nodeset-en balio bezala."

#: 9
msgid "XSLT transformation was terminated by <xsl:message>."
msgstr "XSLT eraldaketa amaitu egin du <xsl:message>-k."

#: 10
msgid "A network error occurred loading an XSLT stylesheet:"
msgstr "Sareko errorea gertatu da XSLT estilo-orria kargatzean:"

#: 11
msgid "An XSLT stylesheet does not have an XML mimetype:"
msgstr "XSLT estilo-orriak ez du XML MIME mota:"

#: 12
msgid "An XSLT stylesheet directly or indirectly imports or includes itself:"
msgstr ""
"XSLT estilo-orriak zuzenki edo zeharka, bere burua izan edo inportatu du."

#: 13
msgid "An XPath function was called with the wrong number of arguments."
msgstr "XPath funtzioa argumentu kopuru okerrarekin deitu da."

#: 14
msgid "An unknown XPath extension function was called."
msgstr "XPath hedapen-funtzio ezezaguna deitu da."

#: 15
msgid "XPath parse failure: ')' expected:"
msgstr "XPath analisi-akatsa: ')' espero zen:"

#: 16
msgid "XPath parse failure: invalid axis:"
msgstr "XPath analisi-akatsa: ardatz baliogabea:"

#: 17
msgid "XPath parse failure: Name or Nodetype test expected:"
msgstr "XPath analisi-akatsa: izen edo Nodetype test-a espero zen:"

#: 18
msgid "XPath parse failure: ']' expected:"
msgstr "XPath analisi-akatsa: ']' espero zen:"

#: 19
msgid "XPath parse failure: invalid variable name:"
msgstr "XPath analisi-akatsa: aldagai-izen baliogabea:"

#: 20
msgid "XPath parse failure: unexpected end of expression:"
msgstr "XPath analisi-akatsa: adierazpen amaiera ez zen espero:"

#: 21
msgid "XPath parse failure: operator expected:"
msgstr "XPath analisi-akatsa: eragilea espero zen:"

#: 22
msgid "XPath parse failure: unclosed literal:"
msgstr "XPath analisi-akatsa: itxi gabeko literala:"

#: 23
msgid "XPath parse failure: ':' unexpected:"
msgstr "XPath analisi-akatsa: ez zen ':' espero:"

#: 24
msgid "XPath parse failure: '!' unexpected, negation is not():"
msgstr "XPath analisi-akatsa: ez zen '!' espero, ezeztapena not() da:"

#: 25
msgid "XPath parse failure: illegal character found:"
msgstr "XPath analisi-akatsa: karaktere ez-legala aurkitu da:"

#: 26
msgid "XPath parse failure: binary operator expected:"
msgstr "XPath analisi-akatsa: eragile bitarra espero zen:"

#: 27
msgid "An XSLT stylesheet load was blocked for security reasons."
msgstr "XSLT estilo-orri baten karga eragotzi da segurtasun arrazoiengatik."

#: 28
msgid "Evaluating an invalid expression."
msgstr "Adierazpen baliogabea ebaluatzen."

#: 29
msgid "Unbalanced curly brace."
msgstr "Itxi gabeko giltza."

#: 30
msgid "Creating an element with an invalid QName."
msgstr "QName baliogabe bat duen elementua sortzen."

#: 31
msgid "Variable binding shadows variable binding within the same template."
msgstr "Aldagai-loturak txantiloi bereko aldagai-lotura iluntzen du."

#: LoadingError
msgid "Error loading stylesheet: %S"
msgstr "Errorea estilo-orria kargatzean: %S"

#: TransformError
msgid "Error during XSLT transformation: %S"
msgstr "Errorea XSLT eraldaketa egitean: %S"
