#: MimeNotCss
msgid ""
"The stylesheet %1$S was not loaded because its MIME type, “%2$S”, is not "
"“text/css”."
msgstr ""
"%1$S estilo orria ez da kargatu bere MIME mota, \"%2$S\" alegia, ez delako "
"\"text/css\"."

#: MimeNotCssWarn
msgid ""
"The stylesheet %1$S was loaded as CSS even though its MIME type, “%2$S”, is "
"not “text/css”."
msgstr ""
"%1$S estilo-orria CSS gisa kargatu da bere MIME mota, \"%2$S\", \"text/css\" "
"ez den arren."

#: PEUnexpEOF2
msgid "Unexpected end of file while searching for %1$S."
msgstr "Espero ez zen fitxategi amaiera %1$S bilatzean."

#: PEParseRuleWSOnly
msgid "Whitespace-only string given to be parsed as rule."
msgstr "Emandako zuriune bakarreko katea arau bezala analizatuko da."

#: PEDeclDropped
msgid "Declaration dropped."
msgstr "Deklarazioa albo batera utzita."

#: PEDeclSkipped
msgid "Skipped to next declaration."
msgstr "Hurrengo deklaraziora salto egin da."

#: PEUnknownProperty
msgid "Unknown property ‘%1$S’."
msgstr "'%1$S' propietate ezezaguna."

#: PEValueParsingError
msgid "Error in parsing value for ‘%1$S’."
msgstr "Errorea '%1$S'(r)en balioa analizatzean."

#: PEExpectEndValue
msgid "Expected end of value but found ‘%1$S’."
msgstr "Balio amaiera espero zen baina '%1$S' aurkitu da."

#: PERuleTrailing
msgid "Expected end of rule but found ‘%1$S’."
msgstr "Erregelaren amaiera espero zen baina '%1$S' aurkitu da."

#: PESkipAtRuleEOF2
msgid "end of at-rule"
msgstr "at-erregelaren amaiera"

#: PEUnknownAtRule
msgid "Unrecognized at-rule or error parsing at-rule ‘%1$S’."
msgstr "at-erregela ezezaguna edo errorea '%1$S' at-erregela analizatzean."

#: PECharsetRuleEOF
msgid "charset string in @charset rule"
msgstr "karaktere-joko katea @charset arauean"

#: PECharsetRuleNotString
msgid "Expected charset string but found ‘%1$S’."
msgstr "Karaktere-joko katea espero zen baina '%1$S' aurkitu da."

#: PEGatherMediaEOF
msgid "end of media list in @import or @media rule"
msgstr "medio zerrenda amaiera @import edo @media arauean"

#: PEGatherMediaNotComma
msgid "Expected ‘,’ in media list but found ‘%1$S’."
msgstr "',' espero zen medio zerrendan baina '%1$S' aurkitu da."

#: PEGatherMediaNotIdent
msgid "Expected identifier in media list but found ‘%1$S’."
msgstr "Identifikatzailea espero zen medio zerrendan baina '%1$S' aurkitu da."

#: PEGatherMediaReservedMediaType
msgid "Found reserved keyword ‘%1$S’ when looking for media type."
msgstr "Erreserbatutako '%1$S' gako-hitza aurkitu da media mota bilatzean."

#: PEParseSourceSizeListEOF
msgid "length value for matched media condition"
msgstr "parekatutako multimedia-baldintzaren luzera-balioa"

#: PEParseSourceSizeListNotComma
msgid "Expected ‘,’ after value but found ‘%1$S’"
msgstr "Balioaren ondoren ',' espero zen baina '%1$S' aurkitu da"

#: PEImportNotURI
msgid "Expected URI in @import rule but found ‘%1$S’."
msgstr "URIa espero zen @import arauan baina '%1$S' aurkitu da."

#: PEImportBadURI
msgid "Invalid URI in @import rule: ‘%1$S’."
msgstr "URI baliogabea @import arauan: '%1$S'."

#: PEImportUnexpected
msgid "Found unexpected ‘%1$S’ within @import."
msgstr "Ezusteko '%1$S' aurkitu da @import barruan."

#: PEGroupRuleEOF2
msgid "end of @media, @supports or @-moz-document rule"
msgstr "@media, @supports edo @-moz-document erregelaren amaiera"

#: PEGroupRuleNestedAtRule
msgid "%1$S rule not allowed within @media or @-moz-document rule."
msgstr "%1$S erregela ez dago baimenduta @media edo @-moz-document erregelan."

#: PEMozDocRuleBadFunc2
msgid ""
"Expected url(), url-prefix(), domain() or regexp() in @-moz-document rule "
"but found ‘%1$S’."
msgstr ""
"url(), url-prefix(), domain() edo regexp() espero zen @-moz-document arauan "
"baina '%1$S' aurkitu da."

#: PEMozDocRuleNotURI
msgid "Expected URI in @-moz-document rule but found ‘%1$S’."
msgstr "URIa espero zen @-moz-document arauan baina '%1$S' aurkitu da."

#: PEMozDocRuleNotString
msgid ""
"Expected string in @-moz-document rule regexp() function but found ‘%1$S’."
msgstr ""
"Karaktere-katea espero zen @-moz-document erregelako regexp() funtzioan "
"baina '%1$S' aurkitu da."

#: PEMozDocRuleEOF
msgid "next URI in @-moz-document rule"
msgstr "hurrengo URIa @-moz-document erregelan"

#: PEAtNSPrefixEOF
msgid "namespace prefix in @namespace rule"
msgstr "Izen-eremu aurrizkia @namespace erregelan"

#: PEAtNSURIEOF
msgid "namespace URI in @namespace rule"
msgstr "Izen-eremu URIa @namespace erregelan"

#: PEAtNSUnexpected
msgid "Unexpected token within @namespace: ‘%1$S’."
msgstr "Ustekabeko tokena @namespace barruan: '%1$S'."

#: PEKeyframeNameEOF
msgid "name of @keyframes rule."
msgstr "@keyframes erregelaren izena."

#: PEKeyframeBadName
msgid "Expected identifier for name of @keyframes rule."
msgstr "Identifikadorea espero zen @keyframes erregelaren izenerako."

#: PEKeyframeBrace
msgid "Expected opening { of @keyframes rule."
msgstr "Irekitzeko { espero zen @keyframes erregelarentzako."

#: PESkipDeclBraceEOF
msgid "closing } of declaration block"
msgstr "deklarazio-blokea ixteko }"

#: PESkipRSBraceEOF
msgid "closing } of invalid rule set"
msgstr "arau multzo baliogabea ixteko }"

#: PEBadSelectorRSIgnored
msgid "Ruleset ignored due to bad selector."
msgstr "Hautatzaile oker bat dela eta arau ezarpena albo batetara utzi da."

#: PEBadSelectorKeyframeRuleIgnored
msgid "Keyframe rule ignored due to bad selector."
msgstr "Keyframe erregela ezikusi da hautatzaile okerra dela eta."

#: PESelectorListExtraEOF
msgid "‘,’ or ‘{’"
msgstr "',' edo '{'"

#: PESelectorListExtra
msgid "Expected ‘,’ or ‘{’ but found ‘%1$S’."
msgstr "',' edo '{' espero zen baina '%1$S' aurkitu da."

#: PESelectorGroupNoSelector
msgid "Selector expected."
msgstr "Hautatzailea espero zen."

#: PESelectorGroupExtraCombinator
msgid "Dangling combinator."
msgstr "Hautatzaile aukeraketa."

#: PECounterStyleNotIdent
msgid "Expected identifier for name of @counter-style rule."
msgstr "Identifikadorea espero zen @counter-style erregelaren izenerako."

#: PECounterStyleBadName
msgid "Name of @counter-style rule can’t be ‘%1$S’."
msgstr "@counter-style erregelaren izenak ezin du '%1$S' izan."

#: PECounterStyleBadBlockStart
msgid "Expected ‘{’ to begin @counter-style rule but found ‘%1$S’."
msgstr ""
"'{' espero zen @counter-style erregela hasteko baina '%1$S' aurkitu da."

#: PECounterStyleEOF
msgid "closing ‘}’ of @counter-style block"
msgstr "@counter-style blokea ixteko '}'"

#: PECounterDescExpected
msgid "Expected counter descriptor but found ‘%1$S’."
msgstr "Kontagailuaren deskriptorea espero zen baina '%1$S' aurkitu da."

#: PEUnknownCounterDesc
msgid "Unknown descriptor ‘%1$S’ in @counter-style rule."
msgstr "'%1$S' deskriptore ezezaguna @counter-style arauan."

#: PECounterExtendsNotIdent
msgid "Expected identifier for extends system but found ‘%1$S’."
msgstr ""
"Identifikatzailea espero zen extends sistemarako baina '%1$S' aurkitu da."

#: PECounterASWeight
msgid ""
"Each weight in the additive-symbols descriptor must be smaller than the "
"previous weight."
msgstr ""
"Sinboloen gehikuntzako deskriptorearen pisu bakoitzak aurreko pisua baino "
"txikiagoa izan behar du."

#: PEClassSelEOF
msgid "class name"
msgstr "klase-izena"

#: PEClassSelNotIdent
msgid "Expected identifier for class selector but found ‘%1$S’."
msgstr ""
"Klase-hautatzailearen identifikatzailea espero zen baina '%1$S' aurkitu da."

#: PECoordinatePair
msgid "Expected coordinate pair but found ‘%1$S’."
msgstr "Koordenatu-parea espero zen baina '%1$S' aurkitu da."

#: PETypeSelEOF
msgid "element type"
msgstr "elementu mota"

