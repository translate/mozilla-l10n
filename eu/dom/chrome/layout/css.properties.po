msgid "Expected ‘}’ to end @font-face rule but found ‘%1$S’."
msgstr "'{' espero zen @font-face erregela amaitzeko baina '%1$S' aurkitu da."

#: PEAnonBoxNotAlone
msgid "Did not expect anonymous box."
msgstr "Ez zen kutxa anonimoa espero."

#: PEFFVUnexpectedEOF
msgid "Unexpected end of @font-feature-values rule."
msgstr "Espero gabeko @font-feature-values erregelaren amaiera."

#: PEFFVBlockStart
msgid "Expected opening { of @font-feature-values rule but found ‘%1$S’."
msgstr ""
"'{' espero zen @font-feature-values erregela hasteko baina '%1$S' aurkitu da."

#: PEFFVValueSetStart
msgid "Expected opening { of feature value set but found ‘%1$S’."
msgstr ""
"'{' espero zen eginbideen balio multzoaren hasieran baina '%1$S' aurkitu da."

#: PEFFVNoFamily
msgid ""
"Expected font family list for @font-feature-values rule but found ‘%1$S’."
msgstr ""
"@font-feature-values erregelaren letra-familiaren zerrenda espero zen baina "
"'%1$S' aurkitu da."

#: PEFFVUnexpectedBlockEnd
msgid "Expected ‘}’ to end @font-feature-values rule but found ‘%1$S’."
msgstr ""
"'}' espero zen @font-feature-values erregela amaitzeko baina '%1$S' aurkitu "
"da."

#: PEFFVUnknownFontVariantPropValue
msgid "Unknown font-variant property value ‘%1$S’."
msgstr "font-variant propietatearen '%1$S' balio ezezaguna."

#: PEFFVExpectedIdent
msgid "Expected identifier but found ‘%1$S’."
msgstr "Identifikatzailea espero zen baina '%1$S' aurkitu da."

#: PEFFVExpectedValue
msgid "Expected non-negative integer value but found ‘%1$S’."
msgstr "Osoko balio ez-negatiboa espero zen baina '%1$S' aurkitu da."

#: PEFFVTooManyValues
msgid "Too many values for feature type ‘%1$S’."
msgstr "Balio gehiegi '%1$S' eginbide motarentzat."

#: PEFFVGenericInFamilyList
msgid "Family list cannot contain generic font family name."
msgstr "Faimilia-zerrendak ezin du letra-izen generikoa izan."

#: PEFFVValueDefinitionTrailing
msgid "Expected end of value definition but found ‘%1$S’."
msgstr "Balio-definizioaren amaiera espero zen baina '%1$S' aurkitu da."

#: PEBadDirValue
msgid "Expected ‘ltr’ or ‘rtl’ in direction selector but found ‘%1$S’."
msgstr ""
"'ltr' edo 'rtl' espero zen noranzkoaren hautatzailean baina '%1$S' aurkitu "
"da."

#: PESupportsConditionStartEOF2
msgid "‘not’, ‘(’, or function"
msgstr "'not', '(', edo function"

#: PESupportsConditionInParensEOF
msgid "‘)’"
msgstr "‘)’"

#: PESupportsConditionNotEOF
msgid "‘not’"
msgstr "'not'"

