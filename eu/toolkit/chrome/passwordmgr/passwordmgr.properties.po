#: rememberValue
msgid "Use Password Manager to remember this value."
msgstr "Erabili pasahitz-kudeatzailea balio hau gogoratzeko."

#: rememberPassword
msgid "Use Password Manager to remember this password."
msgstr "Erabili pasahitz-kudeatzailea pasahitz hau gogoratzeko."

#: savePasswordTitle
msgid "Confirm"
msgstr "Berretsi"

#. # LOCALIZATION NOTE (saveLoginMsg, saveLoginMsgNoUser):
#. # %1$S is brandShortName, %2$S is the login's hostname.
#: saveLoginMsg
msgid "Would you like %1$S to save this login for %2$S?"
msgstr "%1$S(e)k %2$S gunerako saio-hasiera hau gordetzea nahi duzu?"

#: saveLoginMsgNoUser
msgid "Would you like %1$S to save this password for %2$S?"
msgstr "%1$S(e)k %2$S gunerako pasahitz hau gordetzea nahi duzu?"

#: saveLoginButtonAllow.label saveLoginButtonAllow.accesskey
msgid "&Save"
msgstr "&Gorde"

#: saveLoginButtonDeny.label saveLoginButtonDeny.accesskey
msgid "&Don’t Save"
msgstr "&Ez gorde"

#: updateLoginMsg
msgid "Would you like to update this login?"
msgstr "Saio-hasiera hau eguneratu nahi duzu?"

#: updateLoginMsgNoUser
msgid "Would you like to update this password?"
msgstr "Pasahitz hau eguneratu nahi duzu?"

#: updateLoginButtonText
msgid "Update"
msgstr "Eguneratu"

#: updateLoginButtonAccessKey
msgctxt "updateLoginButtonAccessKey"
msgid "U"
msgstr "E"

#: updateLoginButtonDeny.label updateLoginButtonDeny.accesskey
msgid "&Don’t Update"
msgstr "&Ez eguneratu"

#. # LOCALIZATION NOTE (rememberPasswordMsg):
#. # 1st string is the username for the login, 2nd is the login's hostname.
#. # Note that long usernames may be truncated.
#: rememberPasswordMsg
msgid "Would you like to remember the password for “%1$S” on %2$S?"
msgstr ""
"%2$S ostalariko \"%1$S\" erabiltzaile-izenaren pasahitza gogoratu nahi duzu?"

#. # LOCALIZATION NOTE (rememberPasswordMsgNoUsername):
#. # String is the login's hostname.
#: rememberPasswordMsgNoUsername
msgid "Would you like to remember the password on %S?"
msgstr "%S ostalariko pasahitza gogoratu nahi duzu?"

