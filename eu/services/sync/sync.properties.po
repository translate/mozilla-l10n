#. # %1: the user name (Ed), %2: the app name (Firefox), %3: the operating system (Android)
#: client.name2
msgid "%1$S’s %2$S on %3$S"
msgstr "%1$S erabiltzailearen %2$S %3$S sistema eragilean"

#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Azken sinkronizazioa: %S"

#. # signInToSync.description is the tooltip for the Sync buttons when Sync is
#. # not configured.
#: signInToSync.description
msgid "Sign In To Sync"
msgstr "Hasi saioa Sync-en"

#: mobile.label
msgid "Mobile Bookmarks"
msgstr "Mugikorreko laster-markak"

#: error.sync.title
msgid "Error While Syncing"
msgstr "Errorea sinkronizatzean"

#: error.sync.description
msgid ""
"Sync encountered an error while syncing: %1$S.  Sync will automatically "
"retry this action."
msgstr ""
"Syncek errorea aurkitu du sinkronizatzean: %1$S. Sync automatikoki saiatuko "
"da berriro."

#: warning.sync.eol.label
msgid "Service Shutting Down"
msgstr "Zerbitzua ixtear"

#. # %1: the app name (Firefox)
#: warning.sync.eol.description
msgid ""
"Your Firefox Sync service is shutting down soon. Upgrade %1$S to keep "
"syncing."
msgstr ""
"Zure Firefox Sync zerbitzua itxi egingo da laster. Sinkronizatzen "
"jarraitzeko, bertsio-berritu %1$S."

#: error.sync.eol.label
msgid "Service Unavailable"
msgstr "Zerbitzua ez dago erabilgarri"

#. # %1: the app name (Firefox)
#: error.sync.eol.description
msgid ""
"Your Firefox Sync service is no longer available. You need to upgrade %1$S "
"to keep syncing."
msgstr ""
"Zure Firefox Sync zerbitzua ez dago erabilgarri. %1$S bertsio-berritu behar "
"duzu sinkronizatzen jarraitzeko."

#: sync.eol.learnMore.label sync.eol.learnMore.accesskey
msgid "&Learn more"
msgstr "&Argibide gehiago"

#: syncnow.label
msgid "Sync Now"
msgstr "Sinkronizatu orain"

#: syncing2.label
msgid "Syncing…"
msgstr "SInkronizatzen…"

#~ msgid "Remote tabs are being synced…"
#~ msgstr "Urruneko fitxak sinkronizatzen…"

#~ msgid "Sync your other devices again to access their tabs"
#~ msgstr "Sinkronizatu berriro zure beste gailuak beraien fitxak atzitzeko"

#~ msgid "All remote tabs are already open"
#~ msgstr "Urruneko fitxa guztiak irekita daude dagoeneko"

#~ msgid "Recent desktop tabs will be available once they sync"
#~ msgstr "Mahaigaineko azken fitxak eskura egongo dira sinkronizatu eta gero"

#~ msgid "Error While Signing In"
#~ msgstr "Errorea saioa hastean"

#~ msgid "Sync encountered an error while connecting: %1$S.  Please try again."
#~ msgstr "Syncek errorea aurkitu du konektatzean: %1$S. Saiatu berriro."

#~ msgid "&Preferences…"
#~ msgstr "&Hobespenak…"

#~ msgid "Error While Signing Out"
#~ msgstr "Errorea saioa amaitzean"

#~ msgid ""
#~ "Sync encountered an error while connecting.  It's probably ok, and you "
#~ "don't have to do anything about it."
#~ msgstr ""
#~ "Syncek errorea aurkitu du konektatzean. Ziurrenik ez dago arazorik eta ez "
#~ "duzu honetaz arduratu behar."

#~ msgid ""
#~ "Sync has not been able to complete during the last %1$S days. Please "
#~ "check your network settings."
#~ msgstr ""
#~ "Sync ez da sinkronizatzeko gai izan azken %1$S egunetan. Egiaztatu sareko "
#~ "ezarpenak."

#~ msgid "Ser&ver Status"
#~ msgstr "Zer&bitzariaren egoera"

#~ msgid "You need to update Firefox Sync to continue syncing your data."
#~ msgstr ""
#~ "Firefox Sync eguneratu behar duzu zure datuak sinkronizatzen jarraitzeko."

#~ msgid "&Update Firefox Sync"
#~ msgstr "&Eguneratu Firefox Sync"

#~ msgid "Approaching Server Quota"
#~ msgstr "Zerbitzariaren kuotara hurbiltzen"

#~ msgid ""
#~ "You are approaching the server quota. Please review which data to sync."
#~ msgstr ""
#~ "Zerbitzariaren kuoatara hurbiltzen ari zara. Gainbegiratu sinkronizatu "
#~ "beharreko datuak."

#~ msgid "Server Quota Exceeded"
#~ msgstr "Zerbitzariaren kuota gaindituta"

#~ msgid ""
#~ "Sync failed because it exceeded the server quota. Please review which "
#~ "data to sync."
#~ msgstr ""
#~ "Syncek huts egin du zerbitzariaren kuota gainditu delako. Gainbegiratu "
#~ "sinkronizatu beharreko datuak."

#~ msgid "&View Quota"
#~ msgstr "&Ikusi kuota"
