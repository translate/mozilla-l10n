#: MimeNotCss
msgid ""
"The stylesheet %1$S was not loaded because its MIME type, \"%2$S\", is not "
"\"text/css\"."
msgstr ""
"Enkula y'olupapula %1$S teyattikidwa kubanga yankula ya MIME, \"%2$S\", "
"ettali \"text/css\"."

#: MimeNotCssWarn
msgid ""
"The stylesheet %1$S was loaded as CSS even though its MIME type, \"%2$S\", "
"is not \"text/css\"."
msgstr ""
"Enkula y'olupapula %1$S yattikidwa nga CSS newankubadde enkula yayo eya "
"MIME, \"%2$S\", teri \"text/css\"."

#: PEUnexpEOF2
msgid "Unexpected end of file while searching for %1$S."
msgstr "Ekitasubirwa omuweru gwa fayiro muku nonya rwa%1$S."

#: PEParseRuleWSOnly
msgid "Whitespace-only string given to be parsed as rule."
msgstr "Ekiffu nga`tewandikee kyekka ekigambo ekiwereddwa kuyiitamu nga teeka."

#: PEDeclDropped
msgid "Declaration dropped."
msgstr "Okuttegeziddwa ku kusuulidwa."

#: PEDeclSkipped
msgid "Skipped to next declaration."
msgstr "Buuka ku akidako ekittegeziddwa."

#: PEUnknownProperty
msgid "Unknown property '%1$S'."
msgstr "Tekimanyiddwa ekintu '%1$S'."

#: PEValueParsingError
msgid "Error in parsing value for '%1$S'."
msgstr "Ensobi mu kuyisamu omuwenddo gw '%1$S'."

#: PEExpectEndValue
msgid "Expected end of value but found '%1$S'."
msgstr "Ebadde esubira enkomerero kyoka ensanze '%1$S'."

#: PESkipAtRuleEOF
msgid "end of unknown at-rule"
msgstr "enkomerero temanyiddwa ku fuunga"

#: PEUnknownAtRule
msgid "Unrecognized at-rule or error parsing at-rule '%1$S'."
msgstr "Tekizuukiddwa ku fuunga oba ensobi enyisamu ku fuunga '%1$S'."

#: PECharsetRuleEOF
msgid "charset string in @charset rule"
msgstr "seti z`ennukuta ze kigambo mu @charset z`ennukuta fuunga"

#: PECharsetRuleNotString
msgid "Expected charset string but found '%1$S'."
msgstr "Ekisubira ku seti z`ennukuta ze kigambo nanye kifuuse '%1$S'."

#: PEGatherMediaEOF
msgid "end of media list in @import or @media rule"
msgstr ""
"enkomerero yo lukalala rwe puliziganya ku @leeta oba @puliziganyi fuunga"

#: PEGatherMediaNotComma
msgid "Expected ',' in media list but found '%1$S'."
msgstr "Ekisubiziddwa ',' ku lukalala rwe puliziganyi nanye fuunga '%1$S'."

#: PEGatherMediaNotIdent
msgid "Expected identifier in media list but found '%1$S'."
msgstr "Ekisubirwa nnakkalazi mu lukalala rwe puliziganya naye kizuuse '%1$S'."

#: PEImportNotURI
msgid "Expected URI in @import rule but found '%1$S'."
msgstr "Ekisubirwa URI mu @leeta kuva awalala kufunga naye kizuuse '%1$S'."

#: PEImportBadURI
msgid "Invalid URI in @import rule: '%1$S'."
msgstr "Akagata akaafu mu @import rule: '%1$S'."

#: PEImportUnexpected
msgid "Found unexpected '%1$S' within @import."
msgstr "Kizuuse ekitasubirwa '%1$S' mbyemuno @leeta kuva awalala."

#: PEGroupRuleEOF
msgid "end of @media or @-moz-document rule"
msgstr "enkomerero ya @puliziganyi oba @-moz-ekiwandiko ekifuunga"

#: PEGroupRuleNestedAtRule
msgid "%1$S rule not allowed within @media or @-moz-document rule."
msgstr ""

#: PEMozDocRuleBadFunc
msgid ""
"Expected url(), url-prefix(), or domain() in @-moz-document rule but found "
"'%1$S'."
msgstr ""
"Esubirwa url(), url-prefix(), or domain() mu @-moz-document ekifuunga naye "
"kizuuse '%1$S'."

#: PEMozDocRuleNotURI
msgid "Expected URI in @-moz-document rule but found '%1$S'."
msgstr "Esubirwa URI mu @-moz-ekiwandiko ekifuunga naye kizuuse '%1$S'."

#: PEMozDocRuleNotString
msgid ""
"Expected string in @-moz-document rule regexp() function but found '%1$S'."
msgstr ""

#: PEAtNSPrefixEOF
msgid "namespace prefix in @namespace rule"
msgstr ""
"erinnya rye`bbanga ekisokerwa`ekiwandiko mu @erinnya rye`bbanga kifuunga"

#: PEAtNSURIEOF
msgid "namespace URI in @namespace rule"
msgstr "erinnya rye`bbanga URI mu @namespace kifuunga"

#: PEAtNSUnexpected
msgid "Unexpected token within @namespace: '%1$S'."
msgstr "Ekitasubirwa tokkeni mu @erinnya rye`bbanga: '%1$S'."

#: PEKeyframeNameEOF
msgid "name of @keyframes rule."
msgstr "Elinya lya etteka eligobererwa ku @keyframes."

#: PEKeyframeBadName
msgid "Expected identifier for name of @keyframes rule."
msgstr ""
"Ekilaga elinya elya eteka eligobererwa elya @keyframes kibade kisubirwa."

#: PEKeyframeBrace
msgid "Expected opening { of @keyframes rule."
msgstr "{ ebade esubirwa mukugulawo etaka eligobererwa ku @keyframes."

#: PESkipDeclBraceEOF
msgid "closing } of declaration block"
msgstr "ggalawo } ku tteegeza ezibikira"

#: PESkipRSBraceEOF
msgid "closing } of invalid rule set"
msgstr "ggalawo } ku efuunga etekikkirizibwa seti"

#: PEBadSelectorRSIgnored
msgid "Ruleset ignored due to bad selector."
msgstr "Efuunga seti tefirwako kubagaa rwe londda mbii."

#: PEBadSelectorKeyframeRuleIgnored
msgid "Keyframe rule ignored due to bad selector."
msgstr "Eteka eligoberwa elya keyframe telifiridwako kulwa akalonda akachamu."

#: PESelectorListExtraEOF
msgid "',' or '{'"
msgstr "',' oba '{'"

#: PESelectorListExtra
msgid "Expected ',' or '{' but found '%1$S'."
msgstr "Ekisubirwa ',' oba '{' naye kizuuse '%1$S'."

#: PESelectorGroupNoSelector
msgid "Selector expected."
msgstr "Elondaa esubirwa."

#: PESelectorGroupExtraCombinator
msgid "Dangling combinator."
msgstr "Ttekiramu ekiggataa."

#: PEClassSelEOF
msgid "class name"
msgstr "erinnya rye kibina"

#: PEClassSelNotIdent
msgid "Expected identifier for class selector but found '%1$S'."
msgstr "Ekisubirwa nnakkalazi kurwe`kkibina ekilondaa naye kizuuse '%1$S'."

#: PETypeSelEOF
msgid "element type"
msgstr "ekikka kya`kantu"

#: PETypeSelNotType
msgid "Expected element name or '*' but found '%1$S'."
msgstr "Ekisubirwa erinnya rya`kantu oba '*' naye kizuuse '%1$S'."

#: PEUnknownNamespacePrefix
msgid "Unknown namespace prefix '%1$S'."
msgstr "Erinnya rye`bbanga terimanyiddwa ku ekisokerwawo ekingambo '%1$S'."

#: PEAttributeNameEOF
msgid "attribute name"
msgstr "ebisanyizzo rye`rinnya"

#: PEAttributeNameExpected
msgid "Expected identifier for attribute name but found '%1$S'."
msgstr "Ekisubirwa nnakkalazi kurwo erinnya ye ekintu naye kizuunse '%1$S'."

#: PEAttributeNameOrNamespaceExpected
msgid "Expected attribute name or namespace but found '%1$S'."
msgstr ""
"Ekisubirwa erinnya rye ekintu oba erinnya yre bbanga naye kizuunse '%1$S'."

#: PEAttSelNoBar
msgid "Expected '|' but found '%1$S'."
msgstr "Ekisubirwa '|' naye kizuunse '%1$S'."

#: PEAttSelInnerEOF
msgid "part of attribute selector"
msgstr "ekituddu kye ekintu ekilondda"

#: PEAttSelUnexpected
msgid "Unexpected token in attribute selector: '%1$S'."
msgstr "Ekitasubirwa tokeni mu ekintu ekilondda: '%1$S'."

#: PEAttSelValueEOF
msgid "attribute value"
msgstr "omuwendo gwe ekintu"

#: PEAttSelCloseEOF
msgid "']' to end attribute selector"
msgstr "']' ku enkomerero"

#: PEAttSelNoClose
msgid "Expected ']' to terminate attribute selector but found '%1$S'."
msgstr "Ekisubirwa ']' ku fulumya ekintu ekilondda naye kizuunse '%1$S'."

#: PEAttSelBadValue
msgid ""
"Expected identifier or string for value in attribute selector but found "
"'%1$S'."
msgstr ""
"Ekisubirwa nnakkalazi oba ekingambo kurwo mu wenddo mu ekintu ekilondda naye "
"kizuunse '%1$S'."

#: PEPseudoSelEOF
msgid "name of pseudo-class or pseudo-element"
msgstr "erinnya rye ekiffu-ekimbinya oba ekiffu-ekintu"

#: PEPseudoSelBadName
msgid ""
"Expected identifier for pseudo-class or pseudo-element but found '%1$S'."
msgstr ""
"Ekisubirwa nnakkalazi kurwe ekiffu-ekimbinya oba ekiffu-ekintu naye kizuunse "
"'%1$S'."

#: PEPseudoSelNonFunc
msgid ""
"Function token for non-function pseudo-class or pseudo-element, or the other "
"way around, when reading '%1$S'."
msgstr ""
"Omungasho tokeni kurwo tteerii-omungasho ekiffu-ekimbinya oba ekiffu-ekintu, "
"oba okwetolola, bwoba osooma '%1$S'."

#: PEPseudoSelNotPE
msgid "Expected pseudo-element but found '%1$S'."
msgstr "Ekisubirwa ku ekiffu-ekintu naye kinzuunse '%1$S'."

#: PEPseudoSelDoubleNot
msgid "Negation pseudo-class can't be negated '%1$S'."
msgstr "Kkukizzingaya ku ekiffu-ekimbinya teekinyiza kkukizzingaya '%1$S'."

#: PEPseudoSelPEInNot
msgid "Pseudo-elements can't be negated '%1$S'."
msgstr "Biffu-ebintu ttebenyizza kukkizzingaya '%1$S'."

#: PEPseudoSelNewStyleOnly
msgid "This pseudo-element must use the \"::\" form: '%1$S'."
msgstr "Ekiffu-ekintu kkino kirina ku kozesa ku\"::\" fomu: '%1$S'."

#: PEPseudoSelTrailing
msgid ""
"Found trailing token after pseudo-element, which must be the last part of a "
"selector:  '%1$S'."
msgstr ""
"Kizuunse nga kigezzako ku londa enukutaa oruvanyima ekiffu-kintu, ekirina "
"kusembanyo ku londwa:  '%1$S'."

#: PEPseudoSelMultiplePE
msgid "Extra pseudo-element '%1$S'."
msgstr "Jjamu ekiffu-ekintu '%1$S'."

#: PEPseudoSelUnknown
msgid "Unknown pseudo-class or pseudo-element '%1$S'."
msgstr "Tteteregeka ekiffu-ekibina oba ekiffu-ekintu '%1$S'."

#: PENegationEOF
msgid "selector within negation"
msgstr "ennonda mu buvannyuma"

#: PENegationBadInner
msgid "Malformed simple selector as negation pseudo-class argument '%1$S'."
msgstr "Fom`ubwiggi enyangu ennonda ku buvannyuma ekiffu-ekintu '%1$S'."

#: PENegationNoClose
msgid "Missing closing ')' in negation pseudo-class '%1$S'."
msgstr "Kabbuzzee akagalawo ')' mu buvannyuma ekiffu-ekibina '%1$S'."

#: PENegationBadArg
msgid "Missing argument in negation pseudo-class '%1$S'."
msgstr ""
"Kabbuzzee akalangaa ekikka kye enukuntta mu buvannyuma ekiffu-ekibina '%1$S'."

#: PEPseudoClassArgEOF
msgid "argument to pseudo-class selector"
msgstr "akalangaa ekikka kye enukuntta ku ekiffu-ekibina ennonda"

#: PEPseudoClassArgNotIdent
msgid "Expected identifier for pseudo-class parameter but found '%1$S'."
msgstr ""
"Ekisubindwa nnakkalazi ku ekiffu-ekibina ekipimo naye ttekizuunse '%1$S'."

#: PEPseudoClassArgNotNth
msgid "Expected part of argument to pseudo-class but found '%1$S'."
msgstr ""
"Ekisubindwa ekkifu ku akalangaa ekikka kye enukuntaa ku ekiffu-ekibina naye "
"ttekizuunse '%1$S'."

#: PEPseudoClassNoClose
msgid "Missing closing ')' in pseudo-class, found '%1$S' instead."
msgstr "Kabbuzzee akagalawo ')' mu ekiffu-ekibina, kizuunse '%1$S' naye."

#: PEPseudoClassNoArg
msgid "Missing argument in pseudo-class '%1$S'."
msgstr "Kabbuzzee akalaga ekikka kye enukuntaa mu ekiffu-ekibina '%1$S'."

#: PESelectorEOF
msgid "selector"
msgstr "ennonda"

#: PEBadDeclBlockStart
msgid "Expected '{' to begin declaration block but found '%1$S'."
msgstr "Kasubindwawo '{' ku ttandikka eragga buloka naye kazuunse '%1$S'."

#: PEColorEOF
msgid "color"
msgstr "langi"

#: PEColorNotColor
msgid "Expected color but found '%1$S'."
msgstr "Esubindwawo langi naye ezuunse '%1$S'."

#: PEColorComponentEOF
msgid "color component"
msgstr "langi ya ekintuduntudu"

#: PEExpectedPercent
msgid "Expected a percentage but found '%1$S'."
msgstr "Pacenti esubindwa naye ezuunse '%1$S'."

#: PEExpectedInt
msgid "Expected an integer but found '%1$S'."
msgstr "Esubindwa enambaa naye ezuuse '%1$S'."

#: PEColorBadRGBContents
msgid "Expected number or percentage in rgb() but found '%1$S'."
msgstr "Esubindwa enambaa oba pacenti mu rgb() naye ezuuse '%1$S'."

#: PEColorComponentBadTerm
msgid "Expected '%2$S' but found '%1$S'."
msgstr "Esubindwa '%2$S' naye ezuuse '%1$S'."

#: PEColorHueEOF
msgid "hue"
msgstr "hue"

#: PEExpectedComma
msgid "Expected ',' but found '%1$S'."
msgstr "Akasubindwa '.' naye kazuuse '%1$S'."

#: PEColorSaturationEOF
msgid "saturation"
msgstr "kikkuttee"

#: PEColorLightnessEOF
msgid "lightness"
msgstr "ekiggaala"

#: PEColorOpacityEOF
msgid "opacity in color value"
msgstr "langi kwaffu m`uwendo"

#: PEExpectedNumber
msgid "Expected a number but found '%1$S'."
msgstr "Esubindwa enambaa naye ezuuse '%1$S'."

#: PEExpectedCloseParen
msgid "Expected ')' but found '%1$S'."
msgstr "Akasubindwa ')' naye kazuuse '%1$S'."

#: PEDeclEndEOF
msgid "';' or '}' to end declaration"
msgstr "';' oba '}' ku komerero yo kulagaa"

#: PEParseDeclarationNoColon
msgid "Expected ':' but found '%1$S'."
msgstr "Akasubindwa ':' naye kazuuse '%1$S'."

#: PEParseDeclarationDeclExpected
msgid "Expected declaration but found '%1$S'."
msgstr "Esubindwa elagaa naye ezuuse '%1$S'."

#: PEEndOfDeclEOF
msgid "end of declaration"
msgstr "ekomerero ye ndagaa"

#: PEImportantEOF
msgid "important"
msgstr "kikulu"

#: PEExpectedImportant
msgid "Expected 'important' but found '%1$S'."
msgstr "Ekisubindwa 'ekikulu' naye kizuuse '%1$S'."

#: PEBadDeclEnd
msgid "Expected ';' to terminate declaration but found '%1$S'."
msgstr "Akasubindwa ';' ku sazamu endagaa naye kazuuse '%1$S'."

#: PEBadDeclOrRuleEnd2
msgid "Expected ';' or '}' to terminate declaration but found '%1$S'."
msgstr "Akasubindwa ';' oba '}' ku sazamu endagaa naye kazuuse '%1$S'."

#: PEInaccessibleProperty2
msgid "Cannot specify value for internal property."
msgstr "Ttekanyiza kulagaa omuwendo gwa ebya`magguzzi ebyo mudda."

#: PECommentEOF
msgid "end of comment"
msgstr "enkomerero ekyokwogererako"

#: SEUnterminatedString
msgid "Found unclosed string '%1$S'."
msgstr "Kisazindwa nga sikkibike ku kingambo '%1$S'."

#: PEFontDescExpected
msgid "Expected font descriptor but found '%1$S'."
msgstr "Suubidde kannyonnyola ndabika ya bigambo naye nzudde '%1$S'."

#: PEUnknownFontDesc
msgid "Unknown descriptor '%1$S' in @font-face rule."
msgstr "Akannyonnyola akatamanyiddwa '%1$S' mu @font-face rule."

#: PEMQExpectedExpressionStart
msgid "Expected '(' to start media query expression but found '%1$S'."
msgstr ""
"Suubirwa '(' kutandika mbuuza ya bibuuzo by'amawulire naye bizuuse '%1$S'."

#: PEMQExpressionEOF
msgid "contents of media query expression"
msgstr "ebiri mu mbuuza y'ebibuuzo by'amawulire"

#: PEMQExpectedFeatureName
msgid "Expected media feature name but found '%1$S'."
msgstr "Suubidde linnya likwata ku by'amawulire naye nfunye '%1$S'."

#: PEMQExpectedFeatureNameEnd
msgid "Expected ':' or ')' after media feature name but found '%1$S'."
msgstr ""
"Nsuubidde ':' oba ')' oluvannyuma lw'erinnya ly'ebyamawulire naye nzuudde "
"'%1$S'."

#: PEMQNoMinMaxWithoutValue
msgid "Media features with min- or max- must have a value."
msgstr "Eby'amawulire ebirina entono- oba ennene- biteekwa okufuna omuwendo."

#: PEMQExpectedFeatureValue
msgid "Found invalid value for media feature."
msgstr "Zuudde omuwendo gw`eby`amawulire omukyamu."

#: PEBadFontBlockStart
msgid "Expected '{' to begin @font-face rule but found '%1$S'."
msgstr "Kisuubirwa '{' okutandika etteeka lya @font-face naye kizuuse '%1$S'."

#: PEBadFontBlockEnd
msgid "Expected '}' to end @font-face rule but found '%1$S'."
msgstr "Kisuubirwa '}' okutandika etteeka lya @font-face naye kizuuse '%1$S'."

#: PEAnonBoxNotAlone
msgid "Did not expect anonymous box."
msgstr "Akabokisi akatamanyiddwa kabade tekasubiirwa."

#~ msgid "Expected end of value for property but found '%1$S'."
#~ msgstr "Ekisubizirwa enkomerero ku muweddo gw`ekintu nanye kizuuse ´%1$S´. "
