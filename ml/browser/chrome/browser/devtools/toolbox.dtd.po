#: closeCmd.key
msgid "W"
msgstr "W"

#: toolboxCloseButton.tooltip
msgid "Close Developer Tools"
msgstr "ഡവലപ്പര്‍ പ്രയോഗങ്ങള്‍ അടയ്ക്കുക"

#: toolboxOptionsButton.key
msgid "O"
msgstr "O"

#: toolboxNextTool.key
msgid "]"
msgstr "]"

#: toolboxPreviousTool.key
msgid "["
msgstr "["

#: toolboxZoomIn.key
msgid "+"
msgstr "+"

