msgid "Connect"
msgstr "കണക്ട് ചെയ്യുക"

#: header
msgid "Connect to remote device"
msgstr "റിമോട്ട് ഡിവൈസിലേക്കു് കണക്ട് ചെയ്യുക"

#: host
msgid "Host:"
msgstr "ഹോസ്റ്റ്‌:"

#: port
msgid "Port:"
msgstr "പോര്‍ട്ട്‌:"

#: connect
msgctxt "connect"
msgid "Connect"
msgstr "കണക്ട് ചെയ്യുക"

#: connecting
msgid "Connecting…"
msgstr "കണക്ട് ചെയ്യുന്നു…"

#: availableAddons
msgid "Available remote add-ons:"
msgstr "ലഭ്യമായ വിദൂര ആഡ്-ഓണുകള്‍:"

#: availableTabs
msgid "Available remote tabs:"
msgstr "ലഭ്യമായ വിദൂര റ്റാബുകള്‍:"

