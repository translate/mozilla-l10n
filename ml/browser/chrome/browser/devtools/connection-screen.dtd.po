msgid "Error:"
msgstr "തെറ്റു്"

#: errorTimeout
msgid "Error: connection timeout."
msgstr "പിശക്: കണക്ഷന്റെ സമയപരിധി കഴിഞ്ഞു"

#: errorRefused
msgid "Error: connection refused."
msgstr "പിശക്: കണക്ഷന്‍ നിരസിച്ചു."

#: errorUnexpected
msgid "Unexpected error."
msgstr "പ്രതീക്ഷിക്കാത്ത തകരാര്‍."

