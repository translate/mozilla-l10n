#. LOCALIZATION NOTE (reloadPlugin.post): include a starting space as needed
#: reloadPlugin.pre
msgctxt "reloadPlugin.pre"
msgid ""
msgstr "‌"

#: reloadPlugin.middle
msgid "Reload the page"
msgstr "താള്‍ വീണ്ടും ലഭ്യമാക്കുക"

#: reloadPlugin.post
msgid " to try again."
msgstr " വീണ്ടും ശ്രമിക്കാന്‍."

#. This and the other report.* strings should be as short as possible, ideally 2-3 words.
#: report.please
msgid "Send crash report"
msgstr "തകരാറിനെപ്പറ്റിയുള്ള അറിയിപ്പ് അയയ്ക്കുക"

#: report.submitting
msgid "Sending report…"
msgstr "അറിയിപ്പ് അയയ്ക്കുന്നു…"

#: report.submitted
msgid "Crash report sent."
msgstr "തകരാറിനെപ്പറ്റിയുള്ള അറിയിപ്പ് അയച്ചു."

#: report.disabled
msgid "Crash reporting disabled."
msgstr "തകാരാറു് രേഖപ്പെടുത്തുന്നതു് പ്രവര്‍ത്തനരഹിതമാണു്."

#: report.failed
msgid "Submission failed."
msgstr "സമര്‍പ്പിയ്ക്കല്‍ പരാജയപ്പെട്ടു."

#: report.unavailable
msgid "No report available."
msgstr "അറിയിപ്പ് ലഭ്യമല്ല."

#: report.comment
msgid "Add a comment (comments are publicly visible)"
msgstr "ഒരു അഭിപ്രായം ചേര്‍ക്കുക (അഭിപ്രായങ്ങള്‍ എല്ലാവര്‍ക്കുവാന്‍ കാണുവാന്‍ സാധിക്കുന്നു)"

#: report.pageURL
msgid "Include the page’s URL"
msgstr "താളിന്റെ യുആര്‍എല്‍ ഉള്‍പ്പെടുത്തുക"

#: plugin.file
msgid "File"
msgstr "ഫയല്‍"

#: plugin.mimeTypes
msgid "MIME Types"
msgstr "MIME തരങ്ങള്‍"

#: plugin.flashProtectedMode.label
msgid "Enable Adobe Flash protected mode"
msgstr "അഡോബി ഫ്ലാഷ് സംരക്ഷിത രീതി പ്രവര്‍ത്തനസജ്ജമാക്കുക"

#: plugin.enableBlocklists.label
msgid "Block dangerous and intrusive Flash content"
msgstr ""

