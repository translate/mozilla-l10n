#. # strings used by the Find bar, split from browser.properties
#: NotFound
msgid "Phrase not found"
msgstr "വാചകം കണ്ടെത്താനായില്ല "

#: WrappedToTop
msgid "Reached end of page, continued from top"
msgstr "താളിന്റെ അവസാനം വരെ എത്തിയിരിക്കുന്നു, മുകളില്‍ നിന്നും തുടരുന്നു "

#: WrappedToBottom
msgid "Reached top of page, continued from bottom"
msgstr "താളിന്റെ മുകളില്‍ എത്തിയിരിക്കുന്നു, താഴെ നിന്നും തുടരുന്നു"

#: NormalFind
msgid "Find in page"
msgstr "താളില്‍ കണ്ടെത്തുക"

#: FastFind
msgid "Quick find"
msgstr "പെട്ടെന്നുള്ള തെരച്ചില്‍"

#: FastFindLinks
msgid "Quick find (links only)"
msgstr "പെട്ടെന്നുള്ള തെരച്ചില്‍ (ലിങ്കുകള്‍ മാത്രം):"

#: CaseSensitive
msgid "(Case sensitive)"
msgstr "(കേസ് സെന്‍സിറ്റീവ്)"

#. # LOCALIZATION NOTE (FoundMatches): Semicolon-separated list of plural forms.
#. # See: http://developer.mozilla.org/en/docs/Localization_and_Plurals
#. # #1 is currently selected match and #2 the total amount of matches.
#: FoundMatches
msgid "#1 of #2 match;#1 of #2 matches"
msgstr "#1 / #2 പൊരുത്തം;#1 / #2 പൊരുത്തങ്ങള്‍"

#. # LOCALIZATION NOTE (FoundMatchesCountLimit): Semicolon-separated list of plural
#. # forms.
#. # See: http://developer.mozilla.org/en/docs/Localization_and_Plurals
#. # #1 is the total amount of matches allowed before counting stops.
#: FoundMatchesCountLimit
msgid "More than #1 match;More than #1 matches"
msgstr "#1 പൊരുത്തത്തില്‍ കൂടുതല്‍;#1 പൊരുത്തങ്ങളില്‍ കൂടുതല്‍"

#~ msgid "%S match"
#~ msgstr "%S ചേര്‍ച്ച"

#~ msgid "%S matches"
#~ msgstr "%S ചേര്‍ച്ചകള്‍"

#~ msgid "More than %S matches"
#~ msgstr "%S-ല്‍ കൂടുതല്‍ ചേര്‍ച്ചകള്‍"

