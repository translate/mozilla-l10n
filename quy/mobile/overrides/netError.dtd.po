#. %brandDTD;
#: loadError.label
msgid "Problem loading page"
msgstr "Problemas al cargar la página"

#: retry.label
msgid "Try Again"
msgstr "Reintentar"

#. Specific error messages
#: connectionFailure.title
msgid "Unable to connect"
msgstr "No se puede conectar"

#: connectionFailure.longDesc
#, fuzzy
msgctxt "connectionFailure.longDesc"
msgid "&sharedLongDesc2;"
msgstr "&sharedLongDesc;"

#: deniedPortAccess.title
msgid "This address is restricted"
msgstr "Esta dirección está restringida"

#: deniedPortAccess.longDesc
msgctxt "deniedPortAccess.longDesc"
msgid ""
msgstr ""

#: dnsNotFound.title
msgid "Server not found"
msgstr "No se encontró el servidor"

#: dnsNotFound.longDesc2
msgid ""
"<ul> \n"
"<li>Check the address for typing errors such as \n"
"<strong>ww</strong>.example.com instead of \n"
"<strong>www</strong>.example.com</li> \n"
"<li>If you are unable to load any pages, check your device's data or Wi-Fi "
"connection.</li> \n"
"</ul>"
msgstr ""

#: fileNotFound.title
msgid "File not found"
msgstr "No se encontró el archivo"

#: fileNotFound.longDesc
msgid ""
"<ul> \n"
"<li>Check the file name for capitalization or other typing errors.</li> \n"
"<li>Check to see if the file was moved, renamed or deleted.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Comprueba que el nombre del archivo no contenga errores.</li> \n"
"<li>Verifica si el archivo se borró, movió o renombró.</li> \n"
"</ul>"

#: generic.title
msgid "Oops."
msgstr "¡Chin!."

#: generic.longDesc
msgid "<p>&brandShortName; can't load this page for some reason.</p>"
msgstr "<p>&brandShortName; no puede cargar esta página por algúna razón.</p>"

#: malformedURI.title
msgid "The address isn't valid"
msgstr "La dirección no es válida"

#: malformedURI.longDesc
msgid ""
"<ul> \n"
"<li>Web addresses are usually written like \n"
"<strong>http://www.example.com/</strong></li> \n"
"<li>Make sure that you're using forward slashes (i.e. \n"
"<strong>/</strong>).</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Las direcciones web se escriben usualmente como \n"
"<strong>http://www.example.mx/</strong></li> \n"
"<li>Verifica que estás usando las diagonales correctas (p.e. \n"
"<strong>/</strong>).</li> \n"
"</ul>"

#: netInterrupt.title
msgid "The connection was interrupted"
msgstr "La conexión se interrumpió"

#: netInterrupt.longDesc
#, fuzzy
msgctxt "netInterrupt.longDesc"
msgid "&sharedLongDesc2;"
msgstr "&sharedLongDesc;"

#: notCached.title
msgid "Document Expired"
msgstr "El documento expiró"

#: notCached.longDesc
msgid ""
"<p>The requested document is not available in &brandShortName;'s cache.</"
"p><ul><li>As a security precaution, &brandShortName; does not automatically "
"re-request sensitive documents.</li><li>Click Try Again to re-request the "
"document from the website.</li></ul>"
msgstr ""

#: netOffline.title
msgid "Offline mode"
msgstr "Modo sin conexión"

#: netOffline.longDesc2
msgid ""
"<ul> \n"
"<li>Try again. &brandShortName; will attempt to open a connection and reload "
"the page.</li> \n"
"</ul>"
msgstr ""

#: contentEncodingError.title
msgid "Content Encoding Error"
msgstr "Error de codificación de contenido"

#: contentEncodingError.longDesc
#, fuzzy
msgctxt "contentEncodingError.longDesc"
msgid ""
"<ul> \n"
"<li>Please contact the website owners to inform them of this problem.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Contacta con los propietarios de este Sitio Web para informarles sobre "
"el problema.</li> \n"
"</ul>"

#: unsafeContentType.title
msgid "Unsafe File Type"
msgstr "Tipo de archivo inseguro"

#: unsafeContentType.longDesc
#, fuzzy
msgctxt "unsafeContentType.longDesc"
msgid ""
"<ul> \n"
"<li>Please contact the website owners to inform them of this problem.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Contacta con los propietarios de este Sitio Web para informarles sobre "
"el problema.</li> \n"
"</ul>"

#: netReset.title
msgid "The connection was reset"
msgstr "La conexión se reinició"

#: netReset.longDesc
#, fuzzy
msgctxt "netReset.longDesc"
msgid "&sharedLongDesc2;"
msgstr "&sharedLongDesc;"

#: netTimeout.title
msgid "The connection has timed out"
msgstr "Se agotó el tiempo de espera"

#: netTimeout.longDesc
#, fuzzy
msgctxt "netTimeout.longDesc"
msgid "&sharedLongDesc2;"
msgstr "&sharedLongDesc;"

#: protocolNotFound.title
msgid "The address wasn't understood"
msgstr "No se comprende la dirección"

#: protocolNotFound.longDesc
msgid ""
"<ul> \n"
"<li>You might need to install other software to open this address.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Necesitas instalar otro programa para abrir esta dirección.</li> \n"
"</ul>"

#: proxyConnectFailure.title
msgid "The proxy server is refusing connections"
msgstr "El servidor proxy está rechazando las conexiones"

#: proxyConnectFailure.longDesc
msgid ""
"<ul> \n"
"<li>Check the proxy settings to make sure that they are correct.</li> \n"
"<li>Contact your network administrator to make sure the proxy server is \n"
"working.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Comprueba que la configuración del proxy esté correcta.</li> \n"
"<li>Comunícate con el administrador de la red para comprobar que el servidor "
"proxy está funcionando.</li> \n"
"</ul>"

#: proxyResolveFailure.title
msgid "Unable to find the proxy server"
msgstr "No se puede encontrar el servidor proxy"

#: proxyResolveFailure.longDesc2
msgid ""
"<ul> \n"
"<li>Check the proxy settings to make sure that they are correct.</li> \n"
"<li>Check to make sure your device has a working data or Wi-Fi connection.</"
"li> \n"
"</ul>"
msgstr ""

#: redirectLoop.title
msgid "The page isn't redirecting properly"
msgstr "La página no se está redireccionando apropiadamente"

#: redirectLoop.longDesc
msgid ""
"<ul> \n"
"<li>This problem can sometimes be caused by disabling or refusing to "
"accept \n"
"cookies.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Este problema puede estar pasando por haber deshabilitado \n"
"las cookies.</li> \n"
"</ul>"

#: unknownSocketType.title
msgid "Unexpected response from server"
msgstr "Respuesta inesperada del servidor"

#: unknownSocketType.longDesc
msgid ""
"<ul> \n"
"<li>Check to make sure your system has the Personal Security Manager \n"
"installed.</li> \n"
"<li>This might be due to a non-standard configuration on the server.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Verifica que tu sistema tiene instalado el administrador de seguridad \n"
"personal.</li> \n"
"<li>Esto podría ser ocasionado por una configuración no estándar en el "
"servidor.</li> \n"
"</ul>"

#: nssFailure2.title
msgctxt "nssFailure2.title"
msgid "Secure Connection Failed"
msgstr "Falló la conexión segura"

#: nssFailure2.longDesc
msgid ""
"<ul> \n"
"<li>The page you are trying to view cannot be shown because the authenticity "
"of the received data could not be verified.</li> \n"
"<li>Please contact the website owners to inform them of this problem. "
"Alternatively, use the command found in the help menu to report this broken "
"site.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>No se puede mostrar la página que intentas visitar por que no fue "
"posible verificar la autenticidad de sus datos.</li> \n"
"<li>Contacta a los propietarios del sitio para informarles sobre el "
"problema. Alternativamente, puedes reportar dando clic en el menú Ayuda y "
"luego en la opción Reportar como sitio defectuoso.</li> \n"
"</ul>"

#: nssBadCert.title
msgctxt "nssBadCert.title"
msgid "Secure Connection Failed"
msgstr "Falló la conexión segura"

#: nssBadCert.longDesc2
msgid ""
"<ul> \n"
"<li>This could be a problem with the server's configuration, or it could "
"be \n"
"someone trying to impersonate the server.</li> \n"
"<li>If you have connected to this server successfully in the past, the error "
"may \n"
"be temporary, and you can try again later.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Esto podría ser un problema con la configuración del servidor o tal vez "
"alguien intentando hacerse pasar por éste.</li> \n"
"<li>Si te has conectado a este servidor anteriormente con éxito el error "
"podría ser temporal, intentalo más tarde.</li> \n"
"</ul>"

#: sharedLongDesc2
msgid ""
"<ul> \n"
"<li>The site could be temporarily unavailable or too busy. Try again in a "
"few moments.</li> \n"
"<li>If you are unable to load any pages, check your mobile device's data or "
"Wi-Fi connection.</li> \n"
"</ul>"
msgstr ""

#: cspFrameAncestorBlocked.title
msgid "Blocked by Content Security Policy"
msgstr "Bloqueado por la Política de Seguridad de Contenido"

#: cspFrameAncestorBlocked.longDesc
msgid ""
"<p>&brandShortName; prevented this page from loading in this way because the "
"page has a content security policy that disallows it.</p>"
msgstr ""
"<p>&brandShortName; evitó que se cargara esta página en esta forma por que "
"tiene una política de seguridad de contenido que no lo permite.</p>"

#: corruptedContentError.title
msgid "Corrupted Content Error"
msgstr "Error por contenido dañado"

#: corruptedContentError.longDesc
#, fuzzy
msgid ""
"<p>The page you are trying to view cannot be shown because an error in the "
"data transmission was detected.</p><ul><li>Please contact the website owners "
"to inform them of this problem.</li></ul>"
msgstr ""
"<p>No se puede verificar la autenticidad de los datos recibidos, no se "
"mostrará la página.</p><ul><li>Comuníquese con los responsables del sitio "
"web para informarles sobre este problema.</li></ul>"

#: securityOverride.linkText
msgid "Or you can add an exception…"
msgstr "O puedes añadir una excepción…"

#: securityOverride.getMeOutOfHereButton
msgid "Get me out of here!"
msgstr "¡Sácame de aquí!"

#: securityOverride.exceptionButtonLabel
msgid "Add Exception…"
msgstr "Agregar excepción…"

#. <!-- LOCALIZATION NOTE (securityOverride.warningContent) - Do not translate the
#. contents of the <xul:button> tags.  The only language content is the label= field,
#. which uses strings already defined above. The button is included here (instead of
#. netError.xhtml) because it exposes functionality specific to firefox.
#: securityOverride.warningContent
msgid ""
"<p>You should not add an exception if you are using an internet connection "
"that you do not trust completely or if you are not used to seeing a warning "
"for this server.</p> \n"
" \n"
"<button id='getMeOutOfHereButton'>&securityOverride.getMeOutOfHereButton;</"
"button> \n"
"<button id='exceptionDialogButton'>&securityOverride.exceptionButtonLabel;</"
"button>"
msgstr ""
"<p>No debes agregar una excepción si estás usando una conexión a Internet en "
"la que usted no confía completamente o si no están acostumbrados a ver una "
"advertencia para este servidor.</p> \n"
" \n"
"<button id='getMeOutOfHereButton'>&securityOverride.getMeOutOfHereButton;</"
"button> \n"
"<button id='exceptionDialogButton'>&securityOverride.exceptionButtonLabel;</"
"button>"

#: remoteXUL.title
msgid "Remote XUL"
msgstr "XUL Remoto"

#: remoteXUL.longDesc
msgid ""
"<p><ul><li>Please contact the website owners to inform them of this problem."
"</li></ul></p>"
msgstr ""
"<p><ul><li>Por favor, contacta con los propietarios del sitio web para "
"informarles de este problema.</li></ul></p>"

#~ msgid "Page Load Error"
#~ msgstr "Error al cargar la página"

#~ msgid "Failed to Connect"
#~ msgstr "Error al conectar"

#~ msgid ""
#~ "<p>Though the site seems valid, the browser was unable to establish a "
#~ "connection.</p><ul><li>Could the site be temporarily unavailable? Try "
#~ "again later.</li><li>Are you unable to browse other sites?  Check the "
#~ "computer's network connection.</li><li>Is your computer or network "
#~ "protected by a firewall or proxy? Incorrect settings can interfere with "
#~ "Web browsing.</li></ul>"
#~ msgstr ""
#~ "<p>Aunque este sitio web parece válido, el navegador no pudo conectarse.</"
#~ "p><ul><li>¿Estará temporalmente fuera de servicio este sitio web? De ser "
#~ "así intentalo más tarde.</li><li>¿No puedes ver ningún otro sitio web? "
#~ "Verifica la conexión de red de tu computadora.</li><li>¿Estarán "
#~ "protegidos por un firewall o proxy tu computadora y/o la red a la cual "
#~ "estás conectado? Una mal configuración puede interferir el acceso a "
#~ "Internet.</li></ul>"

#~ msgid "Port Restricted for Security Reasons"
#~ msgstr "Puerto restringido por razones de seguridad"

#~ msgid ""
#~ "<p>The requested address specified a port (e.g. <q>mozilla.org:80</q> for "
#~ "port 80 on mozilla.org) normally used for purposes <em>other</em> than "
#~ "Web browsing. The browser has canceled the request for your protection "
#~ "and security.</p>"
#~ msgstr ""
#~ "<p>La dirección especifica un puerto (p.e. <q>mozilla.org:80</q> para el "
#~ "puerto 80 en mozilla.org) usado normalmente para propositos "
#~ "<em>diferentes</em> a la navegación de páginas Web. Se canceló la "
#~ "petición por su seguridad.</p>"

#~ msgid "Address Not Found"
#~ msgstr "No se encontró la dirección"

#~ msgid ""
#~ "<p>The browser could not find the host server for the provided address.</"
#~ "p><ul><li>Did you make a mistake when typing the domain? (e.g. "
#~ "<q><strong>ww</strong>.mozilla.org</q> instead of <q><strong>www</strong>."
#~ "mozilla.org</q>)</li><li>Are you certain this domain address exists?  Its "
#~ "registration may have expired.</li><li>Are you unable to browse other "
#~ "sites?  Check your network connection and DNS server settings.</li><li>Is "
#~ "your computer or network protected by a firewall or proxy?  Incorrect "
#~ "settings can interfere with Web browsing.</li></ul>"
#~ msgstr ""
#~ "<p>El navegador no encontró el servidor especificado en la dirección "
#~ "ingresada.</p><ul><li>Confirma el formato de tu dirección digitada (p.e. "
#~ "<q><strong>ww</strong>.mozilla-mexico.org</q> en lugar de <q><strong>www</"
#~ "strong>.mozilla-mexico.org</q>)</li><li>¿Estás seguro que existe ese "
#~ "dominio/dirección? Tal vez expiró su registro.</li><li>¿Tienes problemas "
#~ "visitando otros sitios web? Verifica tu conexión a Internet y la "
#~ "configuración del servidor DNS.</li><li>¿Estarán protegidos por un "
#~ "firewall o proxy tu computadora y/o red? Una mala configuración puede "
#~ "interferir con la navegación web.</li></ul>"

#~ msgid "File Not Found"
#~ msgstr "No se encontró el archivo"

#~ msgid ""
#~ "<ul><li>Could the item have been renamed, removed, or relocated?</"
#~ "li><li>Is there a spelling, capitalization, or other typographical error "
#~ "in the address?</li><li>Do you have sufficient access permissions to the "
#~ "requested item?</li></ul>"
#~ msgstr ""
#~ "<ul><li>¿Abra renombrado, borrado o movido este elemento?</li><li>¿Tendrá "
#~ "algún error tipográfico como capitalización o la falta de alguna letra en "
#~ "la dirección?</li><li>¿Tiene los permisos necesarios para acceder a este "
#~ "elemento?</li></ul>"

#~ msgid "Cannot Complete Request"
#~ msgstr "No es posible completar esta petición"

#~ msgid ""
#~ "<p>Additional information about this problem or error is currently "
#~ "unavailable.</p>"
#~ msgstr "<p>No hay información disponible sobre este problema o error.</p>"

#~ msgid "Invalid Address"
#~ msgstr "Dirección inválida"

#~ msgid ""
#~ "<p>The provided address is not in a recognized format. Please check the "
#~ "location bar for mistakes and try again.</p>"
#~ msgstr ""
#~ "<p>La dirección digitada tiene un formato no reconocido. Compruebe su "
#~ "sintaxis e intente nuevamente.</p>"

#~ msgid "Data Transfer Interrupted"
#~ msgstr "Se interrumpió la transferencia de datos"

#~ msgid ""
#~ "<p>The browser connected successfully, but the connection was interrupted "
#~ "while transferring information.  Please try again.</p><ul><li>Are you "
#~ "unable to browse other sites? Check the computer's network connection.</"
#~ "li><li>Still having trouble? Consult your network administrator or "
#~ "Internet provider for assistance.</li></ul>"
#~ msgstr ""
#~ "<p>El navegador se conectó satisfactoriamente pero la conexión fue "
#~ "interrumpida mientras se realizaba la transferencia de datos.  Intente "
#~ "nuevamente.</p><ul><li>¿No puede cargar ningún otro Sitio Web? Compruebe "
#~ "la conexión de red de su computadora.</li><li>¿Los problemas continúan? "
#~ "Consulte con el Administrador de la Red o con su proveedor de Acceso a "
#~ "Internet.</li></ul>"

#~ msgid "Offline Mode"
#~ msgstr "Modo desconectado"

#~ msgid ""
#~ "<p>The page you are trying to view cannot be shown because it uses an "
#~ "invalid or unsupported form of compression.</p><ul><li>Please contact the "
#~ "website owners to inform them of this problem.</li></ul>"
#~ msgstr ""
#~ "<p>La página solicitada no se puede cargar debido a un tipo de compresión "
#~ "no soportado o inválido.</p><ul><li>Informe a los dueños de este Sitio "
#~ "Web sobre este problema.</li></ul>"

#~ msgid "Connection Interrupted"
#~ msgstr "Conexión interrumpida"

#~ msgid ""
#~ "<p>The network link was interrupted while negotiating a connection. "
#~ "Please try again.</p>"
#~ msgstr ""
#~ "<p>El enlace se interrumpió mientras se negociaba la conexión. Intenta "
#~ "nuevamente.</p>"

#~ msgid "Network Timeout"
#~ msgstr "Tiempo de espera superado"

#~ msgid ""
#~ "<p>The requested site did not respond to a connection request and the "
#~ "browser has stopped waiting for a reply.</p><ul><li>Could the server be "
#~ "experiencing high demand or a temporary outage?  Try again later.</"
#~ "li><li>Are you unable to browse other sites? Check the computer's network "
#~ "connection.</li><li>Is your computer or network protected by a firewall "
#~ "or proxy?  Incorrect settings can interfere with Web browsing.</"
#~ "li><li>Still having trouble? Consult your network administrator or "
#~ "Internet provider for assistance.</li></ul>"
#~ msgstr ""
#~ "<p>El Sitio Web solicitado no responde, el navegador detendrá sus "
#~ "intentos.</p><ul><li>¿El servidor estará experimentando sobrecarga de "
#~ "peticiones o estará temporalmente fuera de servicio?  Intente nuevamente "
#~ "más tarde.</li><li>¿No puede ver ningún otro Sitio Web? Verifique la "
#~ "conexión de red de su computadora.</li><li>¿Su computadora y/o la red a "
#~ "la cual está conectada están protegidos por un Cortafuegos (firewall) o "
#~ "proxy? Una configuración errónea puede interferir con la navegación Web.</"
#~ "li></ul>"

#~ msgid "Unknown Protocol"
#~ msgstr "Protocolo desconocido"

#~ msgid ""
#~ "<p>The address specifies a protocol (e.g. <q>wxyz://</q>) the browser "
#~ "does not recognize, so the browser cannot properly connect to the site.</"
#~ "p><ul><li>Are you trying to access multimedia or other non-text services? "
#~ "Check the site for extra requirements.</li><li>Some protocols may require "
#~ "third-party software or plugins before the browser can recognize them.</"
#~ "li></ul>"
#~ msgstr ""
#~ "<p>La dirección especifica un protocolo (p.e. <q>wxyz://</q>) no "
#~ "reconocido por el navegador y, por lo tanto, no puede conectarse "
#~ "apropiadamente con el Sitio Web.</p><ul><li>¿Está intentado acceder a "
#~ "contenido multimedia u otro servicio similar? Compruebe si hay "
#~ "requerimientos extra que deba tener.</li><li>Algunos protocolos pueden "
#~ "necesitar programas o complementos de terceros para su correcto "
#~ "funcionamiento.</li></ul>"

#~ msgid "Proxy Server Refused Connection"
#~ msgstr "Conexión rechazada por el Servidor Proxy"

#~ msgid ""
#~ "<p>The browser is configured to use a proxy server, but the proxy refused "
#~ "a connection.</p><ul><li>Is the browser's proxy configuration correct? "
#~ "Check the settings and try again.</li><li>Does the proxy service allow "
#~ "connections from this network?</li><li>Still having trouble? Consult your "
#~ "network administrator or Internet provider for assistance.</li></ul>"
#~ msgstr ""
#~ "<p>El navegador está configurado para conectarse a un Servidor Proxy pero "
#~ "éste rechazó la conexión.</p><ul><li>¿Habrá algún problema con la "
#~ "configuración del Proxy? Compruebe su configuración e intente nuevamente."
#~ "</li><li>¿El Servidor Proxy permite conexiones a través de esta red?</"
#~ "li><li>¿Continúan los problemas? Consulte con el Administrador de la Red "
#~ "o con su proveedor de acceso a Internet.</li></ul>"

#~ msgid "Proxy Server Not Found"
#~ msgstr "No se encontró el Servidor Proxy"

#~ msgid ""
#~ "<p>The browser is configured to use a proxy server, but the proxy could "
#~ "not be found.</p><ul><li>Is the browser's proxy configuration correct? "
#~ "Check the settings and try again.</li><li>Is the computer connected to an "
#~ "active network?</li><li>Still having trouble? Consult your network "
#~ "administrator or Internet provider for assistance.</li></ul>"
#~ msgstr ""
#~ "<p>El navegador está configurado para conectarse a un Servidor Proxy pero "
#~ "no se encontró.</p><ul><li>¿Habrá alguna omisión en la configuración del "
#~ "Servidor Proxy? Compruebe esta configuración e intente nuevamente.</"
#~ "li><li>¿Su computadora está conectada a una Red en correcto "
#~ "funcionamiento?</li><li>¿Continúan los problemas? Consulte con el "
#~ "Administrador de la Red o con su proveedor de Acceso a Internet.</li></ul>"

#~ msgid "Redirect Loop"
#~ msgstr "Ciclo de redirección"

#~ msgid ""
#~ "<p>The browser has stopped trying to retrieve the requested item. The "
#~ "site is redirecting the request in a way that will never complete.</"
#~ "p><ul><li>Have you disabled or blocked cookies required by this site?</"
#~ "li><li><em>NOTE</em>: If accepting the site's cookies does not resolve "
#~ "the problem, it is likely a server configuration issue and not your "
#~ "computer.</li></ul>"
#~ msgstr ""
#~ "<p>Se han detenido los intentos por conseguir el elemento solicitado por "
#~ "parte del Navegador. El Sitio Web está redireccionando la solicitud en "
#~ "una forma en que nunca se completará.</p><ul><li>¿Habrá bloqueado o "
#~ "desactivado el uso de datos de terceros (cookies) usados por este Sitio "
#~ "Web?</li><li><em>NOTA</em>: Si cambió esta configuración y no se resuelve "
#~ "el problema, lo más probable es que se trate de un problema con el "
#~ "Servidor y no con su computadora.</li></ul>"

#~ msgid "Incorrect Response"
#~ msgstr "Respuesta incorrecta"

#~ msgid ""
#~ "<p>The site responded to the network request in an unexpected way and the "
#~ "browser cannot continue.</p>"
#~ msgstr ""
#~ "<p>El Navegador no puede continuar con la respuesta de este Sitio.</p>"

#~ msgid "Suspected Attack Site!"
#~ msgstr "¡Este Sitio web es peligroso!"

#~ msgid ""
#~ "<p>Attack sites try to install programs that steal private information, "
#~ "use your computer to attack others, or damage your system.</p> \n"
