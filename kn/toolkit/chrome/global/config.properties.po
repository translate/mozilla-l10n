#. # Lock column values
#: default
msgid "default"
msgstr "ಪೂರ್ವನಿಯೋಜಿತ"

#: user
msgid "user set"
msgstr "ಬಳಕೆದಾರರಿಗಾಗಿ ಹೊಂದಿತವಾದ"

#: locked
msgid "locked"
msgstr "ಲಾಕ್ ಮಾಡಲಾದ"

#. # Type column values
#: string
msgid "string"
msgstr "ಪದಸಮುಚ್ಚಯ"

#: int
msgid "integer"
msgstr "ಪೂರ್ಣಾಂಕ"

#: bool
msgid "boolean"
msgstr "ಬೂಲಿಯನ್"

#. # Preference prompts
#. # %S is replaced by one of the type column values above
#: new_title
msgid "New %S value"
msgstr "ಹೊಸ %S ಮೌಲ್ಯ"

#: new_prompt
msgid "Enter the preference name"
msgstr "ಆದ್ಯತೆಯ ಹೆಸರನ್ನು ನಮೂದಿಸಿ"

#: modify_title
msgid "Enter %S value"
msgstr "%S ಮೌಲ್ಯವನ್ನು ನಮೂದಿಸಿ"

#: nan_title
msgid "Invalid value"
msgstr "ಅಮಾನ್ಯವಾದ ಮೌಲ್ಯ"

#: nan_text
msgid "The text you entered is not a number."
msgstr "ನೀವು ನಮೂದಿಸಿದ ಪಠ್ಯವು ಒಂದು ಸಂಖ್ಯೆಯಾಗಿಲ್ಲ."
