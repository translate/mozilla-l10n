msgid "Debug Mode"
msgstr "Atkļūdošanas režīms"

#: debug_mode_off_state_label
msgid "Start Debug Mode"
msgstr "Sākt atkļūdošanas režīmu"

#: debug_mode_on_state_label
msgid "Stop Debug Mode"
msgstr "Apturēt atkļūdošanas režīmu"

