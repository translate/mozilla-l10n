#: quitDialogTitle
msgid "Quit %S"
msgstr "Sortir de %S"

#: quitTitle
msgid "&Quit"
msgstr "&Sortir"

#: cancelTitle
msgid "&Cancel"
msgstr "&Anullar"

#: saveTitle
msgid "&Save and Quit"
msgstr "&Enregistrar e sortir"

#: neverAsk2
msgid "&Do not ask next time"
msgstr "Me &demandar pas lo còp que ven"

#: message
msgid ""
"Do you want %S to save your tabs and windows for the next time it starts?"
msgstr ""
"Volètz que %S enregistre vòstres onglets e fenèstras per l'aviada que ven ?"

#: messageNoWindows
msgid "Do you want %S to save your tabs for the next time it starts?"
msgstr "Volètz que %S enregistre vòstres onglets per l'aviada que ven ?"

#: messagePrivate
msgid ""
"You're in private browsing mode. Quitting %S now will discard all your open "
"tabs and windows."
msgstr ""

#~ msgid "Restart %S"
#~ msgstr "Tornar aviar %S"

#~ msgid "&Restart"
#~ msgstr "&Tornar aviar"

#~ msgid "%S will try to restore your tabs and windows when it restarts."
#~ msgstr "%S restaurarà vòstres onglets e fenèstras quand tornarà aviar."
