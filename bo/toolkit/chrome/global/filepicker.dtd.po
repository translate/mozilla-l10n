#: lookInMenuList.label lookInMenuList.accesskey
msgid "&Look in:"
msgstr "དེའི་ནང་དུ་ལྟ:"

#: textInput.label textInput.accesskey
msgid "File &name:"
msgstr "ཡིག་ཆའི་ཁ་བྱང:"

#: filterMenuList.label filterMenuList.accesskey
msgid "Files of &type:"
msgstr "ཡིག་ཆའི་རིགས:"

#: name.label
msgid "Name"
msgstr "ཁ་བྱང"

#: size.label
msgid "Size"
msgstr "ཆེ་ཆུང"

#: lastModified.label
msgid "Last Modified"
msgstr "ཐེངས་རྗེས་མའི་སྒྱུར་བཅོས"

#: showHiddenFiles.label showHiddenFiles.accesskey
msgid "&Show hidden files and directories"
msgstr "ཡིབ་ཡོད་པའི་ཡིག་ཆ་དང་འཇུག་སྣོད་སྟོན"

#: noPermissionError.label
msgid "You do not have the permissions necessary to view this directory."
msgstr "ཁྱོད་ལ་འཇུག་སྣོད་འདིར་དགོས་པའི་ཆོག་འཆན་མི་འདུག"

#: folderUp.tooltiptext
msgid "Go up a level"
msgstr "རིམ་པ་གཅིག་ཡར་སྐྱོད"

#: folderHome.tooltiptext
msgid "Go to home"
msgstr "གཙོ་ངོས་ཐོག་འགྲོ་བ"

#: folderNew.tooltiptext
msgid "Create new directory"
msgstr "འཇུག་སྣོད་གསར་བ་བཟོ"
