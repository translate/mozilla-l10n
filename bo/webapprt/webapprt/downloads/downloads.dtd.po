msgid "&Clear List"
msgstr "ཐོ་འགོད་གཙང་དག་བཟོ"

#: cmd.clearList.tooltip
msgid "Removes completed, canceled, and failed downloads from the list"
msgstr "ལེན་འཇུག་རྫོགས་ཚར་བ་དང་རྩིས་མེད་གཏོང་བ། ལེན་འཇུག་མ་ཐུབ་པ་རྣམས་ཐོ་འགོད་ནས་བསུབ"

#: cmd.find.commandKey
msgid "f"
msgstr "f"

#: cmd.search.commandKey
msgid "k"
msgstr "k"

#: closeWhenDone.label
msgid "Close when downloads complete"
msgstr "ལེན་འཇུག་ཚར་རྗེས་སྒོ་རྒྱག"

#: closeWhenDone.tooltip
msgid "Closes the Downloads window when all files are done downloading"
msgstr "ཡིག་ཆ་ཡོངས་རྫོགས་ལེན་འཇུག་ཚར་རྗེས་ལེན་སྒེའུ་ཁུང་སྒོ་རྒྱག"

#: showFolder.label
msgid "Show this Folder"
msgstr "ཡིག་སྣོད་འདི་སྟོན"

#: searchBox.label
