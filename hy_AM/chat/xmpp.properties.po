#. # LOCALZIATION NOTE (command.*):
#. #  These are the help messages for each command.
#: command.join3
msgid ""
"%S [&lt;room&gt;[@&lt;server&gt;][/&lt;nick&gt;]] [&lt;password&gt;]: Join a "
"room, optionally providing a different server, or nickname, or the room "
"password."
msgstr ""
"%S [&lt;room&gt;[@&lt;server&gt;][/&lt;nick&gt;]] [&lt;password&gt;]: Միացեք "
"զրույցի՝ այլ սպասարկիչում կամ այլ մականունով ու գաղտնաբառով:"

#: command.part2
msgid "%S [&lt;message&gt;]: Leave the current room with an optional message."
msgstr ""
"%S [&lt;message&gt;]: Leave the current channel with an optional message."

#: command.topic
msgid "%S [&lt;new topic&gt;]: Set this room's topic."
msgstr "%S [&lt;new topic&gt;]: Վերնագրել զրույցը:"

#: command.ban
msgid ""
"%S &lt;nick&gt;[&lt;message&gt;]: Ban someone from the room. You must be a "
"room administrator to do this."
msgstr ""
"%S &lt;nick&gt;[&lt;message&gt;]: Արգելել որևէ մեկին: Դուք պետք է լինեք խմբի "
"կառավարիչը՝ դա անելու համար:"

#: command.kick
msgid ""
"%S &lt;nick&gt;[&lt;message&gt;]: Remove someone from the room. You must be "
"a room moderator to do this."
msgstr ""
"%S &lt;nick&gt;[&lt;message&gt;]: Հեռացնել որևէ մեկին սենյակից: Դուք պետք է "
"լինեք խմբի կառավարիչը՝ դա անելու համար:"

#: command.invite
msgid ""
"%S &lt;jid&gt;[&lt;message&gt;]: Invite a user to join the current room with "
"an optional message."
msgstr ""
"%S &lt;jid&gt;[&lt;message&gt;]: հրավիրեք օգտվողի՝ միանալու ընթացիկ սենյակի "
"զրույցին:"

#: command.me
msgid "%S &lt;action to perform&gt;: Perform an action."
msgstr "%S &lt;action to perform&gt;. ձեր գործողությունը:"

#: command.nick
msgid "%S &lt;new nickname&gt;: Change your nickname."
msgstr "%S &lt;new nickname&gt;: Փոխել մականունը:"

#: command.msg
msgid ""
"%S &lt;nick&gt; &lt;message&gt;: Send a private message to a participant in "
"the room."
msgstr ""
"%S &lt;nick&gt; &lt;message&gt;: Ուղարկել անձնական հաղորդագրություն այս "
"սենյակի մասնակցի:"

