#: sync.title
msgid "Sync"
msgstr "Սինք"

#: sync.notconnected
msgid "Not connected"
msgstr "Կապակցված չէ"

#: sync.connect
msgctxt "sync.connect"
msgid "Connect"
msgstr "Միանալ"

#: sync.connected
msgid "Connected"
msgstr "Կապակցված է"

#: sync.details
msgid "Details"
msgstr "Մանրամասներ"

#: sync.deviceName
msgid "This device"
msgstr "Այս սարքը"

#: sync.disconnect
msgid "Disconnect"
msgstr "Անջատվել"

#: sync.syncNow
msgid "Sync Now"
msgstr "Սինք այժմ"

#: sync.setup.title
msgid "Connect to Sync"
msgstr "Միանալ Սինքին"

#: sync.setup.pair
msgid "To activate, select \"Pair a Device\" on your other device."
msgstr "Ակտիվացնելու համար ընտրեք \"Կապակցել սարքի\" Ձեր այլ սարքում։"

#: sync.fallback
msgid "I'm not near my computer…"
msgstr "Ես իմ համակարգչի կողքին չեմ..."

#: sync.setup.manual
msgid "Enter your Sync account information"
msgstr "Նշեք Ձեր Սինքի հաշվի ինֆոն"

#: sync.account
msgid "Account Name"
msgstr "Փոստարկղի անունը"

#: sync.password
msgid "Password"
msgstr "Գաղտնաբառը"

#: sync.recoveryKey
msgid "Recovery Key"
msgstr "Վերականգնող բանալին"

#: sync.customServer
msgid "Use custom server"
msgstr "Օգտ. ընտրված սպասարկիչ"

#: sync.serverURL
msgid "Server URL"
msgstr "Սպասարկիչի URL-ն"

#: sync.setup.connect
msgctxt "sync.setup.connect"
msgid "Connect"
msgstr "Միանալ"

#: sync.setup.cancel
msgid "Cancel"
msgstr "Չեղարկել"

#: sync.setup.tutorial
msgid "Show me how"
msgstr "Ցուցադրել, ինչպես"

#: sync.setup.waiting2
msgid "Waiting for other device…"
msgstr "Սպասում է այլ սարքի..."

#: sync.pair.title
msgid "Pair a Device"
msgstr "Կցված սարք"

#: sync.pair.description
msgid "To activate your new device, select \"Set Up Sync\" on the device."
msgstr "Ակտիվացնելու համար նոր սարքը, ընտրեք \"Կարգ. Սինքը\" Ձեր սարքում։"

#: sync.setup.close
msgid "Close"
msgstr "Փակել"

#: sync.setup.waitingdownload
msgid ""
"Your data is now being downloaded in the background. You can close this "
"window at any time."
msgstr "Տվյալները այժմ կբեռնվեն խորապատկերում։ Կարող եք փակել այս պատուհանը։"
