#: next.tooltip
msgid "Find the next occurrence of the phrase"
msgstr "Գտնված հաջորդ արդյունքը"

#: previous.tooltip
msgid "Find the previous occurrence of the phrase"
msgstr "Գտնված նախորդ արդյունքը"

#: findCloseButton.tooltip
msgid "Close find bar"
msgstr "Փակել փնտրավահանակը"

#: highlightAll.label highlightAll.accesskey
msgid "High&light All"
msgstr "Ընդգծել բոլորը"

#: highlightAll.tooltiptext
msgid "Highlight all occurrences of the phrase"
msgstr "Գունանշել գտնված բոլոր արդյունքները"

#: caseSensitive.label caseSensitive.accesskey
msgid "Mat&ch Case"
msgstr "Հաշ&վի առնել Մեծ(փոքր)ատառը"

#: caseSensitive.tooltiptext
msgid "Search with case sensitivity"
msgstr "Որոնել ըստ մեծ(փոքր)ատառի"

#: entireWord.label entireWord.accesskey
msgid "&Whole Words"
msgstr "Լրիվ բառերը"

#: entireWord.tooltiptext
msgid "Search whole words only"
msgstr "Միայն փնտրել լրիվ բառերը"

#~ msgid "Match &Case"
