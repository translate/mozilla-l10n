#: toggleToolbox.key
msgid "I"
msgstr "A"

#: toggleToolboxF12.keycode
msgid "VK_F12"
msgstr "VK_F12"

#: toggleToolboxF12.keytext
msgid "F12"
msgstr "F12"

#: toolboxCloseButton.tooltip
msgid "Close Developer Tools"
msgstr "Fechar ferramentas de desenvolvimento"

#: toolboxOptionsButton.key
msgid "O"
msgstr "h"

#: toolboxNextTool.key
msgid "]"
msgstr "]"

#: toolboxPreviousTool.key
msgid "["
msgstr "["

#: toolboxZoomIn.key
msgid "+"
msgstr "+"

