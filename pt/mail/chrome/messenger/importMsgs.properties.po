#. #Error strings
#: ImportAlreadyInProgress
msgid ""
"An import operation is currently in progress. Try again when the current "
"import has finished."
msgstr ""
"Já está a decorrer uma operação de importação. Após finalizar a operação "
"atual, tente novamente."

#. #Error strings for settings import
#: ImportSettingsBadModule
msgid "Unable to load settings module"
msgstr "Não foi possível carregar o módulo de definições."

#: ImportSettingsNotFound
msgid ""
"Unable to find settings.  Check to make sure the application is installed on "
"this machine."
msgstr ""
"Não foi possível localizar o módulo de definições. Certifique-se que a "
"aplicação está instalada."

#: ImportSettingsFailed
msgid ""
"An error occurred while importing settings.  Some, or all, of the settings "
"may not have been imported."
msgstr ""
"Ocorreu um erro ao importar as definições. É possível que algumas definições "
"não tenham sido importadas."

#. # LOCALIZATION NOTE : Do not translate the word "%S" below.
#: ImportSettingsSuccess
msgid "Settings were imported from %S"
msgstr "As definições foram importadas do %S."

#. #Error string for mail import
#: ImportMailBadModule
msgid "Unable to load mail import module"
msgstr "Não foi possível carregar o módulo de importação de correio."

#: ImportMailNotFound
msgid ""
"Unable to find mail to import.  Check to make sure the mail application is "
"correctly installed on this machine."
msgstr ""
"Não foi possível localizar correio para importar. Certifique-se que a "
"aplicação está instalada."

#: ImportEmptyAddressBook
msgid "Can't import empty address book %S."
msgstr "Não foi possível importar o livro de endereços %S."

#. # LOCALIZATION NOTE: Do not translate the word "%S" below.
#: ImportMailFailed
msgid "An error occurred importing mail from %S"
msgstr "Ocorreu um erro ao importar o correio de %S."

#. # LOCALIZATION NOTE: Do not translate the word "%S" below.
#: ImportMailSuccess
msgid "Mail was successfully imported from %S"
msgstr "O correio de %S foi importado com sucesso."

#. # Error string for address import
#: ImportAddressBadModule
msgid "Unable to load address book import module."
msgstr ""
"Não foi possível carregar o módulo do livro de endereços para importar."

#: ImportAddressNotFound
msgid ""
"Unable to find any address books to import.  Check to make sure the selected "
"application or format is correctly installed on this machine."
msgstr ""
"Não foi possível encontrar qualquer livro de endereços para importar. "
"Verifique se a aplicação selecionada ou o formato estão instalados no seu "
"computador."

#. # LOCALIZATION NOTE : Do not translate the word "%S" below.
#: ImportAddressFailed
msgid "An error occurred importing addresses from %S."
msgstr "Ocorreu um erro ao importar os endereços de %S."

#. # LOCALIZATION NOTE : Do not translate the word "%S" below.
#: ImportAddressSuccess
msgid "Addresses successfully imported from %S."
msgstr "Endereços de %S importados com sucesso."

#. # Error string for filters import
#: ImportFiltersBadModule
msgid "Unable to load filters import module."
msgstr "Não foi possível carregar o módulo de importação dos filtros."

#. # LOCALIZATION NOTE : The %S will get replaced by the name of the import module.
#: ImportFiltersFailed
msgid "An error occurred importing filters from %S."
msgstr "Ocorreu um erro ao importar os filtros de %S."

#. # LOCALIZATION NOTE : The %S will get replaced by the name of the import module.
#: ImportFiltersSuccess
msgid "Filters successfully imported from %S."
msgstr "Filtros importados com sucesso a partir de %S."

#. # LOCALIZATION NOTE : The %S will get replaced by the name of the import module.
#: ImportFiltersPartial
msgid "Filters partially imported from %S.  Warnings below:"
msgstr "Filtros parcialmente importados a partir de %S. Avisos abaixo:"

