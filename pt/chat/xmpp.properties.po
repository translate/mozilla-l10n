#. # LOCALZIATION NOTE (command.*):
#. #  These are the help messages for each command.
#: command.join3
msgid ""
"%S [&lt;room&gt;[@&lt;server&gt;][/&lt;nick&gt;]] [&lt;password&gt;]: Join a "
"room, optionally providing a different server, or nickname, or the room "
"password."
msgstr ""
"%S [&lt;sala&gt;[@&lt;servidor&gt;][/&lt;nick&gt;]] [&lt;palavra-passe&gt;]: "
"entrar numa sala, indicando uma sala, servidor, nick ou palavra passe "
"diferente."

#: command.part2
msgid "%S [&lt;message&gt;]: Leave the current room with an optional message."
msgstr "%S [&lt;mensagem&gt;]: Sair da sala com uma mensagem opcional."

#: command.topic
msgid "%S [&lt;new topic&gt;]: Set this room's topic."
msgstr "%S [&lt;novo tópico&gt;]: Definir o tópico da sala."

#: command.ban
msgid ""
"%S &lt;nick&gt;[&lt;message&gt;]: Ban someone from the room. You must be a "
"room administrator to do this."
msgstr ""
"%S &lt;nick&gt;[&lt;mensagem&gt;]: bane alguém da sala. Você tem de ter "
"privilégios de administrador para o poder fazer."

#: command.kick
msgid ""
"%S &lt;nick&gt;[&lt;message&gt;]: Remove someone from the room. You must be "
"a room moderator to do this."
msgstr ""
"%S &lt;nick&gt;[&lt;mensagem&gt;]: remove alguém da sala. Você tem de ter "
"privilégios de moderador para o poder fazer."

#: command.invite
msgid ""
"%S &lt;jid&gt;[&lt;message&gt;]: Invite a user to join the current room with "
"an optional message."
msgstr ""
"%S &lt;jid&gt;[&lt;mensagem&gt;]: convida um utilizador a entrar na sala, "
"com uma mensagem adicional."

#: command.me
msgid "%S &lt;action to perform&gt;: Perform an action."
msgstr "%S &lt;ação a executar&gt;: executa uma ação."

#: command.nick
msgid "%S &lt;new nickname&gt;: Change your nickname."
msgstr "%S &lt;novo nickname&gt;: muda o seu nick."

#: command.msg
msgid ""
"%S &lt;nick&gt; &lt;message&gt;: Send a private message to a participant in "
"the room."
msgstr ""
"%S &lt;nick&gt; &lt;mensagem&gt;: envia uma mensagem privada para um "
"participante da sala."

#~ msgid "Unknown error"
#~ msgstr "Erro desconhecido"
