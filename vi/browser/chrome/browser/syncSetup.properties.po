msgstr "Xong"

#: button.syncOptionsCancel.label
msgid "Cancel"
msgstr "Hủy bỏ"

#: invalidEmail.label
msgid "Invalid email address"
msgstr "Địa chỉ email bất hợp lệ"

#: serverInvalid.label
msgid "Please enter a valid server URL"
msgstr "Vui lòng điền một URL máy chủ hợp lệ"

