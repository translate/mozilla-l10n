#: playButton.playLabel
msgid "Play"
msgstr "Chơi"

#: playButton.pauseLabel
msgid "Pause"
msgstr "Tạm dừng"

#: muteButton.muteLabel
msgid "Mute"
msgstr "Tắt tiếng"

#: muteButton.unmuteLabel
msgid "Unmute"
msgstr "Bật tiếng"

#: fullscreenButton.enterfullscreenlabel
msgid "Full Screen"
msgstr "Toàn màn hình"

#: fullscreenButton.exitfullscreenlabel
msgid "Exit Full Screen"
msgstr "Thoát chế độ Toàn màn hình"

#: castingButton.castingLabel
msgid "Cast to Screen"
msgstr "Chiếu ra màn hình"

#: stats.media
msgid "Media"
msgstr "Đa phương tiện"

#. Do not translate this.  Only change the numeric values if you need this dialogue box to appear bigger
#: stats.size
msgid "Size"
msgstr "Dung lượng"

#: stats.activity
msgid "Activity"
msgstr "Hoạt động"

#: stats.activityPaused
msgid "Paused"
msgstr "Tạm dừng"

#: stats.activityPlaying
msgid "Playing"
msgstr "Đang chơi"

#: stats.activityEnded
msgid "Ended"
msgstr "Đã kết thúc"

#: stats.activitySeeking
msgid "(seeking)"
msgstr "(đang dò)"

#: stats.volume
msgid "Volume"
msgstr "Âm lượng"

#: stats.framesParsed
msgid "Frames parsed"
msgstr "Frames parsed (frame đã được phân tách)"

#: stats.framesDecoded
msgid "Frames decoded"
msgstr "Frames decoded (frame đã được giải mã)"

#: stats.framesPresented
msgid "Frames presented"
msgstr "Frames presented (frame được trình chiếu)"

#: stats.framesPainted
msgid "Frames painted"
msgstr "Frames painted (frame được vẽ)"

#: error.aborted
msgid "Video loading stopped."
msgstr "Đã dừng nạp video."

#: error.network
msgid "Video playback aborted due to a network error."
msgstr "Không thể xem video vì lỗi kết nối."

#: error.decode
msgid "Video can't be played because the file is corrupt."
msgstr "Không thể xem video vì file bị hỏng."

#: error.srcNotSupported
msgid "Video format or MIME type is not supported."
msgstr "Định dạng video hoặc loại file không được hỗ trợ."

#: error.noSource2
msgid "No video with supported format and MIME type found."
msgstr "Không có video với định đạng được hỗ trợ."

#: error.generic
msgid "Video playback aborted due to an unknown error."
msgstr "Không thể xem video vì một lỗi chưa biết."

#. the #1 string is the current
#. media position, and the #2 string is the total duration. For example, when at
#. the 5 minute mark in a 6 hour long video, #1 would be "5:00" and #2 would be
#. "6:00:00", result string would be "5:00 of 6:00:00 elapsed".
#: scrubberScale.nameFormat
msgid "#1 of #2 elapsed"
msgstr "#1 trên tổng #2 đã qua"

