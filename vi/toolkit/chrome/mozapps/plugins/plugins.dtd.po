#: report.please
msgid "Send crash report"
msgstr "Gửi báo cáo lỗi"

#: report.submitting
msgid "Sending report…"
msgstr "Đang gửi báo cáo…"

#: report.submitted
msgid "Crash report sent."
msgstr "Báo cáo lỗi đã gửi."

#: report.disabled
msgid "Crash reporting disabled."
msgstr "Việc báo cáo lỗi bị vô hiệu hóa."

#: report.failed
msgid "Submission failed."
msgstr "Việc gửi lên thất bại."

#: report.unavailable
msgid "No report available."
msgstr "Hiện không có báo cáo nào."

#: report.comment
msgid "Add a comment (comments are publicly visible)"
msgstr "Thêm bình luận (bình luận hiển thị công khai)"

#: report.pageURL
msgid "Include the page's URL"
msgstr "Chèn URL của trang"

