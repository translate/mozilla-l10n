#. # Tokenizer errors
#: errGarbageAfterLtSlash
msgid "Garbage after “</”."
msgstr "Có ký tự rác sau “</”."

#: errLtSlashGt
msgid ""
"Saw “</>”. Probable causes: Unescaped “<” (escape as “&lt;”) or mistyped end "
"tag."
msgstr ""
"Thấy “</>”. Có thể do: unescaped “<” (escape ở dạng “&lt;”) hoặc viết sai "
"thẻ kết thúc."

#: errCharRefLacksSemicolon
msgid "Character reference was not terminated by a semicolon."
msgstr "Character reference không được kết thúc bởi dấu chấm phẩy."

#: errNoDigitsInNCR
msgid "No digits in numeric character reference."
msgstr "Không  có chữ số nào trong tham chiếu ký tự."

#: errGtInSystemId
msgid "“>” in system identifier."
msgstr "thuộc về định danh hệ thống."

#: errGtInPublicId
msgid "“>” in public identifier."
msgstr "\">\" thuộc về định danh chung. "

#: errNamelessDoctype
msgid "Nameless doctype."
msgstr "Doctype không tên."

#: errConsecutiveHyphens
msgid ""
"Consecutive hyphens did not terminate a comment. “--” is not permitted "
"inside a comment, but e.g. “- -” is."
msgstr ""
"Các dấu gạch ngang liên tiếp không kết thúc một ghi chú. “--” không được "
"phép chứa bên trong một ghi chú, nhưng e.g. “- -” được phép."

#: errPrematureEndOfComment
msgid "Premature end of comment. Use “-->” to end a comment properly."
msgstr ""
"Ghi chú chưa được kết thúc. Sử dụng \"-->\" để kết thúc đúng một ghi chú."

#: errBogusComment
msgid "Bogus comment."
msgstr "Ghi chú giả."

#: errUnquotedAttributeLt
msgid ""
"“<” in an unquoted attribute value. Probable cause: Missing “>” immediately "
"before."
msgstr ""
"\"<\" là một giá trị chưa được quoted; có thể nguyên nhân là do thiếu \">\" "
"ngay trước đó."

#: errUnquotedAttributeGrave
msgid ""
"“`” in an unquoted attribute value. Probable cause: Using the wrong "
"character as a quote."
msgstr ""
"\"`\" là một giá trị chưa được quoted; có thể nguyên nhân là do sử dụng sai "
"ký tự dùng để quote."

#: errUnquotedAttributeQuote
msgid ""
"Quote in an unquoted attribute value. Probable causes: Attributes running "
"together or a URL query string in an unquoted attribute value."
msgstr ""
"Dấu nháy (quote) xuất hiện trong thuộc tính unquoted. Nguyên nhân có thể: "
"Hai hay nhiều thuộc tính được truyền cùng nhau hoặc một chuỗi truy vấn URL "
"(URL query string) xuất hiện trong giá trị của một thuộc tính unquote."

#: errUnquotedAttributeEquals
msgid ""
"“=” in an unquoted attribute value. Probable causes: Attributes running "
"together or a URL query string in an unquoted attribute value."
msgstr ""
"“=Ký tự \" xuất hiện trong thuộc tính unquoted. Nguyên nhân có thể: Hai hay "
"nhiều thuộc tính được truyền cùng nhau hoặc một chuỗi truy vấn URL (URL "
"query string) xuất hiện trong giá trị của một thuộc tính unquote."

#: errSlashNotFollowedByGt
msgid "A slash was not immediately followed by “>”."
msgstr "A slash was not immediate followed by “>”."

#: errNoSpaceBetweenAttributes
msgid "No space between attributes."
msgstr "Không có khoảng trắng giữa các thuộc tính."

#: errUnquotedAttributeStartLt
msgid ""
"“<” at the start of an unquoted attribute value. Probable cause: Missing “>” "
"immediately before."
msgstr ""
"“<” at the start of an unquoted attribute value. Probable cause: Missing “>” "
"immediately before"

#: errUnquotedAttributeStartGrave
msgid ""
"“`” at the start of an unquoted attribute value. Probable cause: Using the "
"wrong character as a quote."
msgstr ""
"Ký tự \"`\" bắt đầu giá trị một thuộc tính unquoted. Nguyên nhân có thể: Sử "
"dụng sai ký tự dùng để quote."

#: errUnquotedAttributeStartEquals
msgid ""
"“=” at the start of an unquoted attribute value. Probable cause: Stray "
"duplicate equals sign."
msgstr ""
"“=Dấu \" xuất hiện ở vị trí bắt đầu của một giá trị thuộc tính unquoted. "
"Nguyên nhân có thể: Thiếu mất cặp dấu =."

#: errAttributeValueMissing
msgid "Attribute value missing."
msgstr "Giá trị thuộc tính bị thiếu."

#: errBadCharBeforeAttributeNameLt
msgid ""
"Saw “<” when expecting an attribute name. Probable cause: Missing “>” "
"immediately before."
msgstr ""
"Dấu \"<\" xuất hiện thay vì tên thuộc tính. Nguyên nhân có thể: Thiếu mất "
"dấu \">\" ở phía trước."

#: errEqualsSignBeforeAttributeName
msgid ""
"Saw “=” when expecting an attribute name. Probable cause: Attribute name "
"missing."
msgstr ""
"Dấu \"=\" xuất hiện thay vì tên thuộc tính. Nguyên nhân có thể: Thuộc tính "
"bị thiếu tên."

#: errBadCharAfterLt
msgid ""
"Bad character after “<”. Probable cause: Unescaped “<”. Try escaping it as "
"“&lt;”."
msgstr ""
"Ký tự sai sau “<”. Có thể do: Không thoát “<”. Hãy thử kết thúc với “&lt;”."

#: errLtGt
msgid ""
"Saw “<>”. Probable causes: Unescaped “<” (escape as “&lt;”) or mistyped "
"start tag."
msgstr ""
"Thấy “<>”. Có thể do: unescap “<” (escape ở dạng “&lt;”) hoặc viết sai thẻ "
"bắt đầu."

#: errProcessingInstruction
msgid ""
"Saw “<?”. Probable cause: Attempt to use an XML processing instruction in "
"HTML. (XML processing instructions are not supported in HTML.)"
msgstr ""
"Xuất hiện \"<?\". Nguyên nhân có thể: Chỉ dẫn xử lý XML (XML processing "
"instruction) được đặt trong HTML. (Trong HTML, chỉ dẫn xử lý XML không được "
"hỗ trợ.)"

#: errUnescapedAmpersandInterpretedAsCharacterReference
msgid ""
"The string following “&” was interpreted as a character reference. (“&” "
"probably should have been escaped as “&amp;”.)"
msgstr ""
"Chuỗi sau “&” được hiểu là một ký tự tham chiếu. (“&” nên được escape là "
"“&amp;”.)"

#: errNotSemicolonTerminated
msgid ""
"Named character reference was not terminated by a semicolon. (Or “&” should "
"have been escaped as “&amp;”.)"
msgstr ""
"Ký tự đặc biệt được định nghĩa không kết thúc với một dấu chấm phẩy. (Hoặc "
"\"&\" nên được định nghĩa là “&amp;”.)"

#: errNoNamedCharacterMatch
msgid ""
"“&” did not start a character reference. (“&” probably should have been "
"escaped as “&amp;”.)"
msgstr ""
"“&” không bắt đâu một ký tự tham chiếu. (“&” nên được escape là “&amp;”.)"

#: errQuoteBeforeAttributeName
msgid ""
"Saw a quote when expecting an attribute name. Probable cause: “=” missing "
"immediately before."
msgstr ""
"Dấu nháy (quote) thay vì tên thuộc tính. Nguyên nhân có thể: Thiếu mất dấu "
"\"=\" ở phía trước."

#: errLtInAttributeName
msgid "“<” in attribute name. Probable cause: “>” missing immediately before."
msgstr ""
"Dấu \"<\" nằm trong tên thuộc tính. Nguyên nhân có thể: Thiếu mất đấu \"=\" "
"ở phía trước."

#: errQuoteInAttributeName
msgid ""
"Quote in attribute name. Probable cause: Matching quote missing somewhere "
"earlier."
msgstr ""
"Xuất hiện dấu nháy (quote) trong tên thuộc tính. Nguyên nhân có thể: Ở phía "
"trước số lượng dấu nháy không tạo thành đúng cặp."

#: errExpectedPublicId
msgid "Expected a public identifier but the doctype ended."
msgstr ""
"Doctype kết thúc tại vị trí lẽ ra thuộc về một public identifier (định danh "
"công cộng)."

#: errBogusDoctype
msgid "Bogus doctype."
msgstr "Doctype không tồn tại."

#: maybeErrAttributesOnEndTag
msgid "End tag had attributes."
msgstr "Thẻ kết thúc lại có thuộc tính."

#: maybeErrSlashInEndTag
msgid "Stray “/” at the end of an end tag."
msgstr "Lạc mất dấu \"/\" ở kết thúc ở một thẻ kết thúc."

#: errNcrNonCharacter
msgid "Character reference expands to a non-character."
msgstr ""
"Character reference (ký tự tham chiếu) sẽ được biến đổi thành một non-"
"character."

#: errNcrSurrogate
msgid "Character reference expands to a surrogate."
msgstr ""
"Character reference (ký tự tham chiếu) sẽ được biến đổi thành một surrogate "
"(đại diện)."

#: errNcrControlChar
msgid "Character reference expands to a control character."
msgstr ""
"Character reference (ký tự tham chiếu) sẽ được biến đổi thành một control "
"character (ký tự điều khiển)."

#: errNcrCr
msgid "A numeric character reference expanded to carriage return."
msgstr ""
"Một numeric character reference (ký tự tham chiếu số) được biến đổi thành "
"dấu xuống dòng."

#: errNcrInC1Range
msgid "A numeric character reference expanded to the C1 controls range."
msgstr ""
"Một numeric character reference (ký tự tham chiếu số) được biến đổi thành C1 "
"controls range (miền điều khiển C1)."

#: errEofInPublicId
msgid "End of file inside public identifier."
msgstr "Kết thúc tệp bên trong public identifier (định dang công cộng)."

#: errEofInComment
msgid "End of file inside comment."
msgstr "Kết thúc tệp bên trong bình luận."

#: errEofInDoctype
msgid "End of file inside doctype."
msgstr "Kết thúc tệp bên trong doctype."

#: errEofInAttributeValue
msgid "End of file reached when inside an attribute value. Ignoring tag."
msgstr ""
"Kết thúc tập tin khi đang bên trong một giá trị thuộc tính. Loại bỏ thẻ."

#: errEofInAttributeName
msgid "End of file occurred in an attribute name. Ignoring tag."
msgstr "Kết thúc tập tin xảy ra bên trong một tên thuộc tính. Loại bỏ thẻ."

#: errEofWithoutGt
msgid "Saw end of file without the previous tag ending with “>”. Ignoring tag."
msgstr ""
"Kết thúc tập tin trong khi thẻ trước đó không được kết thúc bởi \">\". Loại "
"bỏ thẻ."

#: errEofInTagName
msgid "End of file seen when looking for tag name. Ignoring tag."
msgstr "Thấy dấu hiệu kết thúc tệp khi tìm kiếm tên thẻ. Bỏ qua thẻ."

#: errEofInEndTag
msgid "End of file inside end tag. Ignoring tag."
msgstr "Kết thúc tệp bên trong thẻ đóng. Bỏ qua thẻ."

#: errEofAfterLt
msgid "End of file after “<”."
msgstr "Kết thúc tệp sau ký tự \"<\"."

#: errNcrOutOfRange
msgid "Character reference outside the permissible Unicode range."
msgstr ""
"Character reference (tham chiếu ký tự) nằm ngoài khoảng Unicode cho phép."

#: errNcrUnassigned
msgid "Character reference expands to a permanently unassigned code point."
msgstr ""
"Character reference (tham chiếu ký tự) biến đổi thành một permanent "
"unassigned code point."

#: errDuplicateAttribute
msgid "Duplicate attribute."
msgstr "Trùng lặp thuộc tính."

#: errEofInSystemId
msgid "End of file inside system identifier."
msgstr "Kết thúc tệp bên trong system identifier (định dang hệ thống)."

#: errExpectedSystemId
msgid "Expected a system identifier but the doctype ended."
msgstr ""
"Thay vì một system identifier (định dang hệ thống) thì doctype lại kết thúc."

#: errMissingSpaceBeforeDoctypeName
msgid "Missing space before doctype name."
msgstr "Thiếu khoảng trắng trước tên doctype."

#: errHyphenHyphenBang
msgid "“--!” found in comment."
msgstr "“- - ! ” xuất hiện trong ghi chú."

#: errNcrZero
msgid "Character reference expands to zero."
msgstr "Character reference (tham chiếu ký tự) biến đổi số 0."

#: errNoSpaceBetweenDoctypeSystemKeywordAndQuote
msgid "No space between the doctype “SYSTEM” keyword and the quote."
msgstr "Không có khoảng trắng giữa từ khóa doctype \"SYSTEM\" và dấu nháy."

#: errNoSpaceBetweenPublicAndSystemIds
msgid "No space between the doctype public and system identifiers."
msgstr ""
"Không có khoảng trắng giữa doctype public và system identifier (định dang hệ "
"thống)."

#: errNoSpaceBetweenDoctypePublicKeywordAndQuote
msgid "No space between the doctype “PUBLIC” keyword and the quote."
msgstr "Không có khoảng trắng giữa từ khóa doctype \"PUBLIC\" và dấu nháy."

#. # Tree builder errors
#: errStrayStartTag2
msgid "Stray start tag “%1$S”."
msgstr "Lạc mất thẻ bắt đầu \"%1$S\"."

#: errStrayEndTag
msgid "Stray end tag “%1$S”."
msgstr "Lạc mất thẻ kết thúc \"%1$S\"."

#: errUnclosedElements
msgid "End tag “%1$S” seen, but there were open elements."
msgstr "Tìm thấy thẻ kết thúc \"%1$S\" nhưng vẫn còn thành phần đang mở."

#: errUnclosedElementsImplied
msgid "End tag “%1$S” implied, but there were open elements."
msgstr ""
"Thẻ kết thúc \"%1$S\" được hiểu mặc định nhưng vẫn còn thành phần đang mở."

#: errUnclosedElementsCell
msgid "A table cell was implicitly closed, but there were open elements."
msgstr ""
"Ô của bảng được ngầm định hiểu là kết thúc nhưng vẫn còn thành phần đang mở."

#: errStrayDoctype
msgid "Stray doctype."
msgstr "Lạc mất doctype."

#: errAlmostStandardsDoctype
msgid "Almost standards mode doctype. Expected “<!DOCTYPE html>”."
msgstr "Doctype gần đặt chuẩn. Lẽ ra nên là “<!DOCTYPE html>”."

#: errQuirkyDoctype
msgid "Quirky doctype. Expected “<!DOCTYPE html>”."
msgstr "Doctype què quặt. Lẽ ra nên là “<!DOCTYPE html>”."

#: errNonSpaceInTrailer
msgid "Non-space character in page trailer."
msgstr "Phát hiện ký tự không phải là khoảng trắng tại cuối trang."

#: errNonSpaceAfterFrameset
msgid "Non-space after “frameset”."
msgstr "Phát hiện ký tự không phải là khoảng trắng sau \"frameset\"."

#: errNonSpaceInFrameset
msgid "Non-space in “frameset”."
msgstr "Phát hiện ký tự không phải là khoảng trắng trong \"frameset\"."

#: errNonSpaceAfterBody
msgid "Non-space character after body."
msgstr "Phát hiện ký tự không phải là khoảng trắng sau body."

#: errNonSpaceInColgroupInFragment
msgid "Non-space in “colgroup” when parsing fragment."
msgstr ""
"Phát hiện ký tự không phải là khoảng trắng trong \"colgroup\" khi parse "
"fragment."

#: errNonSpaceInNoscriptInHead
msgid "Non-space character inside “noscript” inside “head”."
msgstr ""
"Phát hiện ký tự không phải là khoảng trắng trong \"noscript\" trong \"head\"."

#: errFooBetweenHeadAndBody
msgid "“%1$S” element between “head” and “body”."
msgstr "Phát hiện phần tử “%1$S” giữa “head” và “body”."

#: errStartTagWithoutDoctype
msgid ""
"Start tag seen without seeing a doctype first. Expected “<!DOCTYPE html>”."
msgstr ""
"Phát hiện bắt đầu một thẻ thay vì doctype. Lẽ ra nên là “<!DOCTYPE html>”."

#: errNoSelectInTableScope
msgid "No “select” in table scope."
msgstr "Không có \"select\" trong phạm vi bảng."

#: errStartSelectWhereEndSelectExpected
msgid "“select” start tag where end tag expected."
msgstr "Phát hiện bắt đầu thẻ \"select\" tại vị trí lẽ ra là kết thúc một thẻ."

#: errStartTagWithSelectOpen
msgid "“%1$S” start tag with “select” open."
msgstr "“%1$S” bắt đầu thẻ với “select” mở."

#: errBadStartTagInHead2
msgid "Bad start tag “%1$S” in “head”."
msgstr "Thẻ mở đầu \"%1$S\" ở \"head\" không tốt."

#: errImage
msgid "Saw a start tag “image”."
msgstr "Đã có thẻ mở đầu \"image\"."

#: errIsindex
msgid "“isindex” seen."
msgstr "thấy \"isindex\"."

#: errFooSeenWhenFooOpen
msgid ""
"An “%1$S” start tag seen but an element of the same type was already open."
msgstr "Thẻ \"%1$S\" cùng loại đang mở."

#: errHeadingWhenHeadingOpen
msgid "Heading cannot be a child of another heading."
msgstr "Heading không thể là con của một heading khác."

#: errFramesetStart
msgid "“frameset” start tag seen."
msgstr "Tìm thấy thẻ mở đầu \"frameset\"."

#: errNoCellToClose
msgid "No cell to close."
msgstr "Không còn ô để đóng."

#: errStartTagInTable
msgid "Start tag “%1$S” seen in “table”."
msgstr "Thẻ mở đầu \"%1$S” được tìm thấy trong “table”."

#: errFormWhenFormOpen
msgid ""
"Saw a “form” start tag, but there was already an active “form” element. "
"Nested forms are not allowed. Ignoring the tag."
msgstr ""
"Tìm thấy một thẻ mở đầu form, nhưng đã có một thẻ form khác đang được sử "
"dụng. Việc lồng form là không được phép. Thẻ được loại bỏ."

#: errTableSeenWhileTableOpen
msgid "Start tag for “table” seen but the previous “table” is still open."
msgstr ""
"Thẻ mở đầu \"table\" được tìm thấy nhưng thẻ \"table\" phía trước vẫn chưa "
"kết thúc."

#: errStartTagInTableBody
msgid "“%1$S” start tag in table body."
msgstr "Thẻ mở đầu \"%1$S\" trong thân bảng."

#: errEndTagSeenWithoutDoctype
msgid ""
"End tag seen without seeing a doctype first. Expected “<!DOCTYPE html>”."
msgstr ""
"Thẻ kết thúc  mà chưa có thẻ doctype ở phía trước. Hãy thêm “<!DOCTYPE "
"html>”."

#: errEndTagAfterBody
msgid "Saw an end tag after “body” had been closed."
msgstr "Tìm thấy thẻ kết thúc phía sau phần \"body\"."

#: errEndTagSeenWithSelectOpen
msgid "“%1$S” end tag with “select” open."
msgstr "Thẻ kết thúc “%1$S” với \"select\" đang mở."

#: errGarbageInColgroup
msgid "Garbage in “colgroup” fragment."
msgstr "Garbage trong mảnh \"colgroup\""

#: errEndTagBr
msgid "End tag “br”."
msgstr "Kết thúc thẻ \"br\"."

#: errNoElementToCloseButEndTagSeen
msgid "No “%1$S” element in scope but a “%1$S” end tag seen."
msgstr "Không có thành phần “%1$S” mặc dù tồn tại thẻ kết thúc “%1$S”."

#: errHtmlStartTagInForeignContext
msgid "HTML start tag “%1$S” in a foreign namespace context."
msgstr "Thẻ HTML mở đầu “%1$S” tồn tại ở một namespace khác."

#: errTableClosedWhileCaptionOpen
msgid "“table” closed but “caption” was still open."
msgstr "“table” đã đóng nhưng “caption” vẫn đang mở."

#: errNoTableRowToClose
msgid "No table row to close."
msgstr "Không có dòng để đóng."

#: errNonSpaceInTable
msgid "Misplaced non-space characters inside a table."
msgstr "Kí tự không phải dấu cách bị đặt sai trong bảng."

#: errUnclosedChildrenInRuby
msgid "Unclosed children in “ruby”."
msgstr "Thành phần con chưa được đóng trong \"ruby\"."

#: errStartTagSeenWithoutRuby
msgid "Start tag “%1$S” seen without a “ruby” element being open."
msgstr "Thẻ bắt đầu \"%1$S\" không được mở bằng phần tử \"ruby\"."

#: errSelfClosing
msgid ""
"Self-closing syntax (“/>”) used on a non-void HTML element. Ignoring the "
"slash and treating as a start tag."
msgstr ""
"Thẻ tự đóng (\"/>\") được dùng cho cách thành phần khác HTML. Hãy bỏ dấu \"/"
"\" và xem nó như một thẻ mở đầu."

#: errNoCheckUnclosedElementsOnStack
msgid "Unclosed elements on stack."
msgstr "Tồn tại thành phần chưa được đóng trong ngăn xếp."

#: errEndTagDidNotMatchCurrentOpenElement
msgid ""
"End tag “%1$S” did not match the name of the current open element (“%2$S”)."
msgstr ""
"Thẻ mở đầu \"%1$S\" không khớp với tên của thành phần đang được mở (\"%2$S"
"\")."

#: errEndTagViolatesNestingRules
msgid "End tag “%1$S” violates nesting rules."
msgstr "Thẻ kết thúc \"%1$S\" vi phạm luật lồng nhau."

#: errEndWithUnclosedElements
#, fuzzy
msgid "End tag for “%1$S” seen, but there were unclosed elements."
msgstr "Tìm thấy thẻ kết thúc \"%1$S\" nhưng vẫn còn thành phần đang mở."
