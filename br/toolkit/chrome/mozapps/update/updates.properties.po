msgid "OK"
msgstr "Mat"

#: okButton.accesskey
msgid "O"
msgstr "M"

#: askLaterButton
msgid "Ask Later"
msgstr "Goulenn diwezhatoc'h"

#: askLaterButton.accesskey
msgctxt "askLaterButton.accesskey"
msgid "A"
msgstr "d"

#: noThanksButton
