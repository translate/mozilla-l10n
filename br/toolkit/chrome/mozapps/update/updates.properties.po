#: updateButton_major
msgid "Get the New Version"
msgstr "Kerc'hat an handelv nevez"

#: updateButton_major.accesskey
msgid "G"
msgstr "l"

#: backButton
msgid "Back"
msgstr "Kent"

#: backButton.accesskey
msgid "B"
msgstr "K"

#: acceptTermsButton
msgid "Accept Terms"
msgstr "Asantiñ an termenoù"

#: acceptTermsButton.accesskey
msgctxt "acceptTermsButton.accesskey"
msgid "A"
msgstr "t"

