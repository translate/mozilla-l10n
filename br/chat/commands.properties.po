#. # LOCALIZATION NOTE (commands):
#. #  %S is a comma separated list of command names.
#: commands
msgid ""
"Commands: %S.\n"
"Use /help &lt;command&gt; for more information."
msgstr ""
"Arc'hadoù : %S.\n"
"Arverit an &lt;arc'had&gt; /help a-benn kaout mioc'h a ditouroù."

#. # LOCALIZATION NOTE (noCommand, noHelp):
#. #  %S is the command name the user typed.
#: noCommand
msgid "No '%S' command."
msgstr "N'eus ket eus an arc'had '%S'."

#: noHelp
msgid "No help message for the '%S' command, sorry!"
msgstr "Kemennadenn skoazell ebet evit an arc'had '%S', diskoñfort !"

#: sayHelpString
msgid "say &lt;message&gt;: send a message without processing commands."
msgstr ""
"say &lt;kemennadenn&gt; : kas ur gemennadenn hep keweriañ an arc'hadoù."

#: rawHelpString
msgid "raw &lt;message&gt;: send a message without escaping HTML entities."
msgstr ""
"raw &lt;kemennadenn&gt; : kas ur gemennadenn hep gwareziñ an elfennoù HTML."

#: helpHelpString
msgid ""
"help &lt;name&gt;: show the help message for the &lt;name&gt; command, or "
"the list of possible commands when used without parameter."
msgstr ""
"help &lt;anv&gt; : diskouez ar gemennadenn skoazellañ evit an arc'had &lt;"
"anv&gt; pe diskouez roll an holl arc'hadoù ma'z eo arveret hep arventenn."

#. # LOCALIZATION NOTE (statusCommand):
#. #  %1$S is replaced with a status command name
#. #   (one of "back", "away", "busy", "dnd", or "offline").
#. #  %2$S is replaced with the localized version of that status type
#. #   (one of the 5 strings below).
#: statusCommand
msgid ""
"%1$S &lt;status message&gt;: set the status to %2$S with an optional status "
"message."
msgstr ""
"%1$S &lt;kemennadenn stad&gt; : kemm ar stad da %2$S gant ur gemennadenn "
"stad diret"

#: back
msgid "available"
msgstr "hegerz"

#: away
msgid "away"
msgstr "ezvezant"

#: busy
msgctxt "busy"
msgid "unavailable"
msgstr "ac'hubet"

#: dnd
msgctxt "dnd"
msgid "unavailable"
msgstr "na zirenkañ"

#: offline
msgid "offline"
msgstr "ezlinenn"
