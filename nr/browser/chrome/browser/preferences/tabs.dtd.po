#: tabsTab.label
msgid "Tabs"
msgstr "Iintjhidisi"

#: openExternalLinksIn.label
msgid "Open links from other applications in:"
msgstr "Vula ihlanganiso kusukela kuseentjenziswa ezinye ku:"

#: newWindow.label
msgid "a new window"
msgstr "ifasidere elitjha"

#: newTabInRecentWindow.label
msgid "a new tab in the most recent window"
msgstr "isitjhidisi esinye kufasidere elisanda kwakhiwa"

#: recentWindowOrTab.label
msgid "the most recent tab/window"
msgstr "isitjhidisi esisanda kwakhiwa/ifasidere"

#: forceNewWindowLinks.label
msgid "Force links that open new windows to open in:"
msgstr "Katelela ihlanganiso elivula amafasidere amatjha livuleke ku:"

#: sameTabOrWindow.label
msgid "the same tab/window as the link"
msgstr "isitjhidisi esifanako/ifasidere njengehlanganiso"

#: newTab.label
msgid "a new tab"
msgstr "isitjhidisi esitjha"

#: hideTabBar.label
msgid "Hide the tab bar when only one web site is open"
msgstr "Fihla isivalo sesitjhidisi lokha nakuvuleke iweb site yinye"

#: loadInBackground.label
msgid "Select new tabs opened from links"
msgstr "Khetha iintjhidisi ezitja ezivulwe kunhlanganiso"

#: loadBookmarksInBackground.label
msgid "Select new tabs opened from bookmarks or history"
msgstr "Khetha iintjhidisi ezitja ezivulwe kumatshwayo weencwadi namkha kumlando"

#: warnOnClose.label
msgid "Warn when closing multiple tabs"
msgstr "Yelelisa lokha uvala iintjhidisi ezimbalwa"

