#: pane.title
msgid "Return Receipts"
msgstr "Buyisela irasidi"

#: useGlobalPrefs.label
#: useGlobalPrefs.accesskey
msgid "&Use my global return receipt preferences for this account"
msgstr "&Sebenzisa ikhetho lami lephasi zombelele engilikhethako lokubuyisa amarasidi"

#: useCustomPrefs.label
#: useCustomPrefs.accesskey
msgid "&Customize return receipts for this account"
msgstr "&Tshwaya amarasidi abuyileko ku-akhawundi le"

#: requestReceipt.label
#: requestReceipt.accesskey
msgid "&When sending messages, always request a return receipt"
msgstr "&Nawuthumela imilayezo, ngaso soke isikhathi bawa irasidi yokubuyisa"

#: receiptArrive.label
msgid "When a receipt arrives:"
msgstr "Irasidi nayifikako:"

#: leaveIt.label
#: leaveIt.accesskey
msgid "Leave it in my &Inbox"
msgstr "Ilise &ebhokisini lami"

# LOCALIZATION NOTE moveToSent.label Translate: 'Sent' according to Netscape glossary
#: moveToSent.label
#: moveToSent.accesskey
msgid "&Move it to my &quot;Sent&quot; folder"
msgstr "&Ithumele kweyami &quot;Thumela&quot; ifolda"

#: requestMDN.label
msgid "When I receive a request for a return receipt:"
msgstr "Nangifumana isibawo serasidi yokubuyisa:"

#: returnSome.label
#: returnSome.accesskey
msgid "Allow r&eturn receipts for some messages"
msgstr "Vumela &ukubuya kwamarasidi kweminye imilayezo"

#: never.label
#: never.accesskey
msgid "&Never send a return receipt"
msgstr "&Ungathomi uthumele irasidi yokubuyisa"

#: notInToCc.label
#: notInToCc.accesskey
msgid "If I'm not in the &To or Cc of the message:"
msgstr "Nangingekho e &Ku namkha Cc womlayezo:"

#: outsideDomain.label
#: outsideDomain.accesskey
msgid "If the &sender is outside my domain:"
msgstr "Nakube &umthumeli ungaphandle kwekhaya lami:"

#: otherCases.label
#: otherCases.accesskey
msgid "In all &other cases:"
msgstr "Kizo zoke &ezinye iinkhathi:"

#: askMe.label
msgid "Ask me"
msgstr "Buza mina"

#: alwaysSend.label
msgid "Always send"
msgstr "Ngaso soke isikhathi thumela"

#: neverSend.label
msgid "Never send"
msgstr "Ungalingi uthumele"

