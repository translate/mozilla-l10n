# Window title
#: windowTitle.label
msgid "Publish Page"
msgstr "Veza Ikhasi"

#: windowTitleSettings.label
msgid "Publish Settings"
msgstr "Veza Ukuhlelwa"

#: publishTab.label
msgid "_: publishTab.label\n"
"Publish"
msgstr "Veza"

#: settingsTab.label
msgid "Settings"
msgstr "Ukuhlela"

#: publishButton.label
msgid "_: publishButton.label\n"
"Publish"
msgstr "Veza"

# Publish Tab Panel
#: siteList.label
msgid "_: siteList.label\n"
"Site Name:"
msgstr "Ibizo Lendawo:"

#: siteList.tooltip
msgid "Choose the site that you want to publish to"
msgstr "Khetha indawo ofuna ukuveza kiyo"

#: newSiteButton.label
msgid "New Site"
msgstr "Indawo Etja"

#: siteDirList.label
msgid "Select or type a directory name in your publishing site:"
msgstr "Khetha nayana tlola ibizo lokukhombisa endawenakho yokuveza:"

#: siteDirList.tooltip
msgid "Choose or enter the name of the remote subdirectory to publish to"
msgstr "Khetha nayana faka ibizo lendawo ekude yokukhomba ofuna ukuveza kiyo"

#: docDirList.label
msgid "Site subdirectory for this page:"
msgstr "Indawo yokukhomba ekhasineli:"

#: publishImgCheckbox.label
msgid "Include images and other files"
msgstr "Fakahlangana iinthombe namanye amafayili"

#: publishImgCheckbox.tooltip
msgid "Publish images and other files referenced by this page"
msgstr "Veza iinthombe namanye amafaayili atjhiwo likhaseli"

#: sameLocationRadio.label
msgid "Use same location as page"
msgstr "Sebenzisa ezinye iindawo njengekhasi"

#: sameLocationRadio.tooltip
msgid "Publish files to the same location as the page"
msgstr "Veza amafayili endaweni yinye nekhasi"

#: useSubdirRadio.label
msgid "Use this site subdirectory:"
msgstr "Sebenzisa lendawo yokukhombisa:"

#: useSubdir.tooltip
msgid "Choose or enter name of remote subdirectory where files will be published"
msgstr "Khetha nayana faka ibizo lendawo ekude yokukhomba lapho amafayili azakuvezwa khiona"

#: chooseDirButton.label
msgid "Choose..."
msgstr "Khethaâ€¦"

#: pageTitle.label
msgid "Page Title:"
msgstr "Isikhundla sekhasi:"

#: pageTitle.tooltip
msgid "Enter a title to identify the page in the window and in bookmarks"
msgstr "Faka isikhundla ukubona ikhasi efasidereni nematshwayweni wencwadi"

#: pageTitleExample.label
msgid "e.g.: &quot;My Web Page&quot;"
msgstr "Isib.. &quot;Ikhasilami leWebhu&quot;"

#: filename.label
msgid "Filename:"
msgstr "Ibizo lefayili:"

#: filename.tooltip
msgid "Enter a name for this file, including '.html' for a web page"
msgstr "Faka ibizo lefayili le, ufakehlangana '.html' yekhasi leWebhu"

#: filenameExample.label
msgid "e.g,: &quot;mypage.html&quot;"
msgstr "Isib.. &quot; ikhasilami.html&quot;"

#: relatedDocs.label
msgid "Other files to publish"
msgstr "Ukuvezwa kwamanye amafayili"

#: setDefaultButton.label
msgid "Set as Default"
msgstr "Beka njengobutjhapho"

#: setDefSiteAndDir.label
msgid "Set as Default Site and Directory"
msgstr "Beka njengendawo yobutjhapho neyekukhombisa"

#: removeButton.label
msgid "Remove Site"
msgstr "Susa indawo"

# Settings Tab Panel
#: publishSites.label
msgid "Publishing Sites"
msgstr "Indawo yokuveza"

#: serverInfo.label
msgid "Server Information"
msgstr "Ilulwazi lokusiza"

#: loginInfo.label
msgid "Login Information"
msgstr "Ilwazi lokungena"

#: siteName.label
msgid "_: siteName.label\n"
"Site Name:"
msgstr "Ibizo lendawo:"

#: siteName.tooltip
msgid "A nickname that identifies this publishing site (e.g.: 'MySite')"
msgstr "Ibizolokudlala elitjengisa indawo yokuvezokhu (isib.. 'Indawami ')"

#: siteUrl.label
msgid "Publishing address (e.g.: 'ftp://ftp.myisp.com/myusername'):"
msgstr "Isiphande sokuveza (isib.. 'ftp://ftp.myisp.com/myusername'):"

#: siteUrl.tooltip
msgid "The FTP:// or HTTP:// address provided by your ISP or web hosting service"
msgstr "I FTP:// nanyana HTTP:// isiphande esinikelwe yi ISP yakho nanyana iWebhu yokwamukela imisebenzi"

#: browseUrl.label
msgid "HTTP address of your homepage (e.g.: 'http://www.myisp.com/myusername'):"
msgstr "I-HTTP isiphande sekhasilakho lekhaya (isib.. 'http://www.myisp.com/myusername'):"

#: browseUrl.tooltip
msgid "The HTTP:// address of your home directory (don't include filename)"
msgstr "I-HTTP isiphande sokukhombisa ikhaya lakho (ungafakihlangana ibizo lefayili)"

#: username.label
msgid "User name:"
msgstr "Ibizo olisebenzisako:"

#: username.tooltip
msgid "The user name you use to log in to your ISP or web hosting service"
msgstr "Ibizo olisebenzisako ukungena ku-ISP yakho nanyana kuWbhu yokwamukela umsebenzi"

#: password.label
msgid "Password:"
msgstr "Inomboro yokungena:"

#: password.tooltip
msgid "The password associated with your user name"
msgstr "Inomboro yokungena: ihlanganiswe nebizo olisebenzisako"

#: savePassword.label
msgid "Save Password"
msgstr "Bulunga Inomboro Yokungena"

#: savePassword.tooltip
msgid "Select this to save your password securely using Password Manager"
msgstr "Khetha lokhu ukubulunga inomborwakho yokungena sebenzisa umphathi wenomborwakho yokungena ngokuphephile"

