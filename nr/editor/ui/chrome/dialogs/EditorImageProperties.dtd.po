# These strings are for use specifically in the editor's image and form image dialogs.
# Window title
#: windowTitle.label
msgid "Image Properties"
msgstr "Iimpahla zeenthombe"

#: pixelsPopup.value
msgid "pixels"
msgstr "iintokana zeenthombe"

#: percentPopup.value
msgid "percent"
msgstr "amaphesenti"

# These are in the Location tab panel
#: locationEditField.label
#: locationEditField.accessKey
msgid "Image &Location:"
msgstr "&Indawo Yesithombe:"

#: locationEditField.tooltip
msgid "Type the image's filename or location"
msgstr "Tlola igama lefayili lesithombe namkha indawo"

#: title.label
#: title.accessKey
msgid "&Tooltip:"
msgstr "&Ipenda yethulusi:"

#: title.tooltip
msgid "The html 'title' attribute that displays as a tooltip"
msgstr "Isabelo sesihloko sehtml esibonakala njengependa yethulusi"

#: altText.label
#: altText.accessKey
msgid "&Alternate text:"
msgstr "&Umtlolo ojamele omunye:"

#: altTextEditField.tooltip
msgid "Type text to display in place of the image"
msgstr "Tlola umtlolo ojame esikhundleni sesithombe"

#: noAltText.label
#: noAltText.accessKey
msgid "&Don't use alternate text"
msgstr "&Ungasebenzisi umtlolo ojamele omunye"

#: previewBox.label
msgid "Image Preview"
msgstr "Bona ngaphambili isithombe"

#: MoreFewerButton.tooltip
msgid "Display more or fewer properties to edit"
msgstr "Bonisa iimpahla ezinengi namkha ezingaphasi ekufanele zilungiswe"

# These controls are in the Dimensions tab panel
# actualSize.label should be same as actualSizeRadio.label + ":"
#: actualSize.label
msgid "Actual Size:"
msgstr "Isayizi Ekungiyo:"

#: actualSizeRadio.label
#: actualSizeRadio.accessKey
msgid "&Actual Size"
msgstr "&Isayizi Ekungiyo"

#: actualSizeRadio.tooltip
msgid "Revert to the image's actual size"
msgstr "Buyela kusayizi yesithombe ekungiyo"

#: customSizeRadio.label
#: customSizeRadio.accessKey
msgid "Custom &Size"
msgstr "&Isayizi Ejayelekileko"

#: customSizeRadio.tooltip
msgid "Change the image's size as displayed in the page"
msgstr "Tjhugulula isayizi yesithombe njengoba iboniswe ekhasini"

#: heightEditField.label
#: heightEditField.accessKey
msgid "Hei&ght:"
msgstr "&Ubude:"

#: widthEditField.label
#: widthEditField.accessKey
msgid "&Width:"
msgstr "&Ububanzi:"

#: constrainCheckbox.label
#: constrainCheckbox.accessKey
msgid "&Constrain"
msgstr "&Minyezela"

#: constrainCheckbox.tooltip
msgid "Maintain the image's aspect ratio"
msgstr "Londa iinthombe zamaphuzu"

# These controls are in the Image Map box of the expanded area
#: imagemapBox.label
msgid "Image Map"
msgstr "Umebhe Wesithombe"

#: removeImageMapButton.label
#: removeImageMapButton.accessKey
msgid "&Remove"
msgstr "&Susa"

#: editImageMapButton.label
msgid "Edit..."
msgstr "Lungisa..."

#: editImageMapButton.tooltip
msgid "Create clickable hotspots for this image"
msgstr "Yakhela isithombesi iindawo ezisebenza khulu ezikghona  ukuqhwarhaza"

# These are the options for image alignment
#: alignment.label
msgid "Align Text to Image"
msgstr "Linganisa umtlolo nesithombe"

#: bottomPopup.value
msgid "At the bottom"
msgstr "Phasi"

#: topPopup.value
msgid "At the top"
msgstr "Phezulu"

#: centerPopup.value
msgid "In the center"
msgstr "Phakathi"

#: wrapRightPopup.value
msgid "Wrap to the right"
msgstr "Songela ngesidleni"

#: wrapLeftPopup.value
msgid "Wrap to the left"
msgstr "Songela ngesinceleni"

# These controls are in the Spacing Box
#: spacingBox.label
msgid "Spacing"
msgstr "Ukuhlukanisa"

#: spacingBox.tooltip
msgid "Type a number for the amount of space around image"
msgstr "Tlola inomboro yobungako besikhundla mazombe nesithombe"

#: leftRightEditField.label
#: leftRightEditField.accessKey
msgid "&Left and Right:"
msgstr "&Sobuncele Nesokudla:"

#: topBottomEditField.label
#: topBottomEditField.accessKey
msgid "&Top and Bottom:"
msgstr "&Phezulu Naphasi:"

#: borderEditField.label
#: borderEditField.accessKey
msgid "Solid &Border:"
msgstr "&Umkhawulo odzimeleleko:"

# These controls are in the Link Box
#: showImageLinkBorder.label
#: showImageLinkBorder.accessKey
msgid "Show &border around linked image"
msgstr "&Bonisa umkhawulo mazombe nesithombe esihlanganisiweko"

# These controls may be added some day; currently not used
#: makePageBackgroundCheckbox.label
msgid "Make Page Background"
msgstr "Yenza Ingemuva Lekhasi"

#: makePageBackgroundCheckbox.tooltip
msgid "Use image as background for your page"
msgstr "Sebenzisa isithombe njengemuva lekhasi lakho"

#: editImageButton.label
msgid "Edit Image"
msgstr "Lungisa isithombe"

#: editImageButton.tooltip
msgid "Edit image file in another program"
msgstr "Lungisa ifayili yesithombe kuhlelo elinye"

# These tabs are currently used in the image input dialog
#: imageInputTab.label
msgid "Form"
msgstr "Iforomo"

#: imageLocationTab.label
msgid "Location"
msgstr "Indawo"

#: imageDimensionsTab.label
msgid "Dimensions"
msgstr "Ukwahlukana"

#: imageAppearanceTab.label
msgid "Appearance"
msgstr "Ukubonakala"

#: imageLinkTab.label
msgid "Link"
msgstr "Ihlanganiso"

