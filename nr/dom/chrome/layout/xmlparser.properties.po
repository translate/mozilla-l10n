# Map Expat error codes to error strings
#: 1
msgid "out of memory"
msgstr "ayinamkhumbulo"

#: 2
msgid "syntax error"
msgstr "kunephoso yekwakheka kwemitjho"

#: 3
msgid "no element found"
msgstr "akunasisetjenziswa esitholakeleko"

#: 4
msgid "not well-formed"
msgstr "ayikakheki kuhle"

#: 5
msgid "unclosed token"
msgstr "isisetjenziswa esingakavalwa"

#: 6
msgid "partial character"
msgstr "isakhiwo esingakapheleleli"

#: 7
msgid "mismatched tag"
msgstr "isilengisi esingafaniko"

#: 8
msgid "duplicate attribute"
msgstr "isabelo esibuyelelweko"

#: 9
msgid "junk after document element"
msgstr "konakele emva kwesisetjenziswa somtlolo"

#: 10
msgid "illegal parameter entity reference"
msgstr "kunesisetjenziswa sepharamitha esingakalungi esikhethiweko"

#: 11
msgid "undefined entity"
msgstr "isisetjenziswa esingakatjhiwo"

#: 12
msgid "recursive entity reference"
msgstr "ukukhethwa kwesisetjenziswa okubuyelelako"

#: 13
msgid "asynchronous entity"
msgstr "isisetjenziswa esikhambelanako"

#: 14
msgid "reference to invalid character number"
msgstr "ukukhuluma ngenomboro yesisetjenziswa engasebenziko"

#: 15
msgid "reference to binary entity"
msgstr "ukukhuluma ngesisetjenziswa esikabili"

#: 16
msgid "reference to external entity in attribute"
msgstr "ukukhuluma ngesisetjenziswa sangaphandle esinesabelo"

#: 17
msgid "xml declaration not at start of external entity"
msgstr "ukuvezwa kwe xml akusekuthomeni kwesisetjenziswa sangaphandle"

#: 18
msgid "unknown encoding"
msgstr "ukukhowuda okungaziwako"

#: 19
msgid "encoding specified in XML declaration is incorrect"
msgstr "ukukhowuda okunqophileko ekuvezwe ku XML akukalungi"

#: 20
msgid "unclosed CDATA section"
msgstr "incenye yeCDATA engakavalwa"

#: 21
msgid "error in processing external entity reference"
msgstr "kunephoso ekusebenzeni ukukhetha kwesisetjenziswa sangaphandle"

#: 22
msgid "document is not standalone"
msgstr "umtlolo awukazijameli"

#: 23
msgid "unexpected parser state"
msgstr "isimo sesisetjenziswa sokusebenza kwelimi esingakalindeleki"

#: 24
msgid "entity declared in parameter entity"
msgstr "kunesisetjenziswa sepharamitha esivezwe kusisetjenziswa sepharamitha"

#: 27
msgid "prefix not bound to a namespace"
msgstr "isiqala asikabekelwa isikhala esithileko segama"

#: 29
msgid "reserved prefix (xml) must not be undeclared or bound to another namespace URI"
msgstr "isiqala esibulungiweko (xml) akukafaneli sibikwe namkha sibekelwe isikhala segama esinye kuURI"

#: 30
msgid "reserved prefix (xmlns) must not be declared or undeclared"
msgstr "isiqalo esibulungiweko (xmlns) akukafanele sibikwe namkha singabikwa"

#: 31
msgid "prefix must not be bound to one of the reserved namespace URIs"
msgstr "isiqalo akukafaneli sibekelwe esinye seenkhala esibulungiweko samagama weURLs"

# %1$S is replaced by the Expat error string, may be followed by Expected (see below)
# %2$S is replaced by URL
# %3$d is replaced by line number
# %4$d is replaced by column number
#: XMLParsingError
msgid "XML Parsing Error: %1$S\n"
"Location: %2$S\n"
"Line Number %3$d, Column %4$d:"
msgstr "Isisetjenziswa selimi seXML sinephoso: %1$S\nIndawo: %2$S\nInomboro yelayini %3$d, Ikholomu %4$d:"

# %S is replaced by a tag name.
# This gets appended to the error string if the error is mismatched tag.
#: Expected
msgid ". Expected: </%S>."
msgstr ". Kulindeleke: </%S>."

