#: MimeNotCss
msgid "The stylesheet %1$S was not loaded because its MIME type, \"%2$S\", is not \"text/css\"."
msgstr "Iphepha lesitayela %1$S alikalayitjhwa ngombana ihlobo layo leMIME, \"%2$S\", nalo alika \"text/css\"."

#: MimeNotCssWarn
msgid "The stylesheet %1$S was loaded as CSS even though its MIME type, \"%2$S\", is not \"text/css\"."
msgstr "Iphepha lesitayela %1$S lilayitjhwe njenge CSS nanoma limhlobo weMIME, \"%2$S\", nalo alika \"text/css\"."

#: PEUnexpEOF
msgid "Unexpected end of file while searching for %1$S."
msgstr "Ukuvaleka kwefayili okungakalindeleki nalirhubhulula ku %1$S."

#: PEParseRuleWSOnly
msgid "Whitespace-only string given to be parsed as rule."
msgstr "Isikhala esinganalitho sentambo kwaphela sinikelwa bona intambo ihlathululwe njengesidwebelo."

#: PEDeclDropped
msgid "Declaration dropped."
msgstr "Ukubika kulisiwe."

#: PEDeclSkipped
msgid "Skipped to next declaration."
msgstr "Dlulela kokunye ukubika."

#: PEUnknownProperty
msgid "Unknown property '%1$S'."
msgstr "Ipahla engaziwako '%1$S'."

#: PEPropertyParsingError
msgid "Error in parsing value for property '%1$S'."
msgstr "Ikinga ekuhlathululeni inani lepahla '%1$S'."

#: PEExpectEndProperty
msgid "Expected end of value for property but found '%1$S'."
msgstr "Inani lepahla ekugcineni kodwana litholakele '%1$S'."

#: PESkipAtRuleEOF
msgid "end of unknown at-rule"
msgstr "ukuphela kokungaziwako em-thethweni"

#: PEUnknownAtRule
msgid "Unrecognized at-rule or error parsing at-rule '%1$S'."
msgstr "Ukungamukeleki em-thethweni namkha iphoso yokuphasisa em-thethweni '%1$S'."

#: PECharsetRuleEOF
msgid "charset string in @charset rule"
msgstr "intambo yetjhaseti e @charset rule"

#: PECharsetRuleNotString
msgid "Expected charset string but found '%1$S'."
msgstr "Elindelweko intambo yetjhaseti kodwana ifunyenwe '%1$S'."

#: PEGatherMediaEOF
msgid "end of media list in @import or @media rule"
msgstr "ukuphela kwerherho leendaba e @import namkha @media rule"

#: PEGatherMediaNotComma
msgid "Expected ',' in media list but found '%1$S'."
msgstr "Elindelweko ',' erherhweni leendaba kodwana lifunyenwe '%1$S'."

#: PEGatherMediaNotIdent
msgid "Expected identifier in media list but found '%1$S'."
msgstr "Esindelweko isazisi erherweni leendaba kodwana sifunyenwe '%1$S'."

#: PEImportNotURI
msgid "Expected URI in @import rule but found '%1$S'."
msgstr "Elindelweko URI e @import rule kodwana ifunyenwe'%1$S'."

#: PEImportUnexpected
msgid "Found unexpected '%1$S' within @import."
msgstr "Ifunyenwe kungakalindelwa '%1$S' ngaphakathi @import."

#: PEGroupRuleEOF
msgid "end of @media or @-moz-document rule"
msgstr "ukuphela kwe @media namkha @-moz-document rule"

# (pofilter) functions: checks to see that function names are not translated: domain() @-moz-document
#: PEMozDocRuleBadFunc
msgid "Expected url(), url-prefix(), or domain() in @-moz-document rule but found '%1$S'."
msgstr "Elindelweko url(), url-prefix(), namkha Idomeyini() e @-moz-documentrule kodwana ifunyenwe '%1$S'."

#: PEMozDocRuleNotURI
msgid "Expected URI in @-moz-document rule but found '%1$S'."
msgstr "Elindelweko URI e @-moz-document rule kodwana ifunyenwe'%1$S'."

#: PEAtNSPrefixEOF
msgid "namespace prefix in @namespace rule"
msgstr "isikhalasebizo isijobelelo e @namespace rule"

#: PEAtNSURIEOF
msgid "namespace URI in @namespace rule"
msgstr "isikhalasebizo URI e @namespace rule"

#: PEAtNSUnexpected
msgid "Unexpected token within @namespace: '%1$S'."
msgstr "Engakalindelwa ithokeni ngaphakathi @namespace: '%1$S'."

#: PESkipDeclBraceEOF
msgid "closing } of declaration block"
msgstr "ukuvala} kwe blogo lokuqiniseka"

#: PESkipRSBraceEOF
msgid "closing } of invalid rule set"
msgstr "ukuvala} kwe sede yomthetho ongasebenzi"

#: PEBadSelectorRSIgnored
msgid "Ruleset ignored due to bad selector."
msgstr "isedeyoMthetho ingaliwe ngenca yomkhethi omumbi."

#: PESelectorListExtraEOF
msgid "',' or '{'"
msgstr "',' namkha '{'"

#: PESelectorListExtra
msgid "Expected ',' or '{' but found '%1$S'."
msgstr "Elindelweko ',' namkha '{' kodwana ifunyenwe '%1$S'."

#: PESelectorGroupNoSelector
msgid "Selector expected."
msgstr "Umkhethi ulindelwe."

#: PESelectorGroupExtraCombinator
msgid "Dangling combinator."
msgstr "Umhlanganisi ongabazako."

# (pofilter) variables: do not translate: %1$S
# (pofilter) singlequoting: checks whether singlequoting is consistent between the two strings
#: PEIDSelNotIdent
#, fuzzy
msgid "Expected identifier for ID selector but found '%1$S'."
msgstr "Olindelweko umazisi wokukhetha I-ID kodwana ufunyenwe."

#: PEIDSelEmpty
msgid "Expected non-empty name for ID selector, but found '%1$S'."
msgstr "Elilindelweko eli-nto ibizo lomkhethi we-ID, kodwana lifunyenwe '%1$S'."

#: PEClassSelEOF
msgid "class name"
msgstr "ibizo leklasi"

#: PEClassSelNotIdent
msgid "Expected identifier for class selector but found '%1$S'."
msgstr "Olindelweko umazisi weklasi lokukhetha kodwana ufunyenwe '%1$S'."

#: PETypeSelEOF
msgid "element type"
msgstr "umhlobo we elemente"

#: PETypeSelNotType
msgid "Expected element name or '*' but found '%1$S'."
msgstr "Elilindelweko ibizo le elemente namkha '*' kodwana lifunyenwe '%1$S'."

#: PEUnknownNamespacePrefix
msgid "Unknown namespace prefix '%1$S'."
msgstr "Isijobelelo sesikhalasebizo elingaziwako '%1$S'."

#: PEAttributeNameEOF
msgid "attribute name"
msgstr "ibizo oliphiwako"

#: PEAttributeNameExpected
msgid "Expected identifier for attribute name but found '%1$S'."
msgstr "Olindelweko umazisi webizo oliphiwako kodwana lifunyenwe '%1$S'."

#: PEAttributeNameOrNamespaceExpected
msgid "Expected attribute name or namespace but found '%1$S'."
msgstr "Elilindelweko ibizo oliphiwako namkha isikhalasebizo kodwana lifunyenwe '%1$S'."

#: PEAttSelNoBar
msgid "Expected '|' but found '%1$S'."
msgstr "Elindelweko '|' kodwana ifunyenwe '%1$S'."

#: PEAttSelInnerEOF
msgid "part of attribute selector"
msgstr "incenye yokuphiwako komkhethi"

#: PEAttSelUnexpected
msgid "Unexpected token in attribute selector: '%1$S'."
msgstr "Ithokeni engakalindeleki yokuphiwako kumkhethi: '%1$S'."

#: PEAttSelValueEOF
msgid "attribute value"
msgstr "ukuqakatheka kokuphiwako"

#: PEAttSelCloseEOF
msgid "']' to end attribute selector"
msgstr "']' ukuqeda kokuphiwa ngumkhethi"

#: PEAttSelNoClose
msgid "Expected ']' to terminate attribute selector but found '%1$S'."
msgstr "Okulindelweko ']' ukuqeda okuphiwa ngumkhethi kodwana kufunyenwe '%1$S'."

#: PEAttSelBadValue
msgid "Expected identifier or string for value in attribute selector but found '%1$S'."
msgstr "Olindelekileko umazisi namkha intambo yokuqakatheka kokuphiwa umkhethi kodwana ufunyenwe '%1$S'."

#: PEPseudoSelEOF
msgid "name of pseudo-class or pseudo-element"
msgstr "ibizo le pusudo-klasi namkha le pusudo-element"

#: PEPseudoSelBadName
msgid "Expected identifier for pseudo-class or pseudo-element but found '%1$S'."
msgstr "Olindelekileko umazisi we pusudo-klasi namkha we pusudo-element kodwana ufunyenwe '%1$S'."

#: PEPseudoSelNonFunc
msgid "Function token for non-function pseudo-class or pseudo-element, or the other way around, when reading '%1$S'."
msgstr "Umsebenzi wethokheni yokusimsebenzi wepusudo-klasi namkha wepusudo-element, namkha ngenye indlela, nawufundako '%1$S'."

#: PEPseudoSelNotPE
msgid "Expected pseudo-element but found '%1$S'."
msgstr "Elindelweko I-pusudo-element kodwana ifunyenwe '%1$S'."

#: PEPseudoSelDoubleNot
msgid "Negation pseudo-class can't be negated '%1$S'."
msgstr "Ukuphikisa ipusudo-klasi angeze yaphikiswa '%1$S'."

#: PEPseudoSelPEInNot
msgid "Pseudo-elements can't be negated '%1$S'."
msgstr "AmaPusudo-elemente angeze aphikiswa '%1$S'."

#: PEPseudoSelNewStyleOnly
msgid "This pseudo-element must use the \"::\" form: '%1$S'."
msgstr "Isinga sakhesi kufanele sisebenzise \"::\" ijamo: '%1$S'."

#: PEPseudoSelTrailing
msgid "Found trailing token after pseudo-element, which must be the last part of a selector:  '%1$S'."
msgstr "Into etholwe ilenga ngemva kwesinga sakhi, ekufanele kube yincenye yokugcina yesikhethi:  '%1$S'."

#: PEPseudoSelMultiplePE
msgid "Extra pseudo-element '%1$S'."
msgstr "Isinga sakhi esingezelelweko '%1$S'."

#: PEPseudoSelUnknown
msgid "Unknown pseudo-class or pseudo-element '%1$S'."
msgstr "Isinga klasi namkha isinga sakhi esingaziwako '%1$S'."

#: PENegationEOF
msgid "selector within negation"
msgstr "isikhethi ngaphakathi kwepikiswano"

#: PENegationBadInner
msgid "Malformed simple selector as negation pseudo-class argument '%1$S'."
msgstr "Isikhethi esilula esingenziwanga kuhle njengeklasi epheze ibe ngiyo yepikiswano '%1$S'."

#: PENegationNoClose
msgid "Missing closing ')' in negation pseudo-class '%1$S'."
msgstr "Ukuvala okutjhodako ')' kuklasi ephikisako epheze ibe ngiyo yepikiswano '%1$S'."

#: PENegationBadArg
msgid "Missing argument in negation pseudo-class '%1$S'."
msgstr "Ipikiswano engekho kuklasi ephikisako epheze ibe ngeyeciniso '%1$S'."

# (review) do not translate: :lang (that means ':' + 'lang' must remain together)
# (review) do not translate:       :lang
# (review) keep : and lang together ie :lang
#: PELangArgEOF
msgid "argument to :lang selector"
msgstr "ipikiswano ku: isikhethi se limi"

#: PELangArgNotIdent
msgid "Expected identifier for lang pseudo-class parameter but found '%1$S'."
msgstr "Kulindeleke isazisi mayelana namaklasi aphese abe ngewelimi kodwana kwatholakala '%1$S'."

#: PELangNoClose
msgid "Missing closing ')' in lang pseudo-class '%1$S'."
msgstr "Kutjhoda ukuvala ')' kuklasi elifana nelelimi '%1$S'."

#: PELangNoArg
msgid "Missing argument in lang pseudo-class '%1$S'."
msgstr "Ipikiswano engekho kuklasi elifana nelelimi '%1$S'."

#: PESelectorEOF
msgid "selector"
msgstr "isikhethi"

#: PEBadDeclBlockStart
msgid "Expected '{' to begin declaration block but found '%1$S'."
msgstr "Kulindeleke '{' ukuthoma sisvalo sesimemezelo kodwana kwatholakala '%1$S'."

#: PEColorEOF
msgid "color"
msgstr "umbala"

#: PEColorNotColor
msgid "Expected color but found '%1$S'."
msgstr "Umbala olindelweko kodwana kwatholakala '%1$S'."

#: PEColorComponentEOF
msgid "color component"
msgstr "umhlobo wombala"

#: PEExpectedPercent
msgid "Expected a percentage but found '%1$S'."
msgstr "Kulindeleke iphesente kodwana kutholakala '%1$S'."

#: PEExpectedInt
msgid "Expected an integer but found '%1$S'."
msgstr "Kulindeleke inomboro kodwana kutholakala '%1$S'."

#: PEColorBadRGBContents
msgid "Expected number or percentage in rgb() but found '%1$S'."
msgstr "Inomboro namkha amaphesente alindelweko ku rgb() kodwana kutholakele '%1$S'."

#: PEColorComponentBadTerm
msgid "Expected '%2$S' but found '%1$S'."
msgstr "Kulindeleke '%2$S' kodwana kutholakala '%1$S'."

#: PEColorHueEOF
msgid "hue"
msgstr "ihlobo lombala"

#: PEExpectedComma
msgid "Expected ',' but found '%1$S'."
msgstr "Kulindeleke ',' kodwana kutholakele '%1$S'."

#: PEColorSaturationEOF
msgid "saturation"
msgstr "ukoma"

#: PEColorLightnessEOF
msgid "lightness"
msgstr "ubulula"

#: PEColorOpacityEOF
msgid "opacity in color value"
msgstr "ubudisi bokuzwisisa inani lombala"

#: PEExpectedNumber
msgid "Expected a number but found '%1$S'."
msgstr "Kulindeleke inomboro kodwana kutholakala '%1$S'."

#: PEExpectedCloseParen
msgid "Expected ')' but found '%1$S'."
msgstr "Kulindeleke ')' kodwana kutholakala '%1$S'."

#: PEDeclEndEOF
msgid "';' or '}' to end declaration"
msgstr "';' namkha '}' ukuqeda isifungo"

#: PEParseDeclarationNoColon
msgid "Expected ':' but found '%1$S'."
msgstr "Kulindeleke ':' kodwana kwatholakala '%1$S'."

#: PEParseDeclarationDeclExpected
msgid "Expected declaration but found '%1$S'."
msgstr "Isifungo ebesilindelwe kodwana kutholakele '%1$S'."

#: PEEndOfDeclEOF
msgid "end of declaration"
msgstr "ukuphela kwesimemezelo"

#: PEImportantEOF
msgid "important"
msgstr "kuqakathekile"

#: PEExpectedImportant
msgid "Expected 'important' but found '%1$S'."
msgstr "Kulindelele 'kuqakathekile' kodwana kwatholakala '%1$S'."

#: PEBadDeclEnd
msgid "Expected ';' to terminate declaration but found '%1$S'."
msgstr "Kulindeleke ';' ukuvala isimemezelo kodwana kwatholakala '%1$S'."

#: PEBadDeclOrRuleEnd
msgid "Expected ';' or '}' to terminate declaration but found '%1$S'."
msgstr "Kulindeleke ';' nanyana '}' ukuqeda isifungo kodwana kwatholakele '%1$S'."

#: PEInaccessibleProperty
msgid "Attempt to use inaccessible property"
msgstr "Ukuzama ukusebenzisa ipahla engafikeleki"

#: PECommentEOF
msgid "end of comment"
msgstr "ukuphela kwekulumo"

#: SEUnterminatedString
msgid "Found unclosed string '%1$S'."
msgstr "Intambo engakavalwa etholakeleko '%1$S'."

