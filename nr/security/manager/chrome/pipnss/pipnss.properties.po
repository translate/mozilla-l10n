msgstr "%S = %S"

#: SSLDisabled
msgid "You cannot connect to %S because SSL is disabled."
msgstr "Awukwazi ukuhlangana ne %S ngombana SSL isilaphaziwe."

#: SSL2Disabled
msgid "You cannot connect to %S because SSL version 2 is disabled."
msgstr "Awukwazi ukuhlangana ne %S ngombana umhlobo we 2 weSSL usilaphaziwe."

#: SSLNoMatchingCiphers
msgid "%S and %S cannot communicate securely because they have no common encryption algorithms."
msgstr "%S ne %S azikwazi ukukhulumisana ngokuvikelekileko ngombana azinamithetho efanako yokufihla."

#: UsersCertRevoked
msgid "Could not establish an encrypted connection with %S because your certificate has been revoked."
msgstr "Ayikwazi ukwakha ihlanganiso efihlekileko ne%S ngombana isitifikhethi sakho sicinyiwe."

#: UsersCertExpired
msgid "Could not establish an encrypted connection with %S because your certificate is expired."
msgstr "Ayikwazi ukwakha ihlanganiso efihlekileko ne %S ngombana isitifikhethi sakho siphelelwe sikhathi."

#: UsersCertRejected
msgid "Could not establish an encrypted connection because your certificate was rejected by %S. Error Code: %S."
msgstr "Ayikwazi ukwakha ihlangansiso efihlekileko ngombana isitifikhethi sakho azange semukelw yi %S. Kunephoso yekhowudi: %S."

#: BadMac
msgid "%S received a message with incorrect Message Authentication Code. If the error occurs frequently, contact the website administrator."
msgstr "%S ifumene umlayezo onekhowudi yokuqinisekisa engakalungi. Nakube iphoso le yenzekala kanengana, thintana nomphathi we-website."

#: PeerResetConnection
msgid "%S has closed the connection. Error Code: %S"
msgstr "%S ivale ihlanganiso. Iphoso yekhowudi: %S"

#: HostResetConnection
msgid "The current transfer has been cancelled."
msgstr "Ithuthukiso lanje lilisiwe."

#: BadPassword
msgid "An incorrect password was provided."
msgstr "Kufakwe Inomboro yokungena engakalungi."

#: BadDatabase
msgid "There is a problem with your certificate database [Error Code: %S]."
msgstr "Kunekinga ngemininingwane yesitifikhethi sakho [Iphoso Ngekhowudi: %S]."

#: BadServer
msgid "%S has sent an incorrect or unexpected message. Error Code: %S"
msgstr "%S ithumele umlayezo ongakalungi namkha umlayezo ongakalindelwa. Iphoso Yekhowudi: %S"

#: BadClient
msgid "%S has received an incorrect or unexpected message. Error Code: %S"
msgstr "%S ifumene umlayezo ongakalungi namkha umlayezo ongakalindelwa. Iphoso Yekhowudi: %S"

#: HostReusedIssuerSerial
msgid "You have received an invalid certificate.  Please contact the server administrator or email correspondent and give them the following information:\n"
"\n"
"Your certificate contains the same serial number as another certificate issued by the certificate authority.  Please get a new certificate containing a unique serial number."
msgstr "Uthole isitifikeyiti esingasebenziko.  Sibawa uthintane nomphathi womsebenzi namkha uthumele umlayezo nge-imeyili  umnikele imininingwana elandelako:\n\n Isitifikeyiti sakho sinesiriyali yenomboro efana neyesitifikeyiti esinye ekhitjhwe mphathi. Sibawa ufunyane isitifikeyiti esinenomboro yesiriyali ehlukileko."

#: SSLGenericError
msgid "Error establishing an encrypted connection to %S. Error Code: %S."
msgstr "Kunephoso ekwakheni ihlanganiso efihlekileko ku %S. Iphoso yekhowudi: %S."

#: TimeOut
