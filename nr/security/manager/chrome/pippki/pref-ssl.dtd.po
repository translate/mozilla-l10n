#: SSLProtocolVersions
msgid "SSL Protocol Versions"
msgstr "Imihlobo Yesivumelwano seSSL"

#: SSLWarnings
msgid "SSL Warnings"
msgstr "Iyelelo leSSL"

#: SSLClientAuthMethod
msgid "Client Certificate Selection"
msgstr "Ikhetho Lesitifikhethi Sekhasimende"

#: prefheader.title
msgid "Secure Sockets Layer (SSL)"
msgstr "Bulunga amazinga weenkhwama (SSL)"

#: enable.ssl2
msgid "Enable SSL version 2"
msgstr "Kghonakalisa umhlobo weSSL 2"

#: enable.ssl3
msgid "Enable SSL version 3"
msgstr "Kghonakalisa umhlobo weSSL 3"

#: enable.tls
msgid "Enable TLS"
msgstr "Kghonakalisa TLS"

#: edit.sslciphers
msgid "Edit Ciphers..."
msgstr "Lungisa Iinsetjenziswa zokufihla..."

#: edit.sslciphers.accesskey
msgid "E"
msgstr "E"

#: warn.description
msgid "&brandShortName; can alert you to the security status of the web page you are viewing. Set &brandShortName; to show a warning and ask permission before:"
msgstr "&brandShortName; ingakuyelelisa ngobujamo bevikelo lekhasi le webhu olibonako. Hlela &brandShortName; ukutjengisa isiyeleliso nokubawa imvumo ngaphambili:"

#: warn.enteringsecure
msgid "Loading a page that supports encryption"
msgstr "Ukulayitjha ikhasi elisekela ukufihleka"

#: warn.enteringweak
msgid "Loading a page that uses low-grade encryption"
msgstr "Ukulayitjha ikhasi elisebenzisa ukufihleka kwezinga eliphasi"

#: warn.insecurepost
msgid "Sending form data from an unencrypted page to an unencrypted page"
msgstr "Ukuthumela umhlobo wemininingwane ukusukela kukhasi elifihlakeleko ukuya kukhasi elingakafihlakali"

#: warn.leavingsecure
msgid "Leaving a page that supports encryption"
msgstr "Ukusukela ikhasi elisekela ukufihleka"

#: warn.secureredirect
msgid "Redirecting from one encrypted page to another"
msgstr "Ukubonisa indlela ukusukela kukhasi elifihlakeleko ukuya kwelinye"

#: warn.secureredirecttoinsecure
msgid "Redirecting from an encrypted page to an unencrypted page"
msgstr "Ukubonisa indlela ukusukela kukhasi elifihlakeleko ukuya kwelingakafihleki"

#: warn.viewmixed
msgid "Viewing a page with an encrypted/unencrypted mix"
msgstr "Ukubona ikhasi elinokufihleka/elinganakufihleka hlangana"

#: certselect.description
msgid "Decide how &brandShortName; selects a security certificate to present to web sites that require one:"
msgstr "Thatha isiqundo bonanyana i-&brandShortName; likhetha isitifikeyiti sevikelo ukujamela indawo ye webhu efunekako:"

#: certselect.auto
msgid "Select Automatically"
msgstr "Khetha Ngokuzenzakalela"

#: certselect.ask
msgid "Ask Every Time"
msgstr "Buza ngaso soke isikhathi"

# Cipher pref window
#: cipher.title
msgid "SSL: Edit Ciphers"
msgstr "SSL: Lungisa iinsetjenziswa zokufihla"

#: cipher.ssl2.label
msgid "SSL2 Cipher Suites"
msgstr "SSL2 Iinsudu Zeensetjenziswa zokufihla"

#: cipher.ssl3.label
msgid "SSL3/TLS Cipher Suites"
msgstr "SSL3/TLS Iinsudu Zeensetjenziswa zokufihla"

#: cipher.ssl3_extra.label
msgid "Extra SSL3/TLS Cipher Suites"
msgstr "Ezingezelelweko SSL3/TLS Iinsudu Zeensetjenziswa zokufihla"

#: cipher.tab.ssl2
msgid "SSL2"
msgstr "SSL2"

#: cipher.tab.ssl3tls
msgid "SSL3/TLS"
msgstr "SSL3/TLS"

#: cipher.tab.ssl3tls_extra
msgid "Extra SSL3/TLS"
msgstr "Ezingezelelweko SSL3/TLS"

# SSL2 Ciphers
#: cipher.ssl2.rc4_128
msgid "_: cipher.ssl2.rc4_128\n"
"128-bit RC4 encryption with RSA and an MD5 MAC"
msgstr "128-ubuncani RC4 ukufihla nge RSA nange MD5 MAC"

#: cipher.ssl2.rc2_128
msgid "128-bit RC2 encryption with RSA and an MD5 MAC"
msgstr "128-ubuncani RC2 ukufihla nge RSA nange MD5 MAC"

#: cipher.ssl2.des_ede3_192
msgid "168-bit Triple DES encryption with RSA and MD5 MAC "
msgstr "168-ubuncani Kathathu DES ukufihla nge RSA ne MD5 MAC "

#: cipher.ssl2.des_64
msgid "56-bit DES encryption with RSA and an MD5 MAC"
msgstr "56-ubuncani DES ukufihla nge RSA nange MD5 MAC"

#: cipher.ssl2.rc4_40
msgid "_: cipher.ssl2.rc4_40\n"
"40-bit RC4 encryption with RSA and an MD5 MAC (export)"
msgstr "40-ubuncani RC4 ukufihla nge RSA nange MD5 MAC (khuphela ngaphandle)"

#: cipher.ssl2.rc2_40
msgid "_: cipher.ssl2.rc2_40\n"
"40-bit RC2 encryption with RSA and an MD5 MAC (export)"
msgstr "40-ubuncani RC2 ukufihla nge RSA nange MD5 MAC (khuphela ngaphandle)"

# SSL3/TLS ciphers
#: cipher.ssl3.rsa_rc4_128_md5
msgid "_: cipher.ssl3.rsa_rc4_128_md5\n"
"128-bit RC4 encryption with RSA and an MD5 MAC"
msgstr "128-ubuncani RC4 ukufihla nge RSA nange MD5 MAC"

#: cipher.ssl3.rsa_rc4_128_sha
msgid "128-bit RC4 encryption with RSA and a SHA1 MAC"
msgstr "128-ubuncani RC4 ukufihla nge RSA nange SHA1 MAC"

#: cipher.ssl3.rsa_fips_des_ede3_sha
msgid "168-bit Triple DES with RSA and a SHA1 MAC (FIPS)"
msgstr "168-ubuncani Kathathu DES nge RSA nange SHA1 MAC (FIPS)"

#: cipher.ssl3.rsa_des_ede3_sha
msgid "168-bit Triple DES with RSA and a SHA1 MAC"
msgstr "168-ubuncani Kathathu DES nge RSA nange SHA1 MAC"

#: cipher.ssl3.rsa_fips_des_sha
msgid "56-bit DES encryption with RSA and a SHA1 MAC (FIPS)"
msgstr "56-ubuncani DES ukufihla nge RSA nange SHA1 MAC (FIPS)"

#: cipher.ssl3.rsa_des_sha
msgid "56-bit DES encryption with RSA and a SHA1 MAC"
msgstr "56-ubuncani DES ukufihla nge RSA nange SHA1 MAC"

#: cipher.ssl3.rsa_1024_rc4_56_sha
msgid "56-bit RC4 encryption with RSA and a SHA1 MAC (export)"
msgstr "56-ubuncani RC4 ukufihla nge RSA nange SHA1 MAC (khuphela ngaphandle)"

#: cipher.ssl3.rsa_1024_des_cbc_sha
msgid "56-bit DES encryption with RSA and a SHA1 MAC (export)"
msgstr "56-ubuncani DES ukufihla nge RSA nange SHA1 MAC (khuphela ngaphandle)"

#: cipher.ssl3.rsa_rc4_40_md5
msgid "_: cipher.ssl3.rsa_rc4_40_md5\n"
"40-bit RC4 encryption with RSA and an MD5 MAC (export)"
msgstr "40-ubuncani RC4 ukufihla nge RSA nange MD5 MAC (khuphela ngaphandle)"

#: cipher.ssl3.rsa_rc2_40_md5
msgid "_: cipher.ssl3.rsa_rc2_40_md5\n"
"40-bit RC2 encryption with RSA and an MD5 MAC (export)"
msgstr "40-ubuncani RC2 ukufihla nge RSA nange MD5 MAC (khuphela ngaphandle)"

# Extra SSL3/TLS ciphers
#: cipher.ssl3.dhe_rsa_aes_256_sha
msgid "256-bit AES encryption with RSA, DHE, and a SHA1 MAC"
msgstr "256-ubuncani AES ukufihla nge RSA, DHE, nange SHA1 MAC"

#: cipher.ssl3.dhe_dss_aes_256_sha
msgid "256-bit AES encryption with DSA, DHE, and a SHA1 MAC"
msgstr "256-ubuncani AES ukufihla nge DSA, DHE, nange SHA1 MAC"

#: cipher.ssl3.rsa_aes_256_sha
msgid "256-bit AES encryption with RSA and a SHA1 MAC"
msgstr "256-ubuncani AES ukufihla nge RSA nange SHA1 MAC"

#: cipher.ssl3.dhe_rsa_aes_128_sha
msgid "128-bit AES encryption with RSA, DHE, and a SHA1 MAC"
msgstr "128-ubuncani AES ukufihla nge RSA, DHE, nange SHA1 MAC"

#: cipher.ssl3.dhe_dss_aes_128_sha
msgid "128-bit AES encryption with DSA, DHE, and a SHA1 MAC"
msgstr "128-ubuncani AES ukufihla nge DSA, DHE, nange SHA1 MAC"

#: cipher.ssl3.rsa_aes_128_sha
msgid "128-bit AES encryption with RSA and a SHA1 MAC"
msgstr "128-ubuncani AES ukufihla nge RSA nange SHA1 MAC"

#: cipher.ssl3.dhe_rsa_des_ede3_sha
msgid "168-bit Triple DES with RSA, DHE, and a SHA1 MAC"
msgstr "168-ubuncani Kathathu DES nge RSA, DHE, nange SHA1 MAC"

#: cipher.ssl3.dhe_dss_des_ede3_sha
msgid "168-bit Triple DES with DSA, DHE, and a SHA1 MAC"
msgstr "168-ubuncani Kathathu DES nge DSA, DHE, nange SHA1 MAC"

#: cipher.ssl3.dhe_rsa_des_sha
msgid "56-bit DES encryption with RSA, DHE, and a SHA1 MAC"
msgstr "56-ubuncani DES ukufihla nge RSA, DHE, nange SHA1 MAC"

#: cipher.ssl3.dhe_dss_des_sha
msgid "56-bit DES encryption with DSA, DHE, and a SHA1 MAC"
msgstr "56-ubuncani DES ukufihla nge DSA, DHE, nange SHA1 MAC"

#: cipher.ssl3.rsa_null_sha
msgid "No encryption with RSA authentication and a SHA1 MAC"
msgstr "Akunafihlo ngokuqinisekisa kwe RSA nange SHA1 MAC"

#: cipher.ssl3.rsa_null_md5
msgid "No encryption with RSA authentication and an MD5 MAC"
msgstr "Akunafihlo ngokuqinisekisa kwe RSA nange MD5 MAC"

