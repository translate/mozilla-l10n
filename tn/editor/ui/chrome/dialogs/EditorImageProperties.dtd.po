# These strings are for use specifically in the editor's image and form image dialogs.
# Window title
#: windowTitle.label
msgid "Image Properties"
msgstr "Ditlhagotshedimosetso tsa Ditshwantsho"

#: pixelsPopup.value
msgid "pixels"
msgstr "dipikisele"

#: percentPopup.value
msgid "percent"
msgstr "phesente"

# These are in the Location tab panel
#: locationEditField.label
#: locationEditField.accessKey
msgid "Image &Location:"
msgstr "&Lefelo la Setshwantsho:"

#: locationEditField.tooltip
msgid "Type the image's filename or location"
msgstr "Tlanya leina la faele gongwe lefelo la setshwantsho"

#: title.label
#: title.accessKey
msgid "&Tooltip:"
msgstr "&Ntlha ya sediriswa:"

#: title.tooltip
msgid "The html 'title' attribute that displays as a tooltip"
msgstr "Letshwaopono la 'title' la html le le bontshang jaaka ntlha ya sediriswa"

#: altText.label
#: altText.accessKey
msgid "&Alternate text:"
msgstr "&Refosanya mafoko:"

#: altTextEditField.tooltip
msgid "Type text to display in place of the image"
msgstr "Tlanya mafoko a a bontshiwang mo boemong jwa setshwantsho"

#: noAltText.label
#: noAltText.accessKey
msgid "&Don't use alternate text"
msgstr "&Se dirise mafoko a a refosantsweng"

#: previewBox.label
msgid "Image Preview"
msgstr "Tebopele ya Setshwantsho"

#: MoreFewerButton.tooltip
msgid "Display more or fewer properties to edit"
msgstr "Bontsha ditlhagotshedimosetso tse dintsi gongwe di se kae go tseleganya"

# These controls are in the Dimensions tab panel
# actualSize.label should be same as actualSizeRadio.label + ":"
#: actualSize.label
msgid "Actual Size:"
msgstr "Bogolo jwa Nnete:"

#: actualSizeRadio.label
#: actualSizeRadio.accessKey
msgid "&Actual Size"
msgstr "&Bogolo jwa Nnete"

#: actualSizeRadio.tooltip
msgid "Revert to the image's actual size"
msgstr "Boela kwa bogolong jwa nnete jwa setshwantsho"

#: customSizeRadio.label
#: customSizeRadio.accessKey
msgid "Custom &Size"
msgstr "&Bogolo jwa Itlhophelo"

#: customSizeRadio.tooltip
msgid "Change the image's size as displayed in the page"
msgstr "Fetola bogolo jwa setshwantsho jaaka bo bontshitswe mo tsebeng"

#: heightEditField.label
#: heightEditField.accessKey
msgid "Hei&ght:"
msgstr "Bog&odimo:"

#: widthEditField.label
#: widthEditField.accessKey
msgid "&Width:"
msgstr "&Bophara:"

#: constrainCheckbox.label
#: constrainCheckbox.accessKey
msgid "&Constrain"
msgstr "&Kgoreletsa"

#: constrainCheckbox.tooltip
msgid "Maintain the image's aspect ratio"
msgstr "Tshola ntlha ya kabo ya setshwantsho"

# These controls are in the Image Map box of the expanded area
#: imagemapBox.label
msgid "Image Map"
msgstr "Mmapa wa Setshwantsho"

#: removeImageMapButton.label
#: removeImageMapButton.accessKey
msgid "&Remove"
msgstr "&Tlosa"

#: editImageMapButton.label
msgid "Edit..."
msgstr "Tseleganya..."

#: editImageMapButton.tooltip
msgid "Create clickable hotspots for this image"
msgstr "Tlhamela setshwantsho seno dihotspot tse di tobetsegang"

# These are the options for image alignment
#: alignment.label
msgid "Align Text to Image"
msgstr "Lebaganya Mafoko le Setshwantsho"

#: bottomPopup.value
msgid "At the bottom"
msgstr "Kwa tlase"

#: topPopup.value
msgid "At the top"
msgstr "Kwa godimo"

#: centerPopup.value
msgid "In the center"
msgstr "Mo gare"

#: wrapRightPopup.value
msgid "Wrap to the right"
msgstr "Phuthelela go ya kwa mojeng"

#: wrapLeftPopup.value
msgid "Wrap to the left"
msgstr "Phuthelela go ya kwa molemeng"

# These controls are in the Spacing Box
#: spacingBox.label
msgid "Spacing"
msgstr "Sebaka"

#: spacingBox.tooltip
msgid "Type a number for the amount of space around image"
msgstr "Tlanya palo ya boleng jwa sebaka go dikologa setshwantsho"

#: leftRightEditField.label
#: leftRightEditField.accessKey
msgid "&Left and Right:"
msgstr "&Molema le Moja:"

#: topBottomEditField.label
#: topBottomEditField.accessKey
msgid "&Top and Bottom:"
msgstr "&Godimo le Tlase:"

#: borderEditField.label
#: borderEditField.accessKey
msgid "Solid &Border:"
msgstr "Komota &Molelwane:"

# These controls are in the Link Box
#: showImageLinkBorder.label
#: showImageLinkBorder.accessKey
msgid "Show &border around linked image"
msgstr "Bontsha &molelwane go dikologa setshwantsho se se gokagantsweng"

# These controls may be added some day; currently not used
#: makePageBackgroundCheckbox.label
msgid "Make Page Background"
msgstr "Dira Lemorago la Tsebe"

#: makePageBackgroundCheckbox.tooltip
msgid "Use image as background for your page"
msgstr "Dirisa setshwantsho jaaka lemorago la tsebe ya gago"

#: editImageButton.label
msgid "Edit Image"
msgstr "Tseleganya Setshwantsho"

#: editImageButton.tooltip
msgid "Edit image file in another program"
msgstr "Tseleganya faele ya setshwantsho mo porokeramong e nngwe"

# These tabs are currently used in the image input dialog
#: imageInputTab.label
msgid "Form"
msgstr "Foromo"

#: imageLocationTab.label
msgid "Location"
msgstr "Lefelo"

#: imageDimensionsTab.label
msgid "Dimensions"
msgstr "Ditekanyo"

#: imageAppearanceTab.label
msgid "Appearance"
msgstr "Tebego"

#: imageLinkTab.label
msgid "Link"
msgstr "Segoke"

