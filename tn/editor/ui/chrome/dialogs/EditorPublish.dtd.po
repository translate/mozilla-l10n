# Window title
#: windowTitle.label
msgid "Publish Page"
msgstr "Phasalatsa Tsebe"

#: windowTitleSettings.label
msgid "Publish Settings"
msgstr "Phasalatsa Boalo"

#: publishTab.label
msgid "_: publishTab.label\n"
"Publish"
msgstr "Phasalatsa"

#: settingsTab.label
msgid "Settings"
msgstr "Boalo"

#: publishButton.label
msgid "_: publishButton.label\n"
"Publish"
msgstr "Phasalatsa"

# Publish Tab Panel
#: siteList.label
msgid "_: siteList.label\n"
"Site Name:"
msgstr "Leina la Setsha:"

#: siteList.tooltip
msgid "Choose the site that you want to publish to"
msgstr "Tlhopha setsha se o batlang go se phasalaletsa mo"

#: newSiteButton.label
msgid "New Site"
msgstr "Setsha se se Nt≈°hwa"

#: siteDirList.label
msgid "Select or type a directory name in your publishing site:"
msgstr "Tlhopha kgotsa tlanya leina la lenanefaele mo setsheng sa gago sa phasalatso:"

#: siteDirList.tooltip
msgid "Choose or enter the name of the remote subdirectory to publish to"
msgstr "Tlhopha kgotsa tsenya leina la lenanefaelepotlana le le kgakala go phasalaletsa mo go lona"

#: docDirList.label
msgid "Site subdirectory for this page:"
msgstr "Lenanefelepotlana la setsha la tsebe e:"

#: publishImgCheckbox.label
msgid "Include images and other files"
msgstr "Akaretsa ditshwantsho le difaele tse dingwe"

#: publishImgCheckbox.tooltip
msgid "Publish images and other files referenced by this page"
msgstr "Phasalatsa ditshwantsho le difaele tse dingwe tse di supiwang jaaka motswedi ke tsebe eno"

#: sameLocationRadio.label
msgid "Use same location as page"
msgstr "Dirisa lefelo le le lengwe le tsebe"

#: sameLocationRadio.tooltip
msgid "Publish files to the same location as the page"
msgstr "Phasalaletsa difaele mo lefelong le le lengwe le tsebe"

#: useSubdirRadio.label
msgid "Use this site subdirectory:"
msgstr "Dirisa lenanefaelepotlana la setsha se:"

#: useSubdir.tooltip
msgid "Choose or enter name of remote subdirectory where files will be published"
msgstr "Tlhopha kgotsa tsenya leina la lenanefaelepotlana le le kgakala le difaele di tla phasalalediwang mo go lona"

#: chooseDirButton.label
msgid "Choose..."
msgstr "Tlhopha..."

#: pageTitle.label
msgid "Page Title:"
msgstr "Setlhogo sa Tsebe:"

#: pageTitle.tooltip
msgid "Enter a title to identify the page in the window and in bookmarks"
msgstr "Tsenya setlhogo go supa tsebe mo fensetereng le mo matshwaotsebeng"

#: pageTitleExample.label
msgid "e.g.: &quot;My Web Page&quot;"
msgstr "e.g.: &quot;Tsebe ya Me ya Inthanete&quot;"

#: filename.label
msgid "Filename:"
msgstr "Leina la faele:"

#: filename.tooltip
msgid "Enter a name for this file, including '.html' for a web page"
msgstr "Tsenya leina la faele e, go akarediwa '.html' ya tsebe ya inthanete"

#: filenameExample.label
msgid "e.g,: &quot;mypage.html&quot;"
msgstr "e.g,: &quot;tsebeyame.html&quot;"

#: relatedDocs.label
msgid "Other files to publish"
msgstr "Difaele tse dingwe tse di phasaladiwang"

#: setDefaultButton.label
msgid "Set as Default"
msgstr "Tlhoma jaaka Serulaganyediwa ruri"

#: setDefSiteAndDir.label
msgid "Set as Default Site and Directory"
msgstr "Tlhoma jaaka Setsha se se Rulaganyeditsweng ruri le Lenanefaele"

#: removeButton.label
msgid "Remove Site"
msgstr "Tlosa Setsha"

# Settings Tab Panel
#: publishSites.label
msgid "Publishing Sites"
msgstr "E phasalatsa Ditsha"

#: serverInfo.label
msgid "Server Information"
msgstr "Tshedimosetso ya Dikgatlhong"

#: loginInfo.label
msgid "Login Information"
msgstr "Tshedimosetso ya go Tsena"

#: siteName.label
msgid "_: siteName.label\n"
"Site Name:"
msgstr "Leina la Setsha:"

#: siteName.tooltip
msgid "A nickname that identifies this publishing site (e.g.: 'MySite')"
msgstr "Leinatlhaletso le le supang setsha seno sa phasalatso (sekai: 'SetshaSaMe')"

#: siteUrl.label
msgid "Publishing address (e.g.: 'ftp://ftp.myisp.com/myusername'):"
msgstr "Aterese ya Phasalatso (sekai.: 'ftp://ftp.myisp.com/leinalamelamodirisi'):"

#: siteUrl.tooltip
msgid "The FTP:// or HTTP:// address provided by your ISP or web hosting service"
msgstr "Aterese ya FTP:// kgotsa HTTP:// e e tlamelwang ke Motlamedi wa gago wa Tirelo ya Inthanete kgotsa tirelo ya go amogela inthanete"

#: browseUrl.label
msgid "HTTP address of your homepage (e.g.: 'http://www.myisp.com/myusername'):"
msgstr "Aterese ya HTTP ya tsebegae ya gago (sekai: 'http://www.myisp.com/leinalamelamodirisi'):"

#: browseUrl.tooltip
msgid "The HTTP:// address of your home directory (don't include filename)"
msgstr "Aterese ya HTTP:// ya lenanefaele la gago la gae (se akaretsa leina la faele)"

#: username.label
msgid "User name:"
msgstr "Leina la modirisi:"

#: username.tooltip
msgid "The user name you use to log in to your ISP or web hosting service"
msgstr "Leina la modirisi le o le dirisang go tsena mo Motlameding wa gago wa Tirelo ya Inthanete kgotsa tirelo ya go amogela inthanete"

#: password.label
msgid "Password:"
msgstr "Khunololamoraba:"

#: password.tooltip
msgid "The password associated with your user name"
msgstr "Khunololamoraba e e amanang le leina la gago la modirisi"

#: savePassword.label
msgid "Save Password"
msgstr "Boloka Khunololamoraba"

#: savePassword.tooltip
msgid "Select this to save your password securely using Password Manager"
msgstr "Tlhopha seno go boloka khunololamoraba ya gago ka pabalesego o dirisa Motsamaisi wa Khunololamoraba"

