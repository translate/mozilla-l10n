#: tabsTab.label
msgid "Tabs"
msgstr "Ditab"

#: openExternalLinksIn.label
msgid "Open links from other applications in:"
msgstr "Bula digoke go tswa kwa dirweboleteng tse dingwe mo:"

#: newWindow.label
msgid "a new window"
msgstr "fensetere e ntšhwa"

#: newTabInRecentWindow.label
msgid "a new tab in the most recent window"
msgstr "tab e ntšhwa mo fensetereng ya bošweng go gaisa"

#: recentWindowOrTab.label
msgid "the most recent tab/window"
msgstr "tab/fensetere ya bošweng go gaisa"

#: forceNewWindowLinks.label
msgid "Force links that open new windows to open in:"
msgstr "Pateletsa digoke tse di bulang difensetere tse dintšhwa go bula mo:"

#: sameTabOrWindow.label
msgid "the same tab/window as the link"
msgstr "tab/fensetere e le nngwe le segoke"

#: newTab.label
msgid "a new tab"
msgstr "tab e ntšhwa"

#: hideTabBar.label
msgid "Hide the tab bar when only one web site is open"
msgstr "Fitlha bara ya tab fela fa go butswe setsha se le sengwe fela sa inthanete"

#: loadInBackground.label
msgid "Select new tabs opened from links"
msgstr "Tlhopha ditab tse dintšhwa tse di butsweng go tswa mo digokeng"

#: loadBookmarksInBackground.label
msgid "Select new tabs opened from bookmarks or history"
msgstr "Tlhopha ditab tse dintšhwa tse di butsweng go tswa mo matshwaotsebeng gongwe hisetori"

#: warnOnClose.label
msgid "Warn when closing multiple tabs"
msgstr "Ntsibose fa go tswalwa ditab tse dintsi"

