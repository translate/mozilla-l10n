#: MimeNotCss
msgid "The stylesheet %1$S was not loaded because its MIME type, \"%2$S\", is not \"text/css\"."
msgstr "Letlhare la setaele %1$S ga le a laisiwa gonne mofuta wa lona wa MIME ke \"%2$S\", ga o \"text/css\"."

# (pofilter) acronyms: acronyms should not be translated: CSS
#: MimeNotCssWarn
msgid "The stylesheet %1$S was loaded as CSS even though its MIME type, \"%2$S\", is not \"text/css\"."
msgstr "Letlhare la setaele %1$S ga le laisitswe le fa mofuta wa lona wa MIME \"%2$S\", o se \"text/css\"."

#: PEUnexpEOF
msgid "Unexpected end of file while searching for %1$S."
msgstr "Bokhutlo jwa faele jo bo sa solofelwa fa go fenenelwa %1$S."

#: PEParseRuleWSOnly
msgid "Whitespace-only string given to be parsed as rule."
msgstr "Go neetswe mogala wa sebakasesweu fela go tlhalosiwa jaaka molawana."

#: PEDeclDropped
msgid "Declaration dropped."
msgstr "Kitsiso e tlogetswe."

#: PEDeclSkipped
msgid "Skipped to next declaration."
msgstr "Go tloletswe kwa kitsisong e e latelang."

#: PEUnknownProperty
msgid "Unknown property '%1$S'."
msgstr "Tlhagotshedimosetso e e sa itsiweng '%1$S'."

#: PEPropertyParsingError
msgid "Error in parsing value for property '%1$S'."
msgstr "Phoso fa go tlhalosiwa boleng jwa tlhagotshedimosetso '%1$S'."

#: PEExpectEndProperty
msgid "Expected end of value for property but found '%1$S'."
msgstr "Go ne go solofetswe bokhutlo jwa boleng jwa tlhagotshedimosetso mme go fitlhetswe '%1$S'."

#: PESkipAtRuleEOF
msgid "end of unknown at-rule"
msgstr "bokhutlo jwa mo-molawaneng o o sa itsiweng"

#: PEUnknownAtRule
msgid "Unrecognized at-rule or error parsing at-rule '%1$S'."
msgstr "Molawana o o sa lemogweng gongwe phoso ya tlhaloso ya molawana '%1$S'."

#: PECharsetRuleEOF
msgid "charset string in @charset rule"
msgstr "mogala wa tšhasete mo moalwaneng wa @tšhasete"

#: PECharsetRuleNotString
msgid "Expected charset string but found '%1$S'."
msgstr "Go ne go solofetswe mogala wa tšhasete mme go fitlhetswe '%1$S'."

#: PEGatherMediaEOF
msgid "end of media list in @import or @media rule"
msgstr "bokhutlo jwa lenane la mmedia mo @tlisateng gongwe molawana wa @mmedia"

#: PEGatherMediaNotComma
msgid "Expected ',' in media list but found '%1$S'."
msgstr "Go ne go solofetswe ',' mo lenaneng la mmedia mme go fitlhetswe '%1$S'."

#: PEGatherMediaNotIdent
msgid "Expected identifier in media list but found '%1$S'."
msgstr "Go ne go solofetswe sesupi mo lenaneng la mmedia mme go fitlhetswe '%1$S'."

# (review) do not translate: @import
#: PEImportNotURI
msgid "Expected URI in @import rule but found '%1$S'."
msgstr "Go ne go solofetswe URI mo molawaneng wa @tlisateng mme go fitlhetswe '%1$S'."

# (review) do not translate: @import
#: PEImportUnexpected
msgid "Found unexpected '%1$S' within @import."
msgstr "Go fitlhetswe '%1$S' e e sa solofelwang mo teng ga @tlisateng."

#: PEGroupRuleEOF
msgid "end of @media or @-moz-document rule"
msgstr "bokhutlo jwa @mmedia gongwe molawana wa @-tokumente ya-moz"

# (review) do not translate: url(), url-prefix(), domain(), @-moz-document
#: PEMozDocRuleBadFunc
msgid "Expected url(), url-prefix(), or domain() in @-moz-document rule but found '%1$S'."
msgstr "Go ne go solofetswe url(), url-prefix(), gongwe motlaagana() mo molawaneng wa @-tokumente ya-moz mme go fitlhetswe '%1$S'."

#: PEMozDocRuleNotURI
msgid "Expected URI in @-moz-document rule but found '%1$S'."
msgstr "Go ne go solofetswe URI mo molawaneng wa @-tokumente ya-moz mme go fitlhetswe '%1$S'."

#: PEAtNSPrefixEOF
msgid "namespace prefix in @namespace rule"
msgstr "setlhogo sa sebaka sa leina mo molawaneng wa @sebaka sa leina"

# (review) do not translate: @namespace
#: PEAtNSURIEOF
msgid "namespace URI in @namespace rule"
msgstr "URI ya sebaka sa leina mo molawaneng wa @sebaka sa leina"

#: PEAtNSUnexpected
msgid "Unexpected token within @namespace: '%1$S'."
msgstr "Letshwao le le sa solofelwang mo @sebakeng sa leina: '%1$S'."

#: PESkipDeclBraceEOF
msgid "closing } of declaration block"
msgstr "go tswalwa } ga sekgoreletsi sa kitsiso"

#: PESkipRSBraceEOF
msgid "closing } of invalid rule set"
msgstr "go tswalwa } ga molawana o e seng wa boammaaruri o o tlhomilweng"

#: PEBadSelectorRSIgnored
msgid "Ruleset ignored due to bad selector."
msgstr "Molawana o ikgatholositswe ka ntlha ya setlhophi se se sa siamang."

#: PESelectorListExtraEOF
msgid "',' or '{'"
msgstr "',' gongwe '{'"

#: PESelectorListExtra
msgid "Expected ',' or '{' but found '%1$S'."
msgstr "Go ne go solofetswe ',' gongwe '{' fela go fitlhetswe '%1$S'."

#: PESelectorGroupNoSelector
msgid "Selector expected."
msgstr "Setlhophi se solofetswe."

#: PESelectorGroupExtraCombinator
msgid "Dangling combinator."
msgstr "Go akgiwa sekopanyi."

#: PEIDSelNotIdent
msgid "Expected identifier for ID selector but found '%1$S'."
msgstr "Go ne go solofetswe sesupi sa setlhophi sa Boitshupo mme go fitlhetswe '%1$S'."

#: PEIDSelEmpty
msgid "Expected non-empty name for ID selector, but found '%1$S'."
msgstr "Go ne go solofetswe leina le le seng lolea la setlhophi sa Boitshupo mme go fitlhetswe '%1$S'."

#: PEClassSelEOF
msgid "class name"
msgstr "leina la tlelase"

#: PEClassSelNotIdent
msgid "Expected identifier for class selector but found '%1$S'."
msgstr "Go ne go solofetswe sesupi sa setlhophi sa tlelase mme go fitlhetswe '%1$S'."

#: PETypeSelEOF
msgid "element type"
msgstr "mofuta wa elemente"

#: PETypeSelNotType
msgid "Expected element name or '*' but found '%1$S'."
msgstr "Go ne go solofetswe leina la elemente gongwe '*' fela go fitlhetswe '%1$S'."

#: PEUnknownNamespacePrefix
msgid "Unknown namespace prefix '%1$S'."
msgstr "Setlhogo sa sebaka sa leina se se sa itsiweng '%1$S'."

#: PEAttributeNameEOF
msgid "attribute name"
msgstr "leina la letshwaopono"

#: PEAttributeNameExpected
msgid "Expected identifier for attribute name but found '%1$S'."
msgstr "Go ne go solofetswe sesupi sa leina la letshwaopono mme go fitlhetswe '%1$S'."

#: PEAttributeNameOrNamespaceExpected
msgid "Expected attribute name or namespace but found '%1$S'."
msgstr "Go ne go solofetswe leina la letshwaopono gongwe sebaka sa leina mme go fitlhetswe '%1$S'."

#: PEAttSelNoBar
msgid "Expected '|' but found '%1$S'."
msgstr "Go ne go solofetswe 'І' mme go fitlhetswe '%1$S'."

#: PEAttSelInnerEOF
msgid "part of attribute selector"
msgstr "karolo ya setlhophi sa letshwaopono"

#: PEAttSelUnexpected
msgid "Unexpected token in attribute selector: '%1$S'."
msgstr "Letshwao le le sa solofelwang mo setlhophing sa letshwaopono: '%1$S'."

#: PEAttSelValueEOF
msgid "attribute value"
msgstr "boleng jwa letshwaopono"

#: PEAttSelCloseEOF
msgid "']' to end attribute selector"
msgstr "']' go feleletsa setlhophi sa letshwaopono"

#: PEAttSelNoClose
msgid "Expected ']' to terminate attribute selector but found '%1$S'."
msgstr "Go solofetswe ']' go khutlisa setlhophi sa letshwaopono mme go fitlhetswe '%1$S'."

#: PEAttSelBadValue
msgid "Expected identifier or string for value in attribute selector but found '%1$S'."
msgstr "Go ne go solofetswe sesupi gongwe mogala wa boleng mo setlhophing sa letshwaopono mme go fitlhetswe '%1$S'."

#: PEPseudoSelEOF
msgid "name of pseudo-class or pseudo-element"
msgstr "leina la tlelase-ketso gongwe elemente-ketso"

#: PEPseudoSelBadName
msgid "Expected identifier for pseudo-class or pseudo-element but found '%1$S'."
msgstr "Go ne go solofetswe sesupi sa tlelase-ketso gongwe elemente-ketso mme go fitlhetswe '%1$S'."

#: PEPseudoSelNonFunc
msgid "Function token for non-function pseudo-class or pseudo-element, or the other way around, when reading '%1$S'."
msgstr "Letshwao la tiro la tlelase-ketso gongwe elemente-ketso e e seng ya tiro, gongwe ka tsela e nngwe, fa go buisiwa '%1$S'."

#: PEPseudoSelNotPE
msgid "Expected pseudo-element but found '%1$S'."
msgstr "Go ne go solofetswe element-ketso mme go fitlhetswe '%1$S'."

#: PEPseudoSelDoubleNot
msgid "Negation pseudo-class can't be negated '%1$S'."
msgstr "Kganetso ya tlelase-ketso e ka seke ya ganediwa '%1$S'."

#: PEPseudoSelPEInNot
msgid "Pseudo-elements can't be negated '%1$S'."
msgstr "Dielemente-ketso dika seke tsa ganediwa '%1$S'."

#: PEPseudoSelNewStyleOnly
msgid "This pseudo-element must use the \"::\" form: '%1$S'."
msgstr "Elemente-ketso e tshwanetse go dirisa mofuta wa \"::\": '%1$S'."

#: PEPseudoSelTrailing
msgid "Found trailing token after pseudo-element, which must be the last part of a selector:  '%1$S'."
msgstr "Go fitlhetswe letshwao le le lepeletseng morago ga elemente-ketso, le e tshwanetseng ya bo e le karolo ya bofelo ya setlhophi:  '%1$S'."

#: PEPseudoSelMultiplePE
msgid "Extra pseudo-element '%1$S'."
msgstr "Elemente-ketso ya tlaleletso '%1$S'."

#: PEPseudoSelUnknown
msgid "Unknown pseudo-class or pseudo-element '%1$S'."
msgstr "Tlelase-ketso gongwe elemente-ketso e e sa itsiweng '%1$S'."

#: PENegationEOF
msgid "selector within negation"
msgstr "setlhophi mo teng ga kganetso"

#: PENegationBadInner
msgid "Malformed simple selector as negation pseudo-class argument '%1$S'."
msgstr "Ga go a bopiwa sentle setlhophi se se bonolo jaaka kganetso ya tlelase-ketso '%1$S'."

#: PENegationNoClose
msgid "Missing closing ')' in negation pseudo-class '%1$S'."
msgstr "Go tlhaela go tswala ')' mo tlelase-ketsong ya kganetso '%1$S'."

#: PENegationBadArg
msgid "Missing argument in negation pseudo-class '%1$S'."
msgstr "Go tlhaela kganetsano mo tlelase-ketsong ya kganetso '%1$S'."

# (review) do not translate: :lang
#: PELangArgEOF
msgid "argument to :lang selector"
msgstr "kganetsano ya : tlhopho ya puo"

#: PELangArgNotIdent
msgid "Expected identifier for lang pseudo-class parameter but found '%1$S'."
msgstr "Go ne go solofetswe sesupi sa parametara ya tlelase-ketso ya puo mme go fitlhetswe '%1$S'."

#: PELangNoClose
msgid "Missing closing ')' in lang pseudo-class '%1$S'."
msgstr "Go tlhaela go tswala ')' mo tlelase-ketsong ya puo '%1$S'."

#: PELangNoArg
msgid "Missing argument in lang pseudo-class '%1$S'."
msgstr "Go tlhaela kganetsano mo tlelase-ketsong ya puo '%1$S'."

#: PESelectorEOF
msgid "selector"
msgstr "setlhophi"

#: PEBadDeclBlockStart
msgid "Expected '{' to begin declaration block but found '%1$S'."
msgstr "Go ne go solofetswe '{' go simolola sekgoreletsi sa kitsiso mme go fitlhetswe '%1$S'."

#: PEColorEOF
msgid "color"
msgstr "mmala"

#: PEColorNotColor
msgid "Expected color but found '%1$S'."
msgstr "Go ne go solofetswe mmala mme go fitlhetswe '%1$S'."

#: PEColorComponentEOF
msgid "color component"
msgstr "karolwana ya mmala"

#: PEExpectedPercent
msgid "Expected a percentage but found '%1$S'."
msgstr "Go ne go solofetswe phesente mme go fitlhetswe '%1$S'."

#: PEExpectedInt
msgid "Expected an integer but found '%1$S'."
msgstr "Go ne go solofetswe palotlalo mme go fitlhetswe '%1$S'."

#: PEColorBadRGBContents
msgid "Expected number or percentage in rgb() but found '%1$S'."
msgstr "Go ne go solofetswe palo gongwe phesente mo rgb() mme go fitlhetswe '%1$S'."

# (review) is the variable order correct?
#: PEColorComponentBadTerm
msgid "Expected '%2$S' but found '%1$S'."
msgstr "Go ne go solofetswe '%1$S' mme go fitlhetswe '%2$S'."

#: PEColorHueEOF
msgid "hue"
msgstr "pharologantshommala"

#: PEExpectedComma
msgid "Expected ',' but found '%1$S'."
msgstr "Go ne go solofetswe ',' mme go fitlhetswe '%1$S'."

#: PEColorSaturationEOF
msgid "saturation"
msgstr "go tlala thata"

#: PEColorLightnessEOF
msgid "lightness"
msgstr "lesedinyana"

#: PEColorOpacityEOF
msgid "opacity in color value"
msgstr "letobo mo boleng jwa mmala"

#: PEExpectedNumber
msgid "Expected a number but found '%1$S'."
msgstr "Go ne go solofetswe palo mme go fitlhetswe '%1$S'."

#: PEExpectedCloseParen
msgid "Expected ')' but found '%1$S'."
msgstr "Go ne go solofetswe ')' mme go fitlhetswe '%1$S'."

#: PEDeclEndEOF
msgid "';' or '}' to end declaration"
msgstr "';' gongwe '}' go khutlisa kitsiso"

#: PEParseDeclarationNoColon
msgid "Expected ':' but found '%1$S'."
msgstr "Go ne go solofetswe ';' mme go fitlhetswe '%1$S'."

#: PEParseDeclarationDeclExpected
msgid "Expected declaration but found '%1$S'."
msgstr "Go ne go solofetswe kitsiso mme go fitlhetswe '%1$S'."

#: PEEndOfDeclEOF
msgid "end of declaration"
msgstr "bokhutlo jwa kitsiso"

#: PEImportantEOF
msgid "important"
msgstr "botlhokwa"

#: PEExpectedImportant
msgid "Expected 'important' but found '%1$S'."
msgstr "Go ne go solofetswe 'botlhokwa' mme go fitlhetswe '%1$S'."

#: PEBadDeclEnd
msgid "Expected ';' to terminate declaration but found '%1$S'."
msgstr "Go ne go solofetswe ';' go khutlisa kitsiso mme go fitlhetswe '%1$S'."

#: PEBadDeclOrRuleEnd
msgid "Expected ';' or '}' to terminate declaration but found '%1$S'."
msgstr "Go ne go solofetswe ';' gongwe '}' go khutlisa kitsiso mme go fitlhetswe '%1$S'."

#: PEInaccessibleProperty
msgid "Attempt to use inaccessible property"
msgstr "Leka go dirisa tlhagotshedimosetso e e sa letlelelwang"

#: PECommentEOF
msgid "end of comment"
msgstr "bokhutlo jwa tshwaelo"

#: SEUnterminatedString
msgid "Found unclosed string '%1$S'."
msgstr "Go bonwe mogala o o sa tswalwang '%1$S'."

