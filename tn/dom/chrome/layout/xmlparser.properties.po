# Map Expat error codes to error strings
#: 1
msgid "out of memory"
msgstr "feletswe ke mmemori"

#: 2
msgid "syntax error"
msgstr "phoso ya popapolelo"

#: 3
msgid "no element found"
msgstr "ga go a bonwa elemente epe"

#: 4
msgid "not well-formed"
msgstr "ga e a tlhamega sentle"

#: 5
msgid "unclosed token"
msgstr "letshwao le le sa tswalwang"

#: 6
msgid "partial character"
msgstr "letshwao la karolwana"

#: 7
msgid "mismatched tag"
msgstr "tag e e sa tshwantshanngwang sentle"

#: 8
msgid "duplicate attribute"
msgstr "khopi ya letshwaopono "

#: 9
msgid "junk after document element"
msgstr "matlakala morago ga elemente ya tokumente"

#: 10
msgid "illegal parameter entity reference"
msgstr "tshupelo ya selo ya parametara e e seng mo molaong "

#: 11
msgid "undefined entity"
msgstr "selo se se sa tlhalosiwang"

#: 12
msgid "recursive entity reference"
msgstr "tshupelo e e ipoeletsang ya selo"

#: 13
msgid "asynchronous entity"
msgstr "selo se se sa tsamaeng segongwe"

#: 14
msgid "reference to invalid character number"
msgstr "tshupelo ya palo e e seng ya boammaaruri ya letshwao "

#: 15
msgid "reference to binary entity"
msgstr "tshupelo ya selo sa baenari "

#: 16
msgid "reference to external entity in attribute"
msgstr "tshupelo ya selo sa kwa ntle mo letshwaoponong "

#: 17
msgid "xml declaration not at start of external entity"
msgstr "kgoeletso ya xml ga se tshimologo ya selo sa kwa ntle "

#: 18
msgid "unknown encoding"
msgstr "khoutufatso e e sa itsiweng"

#: 19
msgid "encoding specified in XML declaration is incorrect"
msgstr "khoutufatso e e totobaditsweng mo kgoeletsong ya XML ga e a siama. "

#: 20
msgid "unclosed CDATA section"
msgstr "karolo e e sa tswalwang ya CDATA "

#: 21
msgid "error in processing external entity reference"
msgstr "phoso fa go diragadiwa tshupelo ya selo sa kwa ntle "

#: 22
msgid "document is not standalone"
msgstr "tokumente ga e eme e le nosi"

#: 23
msgid "unexpected parser state"
msgstr "maemo a a sa solofelwang a dikarolopuo"

#: 24
msgid "entity declared in parameter entity"
msgstr "selo se goeleditswe mo selong sa parametara  "

#: 27
msgid "prefix not bound to a namespace"
msgstr "Setlhogo ga se a golegelwa mo sebakeng sa leina "

#: 29
msgid "reserved prefix (xml) must not be undeclared or bound to another namespace URI"
msgstr "Setlhogo se se beeleditsweng (xml) ga se a tshwanela go goeledilolwa gongwe go golegelelwa mo sebakeng se sengwe sa leina URI"

#: 30
msgid "reserved prefix (xmlns) must not be declared or undeclared"
msgstr "Setlhogo se se beeleditsweng (xmlns) ga se a tshwanela go goelediwa gongwe go goeledilolwa "

#: 31
msgid "prefix must not be bound to one of the reserved namespace URIs"
msgstr "setlhogo ga se a tshwanelwa go golegelelwa go sengwe sa dibaka tsa maina tse di bolokilweng tsa URI"

# %1$S is replaced by the Expat error string, may be followed by Expected (see below)
# %2$S is replaced by URL
# %3$d is replaced by line number
# %4$d is replaced by column number
#: XMLParsingError
msgid "XML Parsing Error: %1$S\n"
"Location: %2$S\n"
"Line Number %3$d, Column %4$d:"
msgstr "XML Phoso ya Parsing ya XML: %1$S\nLefelo: %2$S\nNomore ya Mola, Kholamo %4$d:"

# %S is replaced by a tag name.
# This gets appended to the error string if the error is mismatched tag.
#: Expected
msgid ". Expected: </%S>."
msgstr ". Solofetsweng: </%S>."

