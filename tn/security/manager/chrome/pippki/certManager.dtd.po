#: certmgr.title
msgid "Certificate Manager"
msgstr "Motsamaisi wa Ditifikeiti"

#: certmgr.tab.mine
msgid "Your Certificates"
msgstr "Ditifikeiti tsa Gago"

#: certmgr.tab.others
msgid "Other People's"
msgstr "Tsa Batho ba Bangwe"

#: certmgr.tab.websites
msgid "Web Sites"
msgstr "Ditsha tsa Inthanete"

#: certmgr.tab.ca
msgid "Authorities"
msgstr "Beng ba Dithata"

#: certmgr.ca.builtins
msgid "Built-in CAs"
msgstr "Di-CA tse di Ageletsweng teng"

#: certmgr.ca.mycas
msgid "CAs Managed By You"
msgstr "Di-CA tse di Tsamaisiwang Ke Wena"

#: certmgr.mine
msgid "You have certificates from these organizations that identify you:"
msgstr "O na le ditifikeiti go tswa kwa ditlamong tseno tse di go supang:"

#: certmgr.others
msgid "You have certificates on file that identify these people:"
msgstr "O na le ditifikeiti mo faeleng tse di supang batho bano:"

#: certmgr.websites
msgid "You have certificates on file that identify these web sites:"
msgstr "O na le ditifikeiti mo faeleng tse di supang ditsha tseno tsa inthanete:"

#: certmgr.cas
msgid "You have certificates on file that identify these certificate authorities:"
msgstr "O na le ditifikeiti mo faeleng tse di supang beng ba dithata tsa ditifikeiti:"

#: certmgr.detail.general_tab.title
msgid "General"
msgstr "Ka kakaretso"

#: certmgr.detail.prettyprint_tab.title
msgid "Details"
msgstr "Dintlha"

#: certmgr.subjectinfo.label
msgid "Issued To"
msgstr "Rebotswe Go"

#: certmgr.issuerinfo.label
msgid "Issued By"
msgstr "Rebotswe Ke"

#: certmgr.validity.label
msgid "Validity"
msgstr "Boammaaruri/Go dira"

#: certmgr.fingerprints.label
msgid "Fingerprints"
msgstr "Dikgatiso tsa menwana"

#: certmgr.certdetail.title
msgid "Certificate Detail"
msgstr "Dintlha tsa Setifikeiti"

#: certmgr.certdetail.cn
msgid "Common Name (CN)"
msgstr "Leina le le Tlwaelegileng"

#: certmgr.certdetail.o
msgid "Organization (O)"
msgstr "Setlamo"

#: certmgr.certdetail.ou
msgid "Organizational Unit (OU)"
msgstr "Yuniti ya Setlamo"

#: certmgr.certdetail.serialnumber
msgid "_: certmgr.certdetail.serialnumber\n"
"Serial Number"
msgstr "Palopopo"

#: certmgr.certdetail.sha1fingerprint
msgid "SHA1 Fingerprint"
msgstr "Kgatiso ya monwana ya SHA"

#: certmgr.certdetail.md5fingerprint
msgid "MD5 Fingerprint"
msgstr "Kgatiso ya monwana ya MD5"

#: certmgr.editcert.title
msgid "Edit Security Certificate Settings"
msgstr "Tseleganya Boalo jwa Setifikeiti sa Pabalesego"

#: certmgr.editcacert.title
msgid "Edit CA certificate trust settings"
msgstr "Tseleganya boalo jwa boikanyego jwa setifikeiti sa CA"

#: certmgr.editsslcert.title
msgid "Edit web site certificate trust settings"
msgstr "Tseleganya boalo jwa boikanyego jwa setifikeiti sa sesha sa inthanete"

#: certmgr.editcert.edittrust
msgid "Edit trust settings:"
msgstr "Tseleganya boalo jwa boikanyego:"

#: certmgr.editcert.trustssl
msgid "This certificate can identify web sites."
msgstr "Setifikeiti seno se ka supa ditsha tsa inthanete."

#: certmgr.editcert.trustemail
msgid "This certificate can identify mail users."
msgstr "Setifikeiti seno se ka supa badirisi ba poso."

#: certmgr.editcert.trustobjsign
msgid "This certificate can identify software makers."
msgstr "Setifikeiti seno se ka supa badiri ba dirweboleta."

#: certmgr.editsslcert.edittrust
msgid "_: certmgr.editsslcert.edittrust\n"
"Edit certificate trust settings:"
msgstr "Tseleganya boalo jwa boikanyego jwa setifikeiti:"

#: certmgr.editsslcert.dotrust
msgid "_: certmgr.editsslcert.dotrust\n"
"Trust the authenticity of this certificate."
msgstr "Ikanye boammaaruri jwa setifikeiti seno."

#: certmgr.editsslcert.donttrust
msgid "_: certmgr.editsslcert.donttrust\n"
"Do not trust the authenticity of this certificate."
msgstr "Se ikanye boammaaruri jwa setifikeiti seno."

#: certmgr.editemailcert.title
msgid "Edit email certificate trust settings"
msgstr "Tseleganya boalo jwa boikanyego jwa setifikeiti sa poso ye eleketoroniki"

#: certmgr.editemailcert.edittrust
msgid "_: certmgr.editemailcert.edittrust\n"
"Edit certificate trust settings:"
msgstr "Tseleganya boalo jwa boikanyego jwa setifikeiti:"

#: certmgr.editemailcert.dotrust
msgid "_: certmgr.editemailcert.dotrust\n"
"Trust the authenticity of this certificate."
msgstr "Ikanye boammaaruri jwa setifikeiti seno."

#: certmgr.editemailcert.donttrust
msgid "_: certmgr.editemailcert.donttrust\n"
"Do not trust the authenticity of this certificate."
msgstr "Se ikanye boammaaruri jwa setifikeiti seno."

#: certmgr.deletecert.title
msgid "Delete Certificate"
msgstr "Phimola Setifikeiti"

#: certmgr.deletecert.beforename
msgid "You have requested to delete this certificate:"
msgstr "O kopile go phimola setifikeiti seno:"

#: certmgr.deletecert.aftername
msgid "Are you sure you want to delete this certificate?"
msgstr "A o totobetse gore o batla go phimola setifikeiti seno?"

#: certmgr.deleteusercert.title
msgid "Delete Your Certificate"
msgstr "Phimola Setifikeiti sa Gago"

#: certmgr.deleteusercert.beforename
msgid "You have requested to delete the certificate:"
msgstr "O kopile go phimola setifikeiti:"

#: certmgr.deleteusercert.aftername
msgid "Once you have deleted this certificate, you will not be able to read mail that has been encrypted with it."
msgstr "Fa o sena go phimola setifikeiti seno, o ka seke wa tlhola o kgona go buisa poso e e kwadilweng ka khoutu ya  sona."

#: certmgr.certname
msgid "Certificate Name"
msgstr "Leina la Setifikeiti"

#: certmgr.tokenname
msgid "Security Device"
msgstr "Sediriswa sa Pabalesego"

#: certmgr.purpose
msgid "Purposes"
msgstr "Mabaka"

#: certmgr.issued
msgid "Issued On"
msgstr "Rebotswe ka La"

#: certmgr.expires
msgid "Expires On"
msgstr "Khutla go dira ka La"

#: certmgr.email
msgid "E-Mail Address"
msgstr "Aterese ya Poso ya Eleketoroniki"

#: certmgr.serial
msgid "_: certmgr.serial\n"
"Serial Number"
msgstr "Palotatelano"

#: certmgr.ocsp_info
msgid "Certificates have not been validated with OCSP. Click View to do so."
msgstr "Ditifikeiti ga di ise di rurifadiwe ka OCSP. Kgotlha Leba go dira jalo."

#: certmgr.close.label
msgid "Close"
msgstr "Tswala"

#: certmgr.view.label
msgid "View"
msgstr "Leba"

#: certmgr.edit.label
msgid "Edit"
msgstr "Tseleganya"

#: certmgr.editca.label
msgid "Edit CA Trust"
msgstr "Tseleganya CA Trust"

#: certmgr.add.label
msgid "Add"
msgstr "Tsenya"

#: certmgr.delete.label
msgid "Delete"
msgstr "Phimola"

#: certmgr.backup.label
msgid "Backup"
msgstr "Boloka ga tlaleletso"

#: certmgr.backupall.label
msgid "Backup All"
msgstr "Boloka ga tlaleletso go Tsotlhe"

#: certmgr.restore.label
msgid "Import"
msgstr "Tlisateng"

#: certmgr.details.label
msgid "Certificate Fields"
msgstr "Mafelo a Ditifikeiti"

#: certmgr.fields.label
msgid "Field Value"
msgstr "Boleng jwa Lefelo"

#: certmgr.hierarchy.label
msgid "Certificate Hierarchy"
msgstr "Tlhatlhamano ya Ditifikeiti"

