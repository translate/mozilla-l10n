#: Component_XPCOM
msgid "Cross Platform COM"
msgstr "Cross-Platform COM"

#: Component_Mail
msgid "$ProductName$"
msgstr "$ProductName$"

#: Component_Offline
msgid "Offline Support"
msgstr "Tshegetso ya mo kgaogong ya nakwana"

#: Component_Offline_Long
msgid "Adds Offline support to $ProductName$ Mail."
msgstr "E tsenya tshegetso ya mo Kgaogong ya Nakwana mo Posong ya $ProductName$."

#: Component_RSS
msgid "RSS Support"
msgstr "Tshegetso ya RSS"

#: Component_RSS_Long
msgid "Adds RSS support to $ProductName$."
msgstr "E tsenya tshegetso ya RSS mo $ProductName$."

#: Component_QFA
msgid "Quality Feedback Agent"
msgstr "Moemedi wa Pegelo ya Boleng"

#: Component_QFA_Long
msgid "for reporting $ProductName$ crash information"
msgstr "go bega tshedimosetso ya phutlhamo ya $ProductName$"

