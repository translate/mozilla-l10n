msgstr "Ga e tshegediwe ke mofuta ono wa porotokolo ya LDAP"

## @name CONTROL_NOT_FOUND
## @loc none
#: 93
msgid "Requested LDAP control not found"
msgstr "Taolo ya LDAP e e kopilweng ga e a bonwa"

## @name NO_RESULTS_RETURNED
## @loc none
#: 94
msgid "No results returned"
msgstr "Ga gona dipholo tse di busitsweng"

## @name MORE_RESULTS_TO_RETURN
## @loc none
#: 95
msgid "More results to return"
msgstr "Dipholo tse dingwe tse di tshwanetseng go busiwa"

## @name CLIENT_LOOP
## @loc none
#: 96
