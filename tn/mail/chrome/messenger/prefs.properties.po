#: prefPanel-server
msgid "Server Settings"
msgstr "Boalo jwa Dikgatlho"

#: prefPanel-copies
msgid "Copies & Folders"
msgstr "Dikhopi & Ditsholadifaele"

#: prefPanel-offline-and-diskspace
msgid "Offline & Disk Space"
msgstr "Go sa gokelelwe kana golagane le inthanete Sebaka sa Disiki"

#: prefPanel-diskspace
msgid "Disk Space"
msgstr "Sebaka sa Disiki"

#: prefPanel-addressing
msgid "Composition & Addressing"
msgstr "Tlhamo & Go kwala diaterese"

#: prefPanel-advanced
msgid "Advanced"
msgstr "Tsweletseng"

## LOCALIZATION NOTE (prefPanel-smtp): Don't translate "SMTP"
#: prefPanel-smtp
msgid "Outgoing Server (SMTP)"
msgstr "Dikgatlho tse di tswelang kwa ntle (SMTP)"

# account manager multiple identity support
#LOCALIZATION NOTE: accountName: %1$S 
#: identity-list-title
msgid "Identities for %1$S"
msgstr "Boitshupo jwa  %1$S"

#: identity-edit-req
msgid "You must specify a valid email address for this identity."
msgstr "O tshwanetse go totobatsa aterese ya poso ya eleketoroniki ya boammaaruri ya boitshupo jono."

#: identity-edit-req-title
msgid "Error Creating Identity"
msgstr "Phoso fa go Tlhamiwa Boitshupo"

#: choosefile
msgid "Choose a file"
msgstr "Tlhopha faele"

