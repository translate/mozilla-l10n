#------------------------------------------------------------------------------
#: -601
msgid "Out of memory"
msgstr "E feletswe ke mmemori"

#: -602
msgid "Invalid param"
msgstr "Paramo e e seng ya Boammaaruri"

#: -603
msgid "Invalid member variable"
msgstr "Phapaano ya tokololo e e seng ya boammaaruri"

#: -604
msgid "Invalid pointer"
msgstr "Sesupi se e seng sa boammaaruri"

#: -605
msgid "Parse key has no value"
msgstr "Taetsi ya karolopuo ga ena boleng"

#: -606
msgid "Failed to read readme"
msgstr "E reteletswe ke go buisa mpuise"

#: -607
msgid "Failed to read license"
msgstr "E reteletswe ke go buisa laesense"

#: -608
msgid "Out of bounds in Component/Setup Type list"
msgstr "Kwa ntle ga melelwane mo lenaneng la Mofuta wa Dikarolo/Tlhomo"

#: -609
msgid "Mismatched ref counts"
msgstr "Dipalelo tsa metswedi tse di sa nyalaneng"

#: -610
msgid "No components in the INI file"
msgstr "Ga go na dikarolo mo faeleng ya INI"

#: -611
msgid "No setup types in the INI file"
msgstr "Ga gona mefuta ya tlhomo mo faeleng ya INI"

#: -612
msgid "URL at this index already exists"
msgstr "URL e setse e le teng mo tshupaneng eno"

#: -613
msgid "Couldn't create the directory"
msgstr "Ga e a kgona go tlhoma lenanefaele"

#: -614
msgid "FTP URL malformed"
msgstr "FTP URL ga e a tlhamega"

#: -615
msgid "Download failed"
msgstr "Kopololo e retetse"

#: -616
msgid "Extraction of XPCOM failed"
msgstr "Go utulola XPCOM go retetse"

#: -617
msgid "Failed to fork a process"
msgstr "E reteletswe ke go aroganya thulaganyo"

#: -618
msgid "Couldn't open xpistub library"
msgstr "Ga e a kgone go bula laeborari ya xpistub"

#: -619
msgid "Couldn't get symbol in library"
msgstr "Ga e a kgona go bona letshwao mo laeboraring"

#: -620
msgid "A xpistub call failed"
msgstr "Pitso ya xpistub e retetse"

#: -621
msgid "An Installer module %s (.xpi) failed to install"
msgstr "Mmojule wa motlhomi %s (.xpi) o reteletswe go tlhoma"

#: -622
msgid "Copy of an installer module (.xpi) failed"
msgstr "Khopi ya motlhomi (.xpi) e retetse"

#: -623
msgid "Destination directory doesn't exist"
msgstr "Lenanefaele la bogorogelo ga le gona"

#: -624
msgid "Can't make destination directory.  Please try another directory."
msgstr "Ga e kgone go dira lenanefaele la bogorogelo.  Tsweetswee leka lenanefaele le lengwe."

#: -625
msgid "A previous installation exists."
msgstr "Tlhomo e e fetileng e gona."

#: -626
msgid "Insufficient permission"
msgstr "Tetla e e sa lekanang"

#: -627
msgid "Insufficient disk space"
msgstr "Sebaka sa disiki se se sa lekanang"

