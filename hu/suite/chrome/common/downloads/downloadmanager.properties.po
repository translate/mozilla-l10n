#: paused
msgid "Paused"
msgstr "Szünetel"

#: downloading
msgid "Downloading"
msgstr "Letöltés"

#: uploading
msgid "Uploading"
msgstr "Feltöltés"

#: notStarted
msgid "Not Started"
msgstr "Még nem kezdődött el"

#: failed
msgid "Failed"
msgstr "Sikertelen"

#: finished
msgid "Finished"
msgstr "Kész"

#: canceled
msgid "Canceled"
msgstr "Megszakítva"

#: blocked
msgid "Blocked"
msgstr "Blokkolva"

#. # LOCALIZATION NOTE (downloadsTitleFiles, downloadsTitlePercent): Semi-colon list of
#. # plural forms. See: http://developer.mozilla.org/en/Localization_and_Plurals
#. # %1$S number of files; %2$S overall download percent (only for downloadsTitlePercent)
#. # %% will appear as a single % sign, so %2$S%% is the percent number plus the % sign
#. # examples: 2% of 1 file - Download Manager; 22% of 11 files - Download Manager
#: downloadsTitleFiles
msgid "%1$S file - Download Manager;%1$S files - Download Manager"
msgstr "%1$S fájl - Letöltéskezelő;%1$S fájl - Letöltéskezelő"

#: downloadsTitlePercent
msgid ""
"%2$S%% of %1$S file - Download Manager;%2$S%% of %1$S files - Download "
"Manager"
msgstr ""
"%2$S%%, összesen: %1$S fájl - Letöltéskezelő;%2$S%%, összesen: %1$S fájl - "
"Letöltéskezelő"

#. # LOCALIZATION NOTE (progressTitle):
#. # %1$S is the file name, %2$S is the download state
#. # examples: coolvideo.ogg - Finished; seamonkey-nightly.zip - Paused
#: progressTitle
msgid "%1$S - %2$S"
msgstr "%1$S - %2$S"

#. # LOCALIZATION NOTE (progressTitlePercent):
#. # %1$S is download percent, %2$S is the file name, %3$S is the download state
#. # %% will appear as a single % sign, so %1$S%% is the percent number plus the % sign
#. # examples: 42% of coolvideo.ogg - Paused; 98% of seamonkey-nightly.zip - Downloading
#: progressTitlePercent
msgid "%1$S%% of %2$S - %3$S"
msgstr "%1$S%%, összesen: %2$S - %3$S"

#. # LOCALIZATION NOTE (percentFormat): %1$S is download percent
#. # %% will appear as a single % sign, so %1$S%% is the percent number plus the % sign
#: percentFormat
msgid "%1$S%%"
msgstr "%1$S%%"

#. # LOCALIZATION NOTE (speedFormat):
#. # %1$S rate number; %2$S rate unit
#. # units are taken from toolkit's downloads.properties
#. # example: 2.2 MB/sec
#: speedFormat
msgid "%1$S %2$S/sec"
msgstr "%1$S %2$S/mp"

#. # LOCALIZATION NOTE (timeSingle): %1$S time number; %2$S time unit
#. # example: 1 minute; 11 hours
#: timeSingle
msgid "%1$S %2$S"
msgstr "%1$S %2$S"

#. # LOCALIZATION NOTE (timeDouble):
#. # %1$S time number; %2$S time unit; %3$S time sub number; %4$S time sub unit
#. # example: 11 hours, 2 minutes; 1 day, 22 hours
#: timeDouble
msgid "%1$S %2$S, %3$S %4$S"
msgstr "%1$S %2$S, %3$S %4$S"

#. # LOCALIZATION NOTE (timeElapsedSingle): %1$S time number; %2$S time unit
#. # example: 1 minute elapsed; 11 hours elapsed
#: timeElapsedSingle
msgid "%1$S %2$S elapsed"
msgstr "%1$S %2$S telt el"

#. # LOCALIZATION NOTE (timeElapsedDouble):
#. # %1$S time number; %2$S time unit; %3$S time sub number; %4$S time sub unit
#. # example: 11 hours, 2 minutes elapsed; 1 day, 22 hours elapsed
#: timeElapsedDouble
msgid "%1$S %2$S, %3$S %4$S elapsed"
msgstr "%1$S %2$S, %3$S %4$S telt el"

#. # LOCALIZATION NOTE (sizeSpeed):
#. # %1$S is transfer progress; %2$S download speed
#. # example: 1.1 of 11.1 GB (2.2 MB/sec)
#: sizeSpeed
msgid "%1$S (%2$S)"
msgstr "%1$S (%2$S)"

#. # LOCALIZATION NOTE (statusActive): — is the "em dash" (long dash)
#. # %1$S download status; %2$S time remaining
#. # example: Paused — 11 hours, 2 minutes remaining
#: statusActive
msgid "%1$S — %2$S"
msgstr "%1$S — %2$S"

#: fromSource
msgid "From %S"
msgstr "Innen: %S"

#: toTarget
msgid "To %S"
msgstr "Ide: %S"

#: fileExecutableSecurityWarning
msgid ""
"\"%S\" is an executable file. Executable files may contain viruses or other "
"malicious code that could harm your computer. Use caution when opening this "
"file. Are you sure you want to launch \"%S\"?"
msgstr ""
"„%S” fájl végrehajtható. A végrehajtható fájlok vírusokat vagy más, "
"rosszindulatú kódokat tartalmazhatnak, amelyek kárt okozhatnak a "
"számítógépen. Legyen óvatos a fájl megnyitásakor. Biztosan elindítja: „%S”?"

#: fileExecutableSecurityWarningTitle
msgid "Open Executable File?"
msgstr "Megnyitja a végrehajtható fájlt?"

#: fileExecutableSecurityWarningDontAsk
msgid "Don't ask me this again"
msgstr "Ne kérdezze meg többet"
