#: pref.mouseWheel.title
msgid "Mouse Wheel"
msgstr "Egérgörgő"

#: mouseWheelPanel.label
msgid "Specify mouse wheel behavior when used with this modifier key: "
msgstr ""
"Válassza ki a görgő viselkedését, ha az alábbi módosító billentyűvel együtt "
"használják: "

#: usingJustTheWheel.label
msgid "No modifier key"
msgstr "Nincs módosító billentyű"

#: usingWheelAndAlt.label
msgid "&altKey.label;"
msgstr "&altKey.label;"

#: usingWheelAndCtrl.label
msgid "&ctrlKey.label;"
msgstr "&ctrlKey.label;"

#: usingWheelAndShft.label
msgid "&shiftKey.label;"
msgstr "&shiftKey.label;"

#: mouseWheelGroup.label
msgid "Vertical scrolling"
msgstr "Függőleges görgetés"

#: mouseWheelHorizGroup.label
msgid "Horizontal scrolling"
msgstr "Vízszintes görgetés"

#: sameAsVertical.label sameAsVertical.accesskey
msgid "Same as &vertical scrolling"
msgstr "Megegyezik a függőleges görgetéssel"

#: doNothing.label doNothing.accesskey
msgid "&Do nothing"
msgstr "&Nincs művelet"

#: doNothingHoriz.accesskey
msgid "n"
msgstr "i"

#: scrollDocument.label scrollDocument.accesskey
msgid "&Scroll the document"
msgstr "Dokumentum &görgetése"

#: scrollDocumentHoriz.accesskey
msgid "c"
msgstr "r"

#: history.label history.accesskey
msgid "&Move back and forward in the browsing history"
msgstr "&Előre/Vissza az előzmények közt"

#: historyHoriz.accesskey
msgid "b"
msgstr "l"

#: zoom.label zoom.accesskey
msgid "&Zoom the page in or out"
msgstr "&Nagyítás és kicsinyítés"

#: zoomHoriz.accesskey
msgid "o"
msgstr "k"

#: wheelSpeed.label wheelSpeed.accesskey
msgid "Mouse &wheel speed:"
msgstr "Egérgörgő-se&besség"

#: wheelSpeedHoriz.accesskey
msgid "u"
msgstr "e"

#: reverseDirection.label reverseDirection.accesskey
msgid "&Reverse direction"
msgstr "F&ordított irány"

#: reverseDirectionHoriz.accesskey
msgid "e"
msgstr "t"
