#. # args: host
#: accessingServerFailedError.text
msgid "Cannot access server %1$S!"
msgstr "A(z) %1$S kiszolgáló nem érhető el!"

#: loginFailed.text
msgid "Login failed or invalid session Id."
msgstr "Sikertelen bejelentkezés vagy érvénytelen munkamenet-azonosító"

#: accessDenied.text
msgid "The user is denied access."
msgstr "A felhasználó hozzáférése megtagadva"

#. # args: host
#: noHttpsConfirmation.text
msgid ""
"Insecure login on %1$S!\n"
"Continue?"
msgstr ""
"Nem biztonságos belépés: %1$S!\n"
"Folytatja?"

#: noHttpsConfirmation.check.text
msgid "Don't ask again."
msgstr "Ne kérdezze meg újra."

#: noHttpsConfirmation.label
msgid "Warning!"
msgstr "Vigyázat! "

#. # args: host, prodId, serverVersion, wcapVersion
#: insufficientWcapVersionConfirmation.text
msgid ""
"Server %1$S (%2$S, v%3$S, WCAP v%4$S) doesn't support a sufficient WCAP "
"version! The required version is at least 3.0.0.\n"
"Continue?"
msgstr ""
"A kiszolgáló (%1$S, %2$S, v%3$S, WCAP v%4$S) nem támogatja a megfelelő WCAP-"
"verziót!A szükséges verzió minimum 3.0.0.\n"
"Folytatja?"

#: insufficientWcapVersionConfirmation.label
msgid "Insufficient WCAP version!"
msgstr "Nem elég magas WCAP-verzió!"

#: loginDialog.label
msgid "Calendar Server Password Required"
msgstr "Jelszó szükséges a naptárkiszolgálóhoz"

#: privateItem.title.text
msgid "Private"
msgstr "Magánjellegű"

#: confidentialItem.title.text
msgid "Confidential"
msgstr "Bizalmas"

#: busyItem.title.text
msgid "Busy"
msgstr "Elfoglalt"
