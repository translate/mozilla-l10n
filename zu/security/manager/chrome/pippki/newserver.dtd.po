# <!-- Values for newserver.xul -->
#: newserver.title
msgid "Website Certified by an Unknown Authority"
msgstr "Isiza se-web esiqinisekiswe ngumphathi ongaziwa"

#: newserver.reasons
msgid "Possible reasons for this error:"
msgstr "Izizathu okungaba zidale leli phutha:"

#: newserver.reason1
msgid ""
"- Your browser does not recognize the Certificate Authority that issued the "
"site's certificate."
msgstr "- Umcingi akamazi Umphathi wesitifiketi onikeze isitifiketi sesiza."

#: newserver.reason2
msgid ""
"- The site's certificate is incomplete due to a server misconfiguration."
msgstr ""
"- Isitifiketi sesiza asiqediwe ngenxa yokuhlanganiselwe okungaqondile "
"komlekeleli."

#: newserver.notify
msgid "Please notify the site's webmaster about this problem."
msgstr "Siza wasize umphathi we-web wesiza mayelana nale nkinga."

#: newserver.examine
msgid "Examine Certificate..."
msgstr "Hlola Isitifiketi..."

#: newserver.remember
msgid "Accept this certificate permanently"
msgstr "Amukela lesi sitifiketi unomphela"

#: newserver.session
msgid "Accept this certificate temporarily for this session"
msgstr "Amukela lesi sitifiketi okwesikhashana ngalesi siqendu"

#: newserver.refuse
msgid "Do not accept this certificate and do not connect to this Web site"
msgstr ""

