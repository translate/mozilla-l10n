msgstr "Imininingwane Yesitifiketi"

#: certmgr.certdetail.cn
msgid "Common Name (CN)"
msgstr "Igama Elivamile (CN)"

#: certmgr.certdetail.o
msgid "Organization (O)"
msgstr "Inhlangano (O)"

#: certmgr.certdetail.ou
msgid "Organizational Unit (OU)"
msgstr "Ingxenye Yenhlangano (OU)"

#: certmgr.certdetail.serialnumber
msgid "_: certmgr.certdetail.serialnumber\n"
"Serial Number"
msgstr "Inombolo Yaso"

#: certmgr.certdetail.sha1fingerprint
msgid "SHA1 Fingerprint"
msgstr "Isigxivizo somunwe SHA1"

#: certmgr.certdetail.md5fingerprint
msgid "MD5 Fingerprint"
msgstr "Isigxivizo somunwe MD5"

#: certmgr.editcert.title
msgid "Edit Security Certificate Settings"
msgstr "Lungisa Ukuvikeleka Kokuhlelwa Kwesitifiketi"

#: certmgr.editcacert.title
msgid "Edit CA certificate trust settings"
msgstr "Lungisa ukuhlelwa kokwethembeka kwesitifiketi se-CA"

#: certmgr.editsslcert.title
msgid "Edit web site certificate trust settings"
msgstr "Lungisa ukuhlelwa kokwethembeka kwesifitiketi sengosi yewebhu"

#: certmgr.editcert.edittrust
msgid "Edit trust settings:"
msgstr "Lungisa ukuhlelwa kokwethembeka:"

#: certmgr.editcert.trustssl
msgid "This certificate can identify web sites."
msgstr "Lesi sitifiketi singahlonza izingosi zewebhu."

#: certmgr.editcert.trustemail
msgid "This certificate can identify mail users."
msgstr "Lesi sitifiketi singahlonza abasebenzisa iposi."

#: certmgr.editcert.trustobjsign
msgid "This certificate can identify software makers."
msgstr "Lesi sitifiketi singahlonza abenza izinhlaka ezisetshenziswayo."

#: certmgr.editsslcert.edittrust
msgid "_: certmgr.editsslcert.edittrust\n"
"Edit certificate trust settings:"
msgstr "Lungisa ukuhlelwa kokwethembeka kwesitifiketi:"

#: certmgr.editsslcert.dotrust
msgid "_: certmgr.editsslcert.dotrust\n"
"Trust the authenticity of this certificate."
msgstr "Themba ubuqiniso balesi sitifiketi."

#: certmgr.editsslcert.donttrust
msgid "_: certmgr.editsslcert.donttrust\n"
"Do not trust the authenticity of this certificate."
msgstr "Ungabethembi ubuqiniso balesi sitifiketi."

#: certmgr.editemailcert.title
msgid "Edit email certificate trust settings"
msgstr "Lungisa ukuhlelwa kokwethembeka kwe-imeyili yalesi sitifiketi"

#: certmgr.editemailcert.edittrust
msgid "_: certmgr.editemailcert.edittrust\n"
"Edit certificate trust settings:"
msgstr "Lungisa ukuhlelwa kokwethembeka kwesitifiketi:"

#: certmgr.editemailcert.dotrust
msgid "_: certmgr.editemailcert.dotrust\n"
"Trust the authenticity of this certificate."
msgstr "Themba ubuqiniso balesi sitifiketi."

#: certmgr.editemailcert.donttrust
msgid "_: certmgr.editemailcert.donttrust\n"
"Do not trust the authenticity of this certificate."
msgstr "Ungabethembi ubuqiniso balesi sitifiketi."

#: certmgr.deletecert.title
msgid "Delete Certificate"
msgstr "Cisha Isitifiketi"

#: certmgr.deletecert.beforename
msgid "You have requested to delete this certificate:"
msgstr "Ucele ukucisha lesi sitifiketi:"

#: certmgr.deletecert.aftername
msgid "Are you sure you want to delete this certificate?"
msgstr "Uyaqiniseka yini ukuthi ufuna ukusicisha lesi sitifiketi?"

#: certmgr.deleteusercert.title
msgid "Delete Your Certificate"
msgstr "Cisha Isitifiketi Sakho"

#: certmgr.deleteusercert.beforename
msgid "You have requested to delete the certificate:"
msgstr "Ucele ukucisha lesi sitifiketi:"

#: certmgr.deleteusercert.aftername
msgid "Once you have deleted this certificate, you will not be able to read mail that has been encrypted with it."
msgstr "Uma ususicishile lesi sitifiketi, ngeke usakwazi ukufunda iposi elibhalwe ngombhalo waso ocashile."

#: certmgr.certname
msgid "Certificate Name"
msgstr "Igama Lesitifiketi"

#: certmgr.tokenname
msgid "Security Device"
msgstr "Okuyisivikelo"

#: certmgr.purpose
msgid "Purposes"
msgstr "Izinjongo"

#: certmgr.issued
msgid "Issued On"
msgstr "Sikhishwe Mhlaka"

#: certmgr.expires
msgid "Expires On"
msgstr "Siphelelwa Yisikhathi Mhlaka"

#: certmgr.email
msgid "E-Mail Address"
msgstr "Ikheli Le-imeyili"

#: certmgr.serial
msgid "_: certmgr.serial\n"
"Serial Number"
msgstr "Inombolo Yaso"

#: certmgr.ocsp_info
msgid "Certificates have not been validated with OCSP. Click View to do so."
msgstr "Izitifiketi azenziwanga zasebenza nge-OCSP. Chofoza okuthi Bheka ukuze wenze kanjalo."

#: certmgr.close.label
msgid "Close"
msgstr "Vala"

#: certmgr.view.label
msgid "View"
msgstr "Bona"

#: certmgr.edit.label
msgid "Edit"
msgstr "Lungisa"

#: certmgr.editca.label
msgid "Edit CA Trust"
msgstr "Lungisa Ukwethembeka kwe-CA"

#: certmgr.add.label
msgid "Add"
msgstr "Nezela"

#: certmgr.delete.label
msgid "Delete"
msgstr "Cisha"

#: certmgr.backup.label
msgid "Backup"
msgstr "Gcina Enye Ikhophi"

#: certmgr.backupall.label
msgid "Backup All"
msgstr "Gcina Wonke Amakhophi"

#: certmgr.restore.label
msgid "Import"
msgstr "Yamukela"

#: certmgr.details.label
msgid "Certificate Fields"
msgstr "Imikhakha Yesitifiketi"

#: certmgr.fields.label
msgid "Field Value"
msgstr "Inani Lomkhakha"

