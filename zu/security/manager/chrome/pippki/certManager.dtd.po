msgstr "Unezitifiketi efayeleni ezihlonza laba bantu:"

#: certmgr.websites
msgid "You have certificates on file that identify these web sites:"
msgstr "Unezitifiketi efayeleni ezihlonza lezi zingosi zewebhu:"

#: certmgr.cas
msgid "You have certificates on file that identify these certificate authorities:"
msgstr "Unezitifiketi efayeleni ezihlonza la magunya ezitifiketi:"

#: certmgr.detail.general_tab.title
msgid "General"
msgstr "Okuvamile"

#: certmgr.detail.prettyprint_tab.title
msgid "Details"
msgstr "Imininingwane"

#: certmgr.subjectinfo.label
msgid "Issued To"
msgstr "Sikhishelwe U-"

#: certmgr.issuerinfo.label
msgid "Issued By"
msgstr "Sikhishwe Ngu-"

#: certmgr.validity.label
msgid "Validity"
msgstr "Ukusebenza Kwaso"

#: certmgr.fingerprints.label
msgid "Fingerprints"
msgstr "Izigxivizo zeminwe"

