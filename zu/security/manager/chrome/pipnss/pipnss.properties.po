msgid "%S = %S"
msgstr "%S = %S"

#: SSLDisabled
msgid "You cannot connect to %S because SSL is disabled."
msgstr "Ngeke ukwazi ukuxhuma kwi- %S ngenxa yokuthi i-SSL ikhubekisiwe."

#: SSL2Disabled
msgid "You cannot connect to %S because SSL version 2 is disabled."
msgstr ""
"Ngeke ukwazi ukuxhuma kwi%S ngenxa yokuthi inguqulo 2 ye-SSL ikhubekisiwe."

#: SSLNoMatchingCiphers
msgid ""
"%S and %S cannot communicate securely because they have no common encryption "
"algorithms."
msgstr ""
"I-%S kanye ne-%S azikwazi ukuxoxisana ngokuvikelekile ngenxa yokuthi "
"azinazindlela zokusebenza zokubhala ngokungaqondakali ezifanayo."

#: UsersCertRevoked
msgid ""
"Could not establish an encrypted connection with %S because your certificate "
"has been revoked."
msgstr ""
"Ayikwazanga ukusungula uxhumano olubhalwe ngokungaqondakali ne-%S ngenxa "
"yokuthi isitifiketi siphuciwe."

#: UsersCertExpired
msgid ""
"Could not establish an encrypted connection with %S because your certificate "
"is expired."
msgstr ""
"Ayikwazanga ukusungula uxhumano olubhalwe ngokungaqondakali ne-%S ngenxa "
"yokuthi isitifilketi sakho siphelelwe yisikhathi."

#: UsersCertRejected
msgid ""
"Could not establish an encrypted connection because your certificate was "
"rejected by %S. Error Code: %S."
msgstr ""
"Ayikwazanga ukusungula uxhumano olubhalwe ngokungaqondakali ngenxa yokuthi "
"isitifiketi sakho saliwe yi-%S. Iphutha lekhodi: %S."

#: BadMac
msgid ""
"%S received a message with incorrect Message Authentication Code. If the "
"error occurs frequently, contact the website administrator."
msgstr ""

#: PeerResetConnection
msgid "%S has closed the connection. Error Code: %S"
msgstr "I-%S isivale ukuxhumano. Iphutha Lekhodi: %S"

#: HostResetConnection
msgid "The current transfer has been cancelled."
msgstr "Ukudlulisa kwamanje kunqanyuliwe."

#: BadPassword
msgid "An incorrect password was provided."
msgstr "Igama lokudlula elingalungile linikeziwe."

#: BadDatabase
msgid "There is a problem with your certificate database [Error Code: %S]."
msgstr ""
"Kukhona inkinga ngesiqu solwazi lwezitifiketi zakho [Ikhodi lephutha: %S]."

#: BadServer
msgid "%S has sent an incorrect or unexpected message. Error Code: %S"
msgstr ""
"I-%S ithumele umlayezo okungesiwo noma ongalindelwe. Ikhodi lephutha: %S"

#: BadClient
msgid "%S has received an incorrect or unexpected message. Error Code: %S"
msgstr ""
"I-%S yamukele umlayezo okungesiwo noma ongalindelwe. Ikhodi Iphutha: %S"

#: HostReusedIssuerSerial
msgid ""
"You have received an invalid certificate.  Please contact the server "
"administrator or email correspondent and give them the following "
"information:\n"
"\n"
"Your certificate contains the same serial number as another certificate "
"issued by the certificate authority.  Please get a new certificate "
"containing a unique serial number."
msgstr ""

#: SSLGenericError
msgid "Error establishing an encrypted connection to %S. Error Code: %S."
msgstr ""
"Iphutha ekusunguleni uxhumano olubhalwe ngokungaqondakali oluya ku-%S. "
"Ikhodi lephutha: %S."

#: TimeOut
msgid "Connection request timed out."
msgstr "Isicelo soxhumana sesiphelwe yisikhathi."

#: PeersCertUntrusted
msgid ""
"Could not establish an encrypted connection because certificate presented by "
"%S is not trusted."
msgstr ""
"Ayikwazanga ukusungula uxhumano olubhalwe ngokungaqondakali ngenxa yokuthi "
"isitifikeli esilethwe yi-%S asithembekile."

#: PeersCertRevoked
msgid ""
"Could not establish an encrypted connection because certificate presented by "
"%S has been revoked."
msgstr ""
"Ayikwazanga ukusungula uxhumano olubhalwe ngokuqondakali ngenxa yokuthi "
"isitifiketi esilethwe yi-%S siphuciwe."

#: PeersCertExpired
msgid ""
"Could not establish an encrypted connection because certificate presented by "
"%S has expired."
msgstr ""
"Ayikwazanga ukusungula uxhumano olubhalwe ngokuqondakali ngenxa yokuthi "
"isitifiketi esilethwe yi-%S siphelelwe yisikhathi."

#: PeersCertWrongDomain
msgid ""
"Could not establish an encrypted connection because certificate presented by "
"%S is for a different domain."
msgstr ""
"Ayikwazanga ukusungula uxhumano olubhalwe ngokuqondakali ngenxa yokuthi "
"isitifiketi esilethwe yi-%S esendawo eyahlukile."

#: PeersCertBadSignature
msgid ""
"Could not establish an encrypted connection because certificate presented by "
"%S has an invalid signature."
msgstr ""
"Ayikwazanga ukusungula uxhumano olubhalwe ngokuqondakali ngenxa yokuthi "
"isitifiketi esilethwe yi-%S sinophawu lokusayina olungasebenzi."

#: PeersCertNoGood
msgid ""
"Could not establish an encrypted connection because certificate presented by "
"%S is invalid or corrupted. Error Code: %S"
msgstr ""
"Ayikwazanga ukusungula uxhumano olubhalwe ngokuqondakali ngenxa yokuthi "
"isitifiketi esilethwe yi-%S asisebenzi noma sonakele. Ikhodi lephutha: %S"

#: CRLExpired
msgid ""
"Certificate Revocation List (CRL) from the CA certifying %S is past its Next "
"Update date. Please update the CRL."
msgstr ""
"Certificate Revocation List (CRL) esuka kwi-CA eqinisekisa i-%S idlulile "
"usuku lwayo Lokulungiswa kabusha Olulandelayo. Siza ulungise kabusha i-CRL."

#: CRLNotYetValid
msgid ""
"Certificate Revocation List (CRL) from the CA certifying %S is not yet "
"valid. Please check your system clock."
msgstr ""
"I-Certificate Revocation List (CRL) esuka kwi-CA eqinisekisa i-%S "
"ayikakasebenzi. Siza ubheke iwashi lesistimu yakho."

#: CRLSigNotValid
msgid ""
"Certificate Revocation List (CRL) from the CA certifying %S has an invalid "
"digital signature."
msgstr ""
"Certificate Revocation List (CRL) esuka kwi-CA eqinisekisa i-%S inophawu "
"lokusayina oluyizinombolo."

#: CRLSNotValid
msgid ""
"Certificate Revocation List (CRL) from the CA certifying %S is not valid."
msgstr ""
"I-Certificate Revocation List (CRL) esuka kwi-CA eqinisekisa i-%S ayisebenzi."

#: OCSPMalformedRequest
msgid ""
"Error trying to validate certificate from %S using OCSP - malformed request."
msgstr ""
"Iphutha ekuzameni ukuqinisekisa isitifiketi esisuka kwi-%S usebenzisa i-OCSP "
"- isicelo esakhiwe kabi."

#: OCSPRequestNeedsSig
msgid ""
"Error trying to validate certificate from %S using OCSP - request needs "
"signature."
msgstr ""
"Iphutha ekuzameni ukuqinisekisa isitifiketi esisuka kwi-%S usebenzisa i-OCSP "
"- isicelo sidinga uphawu lokusayina."

#: OCSPUnauthorizedReq
msgid ""
"Error trying to validate certificate from %S using OCSP - unauthorized "
"request."
msgstr ""
"Iphutha ekuzameni ukuqinisekisa isitifiketi esisuka kwi-%S usebenzisa i-OCSP "
"- isicelo esingagunyaziwe."

#: OCSPServerError
msgid "Error trying to validate certificate from %S using OCSP - server error."
msgstr ""
"Iphutha ekuzameni ukuqinisekisa isitifiketi esisuka kwi-%S usebenzisa i-OCSP "
"- iphutha lomlekeleli."

#: OCSPTryServerLater
msgid ""
"Error trying to validate certificate from %S using OCSP - server is busy. "
"Please try again later."
msgstr ""
"Iphutha ekuzameni ukuqinisekisa isitifiketi esisuka kwi-%S usebenzisa i-OCSP "
"- umlekeleli umatasatasa. Siza uzame ngesinye isikhathi."

#: OCSPFutureResponse
msgid ""
"Error trying to validate certificate from %S using OCSP - response contains "
"a date which is in the future."
msgstr ""
"Iphutha ekuzameni ukuqinisekisa isitifiketi esisuka kwi-%S usebenzisa i-OCSP "
"- impendulo iqukethe usuku olusesikhathini esizayo."

#: OCSPOldResponse
msgid "Error trying to validate certificate from %S using OCSP - old response."
msgstr ""
"Iphutha ekuzameni ukuqinisekisa isitifiketi esisuka kwi-%S usebenzisa i-OCSP "
"- impendulo endala."

#: OCSPCorruptedResponse
msgid ""
"Error trying to validate certificate from %S using OCSP - corrupted or "
"unknown response. Error Code: %S."
msgstr ""
"Iphutha ekuzameni ukuqinisekisa isitifiketi esisuka kwi-%S usebenzisa i-OCSP "
"- impendulo eyonakele noma engaziwa. Ikhodi Lephutha: %S."

#: OCSPUnauthorizedResponse
msgid ""
"Error trying to validate certificate from %S using OCSP - unauthorized "
"response."
msgstr ""
"Iphutha ekuzameni ukuqinisekisa isitifiketi esisuka kwi-%S usebenzisa i-OCSP "
"- impendulo engagunyaziwe."

#: OCSPUnknownCert
msgid ""
"Error trying to validate certificate from %S using OCSP - unknown "
"certificate."
msgstr ""
"Iphutha ekuzameni ukuqinisekisa isitifiketi esisuka kwi-%S usebenzisa i-OCSP "
"- isitifiketiesingaziwa."

#: OCSPNoDefaultResponder
msgid ""
"Error trying to validate certificate from %S using OCSP - no default "
"responder specified."
msgstr ""
"Iphutha ekuzameni ukuqinisekisa isitifiketi esisuka kwi-%S usebenzisa i-OCSP "
"- akekho umphenduli wendalo ocacisiwe."

#: OCSPDirLookup
msgid ""
"Error trying to validate certificate from %S using OCSP - directory lookup "
