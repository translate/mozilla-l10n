# Map Expat error codes to error strings
#: 1
msgid "out of memory"
msgstr "ayinankumbulo"

#: 2
msgid "syntax error"
msgstr "iphutha lolimi"

#: 3
msgid "no element found"
msgstr "asikho isici esitholakele"

#: 4
msgid "not well-formed"
msgstr "ayakhekile kahle"

#: 5
msgid "unclosed token"
msgstr "uphawu olungavaliwe"

#: 6
msgid "partial character"
msgstr "uhlamvu oluyingxenye"

#: 7
msgid "mismatched tag"
msgstr "isigqebhezana esingafanele"

#: 8
msgid "duplicate attribute"
msgstr "imfanelo ephindekile"

#: 9
msgid "junk after document element"
msgstr "okungenanzuzo ngemva kwesici sedokhumenti"

#: 10
msgid "illegal parameter entity reference"
msgstr "ukubhekisela entweni yepharamitha engemthetho"

#: 11
msgid "undefined entity"
msgstr "into engachaziwe"

#: 12
msgid "recursive entity reference"
msgstr "ukubhekisela entweni okuphindwayo"

#: 13
msgid "asynchronous entity"
msgstr "into engahambisani"

#: 14
msgid "reference to invalid character number"
msgstr "ukubhekisela enombolweni engasebenzi"

#: 15
msgid "reference to binary entity"
msgstr "ukubhekisela entweni engambili"

#: 16
msgid "reference to external entity in attribute"
msgstr "ukubhekisela entweni yangaphandle emfanelweni"

#: 17
msgid "xml declaration not at start of external entity"
msgstr "isimemezelo se-xml asikho ekuqaleni kwento yangaphandle"

#: 18
msgid "unknown encoding"
msgstr "ukufaka ikhodi okungaziwa"

#: 19
msgid "encoding specified in XML declaration is incorrect"
msgstr "ukufaka ngekhodi okushiwo esimemezelweni se-XML akuqondile"

#: 20
msgid "unclosed CDATA section"
msgstr "ingxenye ye-CDATA engavaliwe"

#: 21
msgid "error in processing external entity reference"
msgstr "iphutha ekwenzeni ukubhekisela kwento yangaphandle"

#: 22
msgid "document is not standalone"
msgstr "le dokhumenti ayimile yodwa"

#: 23
msgid "unexpected parser state"
msgstr "isimo sokuchaza esingalindelekile"

#: 24
msgid "entity declared in parameter entity"
msgstr "into emenyezelwe ngento yepharamitha"

#: 27
msgid "prefix not bound to a namespace"
msgstr "isandulela asigcinile esikhaleni segama"

#: 29
msgid "reserved prefix (xml) must not be undeclared or bound to another namespace URI"
msgstr "isandulela esigciniwe (xml) akumelwe singamenyezelwa noma sigcine kwesinye isikhala segama se-URI"

#: 30
msgid "reserved prefix (xmlns) must not be declared or undeclared"
msgstr "isandulela esigciniwe (xmlns) akumelwe simenyezelwe noma singamenyezelwa"

#: 31
msgid "prefix must not be bound to one of the reserved namespace URIs"
msgstr "isandulela akufanele sigcinwe kwesinye sezikhala zamagama se-URI ezigciniwe"

# %1$S is replaced by the Expat error string, may be followed by Expected (see below)
# %2$S is replaced by URL
# %3$d is replaced by line number
# %4$d is replaced by column number
#: XMLParsingError
msgid "XML Parsing Error: %1$S\n"
"Location: %2$S\n"
"Line Number %3$d, Column %4$d:"
msgstr "Iphutha lokuchaza i-XML: %1$S\nIndawo: %2$S\nInombolo Yomugqa %3$d, Ikhalamu %4$d:"

# %S is replaced by a tag name.
# This gets appended to the error string if the error is mismatched tag.
#: Expected
msgid ". Expected: </%S>."
msgstr ". Okulindelekile: </%S>."

