#: MimeNotCss
msgid "The stylesheet %1$S was not loaded because its MIME type, \"%2$S\", is not \"text/css\"."
msgstr "Ishidi lesitayela %1$S alilayishwanga ngenxa yohlobo lwalo lwe-MIME, \"%2$S\", ayiyona i-\"text/css\"."

#: MimeNotCssWarn
msgid "The stylesheet %1$S was loaded as CSS even though its MIME type, \"%2$S\", is not \"text/css\"."
msgstr "Ishidi lesitayela %1$S lilayishwe njengo-CSS naphezu kohlobo lwalo lwe-MIME, \"%2$S\", ayiyona i-\"text/css\"."

#: PEUnexpEOF
msgid "Unexpected end of file while searching for %1$S."
msgstr "Ifayela liphele kungalindelekile lapho kucingwa i-%1$S."

#: PEParseRuleWSOnly
msgid "Whitespace-only string given to be parsed as rule."
msgstr "Intanjana yesikhala esimhlophe kuphela enikeziwe ukuze ichazwe njengomthetho."

#: PEDeclDropped
msgid "Declaration dropped."
msgstr "Isimemezelo siyekiwe."

#: PEDeclSkipped
msgid "Skipped to next declaration."
msgstr "Yeqele esimemezelweni esilandelayo."

#: PEUnknownProperty
msgid "Unknown property '%1$S'."
msgstr "Impahla engaziwa '%1$S'."

#: PEPropertyParsingError
msgid "Error in parsing value for property '%1$S'."
msgstr "Iphutha ekuchazeni inani lempahla '%1$S'."

#: PEExpectEndProperty
msgid "Expected end of value for property but found '%1$S'."
msgstr "Ukuphela okulindelekile kwenani lempahla kodwa kutholakale i-'%1$S'."

#: PESkipAtRuleEOF
msgid "end of unknown at-rule"
msgstr "ukuphela kokungaziwa emthethweni"

#: PEUnknownAtRule
msgid "Unrecognized at-rule or error parsing at-rule '%1$S'."
msgstr "Okungaqashelwe emthethweni noma iphutha ekuchazeni umthetho '%1$S'."

#: PECharsetRuleEOF
msgid "charset string in @charset rule"
msgstr "intanjana yeshasethi @charset rule"

#: PECharsetRuleNotString
msgid "Expected charset string but found '%1$S'."
msgstr "Intanjana yeshasethi elindelekile kodwa kwatholakala '%1$S'."

#: PEGatherMediaEOF
msgid "end of media list in @import or @media rule"
msgstr "ukuphela kohlu lwabezindaba @import noma @media rule"

# (review) please check
#: PEGatherMediaNotComma
msgid "Expected ',' in media list but found '%1$S'."
msgstr "Okulindelekile ',' ohlwini lwabezindaba kodwa kwatholakala '%1$S'."

#: PEGatherMediaNotIdent
msgid "Expected identifier in media list but found '%1$S'."
msgstr "Kulindeleke okubonisayo ohlwini lwabezindaba kodwa kwatholakala '%1$S'."

#: PEImportNotURI
msgid "Expected URI in @import rule but found '%1$S'."
msgstr "Kulindeleke i-URI @import rule kodwa kwatholakala '%1$S'."

#: PEImportUnexpected
msgid "Found unexpected '%1$S' within @import."
msgstr "Kutholakale okungalindelekile '%1$S' ngaphakathi @import."

#: PEGroupRuleEOF
msgid "end of @media or @-moz-document rule"
msgstr "ukuphela @media noma @-moz-document rule"

# (pofilter) functions: checks to see that function names are not translated: url-prefix() domain()
#: PEMozDocRuleBadFunc
msgid "Expected url(), url-prefix(), or domain() in @-moz-document rule but found '%1$S'."
msgstr "Kulindeleke i-url(), isandulela se-url(), noma isijobelelo somxhumi() ku @-moz-document rule kodwa kwatholakala '%1$S'."

#: PEMozDocRuleNotURI
msgid "Expected URI in @-moz-document rule but found '%1$S'."
msgstr "Kulindeleke i-URI ku @-moz-document rule kodwa kwatholakala '%1$S'."

#: PEAtNSPrefixEOF
msgid "namespace prefix in @namespace rule"
msgstr "isandulela sesikhala segama @namespace rule"

#: PEAtNSURIEOF
msgid "namespace URI in @namespace rule"
msgstr "isikhala segama se-URI @namespace rule"

#: PEAtNSUnexpected
msgid "Unexpected token within @namespace: '%1$S'."
msgstr "Ithokheni elingalindelekile ngaphakathi @namespace: '%1$S'."

#: PESkipDeclBraceEOF
msgid "closing } of declaration block"
msgstr "ukuvalwa } kwebhulokhi lesimemezelo"

#: PESkipRSBraceEOF
msgid "closing } of invalid rule set"
msgstr "ukuvalwa } kohlelo lomthetho olungasebenzi"

#: PEBadSelectorRSIgnored
msgid "Ruleset ignored due to bad selector."
msgstr "Uhlelo lomthetho alunakwanga ngenxa yokukhetha okubi."

#: PESelectorListExtraEOF
msgid "',' or '{'"
msgstr "',' noma '{'"

#: PESelectorListExtra
msgid "Expected ',' or '{' but found '%1$S'."
msgstr "Kulindeleke ',' noma '{' kodwa kwatholakala '%1$S'."

#: PESelectorGroupNoSelector
msgid "Selector expected."
msgstr "Kulindeleke okokukhetha."

#: PESelectorGroupExtraCombinator
msgid "Dangling combinator."
msgstr "Into yokuhlanganisa okulengayo."

#: PEClassSelEOF
msgid "class name"
msgstr "igama lekilasi"

#: PEClassSelNotIdent
msgid "Expected identifier for class selector but found '%1$S'."
msgstr "Kulindeleke okubonisayo kokukhetha kwekilasi kodwa kwatholakala '%1$S'."

#: PETypeSelEOF
msgid "element type"
msgstr "uhlobo lwesici"

#: PETypeSelNotType
msgid "Expected element name or '*' but found '%1$S'."
msgstr "Igama lesici esilindelekile noma '*' kodwa kwatholakala '%1$S'."

#: PEUnknownNamespacePrefix
msgid "Unknown namespace prefix '%1$S'."
msgstr "Isandulela sesikhala segama esingaziwa '%1$S'."

#: PEAttributeNameEOF
msgid "attribute name"
msgstr "imfanelo yegama"

#: PEAttributeNameExpected
msgid "Expected identifier for attribute name but found '%1$S'."
msgstr "Kulindeleke okubonisayo kwegama lemfanelo kodwa kwatholakala '%1$S'."

#: PEAttributeNameOrNamespaceExpected
msgid "Expected attribute name or namespace but found '%1$S'."
msgstr "Kulindeleke igama lemfanelo kodwa isikhala segama kodwa kwatholakala '%1$S'."

#: PEAttSelNoBar
msgid "Expected '|' but found '%1$S'."
msgstr "Kulindeleke '|' kodwa kwatholakala '%1$S'."

#: PEAttSelInnerEOF
msgid "part of attribute selector"
msgstr "ingxenye yokokukhetha imfanelo"

#: PEAttSelUnexpected
msgid "Unexpected token in attribute selector: '%1$S'."
msgstr "Ithokheni engalindelekile kokokukhetha imfanelo: '%1$S'."

#: PEAttSelValueEOF
msgid "attribute value"
msgstr "inani lemfanelo"

#: PEAttSelCloseEOF
msgid "']' to end attribute selector"
msgstr "']' ukuqeda okokukhetha imfanelo"

#: PEAttSelNoClose
msgid "Expected ']' to terminate attribute selector but found '%1$S'."
msgstr "Kulindeleke ']' ukuze iqede okokukhetha imfanelo kodwa kwatholakala '%1$S'."

#: PEAttSelBadValue
msgid "Expected identifier or string for value in attribute selector but found '%1$S'."
msgstr "Kulindeleke okubonisayo noma intanjana yenani kokokukhetha imfanelo kodwa kwatholakala '%1$S'."

#: PEPseudoSelEOF
msgid "name of pseudo-class or pseudo-element"
msgstr "igama lekilasi lamanga noma isici samanga"

#: PEPseudoSelBadName
msgid "Expected identifier for pseudo-class or pseudo-element but found '%1$S'."
msgstr "Kulindeleke okubonisayo kwekilasi lamanga noma isici samanga kodwa kwatholakala '%1$S'."

#: PEPseudoSelNonFunc
msgid "Function token for non-function pseudo-class or pseudo-element, or the other way around, when reading '%1$S'."
msgstr "Ithokheni lesenzo lekilasi lamanga elingasebenzi noma isici samanga, noma ngale enye indlela, uma ufunda '%1$S'."

#: PEPseudoSelNotPE
msgid "Expected pseudo-element but found '%1$S'."
msgstr "Kulindeleke isici samanga kodwa kwatholakala '%1$S'."

#: PEPseudoSelDoubleNot
msgid "Negation pseudo-class can't be negated '%1$S'."
msgstr "Ikilasi lamanga eliphikisayo ngeke liphikiswe '%1$S'."

#: PEPseudoSelPEInNot
msgid "Pseudo-elements can't be negated '%1$S'."
msgstr "Izici zamanga ngeke ziphikiswe '%1$S'."

#: PEPseudoSelNewStyleOnly
msgid "This pseudo-element must use the \"::\" form: '%1$S'."
msgstr "Lesi sici samanga kumelwe sisebenzise \"::\" isakhiwo: '%1$S'."

#: PEPseudoSelTrailing
msgid "Found trailing token after pseudo-element, which must be the last part of a selector:  '%1$S'."
msgstr "Kutholakale ithokheni elilengayo ngemva kwesici samanga, okufanele sibe ingxenye yokugcina yokokukhetha:  '%1$S'."

#: PEPseudoSelMultiplePE
msgid "Extra pseudo-element '%1$S'."
msgstr "Isici samanga esengeziwe '%1$S'."

#: PEPseudoSelUnknown
msgid "Unknown pseudo-class or pseudo-element '%1$S'."
msgstr "Ikilasi lamanga elingaziwa noma isici samanga '%1$S'."

#: PENegationEOF
msgid "selector within negation"
msgstr "umkhethi ongaphakathi ekuphikiseni"

#: PENegationBadInner
msgid "Malformed simple selector as negation pseudo-class argument '%1$S'."
msgstr "Umkhethi ovamile ongakhekile kahle ekuphikiseni impikiswano yekilasi lamanga '%1$S'."

#: PENegationNoClose
msgid "Missing closing ')' in negation pseudo-class '%1$S'."
msgstr "Akukho okuvalayo ')' ekuphikisaneni nekilasi lamanga '%1$S'."

#: PENegationBadArg
msgid "Missing argument in negation pseudo-class '%1$S'."
msgstr "Ayikho impikiswano okungaphikiswa ngayo ikilasi lamanga '%1$S'."

#: PELangArgEOF
msgid "argument to :lang selector"
msgstr "impikiswano :umkhethi wolimi"

#: PELangArgNotIdent
msgid "Expected identifier for lang pseudo-class parameter but found '%1$S'."
msgstr "Bekulindeleke okuzobonisa izici zekilasi lamanga lolimi kodwa kwatholakala '%1$S'."

#: PELangNoClose
msgid "Missing closing ')' in lang pseudo-class '%1$S'."
msgstr "Akukho okuvalayo ')' ekilasini lamanga lolimi '%1$S'."

#: PELangNoArg
msgid "Missing argument in lang pseudo-class '%1$S'."
msgstr "Ayikho impikiswano ekilasini lamanga lolimi '%1$S'."

#: PESelectorEOF
msgid "selector"
msgstr "umkhethi"

#: PEBadDeclBlockStart
msgid "Expected '{' to begin declaration block but found '%1$S'."
msgstr "Bekulindeleke '{' ukuze kuqalwe ibhulokhi yesimemezelo kodwa kwatholakala '%1$S'."

#: PEColorEOF
msgid "color"
msgstr "umbala"

#: PEColorNotColor
msgid "Expected color but found '%1$S'."
msgstr "Umbala olindelekile kodwa kwatholakala '%1$S'."

#: PEColorComponentEOF
msgid "color component"
msgstr "okwakhe umbala"

#: PEExpectedPercent
msgid "Expected a percentage but found '%1$S'."
msgstr "Iphesenti elilindelekile kodwa kutholakale '%1$S'."

#: PEExpectedInt
msgid "Expected an integer but found '%1$S'."
msgstr "Bekulindeleke inombolo ephelele kodwa kwatholakala '%1$S'."

#: PEColorBadRGBContents
msgid "Expected number or percentage in rgb() but found '%1$S'."
msgstr "Bekulindeleke inombolo noma iphesenti ku-rgb() kodwa kwatholakala '%1$S'."

#: PEColorComponentBadTerm
msgid "Expected '%2$S' but found '%1$S'."
msgstr "Bekulindeleke '%2$S' kodwa kwatholakala '%1$S'."

#: PEColorHueEOF
msgid "hue"
msgstr "umbala"

#: PEExpectedComma
msgid "Expected ',' but found '%1$S'."
msgstr "Bekulindeleke ',' kodwa kwatholakala '%1$S'."

#: PEColorSaturationEOF
msgid "saturation"
msgstr "sekufike ekugcineni"

#: PEColorLightnessEOF
msgid "lightness"
msgstr "kulula"

#: PEColorOpacityEOF
msgid "opacity in color value"
msgstr "ayiwudlulisi umbala"

#: PEExpectedNumber
msgid "Expected a number but found '%1$S'."
msgstr "Bekulindeleke inombolo kodwa kwatholakala '%1$S'."

#: PEExpectedCloseParen
msgid "Expected ')' but found '%1$S'."
msgstr "Bekulindeleke ')' kodwa kwatholakala '%1$S'."

#: PEDeclEndEOF
msgid "';' or '}' to end declaration"
msgstr "';' noma '}' ukuqeda isimemezelo"

#: PEParseDeclarationNoColon
msgid "Expected ':' but found '%1$S'."
msgstr "Bekulindeleke ':' kodwa kwatholakala '%1$S'."

#: PEParseDeclarationDeclExpected
msgid "Expected declaration but found '%1$S'."
msgstr "Bekulindeleke isimemezelo kodwa kwatholakala '%1$S'."

#: PEEndOfDeclEOF
msgid "end of declaration"
msgstr "ukuphela kwesimemezelo"

#: PEImportantEOF
msgid "important"
msgstr "kubalulekile"

#: PEExpectedImportant
msgid "Expected 'important' but found '%1$S'."
msgstr "Bekulindeleke 'okubalulekile' kodwa kwatholakala '%1$S'."

#: PEBadDeclEnd
msgid "Expected ';' to terminate declaration but found '%1$S'."
msgstr "Bekulindeleke ';' ukuba kuphelise isimemezelo kodwa kwatholakala '%1$S'."

#: PEBadDeclOrRuleEnd
msgid "Expected ';' or '}' to terminate declaration but found '%1$S'."
msgstr "Bekulindeleke ';' noma '}' ukuba kuphelise isimemezelo kodwa kwatholakala '%1$S'."

#: PEInaccessibleProperty
msgid "Attempt to use inaccessible property"
msgstr "Ukuzama ukusebenzisa impahla okungafinyeleleki kuyo"

#: PECommentEOF
msgid "end of comment"
msgstr "ukuphela kwenkulumo"

#: SEUnterminatedString
msgid "Found unclosed string '%1$S'."
msgstr "Kutholakale intanjana engavaliwe '%1$S'."

