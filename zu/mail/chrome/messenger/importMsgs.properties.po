#Error strings
#: ImportAlreadyInProgress
msgid "An import operation is currently in progress. Try again when the current import has finished."
msgstr "Kukhona okuthunyelwayo. Phinda uzame lapho sekuqediwe."

#Error strings for settings import
#: ImportSettingsBadModule
msgid "Unable to load settings module"
msgstr "Ayikwazi ukufaka ukwaziswa kokuhlela"

#: ImportSettingsNotFound
msgid "Unable to find settings.  Check to make sure the application is installed on this machine."
msgstr "Ayikwazi ukuthola ukuhlelwa kwezinto.  Hlola ukuze uqiniseke ukuthi indlela esetshenziswayo ifakiwe kulomshini."

#: ImportSettingsFailed
msgid "An error occurred while importing settings.  Some, or all, of the settings may not have been imported."
msgstr "Kwenzeke iphutha lapho kuthunyelwa ukuhlelwa kwezinto.  Okunye, noma konke ukuhlelwa kungenzeka ukuthi akuthunyelwanga."

# LOCALIZATION NOTE : Do not translate the word "%S" below.
#: ImportSettingsSuccess
msgid "Settings were imported from %S"
msgstr "Ukuhlelwa kuthunyelwe kususelwa e-%S"

#Error string for mail import
#: ImportMailBadModule
msgid "Unable to load mail import module"
msgstr "Ayikwazi ukufaka ukwaziswa kokuthumela iposi"

#: ImportMailNotFound
msgid "Unable to find mail to import.  Check to make sure the mail application is correctly installed on this machine."
msgstr "Ayikwazi ukuthola iposi elizothunyelwa.  Hlola ukuze uqiniseke ukuthi indlela esetshenziswa eposini ifakwe kahle kulomshini."

#: ImportEmptyAddressBook
msgid "Can't import empty address book %S."
msgstr "Ayikwazi ukuthumela incwadi yamakheli I-%S engenalutho."

# LOCALIZATION NOTE : Do not translate the word "%S" below.
#: ImportMailFailed
msgid "An error occurred importing mail from %S"
msgstr "Kwenzeke iphutha lapho kuthunyelwa iposi lisuka e-%S"

# LOCALIZATION NOTE : Do not translate the word "%S" below.
#: ImportMailSuccess
msgid "Mail was successfully imported from %S"
msgstr "Iposi lithunyelwe ngempumelelo lisuka e-%S"

# Error string for address import
#: ImportAddressBadModule
msgid "Unable to load address book import module."
msgstr "Ayikwazi ukufaka ukwaziswa kwencwadi yamakheli."

#: ImportAddressNotFound
msgid "Unable to find any address books to import.  Check to make sure the selected application or format is correctly installed on this machine."
msgstr "Ayikwazi ukuthola noma iyiphi incwadi yamakheli yokuyithumela.  Hlola ukuze uthole ukuthi ukwaziswa kwendlela ekhethiwe kufakwe kahle kulomshini."

# LOCALIZATION NOTE : Do not translate the word "%S" below.
#: ImportAddressFailed
