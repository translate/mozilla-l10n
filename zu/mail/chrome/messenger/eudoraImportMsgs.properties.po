#
# The following are used by the outlook express import code to display status/error 
# and informational messages
#
# Short name of import module
## @name EUDORAIMPORT_NAME
## @loc None
#: 2000
msgid "Eudora"
msgstr "Eudora"

# Description of import module
## @name EUDORAIMPORT_DESCRIPTION
## @loc None
#: 2001
msgid "Eudora mail and address books"
msgstr "Imeyili ye-Eudora nezincwadi zamakheli"

# Success message
## @name EUDORAIMPORT_MAILBOX_SUCCESS
## @loc None
# LOCALIZATION NOTE (2002): In the following sentence,
# the %S represents a string to be inserted at runtime (the name of the Mailbox), 
# and the %d is a number (the number of messages imported). Do not translate %d or %S, but
# instead insert them in your text at the appropriate places.
#: 2002
msgid "Mailbox %S, imported %d messages"
msgstr "Ibhokisi leposi %S, imiyalezo eyamukelwe ye-%d"

# Error message
## @name EUDORAIMPORT_MAILBOX_BADPARAM
## @loc None
#: 2003
msgid "Bad parameter passed to import mailbox."
msgstr "Kudluliswe isilinganiso esingaqondile ukuze kwamukelwe ibhokisi leposi."

# Error message
## @name EUDORAIMPORT_MAILBOX_BADSOURCEFILE
## @loc None
# LOCALIZATION NOTE (2004): In the following sentence, do not translate the "%S". Instead,
# place it in your sentence where you wish to display the name of the mailbox.
#: 2004
msgid "Error accessing file for mailbox %S."
msgstr "Iphutha ekuvuleni ifayela lebhokisi leposi %S."

# Error message
## @name EUDORAIMPORT_MAILBOX_CONVERTERROR
## @loc None
# LOCALIZATION NOTE (2005): In the following sentence, do not translate the "%S". Instead,
# place it in your sentence where you wish to display the name of the mailbox.
#: 2005
msgid "Error importing mailbox %S, all messages may not be imported from this mailbox."
msgstr "Kwenzeke iphutha lapho kwamukelwa ibhokisi leposi i-%S, kungenzeka ukuthi ayamukelwanga yonke imiyalezo kuleli bhokisi."

# Description
## @name EUDORAIMPORT_ACCOUNTNAME
## @loc None
# LOCALIZATION NOTE (2006): Do not translate "Eudora" below.
#: 2006
msgid "Eudora Settings"
msgstr "Ukuhlelwa Kwe-Eudora"

# Description
## @name EUDORAIMPORT_NICKNAMES_NAME
## @loc None
# LOCALIZATION NOTE (2007): Do not translate "Eudora" below.
#: 2007
msgid "Eudora Nicknames"
msgstr "Izidlaliso Ze-Eudora"

# Description
## @name EUDORAIMPORT_ADDRESS_SUCCESS
## @loc None
# LOCALIZATION NOTE (2008): In the following sentence, do not translate the "%S". Instead,
# place it in your sentence where you wish to display the name of the address book.
#: 2008
msgid "Imported address book %S"
msgstr "Incwadi yamakheli eyamukelwe i-%S"

# Error message
## @name EUDORAIMPORT_ADDRESS_BADPARAM
## @loc None
#: 2009
msgid "Bad parameter passed to import address book."
msgstr "Kudluliswe isilinganiso esingaqondile ukuze kwamukelwe incwadi yamakheli."

# Error message
## @name EUDORAIMPORT_ADDRESS_BADSOURCEFILE
## @loc None
# LOCALIZATION NOTE (2010): In the following sentence, do not translate the "%S". Instead,
# place it in your sentence where you wish to display the name of the address book.
#: 2010
msgid "Error accessing file for address book %S."
msgstr "Iphutha ekuvuleni ifayela lencwadi yamakheli %S."

# Error message
## @name EUDORAIMPORT_ADDRESS_CONVERTERROR
## @loc None
# LOCALIZATION NOTE (2011): In the following sentence, do not translate the "%S". Instead,
# place it in your sentence where you wish to display the name of the address book.
#: 2011
msgid "Error importing address book %S, all addresses may not have been imported."
msgstr "Iphutha ekwamukeleni incwadi yamakheli %S, kungenzeka awamukelwanga wonke amakheli."

