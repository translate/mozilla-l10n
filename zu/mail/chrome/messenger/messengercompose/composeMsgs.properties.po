## @name NS_MSG_UNABLE_TO_OPEN_FILE 
#: 12500
msgid "Unable to open the file %P0%."
msgstr "Ayikwazi ukuvula ifayela %PO%."

## @name NS_MSG_UNABLE_TO_OPEN_TMP_FILE
#: 12501
msgid "Unable to open the temporary file %P0%. Check your 'Temporary Directory' setting."
msgstr "Ayikwazi ukuvula ifayela lesikhashana i-%P%. Bheka uhlelo lwakho 'Lwesiqondiso Sesikhashana.'"

## @name NS_MSG_UNABLE_TO_SAVE_TEMPLATE
#: 12502
msgid "Unable to save your message as template."
msgstr "Ayikwazi ukugcina umyalezo wakho njengozosebenza njalo."

## @name NS_MSG_UNABLE_TO_SAVE_DRAFT
#: 12503
msgid "Unable to save your message as draft."
msgstr "Ayikwazi ukugcina umyalezo wakho njenge-draft."

## @name NS_MSG_LOAD_ATTACHMNTS
#: 12504
msgid "Loading attachments..."
msgstr "Ifaka amafayela ananyatheliswayo..."

## @name NS_MSG_LOAD_ATTACHMNT
#: 12505
msgid "Loading attachment..."
msgstr "Ifaka ifayela elinanyatheliswayo..."

## @name NS_MSG_COULDNT_OPEN_FCC_FOLDER
#: 12506
msgid "Couldn't open Sent Mail folder. Please verify that your Mail preferences are correct."
msgstr "Ayikwazi ukuvula Ifolda ethunyelwe ngeposi. Siza uqiniseke ukuthi izinto ozisebenzisile ePosini lakho ziqondile."

## @name NS_MSG_CANT_POST_TO_MULTIPLE_NEWS_HOSTS
#: 12507
msgid "Can't post to multiple news hosts. Try again."
msgstr "Ayikwazi ukuthumela kubantu abaningi. Phinda uzame."

## @name NS_MSG_ASSEMB_DONE_MSG
#: 12508
msgid "Assembling message...Done"
msgstr "Iqoqa imiyalezo... Kwenziwe"

## @name NS_MSG_ASSEMBLING_MSG
#: 12509
msgid "Assembling message..."
msgstr "Iqoqa imiyalezo..."

## @name NS_MSG_NO_SENDER
#: 12510
msgid "No sender was specified. Please fill in your email address in the Mail & Newsgroups account settings."
msgstr "Akushiwongo ukuthi uthunyelwe kubani. Siza ubhale ikheli okuthunyelwa kulo ePosini & Uhlelo lwamaqembu ezindaba ze-akhawunti."

## @name NS_MSG_NO_RECIPIENTS
#: 12511
msgid "No recipients were specified. Please enter a recipient or newsgroup in the addressing area."
msgstr "Akushiwongo ukuthi uthunyelwe kubani. Siza ubhale ikheli okuthunyelwa kulo ePosini & Uhlelo lwamaqembu ezindaba ze-akhawunti. Siza usho ukuthi uthunyelwa kubani noma iqembu lezindaba elisendaweni yamakheli."

## @name NS_MSG_ERROR_WRITING_FILE
#: 12512
msgid "Error writing temporary file."
msgstr "Kwenzeke iphutha lapho kubhalwa ifayela lesikhashana."

## @name NS_ERROR_COULD_NOT_LOGIN_TO_SMTP_SERVER
#: 12513
msgid "An error occurred sending mail: Unable to connect to SMTP server %S. The server may be down or may be incorrectly configured. Please verify that your Mail/News account settings are correct and try again."
msgstr "Kwenzeke iphutha lapho kuthunyelwa umyalezo: Ayikwazi ukuxhuma iseva %S ku-SBTP. Kungenzeka ukuthi iseva ayisebenzi noma ayixhunywanga kahle. Siza uqiniseke ukuthi uhlelo lwePosi lakho/Izindaba luqondile futhi uphinde uzame."

## @name NS_ERROR_SENDING_FROM_COMMAND
#: 12514
msgid "An error occurred while sending mail. The mail server responded:  %s.  Please verify that your email address is correct in your Mail preferences and try again."
msgstr "Kwenzeke iphutha lapho kuthunyelwa iposi. Iseva yeposi iyasebenza: %S. Siza uqiniseke ukuthi ikheli lakho leposi liqondile kulokho okukhethile ePosini lakho bese uphinda uzame."

## @name NS_ERROR_SENDING_RCPT_COMMAND
#: 12515
msgid "An error occurred while sending mail. The mail server responded:  %s. Please check the message recipients and try again."
msgstr "Kwenzeke iphutha lapho kuthunyelwa iposi. Iseva yeposi iyasebenza: %S. Siza uhlole ukuthi lomyalezo uthunyelwa kobani bese uphinda uzame."

## @name NS_ERROR_SENDING_DATA_COMMAND
#: 12516
msgid "An (SMTP) error occurred while sending mail. The server responded:  %s."
msgstr "Kwenzeke iphutha le-(SMTP) lapho kuthunyelwa iposi. Iseve iyasebenza: %S."

## @name NS_ERROR_SENDING_MESSAGE
#: 12517
msgid "An error occurred while sending mail. The mail server responded:  %s. Please check the message and try again."
msgstr "Kwenzeke iphutha lapho kunyelwa iposi. Iseva yeposi iyasebenza: %S. Siza uhlole umyalezo bese uzama futhi."

## @name NS_ERROR_POST_FAILED
#: 12518
msgid "The message could not be posted because connecting to the news server failed. The server may be unavailable or is refusing connections. Please verify that your news server setting is correct and try again, or else contact your network administrator."
msgstr "Lomyalezo awuzange uthunyelwe  ngenxa yokuthi ukuxhumana neseva yomzila wezindaba kuhlulekile. Siza uqiniseke ukuthi uhlelo lwezindaba lweseva luqondile bese uphinda uzame, noma uma kungenjalo thintana nomqondisi wamakhomputha."

## @name NS_ERROR_QUEUED_DELIVERY_FAILED
#: 12519
msgid "An error occurred delivering unsent messages."
msgstr "Kwenzeke iphutha lapho kuthunyelwa imiyalezo ebingathunyelwanga."

## @name NS_ERROR_SEND_FAILED
#: 12520
msgid "Sending of message failed."
msgstr "Ukuthumela imiyalezo kuhlulekile."

## @name SMTP_DELIV_MAIL
#: 12521
msgid "Delivering mail..."
msgstr "Ukuthumela iposi..."

## @name SMTP_MESSAGE_SENT_WAITING_MAIL_REPLY
#: 12522
msgid "Mail: Message sent; waiting for reply..."
msgstr "Iposi: Umyalezo uthunyelwe; ulindele impendulo..."

## @name SMTP_PROGRESS_MAILSENT
#: 12523
msgid "Mail sent successfully"
msgstr "Umyalezo uthunyelwe ngempumelelo"

## @name NS_ERROR_SMTP_SERVER_ERROR
#: 12524
msgid "An error occurred sending mail: SMTP server error. The server responded:  %s Contact your mail administrator for assistance."
msgstr "Kwenzeke iphutha lapho lapho kuthunyelwe iposi: iseva i-SMTP yenze iphutha. Iseva iyasebenza. %S Thintana nomqondisi weposi ukuze uthole usizo."

## @name NS_MSG_UNABLE_TO_SEND_LATER
#: 12525
msgid "Unable to save your message in order to send it later."
msgstr "Ayikwazi ukulonda umyalezo wakho ukuze iwuthumele kamuva."

## @name NS_ERROR_COMMUNICATIONS_ERROR
#: 12526
msgid "A communications error occurred: %d.  Please try again."
msgstr "Kwenzeke iphutha lokuxhumana: %d. Siza uphinde uzame."

## @name NS_ERROR_BUT_DONT_SHOW_ALERT
#: 12527
msgid "THIS IS JUST A PLACEHOLDER.  YOU SHOULD NEVER SEE THIS STRING."
msgstr "LENA INDAWO NJE YOKUGCINA UKWAZISWA. AKUFANELE UBONE LAMAGAMA."

## @name NS_ERROR_TCP_READ_ERROR
#: 12528
msgid "A network error occurred while receiving data.  (Network Error: %s)  Try connecting again."
msgstr "Kwenzeke iphutha lapho kwamukelwa ukwaziswa. (Iphutha lomshini: %S) Zama ukuphinda uxhume izintambo."

## @name NS_ERROR_COULD_NOT_GET_USERS_MAIL_ADDRESS
#: 12529
msgid "An error occurred sending mail:  the return mail address was invalid.  Please verify that your email address is correct in your Mail preferences and try again."
msgstr "Kwenzeke iphutha kuthunyelwa iposi: Ikhali lokuyisela emuva iposi belingasebenzi. Siza uqiniseke ukuthi ikheli leposi lakho liqondile kokukhethile ezintweni zeposi bese uphinda uzame."

## @name NS_ERROR_SMTP_PASSWORD_UNDEFINED
#: 12530
msgid "Error getting mail password."
msgstr "Kwenzeke iphutha lapho kufunwa igama lokungena."

## @name NS_ERROR_MIME_MPART_ATTACHMENT_ERROR
#: 12531
msgid "Attachment error."
msgstr "Iphutha lokunamathelisa ifayela."

## @name NS_MSG_FAILED_COPY_OPERATION
#: 12532
msgid "The message was sent successfully, but could not be copied to your Sent folder."
msgstr "Umyalezo uthunyelwe ngempumelelo, kodwa ikhophi yawo ayifakwanga kufolda  yezinto ozithumele."

## @name NS_MSG_FAILURE_ON_OBJ_EMBED_WHILE_SAVING
#: 12533
msgid "There was a problem including the file %.200s in the message. Would you like to continue saving the message without this file?"
msgstr "Kube nenkinga ekufakeni ifayele engu-%200s. Ungathanda ukuqhubeka uthumela lomyalezo ingekho lefayeli?"

## @name NS_MSG_ASSEMBLING_MESSAGE
#: 12534
msgid "Assembling mail information..."
msgstr "Iqoqa ukwaziswa kweposi..."

## @name NS_MSG_GATHERING_ATTACHMENT
#: 12535
msgid "Attaching %s..."
msgstr "Inamathelisa i-%S"

## @name NS_MSG_CREATING_MESSAGE
#: 12536
msgid "Creating mail message..."
msgstr "Yakha umyalezo weposi..."

## @name NS_MSG_FAILURE_ON_OBJ_EMBED_WHILE_SENDING
#: 12537
msgid "There was a problem including the file %.200s in the message. Would you like to continue sending the message without this file?"
msgstr "Kube nenkinga ekufakeni ifayele engu-%200s. Ungathanda ukuqhubeka uthumela lomyalezo ingekho lefayeli?"

## @name NS_MSG_START_COPY_MESSAGE
#: 12538
msgid "Copying message to %S folder..."
msgstr "Ukufaka ikhophi yalomyalezo ku-%S..."

## @name NS_MSG_START_COPY_MESSAGE_COMPLETE
#: 12539
msgid "Copy complete."
msgstr "Ukwenza ikhophi kuphelile."

## @name NS_MSG_START_COPY_MESSAGE_FAILED
#: 12540
msgid "Copy failed."
msgstr "Ukwenza ikhophi kuhlulekile."

## @name NS_MSG_LARGE_MESSAGE_WARNING
#: 12541
msgid "Warning! You are about to send a %d byte message. Are you sure that you want to do this?"
msgstr "Isixwyiso! Uzothumela  umyalezo oyi-%d byte. Uyaqiniseka ukuthi ufuna ukwenza lokhu?"

#: noIdentities
msgid "You don't have any email identities yet.  Create one with the Account Wizard."
msgstr "Awukabi nazi izimpawu zePosi. Yakha ezinoPhau lwe-akhawunti."

## @name NS_SMTP_PASSWORD_PROMPT_TITLE
#: 12542
msgid "Mail Server Password Required"
msgstr "Kudingeka Igama Lokungena Kuseva Yeposi"

## @name NS_SMTP_PASSWORD_PROMPT1
#: 12543
msgid "Enter your password for %S:"
msgstr "Bhala igama lokungena le-%S."

## @name NS_SMTP_PASSWORD_PROMPT2
#: 12544
msgid "Enter your password for %S on %S:"
msgstr "Bhala igama lakho lokungena le-%S:"

## @name NS_SMTP_CONNECTING_TO_SERVER
#: 12545
msgid "Connecting to server..."
msgstr "Ukuxhumanisa neseva..."

## @name NS_MSG_SENDING_MESSAGE
#: 12550
msgid "Sending message..."
msgstr "Thumela umyalezo..."

## @name NS_MSG_POSTING_MESSAGE
#: 12551
msgid "Posting message..."
msgstr "Posa umyalezo..."

## @name NS_ERROR_MSG_MULTILINGUAL_SEND
#: 12553
msgid "The message you composed contains characters not found in the selected Character Encoding. While you can choose a different Character Encoding, it is usually safe to use Unicode (UTF-8) for mail. If you just send in the current Character Encoding, be aware that those not covered by the current Character Encoding will be turned to question marks and illegible by the recipient."
msgstr "Umyalezo owubhalile unezinhlamvu ezingatholakali Kumakhodi Ezinhlamvu ezikhethiwe. Nakuba ungase ukhethe Ikhodi ehlukile yohlamvu, ngokuvamile kulondekile ukusebenzisa i-Unicode (UTF-8) lapho uthumela iposi. Uma uthumela nje Izinhlamvu Ezinamakhodi ezisebenzayo, qaphela ukuthi umuntu owamukela lomyalezo uyobuza noma angakwazi ukuzifunda lezo ezingekho kumakhodi Ezinhlamvu asebenzayo."

#: sendInUTF8
msgid "Send in UTF-8"
msgstr "Thumela i-UTF-8"

#: sendAnyway
msgid "Send anyway"
msgstr "Sale usuthumela nje"

## @name NS_ERROR_NNTP_NO_CROSS_POSTING
#: 12554
msgid "You can only send a message to one news server at a time."
msgstr "Ungathumela umyalezo kuseva eyodwa yezindaba ngesikhathi."

## @name NS_MSG_CANCELLING
#: 12555
msgid "Cancelling..."
msgstr "Iyakhansela..."

## @name NS_MSG_SEND_ERROR_TITLE
#: 12556
msgid "Send Message Error"
msgstr "Thumela Umyalezo Wokuthi Kwenzeke Iphutha"

## @name NS_MSG_SENDLATER_ERROR_TITLE
#: 12557
msgid "Send Later Error"
msgstr "Iphutha Lokuthumela Kamuva"

## @name NS_MSG_SAVE_DRAFT_TITLE
#: 12558
msgid "Save Draft Error"
msgstr "Gcina Umyalezo Wokuthi Kwenzeke Iphutha Ongakathunyelwa"

## @name NS_MSG_SAVE_TEMPLATE_TITLE
#: 12559
msgid "Save Template Error"
msgstr "Gcina Umyalezo Wokuthi Kwenzeke Iphutha Ozosetshenziswa Njalo"

## @name NS_ERROR_SEND_FAILED_BUT_NNTP_OK
#: 12560
msgid "Your message has been posted to the newsgroup but has not been sent to other recipient."
msgstr "Umyalezo wakho uthunyelwe eqenjini lezindaba kodwa awukathunyelwa kwabanye okufanele uthunyelwe kubo."

## @name NS_MSG_ASK_TO_COMEBACK_TO_COMPOSE
#: 12561
msgid "Would you like to return to the compose window?"
msgstr "Ungathanda ukubuyela ewindini lokuhlelwa komsebenzi ngononina?"

## @name NS_MSG_GENERIC_FAILURE_EXPLANATION
#: 12562
msgid "Please verify that your Mail & Newsgroups account settings are correct and try again."
msgstr "Siza uqinisekise ukuthi ukuhlelwa kwePosi &Amaqembu Ezindaba kuqondile bese uphinde uzame."

## @name NS_MSG_ERROR_READING_FILE
#: 12563
msgid "Error reading file."
msgstr "Ifayela lokufunda amaphutha."

## @name NS_FOLLOWUPTO_SENDER
#: 12564
msgid "The author of this message has requested that responses be sent only to the author. If you also want to reply to the newsgroup, add a new row to the addressing area, choose Newsgroup from the recipients list, and enter the name of the newsgroup."
msgstr "Umlobi walomyalezo ucele ukuba izimpendulo zithunyelwe kuye kuphela. Uma ufuna ukuba impendulo ithunyelwe naseqenjini lezindaba, nezela umugqa omusha endaweni yamakheli, khetha Iqembu Lezindaba ohlwini lwabantu abanyelwayo, bese ubhala igama leqembu lezindaba."

## @name NS_ERROR_SMTP_SEND_FAILED
#: 12565
msgid "The message could not be sent because connecting to SMTP server %S failed. The server may be unavailable or is refusing SMTP connections. Please verify that your SMTP server setting is correct and try again, or else contact your network administrator."
msgstr "Umyalezo awuthunyelwanga ngoba i-%S ixhulekile ukuxhuma  iseva i-SMTP. Kungenzeka ukuthi leseva ayikho noma iyenqaba ukuxhuma i-SMTP>"

## @name NS_MSG_UNDISCLOSED_RECIPIENTS
## LOCALIZATION NOTE: this string must be using only US_ASCII characters
#: 12566
msgid "undisclosed-recipients"
msgstr "abathunyelelwa Umyalezo Abangashiwo Ngamagama"

## @name NS_ERROR_SMTP_TEMP_SIZE_EXCEEDED
#: 12567
msgid "The size of the message you are trying to send exceeds a temporary size limit of the server. The message was not sent, try to reduce the message size and try again. The server responded:  %s."
msgstr "Ubukhulu bomyalezo ozama ukuwuthumela budlula isilinganiso sesikhashana seseva. Umyalezo awuthunyelwanga, zama ukunciphisa ubukhulu bomyalezo bese uphinda uzame. Iseva iyavuma: %s."

## @name NS_ERROR_SMTP_PERM_SIZE_EXCEEDED_1
#: 12568
msgid "The size of the message you are trying to send exceeds the global size limit (%d bytes) of the server. The message was not sent, try to reduce the message size and try again."
msgstr "Ubukhulu bomyalezo ozama ukuwuthumela budlula isilinganiso (%d bytes) sawo wonke amafayela axhunywe kuleseva. Umyalezo awuthunyelwanga, zama ukunciphisa ubukhulu bomyaleze bese uphinda uzame."

## @name NS_ERROR_SMTP_PERM_SIZE_EXCEEDED_2
#: 12569
msgid "The size of the message you are trying to send exceeds the global size limit of the server. The message was not sent, try to reduce the message size and try again. The server responded:  %s."
msgstr "Ubukhulu bomyalezo ozama ukuwuthumela budlula isilinganiso sawoo wonke mafayela axhunywe kuleseva. Umyalezo awuthunyelwanga, zama ukunciphisa ubukhulu bomyalezo bese uphinda uzame. Iseva ivumile: %s."

## @name NS_MSG_ERROR_ATTACHING_FILE
#: 12570
msgid "There was an error attaching %S. Please check if you have access to the file."
msgstr "Kwenzeke iphutha lapho kunanyatheliswa i%S. Siza uhlole ukuthi uyakwazi yini ukungena kulefayili."

## @name NS_MSG_ERROR_DOING_FCC
#: 12571
msgid "There was an error copying the message to the Sent folder. Retry?"
msgstr "Kwenzeke iphutha lapho kufakwa ikhophi yomyalezo ku-Folder yezinto ezithunyelwe. Ufuna Ukuphinde uzame?"

## @name NS_ERROR_SMTP_GREETING
#: 12572
msgid "An error occurred sending mail: The mail server sent an incorrect greeting:  %s."
msgstr "Kwenzeke iphutha lapho kuthunyelwa iposi: Iseva yeposi ithumele amazwi okubingelela angaqondile: %s."

## @name NS_ERROR_COULD_NOT_LOGIN_TO_SMTP_SERVER_WITH_STARTTLS1
#: 12573
msgid "An error occurred sending mail: Unable to connect to SMTP server %S via STARTTLS since it doesn't support EHLO. Please verify that your Mail/News account settings are correct and try again."
msgstr "Kwenzeke iphutha lapho kuthunyelwa iposi: Ayikwazi ukuxhuma i-%S ku-SMTP nge-STRTTLS ngoba ayiyisekeli i-EHLO. Siza uqiniseke ukuthi ukuhlelwa kwe-akhawunti yakhe yePosi/Yezindaba kuqondile bese uphinda uzame."

## @name NS_ERROR_COULD_NOT_LOGIN_TO_SMTP_SERVER_WITH_STARTTLS2
#: 12574
msgid "An error occurred sending mail: Unable to connect to SMTP server %S via STARTTLS since it doesn't offer STARTTLS In EHLO reponse. Please verify that your Mail/News account settings are correct and try again."
msgstr "Kwenzeke iphutha lapho kuthunyelwa iposi: Ayikwazi ukuxhuma i-%S ku-SMTP nge-STRTTLS ngoba ayinayo i-STRTTLS ku-EHLO.  Siza uqiniseke ukuthi ukuhlelwa kwe-akhawunti yakhe yePosi/Yezindaba kuqondile bese uphinda uzame."

## Strings use for the save message dialog shown when the user close a message compose window
#: saveDlogTitle
msgid "_: saveDlogTitle\n"
"Save Message"
msgstr "Gcina Umyalezo"

#: saveDlogMessage
msgid "Message has not been sent. Do you want to save the message in the Drafts folder?"
msgstr "Umyalezo awuthunyelwanga. Uyafuna ukugcina umyalezo ku-folder yemiyalezo engakathunyelwa?"

#: saveDlogSaveBtn
msgid "Save"
msgstr "Gcina"

#: saveDlogDontSaveBtn
msgid "Don't Save"
msgstr "Ungagcomo"

#: saveDlogCancelBtn
msgid "Cancel"
msgstr "Khansela"

## generics string
#: defaultSubject
msgid "(no subject)"
msgstr "(akunasihloko)"

#: chooseFileToAttach
msgid "Attach File(s)"
msgstr "Namathelisa Amafayeli"

##
#: windowTitlePrefix
msgid "Compose:"
msgstr "Hlela Umsebenzi:"

## String used by the dialog that ask the user to enter a subject
#: sendMsgTitle
msgid "_: sendMsgTitle\n"
"Send Message"
msgstr "Thumela umyalezoâ€¦"

#: subjectDlogMessage
msgid "You did not specify a subject for this message. If you would like to provide one, please type it now."
msgstr "Awusishongo isihloko salomyalezo. Uma ufuna ukusibhala, siza usibhale manje."

## String used by the dialog that informs the user about the newsgroup recipient
#: recipientDlogMessage
msgid "This account only supports email recipients. Continuing will ignore newsgroups."
msgstr "Le-akhawunti isekela abantu abathunyelelwa iposi. Ukuqhubeka kuzokwenza ukuba ingawanaki amaqembu ezindaba."

#String used by the alert that tells the user an e-mail address is invalid
#: addressInvalid
msgid "%1$S is not a valid e-mail address because it is not of the form user@host. You must correct it before sending the e-mail."
msgstr "I%1$S ayilona ikheli leposi elisebenzayo ngoba ayisilo elohlobo user@host. Kufanele ulilungise ngaphambi kokuba uthumele iposi."

## String used by the dialog that ask the user to attach a web page
#: attachPageDlogTitle
msgid "Please specify a location to attach"
msgstr "Siza usho indawo okufanele kunanyatheliswe kuyo"

#: attachPageDlogMessage
msgid "Web Page (URL):"
msgstr "Ikhasi Le-inthaneti (URL):"

## String used for attachment pretty name, when attachment is a message
#: messageAttachmentSafeName
msgid "Attached Message"
msgstr "Umyalezo Onanyathelisiwe"

## String used for attachment pretty name, when attachment is message part
#: partAttachmentSafeName
msgid "Attached Message Part"
msgstr "Ingxenye Yomyalezo Enanyathelisiwe"

## String used by the Initialization Error dialog
#: initErrorDlogTitle
msgid "Message Compose"
msgstr "Hlela Umyalezo"

## LOCALIZATION NOTE (initErrorDlogMessage): %1$s will be replaced by the error string.  do not translate \n
#: initErrorDlogMessage
msgid "An error occurred while creating a message compose window. Please try again.\n"
"%1$s"
msgstr "Kwenzeke iphutha lapho kwakhiwa iwindi lomyalezo ohleliwe. Siza uphinde uzame.\n%1$s."

## Strings used by Save as Draft/Template dialog
#: SaveDialogTitle
msgid "_: SaveDialogTitle\n"
"Save Message"
msgstr "Gcina Umyalezo"

## LOCALIZATION NOTE (SaveDialogMsg): %1$S is folder name, %2$S is host name
#: SaveDialogMsg
msgid "Your message has been saved to the folder %1$S under %2$S."
msgstr "Umyalezo wakho ugciniwe ku-folder %1$S ngaphansi kwe-%2$S."

#: CheckMsg
msgid "Do not show me this dialog box again."
msgstr "Ungaphindi ungibonise lelibhokisi lokukhulumisana."

## Strings used by prompt when Quitting while in progress
#: quitComposeWindowTitle
msgid "Sending Message"
msgstr "Thumela Umyalezo"

## LOCALIZATION NOTE (quitComposeWindowMessage): don't translate \n
#: quitComposeWindowMessage
msgid "Mail is currently in the process of sending a message.\n"
"Would you like to wait until the message has been sent before quitting or quit now?"
msgstr "Iposi njengamanje lithumela umyalezo.\nUngathanda ukulinda kuze kube yilapho umyalezo usuthunyelwe ngaphambi kokuba uphume noma ufuna ukuphuma manje?"

#: quitComposeWindowQuitButtonLabel
msgid "Quit"
msgstr "Phuma"

#: quitComposeWindowWaitButtonLabel
msgid "Wait"
msgstr "Linda"

