msgid "Network"
msgstr "Inethiwekhi"

#: connection.label
msgid "Connection"
msgstr "Ukuxhuma"

#: connectionDesc.label
msgid "Configure how &brandShortName; connects to the Internet"
msgstr "Misa indlela yokuthi i-&brandShortName; ixhumeka kanjani ku-inthanethi"

#: connectionSettings.label connectionSettings.accesskey
msgid "S&ettings…"
msgstr "I&zilungiselelo…"

#: httpCache.label
msgid "Cached Web Content"
msgstr "Okuqukethwe kwewebhu okugciniwe"

