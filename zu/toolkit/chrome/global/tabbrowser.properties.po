#: tabs.loading
msgid "Loading..."
msgstr "Iyalayisha..."

#: tabs.untitled
msgid "(Untitled)"
msgstr "(Ayinasihloko)"

#: tabs.closeTab
msgid "Close Tab"
msgstr "Vala Isigqebhe"

#: tabs.close
msgid "Close"
