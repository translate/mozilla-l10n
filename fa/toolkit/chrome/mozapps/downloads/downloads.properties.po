msgstr "دقیقه"

#: hours
msgid "hour;hours"
msgstr "ساعت"

#: days
msgid "day;days"
msgstr "روز"

#. # LOCALIZATION NOTE (paused): — is the "em dash" (long dash)
#: paused
msgid "Paused —  #1"
msgstr "مکث شده — #1"

#: downloading
msgid "Downloading"
msgstr "در حال بارگیری"

#: notStarted
msgid "Not Started"
msgstr "آغاز نشده"

#: failed
msgctxt "failed"
msgid "Failed"
msgstr "شکست خورده"

