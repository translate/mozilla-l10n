msgstr "نصب شکست خورد"

#: pauseButtonPause
msgid "Pause"
msgstr "مکث"

#: pauseButtonResume
msgid "Resume"
msgstr "ازسرگیری"

#: hideButton
msgid "Hide"
msgstr "مخفی کردن"

#: hideButton.accesskey
msgid "H"
msgstr "خ"

#: applyingUpdate
