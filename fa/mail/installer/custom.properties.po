msgstr "انتخاب گزینه‌های نصب"

#: OPTIONS_MAKE_DEFAULT
msgid "&Use $BrandShortName as my default mail application"
msgstr "&Use $BrandShortName as my default mail application"

#: SHORTCUTS_PAGE_TITLE
