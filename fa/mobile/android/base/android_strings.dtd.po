msgctxt "pref_category_general"
msgid "General"
msgstr "عمومی"

#: pref_category_general_summary3
msgid "Home, language, tab queue"
msgstr "خانه، زبان، صف زبانه"

#. Localization note (pref_category_language) : This is the preferences
#. section in which the user picks the locale in which to display Firefox
#. UI. The locale includes both language and region concepts.
#: pref_category_language
msgid "Language"
msgstr "زبان"

#: pref_category_language_summary
msgid "Change the language of your browser"
msgstr "زبان مرورگر خود را تغییر دهید"

#: pref_browser_locale
msgid "Browser language"
msgstr "زبان مرورگر"

#. Localization note (locale_system_default) : This string indicates that
#. Firefox will use the locale currently selected in Android's settings
#. to display browser chrome.
#: locale_system_default
msgid "System default"
msgstr "پیش‌فرض‌ سیستم"

#. Localization note (overlay_share_label) : This is the label that appears
#. in Android's intent chooser when sending a link to Firefox to bookmark,
#. send to another device, or add to Reading List.
#: overlay_share_label
msgid "Add to &brandShortName;"
msgstr "اضافه‌کردن به  &brandShortName;"

#. Localization note (overlay_share_bookmark_btn_label) : This string is
#. used in the share overlay menu to select an action. It is the verb
#. "to bookmark", not the noun "a bookmark".
#: overlay_share_bookmark_btn_label
msgctxt "overlay_share_bookmark_btn_label"
msgid "Bookmark"
msgstr "نشانک"

#: overlay_share_bookmark_btn_label_already
msgctxt "overlay_share_bookmark_btn_label_already"
msgid "Already bookmarked"
msgstr "به تازگی نشانک‌گذاری‌شده"

#: overlay_share_send_other
msgid "Send to other devices"
msgstr "ارسال به دستگاه‌های دیگر"

#. Localization note (overlay_share_send_tab_btn_label) : Used on the
#. share overlay menu to represent the "Send Tab" action when the user
#. either has not set up Sync, or has no other devices to send a tab
#. to.
#: overlay_share_send_tab_btn_label
msgid "Send to another device"
msgstr "ارسال به دستگاه‌های دیگر"

#: overlay_share_no_url
msgid "No link found in this share"
msgstr " پیوندی در این اشتراک یافت نشد."

#: overlay_share_select_device
msgid "Select device"
msgstr "انتخاب‌دستگاه"

#. Localization note (overlay_no_synced_devices) : Used when the menu option
#. to send a tab to a synced device is pressed and no other synced devices
#. are found.
#: overlay_no_synced_devices
msgid "No Firefox Account connected devices found"
msgstr "هیچ حساب فایرفاکس متصل شده به این دستگاه پیدا نشد"

#: pref_category_search3
msgctxt "pref_category_search3"
msgid "Search"
msgstr "جستجو"

#: pref_category_search_summary2
msgid "Add, set default, show suggestions"
msgstr "اضافه، تنظیم به عنوان پیش فرض، نمایش پیشنهادات"

#: pref_category_accessibility
msgctxt "pref_category_accessibility"
msgid "Accessibility"
msgstr "دسترسی‌پذیری"

#: pref_category_accessibility_summary2
msgid "Text size, zoom, voice input"
msgstr "اندازه متن، بزرگنمایی، ورودی صوتی"

#: pref_category_privacy_short
msgctxt "pref_category_privacy_short"
msgid "Privacy"
msgstr "حریم‌خصوصی"

#: pref_category_privacy_summary4
