msgid "Yesterday"
msgstr "ඊයේ"

#. # LOCALIZATION NOTE (monthDate): #1 month name; #2 date number; e.g., January 22
#: monthDate2
msgctxt "monthDate2"
msgid "%1$S %2$S"
msgstr "%1$S %2$S"

#: fileDoesNotExistOpenTitle
msgid "Cannot Open %S"
