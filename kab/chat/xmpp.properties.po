#: options.connectServer
msgid "Server"
msgstr "Aqeddac"

#: options.connectPort
msgid "Port"
msgstr "Tabburt"

#: options.domain
msgid "Domain"
msgstr "Taɣult"

#. # LOCALIZATION NOTE (*.protocolName)
#. #  This name is used whenever the name of the protocol is shown.
#: gtalk.protocolName
msgid "Google Talk"
msgstr "Google Talk"

#: odnoklassniki.protocolName
msgid "Odnoklassniki"
msgstr "Odnoklassniki"

#. # LOCALIZATION NOTE (gtalk.usernameHint):
#. #  This is displayed inside the accountUsernameInfoWithDescription
#. #  string defined in imAccounts.properties when the user is
#. #  configuring a Google Talk account.
#: gtalk.usernameHint
msgid "email address"
msgstr "tansa imayl"

#. # LOCALIZATION NOTE (odnoklassniki.usernameHint):
#. #  This is displayed inside the accountUsernameInfoWithDescription
#. #  string defined in imAccounts.properties when the user is
#. #  configuring a Odnoklassniki account.
#: odnoklassniki.usernameHint
msgid "Profile ID"
msgstr "Asulay n umaɣnu"

#. # LOCALZIATION NOTE (command.*):
#. #  These are the help messages for each command.
#: command.join3
msgid ""
"%S [&lt;room&gt;[@&lt;server&gt;][/&lt;nick&gt;]] [&lt;password&gt;]: Join a "
"room, optionally providing a different server, or nickname, or the room "
"password."
msgstr ""
"%S [&lt;taxxamt&gt;[@&lt;aqeddac&gt;][/&lt;meffer isem&gt;]] [&lt;awal "
"uffir&gt;]: Ddu ɣer texxamt, neɣ mudd-d aqeddac nniḍen, neɣ meffer isem, neɣ "
"awal uffir n texxamt."

#: command.part2
msgid "%S [&lt;message&gt;]: Leave the current room with an optional message."
msgstr ""

#: command.topic
msgid "%S [&lt;new topic&gt;]: Set this room's topic."
msgstr "%S [&lt;new topic&gt;]: Sbadu asentel n udiwenni n texxamt agi."

#: command.ban
msgid ""
"%S &lt;nick&gt;[&lt;message&gt;]: Ban someone from the room. You must be a "
"room administrator to do this."
msgstr ""
"%S &lt;meffer isem&gt;[&lt;izen&gt;]: Kkes amdan si texxamt. Yessefk ad "
"tiliḍ d anedbal akken ad tekkseḍ imdanen."

#: command.kick
msgid ""
"%S &lt;nick&gt;[&lt;message&gt;]: Remove someone from the room. You must be "
"a room moderator to do this."
msgstr ""
"%S &lt;meffer isem&gt;[&lt;izen&gt;]: Kkes amdan si texxamt. Yessefk ad "
"tiliḍ d amḍebbeṛ akken ad tekkseḍ imdanen."

#: command.invite
msgid ""
"%S &lt;jid&gt;[&lt;message&gt;]: Invite a user to join the current room with "
"an optional message."
msgstr ""

#: command.me
msgid "%S &lt;action to perform&gt;: Perform an action."
msgstr "%S &lt;tigawt ara estselkmeḍ&gt;: Selkem tigawt."

#: command.nick
msgid "%S &lt;new nickname&gt;: Change your nickname."
msgstr "%S &lt;new meffer isem&gt;: Snifel meffer isem-ik."

#: command.msg
msgid ""
"%S &lt;nick&gt; &lt;message&gt;: Send a private message to a participant in "
"the room."
msgstr ""
"%S &lt;meffer isem&gt; &lt;izen&gt;: Azen izen uslig i wemttekki di texxamt."
