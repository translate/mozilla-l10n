#. # Status Text
#: paused2
msgid "Paused"
msgstr "Isteɛfa"

#: processing
msgid "Processing"
msgstr "Asesfer"

#: notStarted
msgid "Not Started"
msgstr "Ur yekkir ara"

#: failed
msgid "Failed"
msgstr "Ur yeddi ara"

#: waitingForInput
msgid "Waiting for input"
msgstr "Iṭraǧu anekcam"

#: waitingForRetry
msgid "Waiting for retry"
msgstr "Ittṛaǧu aɛaḍ tikelt nniḍen"

#: completed
msgid "Completed"
msgstr "Immed"

#: canceled
msgid "Canceled"
msgstr "Ifsex"

#. # LOCALIZATION NOTE (yesterday): Displayed time for files finished yesterday
#: yesterday
msgid "Yesterday"
msgstr "Iḍelli"

#. # LOCALIZATION NOTE (monthDate): #1 month name; #2 date number; e.g., January 22
