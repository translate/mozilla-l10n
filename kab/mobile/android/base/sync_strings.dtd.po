#. Firefox Account strings.
#. Localization note: these are shown in screens after the user has
#. created or signed in to an account, and take the user back to
#. Firefox.
#: fxaccount_back_to_browsing
msgid "Back to browsing"
msgstr "Uɣal ɣer tunigin"

#: fxaccount_getting_started_welcome_to_sync
msgid "Welcome to &syncBrand.shortName.label;"
msgstr "Ansuf yissek ɣer &syncBrand.shortName.label;"

#: fxaccount_getting_started_description2
msgid "Sign in to sync your tabs, bookmarks, logins & more."
msgstr "Kcem akken ad temtawiḍ iccaren, ticraḍ, inekcam & ugar."

#: fxaccount_getting_started_get_started
msgid "Get started"
msgstr "Bdu"

#: fxaccount_getting_started_old_firefox
msgid "Using an older version of &syncBrand.shortName.label;?"
msgstr "Seqdec lqem aqbuṛ n &syncBrand.shortName.label;?"

#: fxaccount_status_signed_in_as
msgid "Signed in as"
msgstr "Tkecmeḍ s yisem"

#: fxaccount_status_manage_account
msgid "Manage account"
msgstr "Sefrek amiḍan"

#: fxaccount_status_auth_server
msgid "Account server"
msgstr "Aqeddac n umiḍan"

#: fxaccount_status_sync_now
msgid "Sync now"
msgstr "Mtawi tura"

#: fxaccount_status_syncing2
msgid "Syncing…"
msgstr "Amtawi…"

#: fxaccount_status_device_name
msgid "Device name"
msgstr "Isem n yibenk"

#: fxaccount_status_sync_server
msgid "Sync server"
msgstr "Aqeddac n umtawi"

#: fxaccount_status_sync
msgid "&syncBrand.shortName.label;"
msgstr "&syncBrand.shortName.label;"

#: fxaccount_status_sync_enabled
msgid "&syncBrand.shortName.label;: enabled"
msgstr "&syncBrand.shortName.label;: irmed"

#: fxaccount_status_needs_verification2
msgid "Your account needs to be verified. Tap to resend verification email."
msgstr ""
"Amiḍan inek yesra asenqed. Senned akken ad tazneḍ tikelt nniḍen imayl n "
"usenqed."

#: fxaccount_status_needs_credentials
msgid "Cannot connect. Tap to sign in."
msgstr "Ur yezmir ara ad yeqqen. Senned akken ad tkecmeḍ."

#: fxaccount_status_needs_upgrade
msgid "You need to upgrade &brandShortName; to sign in."
msgstr "Teriḍ ad leqqmeḍ &brandShortName; akken ad tkecmeḍ."

#: fxaccount_status_needs_master_sync_automatically_enabled
msgid ""
"&syncBrand.shortName.label; is set up, but not syncing automatically. Toggle "
"“Auto-sync data” in Android Settings &gt; Data Usage."
msgstr ""
"&syncBrand.shortName.label; yettusbadu, acukan ur yemtaway ara s wudem "
"awurman. Sit ɣef “Mtawi isefka s wudem awuman” deg iɣewwaṛen Android &gt; "
"Aseqdec n isefka."

#: fxaccount_status_needs_master_sync_automatically_enabled_v21
msgid ""
"&syncBrand.shortName.label; is set up, but not syncing automatically. Toggle "
"“Auto-sync data” in the menu of Android Settings &gt; Accounts."
msgstr ""
"&syncBrand.shortName.label; yettusbadu, acukan ur yemtaway ara s wudem "
"awurman. Sit ɣef “Mtawi isefka s wudem awuman” deg umuɣ iɣewwaṛen Android "
"&gt; Imiḍanen."

#: fxaccount_status_needs_finish_migrating
msgid "Tap to sign in to your new Firefox Account."
msgstr "Senned akken ad tkecmeḍ ɛer umiḍan inek Firefox amaynut."

#: fxaccount_status_bookmarks
msgid "Bookmarks"
msgstr "Ticraḍ n usebtar"

#: fxaccount_status_history
msgctxt "fxaccount_status_history"
msgid "History"
msgstr "Amazray"

#: fxaccount_status_passwords2
msgctxt "fxaccount_status_passwords2"
msgid "Logins"
msgstr "Inekcam"

#: fxaccount_status_tabs
msgid "Open tabs"
msgstr "Ldi iccaren"

#: fxaccount_status_legal
msgid "Legal"
msgstr "Usḍif"

#. Localization note: when tapped, the following two strings link to
#. external web pages.  Compare fxaccount_policy_{linktos,linkprivacy}:
#. these strings are separated to accommodate languages that decline
#. the two uses differently.
#: fxaccount_status_linktos2
msgid "Terms of service"
msgstr "Tiwtilin n useqdec"

#: fxaccount_status_linkprivacy2
msgid "Privacy notice"
msgstr "Tasertit n tbaḍnit"

#: fxaccount_status_more
msgid "More&ellipsis;"
msgstr "Ugar&ellipsis;"

#: fxaccount_remove_account
msgid "Disconnect&ellipsis;"
msgstr "Ffeq si&ellipsis;"

#: fxaccount_remove_account_dialog_title
msgid "Remove Firefox Account?"
msgstr "Kkes amidan Firefox?"

#: fxaccount_remove_account_dialog_message
msgid ""
"&brandShortName; will stop syncing with your account, but won’t delete any "
"of your browsing data on this device."
msgstr ""
"&brandShortName; ad iseḥbes amtawi d umiḍan inek, acu kan ula ur itekkes ara "
"isefka yinek n tunigin seg uselkim agi."

#. Localization note: format string below will be replaced
#. with the Firefox Account's email address.
#: fxaccount_remove_account_toast
msgid "Firefox Account &formatS; removed."
msgstr "Amiḍan Firefox &formatS; yettwakkes"

#: fxaccount_enable_debug_mode
msgid "Enable Debug Mode"
msgstr "Rmed Askar n tseɣtit"

#. Localization note: this is the name shown by the Android system
#. itself for a Firefox Account. Don't localize this.
#: fxaccount_account_type_label
msgid "Firefox"
msgstr "Firefox"

#. Localization note: these are shown by the Android system itself,
#. when the user navigates to the Android > Accounts > {Firefox
#. Account} Screen. The link takes the user to the Firefox Account
#. status activity, which lets them manage their Firefox
#. Account.
#: fxaccount_options_title
msgid "&syncBrand.shortName.label; Options"
msgstr "Iɣewwaṛen n &syncBrand.shortName.label;"

#: fxaccount_options_configure_title
msgid "Configure &syncBrand.shortName.label;"
msgstr "Swel &syncBrand.shortName.label;"

#. Localization note: these error messages are shown after a request
#. has been made to the remote server, and an error of some type has
#. been returned.
#: fxaccount_remote_error_UPGRADE_REQUIRED
msgid "You need to upgrade Firefox"
msgstr "Tesriḍ ad tleqmeḍ Firefox"

#. Localization note: the format string will be fxaccount_sign_in_button_label, linkified.
#: fxaccount_remote_error_ATTEMPT_TO_CREATE_AN_ACCOUNT_THAT_ALREADY_EXISTS_2
msgid "Account already exists. &formatS1;"
msgstr "Amiḍan yella yakan. &formatS1;"

#: fxaccount_remote_error_ATTEMPT_TO_ACCESS_AN_ACCOUNT_THAT_DOES_NOT_EXIST
msgctxt "fxaccount_remote_error_ATTEMPT_TO_ACCESS_AN_ACCOUNT_THAT_DOES_NOT_EXIST"
msgid "Invalid email or password"
msgstr "Imayl neɣ awal uffir mačči d ameɣtu"

#: fxaccount_remote_error_INCORRECT_PASSWORD
msgctxt "fxaccount_remote_error_INCORRECT_PASSWORD"
msgid "Invalid email or password"
msgstr "Imayl neɣ awal uffir mačči d ameɣtu"

#: fxaccount_remote_error_ATTEMPT_TO_OPERATE_ON_AN_UNVERIFIED_ACCOUNT
msgid "Account is not verified"
msgstr "Amiḍan ur yettwasenqed ara"

#: fxaccount_remote_error_CLIENT_HAS_SENT_TOO_MANY_REQUESTS
msgctxt "fxaccount_remote_error_CLIENT_HAS_SENT_TOO_MANY_REQUESTS"
msgid "Server busy, try again soon"
msgstr "Aqeddac ur yestufi iara, ɣreḍ tikelt nniden"

#: fxaccount_remote_error_SERVICE_TEMPORARILY_UNAVAILABLE_TO_DUE_HIGH_LOAD
msgctxt "fxaccount_remote_error_SERVICE_TEMPORARILY_UNAVAILABLE_TO_DUE_HIGH_LOAD"
msgid "Server busy, try again soon"
msgstr "Aqeddac ur yestufi iara, ɣreḍ tikelt nniden"

#: fxaccount_remote_error_UNKNOWN_ERROR
msgid "There was a problem"
msgstr "Yella wugur"

#: fxaccount_remote_error_ACCOUNT_LOCKED
msgid "Account is locked. &formatS1;"
msgstr "Amiḍan yewḥel. &formatS1;"

#: fxaccount_sync_sign_in_error_notification_title2
msgid "&syncBrand.shortName.label; is not connected"
msgstr "&syncBrand.shortName.label; ur yeqqin ara"

#. Localization note: the format string below will be replaced
#. with the Firefox Account's email address.
#: fxaccount_sync_sign_in_error_notification_text2
msgctxt "fxaccount_sync_sign_in_error_notification_text2"
msgid "Tap to sign in as &formatS;"
msgstr "Senned akken ad tkecmeḍ s yiwem &formatS;"

#: fxaccount_sync_finish_migrating_notification_title
msgid "Finish upgrading &syncBrand.shortName.label;?"
msgstr "Fak aleqqem n &syncBrand.shortName.label;?"

#. Localization note: the format string below will be replaced
#. with the Firefox Account's email address.
#: fxaccount_sync_finish_migrating_notification_text
msgctxt "fxaccount_sync_finish_migrating_notification_text"
msgid "Tap to sign in as &formatS;"
msgstr "Senned akken ad tkecmeḍ s yiwem &formatS;"

