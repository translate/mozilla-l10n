#~ msgstr "Ma’lumot jo‘natish"

#~ msgid "Report sent"
#~ msgstr "Ma’lumot jo‘natildi"

#~ msgid "Try again"
#~ msgstr "Yana urinib ko‘rish"

#~ msgid "Report"
#~ msgstr "Ma’lumot berish"

