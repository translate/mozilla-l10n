#. # Loading indicator messages
#: loading_error_indicator
msgid "Error"
msgstr "Xato"

#: loading_error
msgid "An error occurred while loading the PDF."
msgstr "PDF yuklanayotganda xato yuz berdi."

#: invalid_file_error
msgid "Invalid or corrupted PDF file."
msgstr "Xato yoki buzuq PDF fayli."

#: missing_file_error
msgid "Missing PDF file."
msgstr "PDF fayl kerak."

#: unexpected_response_error
msgid "Unexpected server response."
msgstr "Kutilmagan server javobi."

#. # LOCALIZATION NOTE (text_annotation_type.alt): This is used as a tooltip.
#. # "{{type}}" will be replaced with an annotation type from a list defined in
#. # the PDF spec (32000-1:2008 Table 169 – Annotation types).
#. # Some common types are e.g.: "Check", "Text", "Comment", "Note"
#: text_annotation_type.alt
msgid "[{{type}} Annotation]"
msgstr "[{{type}} Annotation]"

#: password_label
msgid "Enter the password to open this PDF file."
msgstr "PDF faylni ochish uchun parolni kiriting."

#: password_invalid
msgid "Invalid password. Please try again."
msgstr "Parol - notoʻgʻri. Qaytadan urinib koʻring."

#: password_ok
msgid "OK"
msgstr "OK"

#: password_cancel
msgid "Cancel"
msgstr "Bekor qilish"

#: printing_not_supported
msgid "Warning: Printing is not fully supported by this browser."
msgstr "Diqqat: chop qilish bruzer tomonidan toʻliq qoʻllab-quvvatlanmaydi."

#: printing_not_ready
msgid "Warning: The PDF is not fully loaded for printing."
msgstr "Diqqat: PDF fayl chop qilish uchun toʻliq yuklanmadi."

#: web_fonts_disabled
msgid "Web fonts are disabled: unable to use embedded PDF fonts."
msgstr ""
"Veb shriftlar oʻchirilgan: ichki PDF shriftlardan foydalanib boʻlmmaydi."

#: document_colors_disabled
msgid ""
"PDF documents are not allowed to use their own colors: 'Allow pages to "
"choose their own colors' is deactivated in the browser."
msgstr ""
"PDF hujjat oʻzining ranglaridan foydalanishga ruxsat bermaydi: 'Sahifalarga "
"oʻzining rangidan foydalanishga ruxsat berish' ushbu brauzerda oʻchirib "
"qoʻyilgan."
