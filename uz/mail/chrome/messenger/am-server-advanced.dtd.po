#: serverAdvanced.label
msgid "Advanced Account Settings"
msgstr "Qo‘shimcha hisob sozlamalari"

#. Do not translate "IMAP"
#: serverDirectory.label serverDirectory.accesskey
msgid "IMAP server &directory:"
msgstr "IMAP server &direktoriyasi:"

#: usingSubscription.label usingSubscription.accesskey
msgid "Sho&w only subscribed folders"
msgstr "Faqat obuna bo‘lingan jildlarni &ko‘rsatish"

#: dualUseFolders.label dualUseFolders.accesskey
msgid "Server supports &folders that contain sub-folders and messages"
msgstr ""
"Server faqat ichida jildlari va xabarlari bo‘lgan &jildlar bilan ishlay oladi"

#: maximumConnectionsNumber.label maximumConnectionsNumber.accesskey
msgid "&Maximum number of server connections to cache"
msgstr "Keshga ulangan serverlarning &maksimal miqdori"

#: forceSelect.label forceSelect.accesskey
msgid "Send IMAP select to server when checking &for new mail"
msgstr ""

#: forceSelect.tooltip
msgid ""
"Some servers need an additional IMAP select so new email is reliably "
"received, or is shown as read after it has been read on another device. "
"Selecting this option will cause some increased network traffic."
msgstr ""

#. Do not translate "IMAP"
#: namespaceDesc.label
msgid "These preferences specify the namespaces on your IMAP server"
msgstr "Ushbu parametrlar IMAP serverdagi nom maydonchalarini ko‘rsatadi"

#: personalNamespace.label personalNamespace.accesskey
msgid "&Personal namespace:"
msgstr "&Shaxsiy nom maydonchasi:"

#: publicNamespace.label publicNamespace.accesskey
msgid "P&ublic (shared):"
msgstr "&Ommaviy (bo‘lishilgan):"

#: otherUsersNamespace.label otherUsersNamespace.accesskey
msgid "&Other Users:"
msgstr "B&oshqa foydalanuvchilar:"

#: overrideNamespaces.label overrideNamespaces.accesskey
msgid "&Allow server to override these namespaces"
msgstr "Serverga nom maydonchalarini almashtirishga &ruxsat berish"

#: pop3DeferringDesc.label
msgid ""
"When downloading mail from this account's server, use the following folder "
"to store new messages:"
msgstr ""
"Ushbu hisobning serveridan xatni yuklab olayotganda yangi xabarlarni saqlash "
"uchun quyidagi jilddan foydalaning:"

#: accountInbox.label accountInbox.accesskey
msgid "Inbox for thi&s account"
msgstr "&Ushbu hisob uchun Kiruvchi xatlar"

#: deferToServer.label deferToServer.accesskey
msgid "Inbox for &different account"
msgstr "&Boshqa hisob uchun \"Kiruvchi xatlar\""

#: deferGetNewMail.label deferGetNewMail.accesskey
msgid "&Include this server when getting new mail"
msgstr "Yangi xat olinganda ushbu serverni &qo‘shish"
