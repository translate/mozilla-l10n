#: hostColon
msgid "Host:"
msgstr "Host:"

#: domainColon
msgid "Domain:"
msgstr "Domen:"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "Faqat kodlangan aloqalar"

#: forAnyConnection
msgid "Any type of connection"
msgstr "Istalgan ulanish turi"

#: expireAtEndOfSession
msgid "At end of session"
msgstr "Seans oxirida"

#: showDetails
msgid "Show Details"
msgstr "Tafsilotlarni koʻrsatish"

#: hideDetails
msgid "Hide Details"
msgstr "Tafsilotlarni yashirish"

#: detailsAccessKey
msgid "T"
msgstr "T"

#: permissionToSetACookie
msgid "The site %S wants to set a cookie."
msgstr "%S sayti kuki oʻrnatishni xohlaydi."

#: permissionToSetSecondCookie
msgid "The site %S wants to set a second cookie."
msgstr "%S sayti ikkinchi kuki oʻrnatishni xohlaydi."

#. #  LOCALIZATION NOTE (PermissionToSetAnotherCookie): First %S: sitename, second %S: number of cookies already present for that site
#: permissionToSetAnotherCookie
msgid ""
"The site %S wants to set another cookie.\n"
"You already have %S cookies from this site."
msgstr ""
"%S sayti boshqa kuki oʻrnatishni xohlaydi.\n"
"Ushbu saytdan %S ta kukilar bor."

#: permissionToModifyCookie
msgid "The site %S wants to modify an existing cookie."
msgstr "%S sayti mavjud kukini oʻzgartirishni xohlaydi."
