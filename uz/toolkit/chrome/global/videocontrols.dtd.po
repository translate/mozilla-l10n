msgid "Pause"
msgstr "Pauza"

#: muteButton.muteLabel
msgid "Mute"
msgstr "Tovushni oʻchirish"

#: muteButton.unmuteLabel
msgid "Unmute"
msgstr "Tovushni yoqish"

#: fullscreenButton.enterfullscreenlabel
msgid "Full Screen"
msgstr "Butun ekranga"

#: fullscreenButton.exitfullscreenlabel
msgid "Exit Full Screen"
msgstr "\"Butun ekranga\" usulidan chiqish"

#: castingButton.castingLabel
msgid "Cast to Screen"
msgstr "Ekranga uzatish"

#: stats.media
msgid "Media"
msgstr "Mediya"

#. Do not translate this.  Only change the numeric values if you need this dialogue box to appear bigger
#: stats.size
msgid "Size"
msgstr "Hajmi"

#: stats.activity
msgid "Activity"
msgstr "Faolligi"

#: stats.activityPaused
msgid "Paused"
msgstr "Pauza qilingan"

#: stats.activityPlaying
msgid "Playing"
msgstr "Ijro etilmoqda"

#: stats.activityEnded
msgid "Ended"
msgstr "Tugadi"

#: stats.activitySeeking
msgid "(seeking)"
msgstr "(izlanmoqda)"

#: stats.volume
msgid "Volume"
msgstr "Tovush"

#: stats.framesParsed
msgid "Frames parsed"
msgstr "Qayta ishlangan kadrlar"

#: stats.framesDecoded
msgid "Frames decoded"
msgstr "Kodlangan kadrlar"

#: stats.framesPresented
msgid "Frames presented"
msgstr "Namoyish qilingan kadrlar"

#: stats.framesPainted
msgid "Frames painted"
msgstr "Boʻyalgan kadrlar"

#: error.aborted
msgid "Video loading stopped."
msgstr "Video yuklash toʻxtatdi"

#: error.network
msgid "Video playback aborted due to a network error."
msgstr "Video ijro etish tarmoq xatosi tufayli bekor qilingan"

#: error.decode
msgid "Video can't be played because the file is corrupt."
msgstr "Videoni koʻrsatib boʻlmaydi, chunki fayl buzilgan."

#: error.srcNotSupported
msgid "Video format or MIME type is not supported."
msgstr "Video format yoki MIME turi mos kelmaydi."

#: error.noSource2
msgid "No video with supported format and MIME type found."
msgstr "Mos video format va MIME turi topilmadi. "

#: error.generic
msgid "Video playback aborted due to an unknown error."
msgstr "Videoni koʻrsatish noma`lum sabablarga koʻra bekor qilindi. "

#. the #1 string is the current
#. media position, and the #2 string is the total duration. For example, when at
#. the 5 minute mark in a 6 hour long video, #1 would be "5:00" and #2 would be
#. "6:00:00", result string would be "5:00 of 6:00:00 elapsed".
#: scrubberScale.nameFormat
msgid "#1 of #2 elapsed"
msgstr "#1 / #2 oʻtdi"

