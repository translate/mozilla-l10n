#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "So‘nggi sinx: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "So‘nggi sinx: amalga oshmoqda…"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Hisobi: %S"

#: notconnected.label
msgid "Not connected"
msgstr "Ulanmagan"

#: connecting.label
msgid "Connecting…"
msgstr "Aloqa oʻrnatilmoqda..."

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "\"Firefox sinx\" hisobingiz o‘chirilgan"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Amalni qaytarish"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 \"Firefox sinx\"ning so‘nggi versiyasi bilan mos emas. So‘nggi "
"versiyasiga yangilang."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 \"Firefox sinx\"ning eskiroq versiyalari bilan mos emas. Boshqa "
"kompyuter(lar)dagi Firefoxni yangilang."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox sinx"

#: sync.update.button
msgid "Learn More"
msgstr "Ko‘proq o‘rganish"

#: sync.update.close
msgid "Close"
msgstr "Yopish"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "Sinxronlashni moslab bo‘lmadi"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "Birorta ham internet ulanishlari mavjud emas"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S Sync’ga ulana olmadi. Yana urinib ko‘rishni xohlaysizmi?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Yana urinib ko‘rish"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Qo‘lbola moslash"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "Boshqa kompyuterlardan ichki oynalar yo‘q"

#. # LOCALIZATION NOTE (promoBanner.message.text): First part of the message displayed in a
#. # banner on about:home. The final space separates this text from the link.
#: promoBanner.message.text
msgid "Sync is brand new and easier than ever. "
msgstr "Sinxronlash yangi brend va u har doimgidan oson. "

#. # LOCALIZATION NOTE (promoBanner.message.link): Second part of the message, styled as a link.
#: promoBanner.message.link
msgid "Tap here to learn more"
msgstr "Ko‘proq o‘rganish uchun bu yerga bosing"
