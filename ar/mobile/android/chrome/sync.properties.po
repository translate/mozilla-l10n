#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "آخر مزامنة: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "آخر مزامنة: قيد الإجراء…"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "الحساب: %S"

#: notconnected.label
msgid "Not connected"
msgstr "غير متصل"

#: connecting.label
msgid "Connecting…"
msgstr "يتصل…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "أُزيل حسابك في تزامُن فَيَرفُكس"

#: notificationDisconnect.button
msgid "Undo"
msgstr "تراجَع"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 غير متوافق مع آخر نسخة من تزامُن فَيَرفُكس. يُرجى التحديث إلى آخر نسخة."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 غير متوافق مع النسخ الأقدم من تزامُن فَيَرفُكس. يُرجى تحديث فَيَرفُكس على "
"حاسوبك الآخر."

#: sync.update.title
msgid "Firefox Sync"
msgstr "تزامُن فَيَرفُكس"

#: sync.update.button
msgid "Learn More"
msgstr "اعرف المزيد"

#: sync.update.close
msgid "Close"
msgstr "أغلِق"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "تعذر إعداد تزامُن"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "لا اتصالات متاحة بالإنترنت"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "لم يتمكن %S من الاتصال بتزامُن. أتريد المحاولة مجددًا؟"

#: sync.setup.tryagain
msgid "Try again"
msgstr "حاول مجددًا"

#: sync.setup.manual
msgid "Manual setup"
msgstr "إعداد يدوي"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "لا ألسنة من أجهزتك الأخرى."
