#. # LOCALIZATION NOTE (connection.error.*):
#. #   These will show in the account manager if the account is
#. #   disconnected because of an error.
#: connection.error.lost
msgid "Lost connection with server"
msgstr "فُقِد الاتصال بالخادوم"

#: connection.error.timeOut
msgid "Connection timed out"
msgstr "‏انتهت مهلة الاتّصال"

#: connection.error.certError
msgid "Certification error when connecting to server"
msgstr "خطأ في الشهادة أثناء الاتصال بالخادوم"

#. # LOCALIZATION NOTE (joinChat.*):
#. #   These show up on the join chat menu. An underscore is for the access key.
#: joinChat.channel
msgid "_Channel"
msgstr "ال_قناة"

#: joinChat.password
msgid "_Password"
msgstr "_كلمة السر"

#. # LOCALIZATION NOTE (options.*):
#. #   These are the protocol specific options shown in the account manager and
#. #   account wizard windows.
#: options.server
msgid "Server"
msgstr "الخادوم"

#: options.port
msgid "Port"
msgstr "المَنْفذ"

#: options.ssl
msgid "Use SSL"
msgstr "استخدم SSL"

#: options.encoding
msgid "Character Set"
msgstr "الترمير"

#: options.quitMessage
msgid "Quit message"
msgstr "رسالة الخروج"

#: options.partMessage
msgid "Part message"
msgstr "رسالة المغادرة"

#: options.showServerTab
msgid "Show messages from the server"
msgstr "اعرض رسائل الخادوم"

#. # LOCALIZATION NOTE (ctcp.ping): Semi-colon list of plural forms.
#. #  See: http://developer.mozilla.org/en/docs/Localization_and_Plurals
#. #   %1$S is the nickname of the user who was pinged.
#. #   #2 is the delay (in seconds).
#: ctcp.ping
msgid "Ping reply from %1$S in #2 second.;Ping reply from %1$S in #2 seconds."
msgstr "Ping reply from %1$S in #2 second.;Ping reply from %1$S in #2 seconds."

#. # LOCALIZATION NOTE (ctcp.version):
#. #   %1$S is the nickname of the user whose version was requested.
#. #   %2$S is the version response from the client.
#: ctcp.version
msgid "%1$S is using \"%2$S\""
msgstr "‏%1$S يستخدم \"%2$S\""

#. # LOCALIZATION NOTE (ctcp.time):
#. #   %1$S is the nickname of the user whose time was requested.
#. #   %2$S is the time response.
#: ctcp.time
msgid "The time for %1$S is %2$S."
msgstr "وقت %1$S هو %2$S."

#. # LOCALZIATION NOTE (command.*):
#. #   These are the help messages for each command, the %S is the command name
#. #   Each command first gives the parameter it accepts and then a description of
#. #   the command.
#: command.action
msgid "%S &lt;action to perform&gt;: Perform an action."
msgstr "%S &lt;action to perform&gt;: Perform an action."

#: command.ctcp
msgid "%S &lt;nick&gt; &lt;msg&gt;: Sends a CTCP message to the nick."
msgstr "%S &lt;nick&gt; &lt;msg&gt;: Sends a CTCP message to the nick."

#: command.chanserv
msgid "%S &lt;command&gt;: Send a command to ChanServ."
msgstr "%S &lt;command&gt;: Send a command to ChanServ."

#: command.deop
msgid ""
"%S &lt;nick1&gt;[,&lt;nick2&gt;]*: Remove channel operator status from "
"someone. You must be a channel operator to do this."
msgstr ""
"%S &lt;nick1&gt;[,&lt;nick2&gt;]*: Remove channel operator status from "
"someone. You must be a channel operator to do this."

#: command.devoice
msgid ""
"%S &lt;nick1&gt;[,&lt;nick2&gt;]*: Remove channel voice status from someone, "
"preventing them from speaking if the channel is moderated (+m). You must be "
"a channel operator to do this."
msgstr ""
"%S &lt;nick1&gt;[,&lt;nick2&gt;]*: Remove channel voice status from someone, "
"preventing them from speaking if the channel is moderated (+m). You must be "
"a channel operator to do this."

#: command.invite
msgid ""
"%S &lt;nick&gt; [&lt;room&gt;]: Invite someone to join you in the specified "
"channel, or the current channel."
msgstr ""
"%S &lt;nick&gt; [&lt;room&gt;]: Invite someone to join you in the specified "
"channel, or the current channel."

#: command.join
msgid ""
"%S &lt;room1&gt;[,&lt;room2&gt;]* [&lt;key1&gt;[,&lt;key2&gt;]*]: Enter one "
"or more channels, optionally providing a channel key for each if needed."
msgstr ""
"%S &lt;room1&gt;[,&lt;room2&gt;]* [&lt;key1&gt;[,&lt;key2&gt;]*]: Enter one "
"or more channels, optionally providing a channel key for each if needed."

#: command.kick
msgid ""
"%S &lt;nick&gt; [&lt;message&gt;]: Remove someone from a channel. You must "
"be a channel operator to do this."
msgstr ""
"%S &lt;nick&gt; [&lt;message&gt;]: Remove someone from a channel. You must "
"be a channel operator to do this."

#: command.list
msgid ""
"%S: Display a list of chat rooms on the network. Warning, some servers may "
"disconnect you upon doing this."
msgstr ""
"%S: Display a list of chat rooms on the network. Warning, some servers may "
"disconnect you upon doing this."

#: command.memoserv
msgid "%S &lt;command&gt;: Send a command to MemoServ."
msgstr "%S &lt;command&gt;: Send a command to MemoServ."

#: command.mode
msgid ""
"%S [&lt;channel&gt;] [(+|-)&lt;new mode&gt;] [&lt;nick&gt;]: Set or unset a "
"channel or user mode."
msgstr ""
"%S [&lt;channel&gt;] [(+|-)&lt;new mode&gt;] [&lt;nick&gt;]: Set or unset a "
"channel or user mode."

#: command.msg
msgid ""
"%S &lt;nick&gt; &lt;message&gt;: Send a private message to a user (as "
"opposed to a channel)."
msgstr ""
"%S &lt;nick&gt; &lt;message&gt;: Send a private message to a user (as "
"opposed to a channel)."

#: command.nick
msgid "%S &lt;new nickname&gt;: Change your nickname."
msgstr "%S &lt;new nickname&gt;: Change your nickname."

#: command.nickserv
msgid "%S &lt;command&gt;: Send a command to NickServ."
msgstr "%S &lt;command&gt;: Send a command to NickServ."

#: command.notice
msgid "%S &lt;target&gt; &lt;message&gt;: Send a notice to a user or channel."
msgstr "%S &lt;target&gt; &lt;message&gt;: Send a notice to a user or channel."

#: command.op
msgid ""
"%S &lt;nick1&gt;[,&lt;nick2&gt;]*: Grant channel operator status to someone. "
"You must be a channel operator to do this."
msgstr ""
"%S &lt;nick1&gt;[,&lt;nick2&gt;]*: Grant channel operator status to someone. "
"You must be a channel operator to do this."

#: command.operserv
msgid "%S &lt;command&gt;: Send a command to OperServ."
msgstr "%S &lt;command&gt;: Send a command to OperServ."

#: command.part
msgid "%S [message]: Leave the current channel with an optional message."
msgstr "%S [message]: Leave the current channel with an optional message."

#: command.ping
msgid ""
"%S [&lt;nick&gt;]: Asks how much lag a user (or the server if no user "
"specified) has."
msgstr ""
"%S [&lt;nick&gt;]: Asks how much lag a user (or the server if no user "
"specified) has."

#: command.quit
msgid ""
"%S &lt;message&gt;: Disconnect from the server, with an optional message."
msgstr ""
"%S &lt;message&gt;: Disconnect from the server, with an optional message."

#: command.quote
msgid "%S &lt;command&gt;: Send a raw command to the server."
msgstr "%S &lt;command&gt;: Send a raw command to the server."

#: command.time
msgid "%S: Displays the current local time at the IRC server."
msgstr "%S: Displays the current local time at the IRC server."

#: command.topic
msgid "%S [&lt;new topic&gt;]: View or change the channel topic."
msgstr "%S [&lt;new topic&gt;]: View or change the channel topic."

#: command.umode
msgid "%S (+|-)&lt;new mode&gt;: Set or unset a user mode."
msgstr "%S (+|-)&lt;new mode&gt;: Set or unset a user mode."

#: command.version
msgid "%S &lt;nick&gt;: Request the version of a user's client."
msgstr "%S &lt;nick&gt;: Request the version of a user's client."

#: command.voice
msgid ""
"%S &lt;nick1&gt;[,&lt;nick2&gt;]*: Grant channel voice status to someone. "
"You must be a channel operator to do this."
msgstr ""
"%S &lt;nick1&gt;[,&lt;nick2&gt;]*: Grant channel voice status to someone. "
"You must be a channel operator to do this."

#: command.wallops
msgid ""
"%S &lt;message&gt;: If you don't know what this is, you probably can't use "
"it (sends a command to all connected with the +w flag and all operators on "
"the server."
msgstr ""
"%S &lt;message&gt;: If you don't know what this is, you probably can't use "
"it (sends a command to all connected with the +w flag and all operators on "
"the server."

#: command.whois
msgid "%S &lt;nick&gt;: Get information on a user."
msgstr "%S &lt;nick&gt;: Get information on a user."

#: command.whowas
msgid "%S &lt;nick&gt;: Get information on a user that has logged off."
msgstr "%S &lt;nick&gt;: Get information on a user that has logged off."

#. # LOCALIZATION NOTE (message.*):
#. #    These are shown as system messages in the conversation.
#. #    %1$S is the nick and %2$S is the nick and host of the user who joined.
#: message.join
msgid "%1$S [%2$S] entered the room."
msgstr "دخل %1$S ‏[%2$S] إلى الغرفة."

#. #    %1$S is the nick of who kicked you.
#. #    %2$S is message.kicked.reason, if a kick message was given.
#: message.kicked.you
msgid "You have been kicked by %1$S%2$S."
msgstr "ركلك %1$S%2$S."

#. #    %1$S is the nick that is kicked, %2$S the nick of the person who kicked
#. #    %1$S. %3$S is message.kicked.reason, if a kick message was given.
#: message.kicked
msgid "%1$S has been kicked by %2$S%3$S."
msgstr "رُكِل %1$S بواسطة %2$S%3$S."

#. #    %S is the kick message
#: message.kicked.reason
msgctxt "message.kicked.reason"
msgid ": %S"
msgstr ": %S"

#. #    %1$S is the nickname of the user whose mode was changed, %2$S is the new
#. #    mode and %3$S is who set the mode.
#: message.mode
msgid "mode (%1$S %2$S) by %3$S."
msgstr "mode (%1$S %2$S) by %3$S."

#. #    %1$S is the old nick and %2$S is the new nick.
#: message.nick
msgid "%1$S is now known as %2$S."
msgstr "تغيّر اسم %1$S إلى %2$S."

#. #    %S is your new nick.
#: message.nick.you
msgid "You are now known as %S."
msgstr "تغيّر اسمك إلى %S."

#. #    The paramter is the message.parted.reason, if a part message is given.
#: message.parted.you
msgid "You have left the room (Part%1$S)."
msgstr "غادرت الغرفة (مغادرة%1$S)."

#. #    %1$S is the user's nick, %2$S is message.parted.reason, if a part message is given.
#: message.parted
msgid "%1$S has left the room (Part%2$S)."
msgstr "غادر %1$S الغرفة (مغادرة%2$S)."

#. #    %S is the part message supplied by the user.
#: message.parted.reason
msgctxt "message.parted.reason"
msgid ": %S"
msgstr ": %S"

#. #    %1$S is the user's nick, %2$S is message.quit2 if a quit message is given.
#: message.quit
msgid "%1$S has left the room (Quit%2$S)."
msgstr "غادر %1$S الغرفة (خروج%2$S)."

#. #    The paramter is the quit message given by the user.
#: message.quit2
msgctxt "message.quit2"
msgid ": %S"
msgstr ": %S"

#. #    %1$S is the user who changed the topic, %2$S is the new topic.
#: message.topicChanged
msgid "%1$S has changed the topic to: %2$S."
msgstr "غيّر %1$S الموضوع إلى %2$S."

#. #    %1$S is the user who cleared the topic.
#: message.topicCleared
msgid "%1$S has cleared the topic."
msgstr "مسح %1$S الموضوع."

#. #    %1$S is the conversation name, %2$S is the topic.
#: message.topic
msgid "The topic for %1$S is: %2$S."
msgstr "موضوع %1$S هو: %2$S."

#. #    %S is the conversation name.
#: message.topicNotSet
msgid "There is no topic for %S."
msgstr "لا موضوع للغرفة %S."

#. #    %1$S is the nickname of the invited user, %2$S is the conversation name
#. #    they were invited to.
#: message.invited
msgid "%1$S was successfully invited to %2$S."
msgstr "دُعي %1$S بنجاح إلى الغرفة %2$S."

#. #    %S is the nickname of the user who was summoned.
#: message.summoned
msgid "%S was summoned."
msgstr "استُدعي %S."

#. #    %S is the nickname of the user whose WHOIS information follows this message.
#: message.whois
msgid "WHOIS information for %S:"
msgstr "معلومات WHOIS ل‍ %S:"

#. #    %1$S is the nickname of the (offline) user whose WHOWAS information follows this message.
#: message.whowas
msgid "%1$S is offline. WHOWAS information for %1$S:"
msgstr "‏%1$S غير متصل. معلومات WHOWAS ل‍ %1$S:"

#. #    %1$S is the entry description (from tooltip.*), %2$S is its value.
#: message.whoisEntry
msgid "਀a0਀a0%1$S: %2$S"
msgstr "਀a0਀a0%1$S: %2$S"

#. #    %S is the nickname that is not known to the server.
#: message.unknownNick
msgid "%S is an unknown nickname."
msgstr "اسم كنية غير معروف: %S."

#. # LOCALIZATION NOTE (error.*):
#. #    These are shown as error messages in the server tab.
#. #    %S is the channel name.
#: error.noChannel
msgctxt "error.noChannel"
msgid "There is no channel: %S."
msgstr "لا قناة: %S."

#: error.tooManyChannels
msgid "Cannot join %S; you've joined too many channels."
msgstr "تعذّر الانضمام إلى %S؛ لقد انضممت إلى قنوات كثيرة."

#. #    %1$S is your new nick, %2$S is the kill message from the server.
#: error.nickCollision
msgid "Nick already in use, changing nick to %1$S [%2$S]."
msgstr "اسم الكنية مستخدم بالفعل، سيُغيّر إلى %1$S ‏[%2$S]."

#: error.banned
msgid "You are banned from this server."
msgstr "أنت محظور من على هذا الخادوم."

#: error.bannedSoon
msgid "You will soon be banned from this server."
msgstr "ستُحظر قريبا جدا من على هذا الخادوم."

#: error.mode.wrongUser
msgid "You cannot change modes for other users."
msgstr "You cannot change modes for other users."

#. #    %S is the nickname or channel name that isn't available.
#: error.noSuchNick
msgid "%S is not online."
msgstr "‏%S متّصل الآن."

#: error.wasNoSuchNick
msgid "There was no nickname: %S"
msgstr "لم يُكن هناك اسم كنية: %S"

#: error.noSuchChannel
msgctxt "error.noSuchChannel"
msgid "There is no channel: %S."
msgstr "لا قناة: %S."

#. #    %S is the channel name.
#: error.cannotSendToChannel
msgid "You cannot send messages to %S."
msgstr "لا يمكنك إرسال رسائل إلى %S."

#: error.nonUniqueTarget
msgid ""
"%S is not a unique user@host or shortname or you have tried to join too many "
"channels at once."
msgstr ""
"%S is not a unique user@host or shortname or you have tried to join too many "
"channels at once."

#: error.notChannelOp
msgid "You are not a channel operator on %S."
msgstr "لست منظّم قناة على %S."

#. # LOCALIZATION NOTE (tooltip.*):
#. #    These are the descriptions given in a tooltip with information received
#. #    from a whois response.
#. #    The human readable ("realname") description of the user.
#: tooltip.realname
msgid "Name"
msgstr "الاسم"

#. #    The username and hostname that the user connects from (usually based on the
#. #    reverse DNS of the user's IP, but often mangled by the server to
#. #    protect users).
#: tooltip.connectedFrom
msgid "Connected from"
msgstr "متّصل من"

#. # The away message of the user
#: tooltip.away
msgid "Away"
msgstr "غائب"

#: tooltip.ircOp
msgid "IRC Operator"
msgstr "منظِّم IRC"

#: tooltip.channels
msgid "Currently on"
msgstr "حاليا على"

#: tooltip.server
msgid "Connected to"
msgstr "متّصل ب‍"

#. #    %1$S is the server name, %2$S is the server location.
#: tooltip.serverValue
msgid "%1$S (%2$S)"
msgstr "%1$S (%2$S)"

#: tooltip.idleTime
msgid "Idle for"
msgstr "خامل منذ"
