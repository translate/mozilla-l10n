#: tooltip.subscription
msgid "Subscription"
msgstr "Abonnement"

#: tooltip.fullName
msgid "Full Name"
msgstr "Fullt namn"

#: tooltip.nickname
msgid "Nickname"
msgstr "Kallenamn"

#: tooltip.email
msgid "Email"
msgstr "E-postadresse"

#: tooltip.birthday
msgid "Birthday"
msgstr "Fødselsdag"

#: tooltip.userName
msgid "Username"
msgstr "Brukarnamn"

#: tooltip.title
msgid "Title"
msgstr "Tittel"

#: tooltip.organization
msgid "Organization"
msgstr "Organisasjon"

#: tooltip.locality
msgid "Locality"
