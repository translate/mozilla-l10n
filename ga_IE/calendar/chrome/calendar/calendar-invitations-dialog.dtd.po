msgid "Accept"
msgstr "Glac Leis"

#: calendar.invitations.list.decline.button.label
msgid "Decline"
msgstr "Diúltaigh"

#: calendar.invitations.list.recurrentevent.text
msgid "Repeating event"
msgstr "Á athdhéanamh gach"

#: calendar.invitations.list.alldayevent.text
msgid "All day event"
msgstr "Imeacht ar feadh an lae"

#: calendar.invitations.list.location.text
msgid "Location: "
msgstr "Suíomh: "

#: calendar.invitations.list.organizer.text
msgid "Organizer: "
msgstr "Eagraí: "

#: calendar.invitations.list.attendee.text
msgid "Attendee: "
msgstr "Rannpháirtí: "

#: calendar.invitations.list.none.text
msgid "None"
msgstr "Gan sonrú"
