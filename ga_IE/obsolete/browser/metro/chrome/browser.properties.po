#. # LOCALIZATION NOTE : FILE Capitalized phrases like "Top Sites", "Bookmarks",
#. # and "Recent History" are typically used as proper noun phrases to refer to
#. # the specific visual set (usually displayed as a grid) of top sites,
#. # bookmarks, etc. displayed on screen, rather than those concepts in general.
#. # Buttons (like with contextAppbar2.pin.topSites) refer to actions against
#. # the specific on-screen sets with similarly-named headings.
#. # LOCALIZATION NOTE (browser.search.contextTextSearchLabel2): search context
#. # menu item text will be: |Search (browser.search.defaultenginename) for "string"|
#. # browser.search.defaultenginename is defined in region.properties
#: browser.search.contextTextSearchLabel2
msgid "Search %S for \"%S\""
msgstr "Déan cuardach %S ar \"%S\""

#. # Contextual Appbar - Button Labels
#: contextAppbar2.pin.topSites
msgid "Pin to Top Sites"
msgstr "Feistigh leis na Barrshuímh"

#: contextAppbar2.pin.bookmarks
msgid "Pin to Bookmarks"
msgstr "Feistigh leis na Leabharmharcanna"

#: contextAppbar2.pin.recentHistory
msgid "Pin to Recent History"
msgstr "Feistigh leis an Stair is Déanaí"

#: contextAppbar2.pin.downloads
msgid "Pin to Downloads"
msgstr "Feistigh leis na hÍosluchtuithe"

#: contextAppbar2.unpin.topSites
msgid "Unpin from Top Sites"
msgstr "Dífheistigh ó na Barrshuímh"

#: contextAppbar2.hide.bookmarks
msgid "Hide Bookmark"
msgstr "Cuir Leabharmharc i bhFolach"

#: contextAppbar2.hide.recentHistory
msgid "Hide"
msgstr "Folaigh"

#: contextAppbar2.unpin.downloads
msgid "Unpin from Downloads"
msgstr "Dífheistigh ó na hÍosluchtuithe"

#. # LOCALIZATION NOTE (contextAppbar2.delete): Deletes selected pages.
#: contextAppbar2.delete
msgid "Delete"
msgstr "Scrios"

#. # LOCALIZATION NOTE (contextAppbar2.restore): Undoes a previous deletion.
#. # Button with this label only appears immediately after a deletion.
#: contextAppbar2.restore
msgid "Undo delete"
msgstr "Cealaigh: scriosadh"

#. # LOCALIZATION NOTE (contextAppbar2.clear): Unselects pages without modification.
#: contextAppbar2.clear
msgid "Clear selection"
msgstr "Glan roghnúchán"

#. # Clear private data
#: clearPrivateData.clearButton
msgid "Clear"
msgstr "Bánaigh"

#: clearPrivateData.title2
msgid "Clear private data"
msgstr "Glan sonraí príobháideacha"

#. # LOCALIZATION NOTE (clearPrivateData.message3): "Options" is the optionsCharm.
#: clearPrivateData.message3
msgid ""
"This will permanently delete the private data you have selected in \"Options"
"\"."
msgstr ""
"Scriosfar na sonraí príobháideacha a roghnaigh tú faoi \"Roghanna\" go brách."

#. # Settings Charms
#: aboutCharm1
msgid "About"
msgstr "Maidir Leis"

#: optionsCharm
msgid "Options"
msgstr "Roghanna"

# all of these look good as verbs as of 8 Feb 2011
#: searchCharm
msgid "Search"
msgstr "Cuardaigh"

#: feedbackCharm
msgid "Feedback (online)"
msgstr "Aiseolas (ar líne)"

#: helpOnlineCharm
msgid "Help (online)"
msgstr "Cabhair (ar líne)"

#. # General
#. # LOCALIZATION NOTE (browserForSaveLocation): Title for the "Save..." file picker dialog
#: browserForSaveLocation
msgid "Save Location"
msgstr "Sábháil"

#. # LOCALIZATION NOTE (browserForSaveLocation): Title for the "Open..." file picker dialog
#: browserForOpenLocation
msgid "Open Location"
msgstr "Oscail"

#. # Download Manager
#: downloadsUnknownSize
msgid "Unknown size"
msgstr "Méid anaithnid"

#: downloadOpen
msgid "Open"
msgstr "Oscail"

#: downloadSave
msgid "Save"
msgstr "Sábháil"

#: downloadCancel
msgid "Cancel"
msgstr "Cealaigh"

#: downloadTryAgain
msgid "Try Again"
msgstr "Déan Iarracht Arís"

#: downloadOpenNow
msgid "Open it now"
msgstr "Oscail anois é"

#. # LOCALIZATION NOTE (downloadShowInFiles): 'Files' refers to the Windows 8 file explorer
#: downloadShowInFiles
msgid "Show in Files"
msgstr "Taispeáin i gComhaid"

#. # Alerts
#: alertLinkBookmarked
msgid "Bookmark added"
msgstr "Leabharmharc cruthaithe"

#: alertDownloads
msgid "Downloads"
msgstr "Íosluchtuithe"

#: alertDownloadsStart
msgid "Downloading: %S"
msgstr "%S á íosluchtú"

#: alertDownloadsDone
msgid "%S has finished downloading"
msgstr "Tá íosluchtú %S críochnaithe"

#. # LOCALIZATION NOTE (alertDownloadsStart2): Used in a notification bar for download progress
#. # #1 is the file name, #2 is the amount downloaded so far / total amount to download, and #3 is seconds remaining.
#: alertDownloadsStart2
msgid "Downloading #1, #2, #3"
msgstr "#1 á íosluchtú, #2, #3"

#: alertDownloadsDone2
msgid "%S has been downloaded"
msgstr "Bhí %S íosluchtaithe"

#: alertTapToSave
msgid "Tap to save this file."
msgstr "Tapáil chun an comhad seo a shábháil."

#: alertDownloadsSize
msgid "Download too big"
msgstr "Tá an t-íosluchtú rómhór"

#: alertDownloadsNoSpace
msgid "Not enough storage space"
msgstr "Níl go leor spás saor ann"

#. # LOCALIZATION NOTE (alertDownloadSave2): #1 is the file name, #2 is the file size, #3 is the file host
#: alertDownloadSave2
msgid "Do you want to open or save #1 (#2) from #3?"
msgstr "An bhfuil fonn ort #1 (#2) a oscailt nó a shábháil ó #3?"

#. # LOCALIZATION NOTE (alertDownloadMultiple): Semi-colon list of plural forms.
#. # See: http://developer.mozilla.org/en/docs/Localization_and_Plurals
#. # #1 is the number of files, #2 is (amount downloaded so far / total amount to download) and #3 is seconds remaining
#: alertDownloadMultiple
msgid "Downloading one file, #2, #3;Downloading #1 files, #2, #3"
msgstr ""
"Comhad amháin á íosluchtú, #2, #3;#1 chomhad á n-íosluchtú, #2, #3;#1 "
"chomhad á n-íosluchtú, #2, #3;#1 gcomhad á n-íosluchtú, #2, #3;#1 comhad á n-"
"íosluchtú, #2, #3"

#. # LOCALIZATION NOTE (alertMultipleDownloadsComplete): Semi-colon list of plural forms.
#. # See: http://developer.mozilla.org/en/docs/Localization_and_Plurals
#. # #1 is the number of downloads completed
#: alertMultipleDownloadsComplete
msgid "One download has been completed;#1 downloads have been completed"
msgstr ""
"Íosluchtú amháin críochnaithe;#1 íosluchtú críochnaithe;#1 íosluchtú "
"críochnaithe;#1 n-íosluchtú críochnaithe;#1 íosluchtú críochnaithe"

#: alertDownloadFailed
msgid "Sorry, downloading %S failed."
msgstr "Tá brón orainn, theip ar íosluchtú %S."

#. # Popup Blocker
#. # LOCALIZATION NOTE (popupWarning.message): Semi-colon list of plural forms.
#. # See: http://developer.mozilla.org/en/docs/Localization_and_Plurals
#. # #1 is brandShortName and #2 is the number of pop-ups blocked.
#: popupWarning.message
msgid ""
"#1 prevented this site from opening a pop-up window.;#1 prevented this site "
"from opening #2 pop-up windows."
msgstr ""
"Níor lig #1 don suíomh seo preabfhuinneog a oscailt.;Níor lig #1 don suíomh "
"seo #2 phreabfhuinneog a oscailt.;Níor lig #1 don suíomh seo #2 "
"phreabfhuinneog a oscailt.;Níor lig #1 don suíomh seo #2 bpreabfhuinneog a "
"oscailt.;Níor lig #1 don suíomh seo #2 preabfhuinneog a oscailt."

#: popupButtonAllowOnce2
msgid "Allow once"
msgstr "Ceadaigh uair amháin"

#: popupButtonAlwaysAllow3
msgid "Always allow"
msgstr "Ceadaigh i gcónaí"

#: popupButtonNeverWarn3
msgid "Never allow"
msgstr "Ná ceadaigh riamh"

#. # ContentPermissionsPrompt
#: contentPermissions.alwaysForSite
msgid "Always for this site"
msgstr "I gcónaí ar an suíomh seo"

#: contentPermissions.neverForSite
msgid "Never for this site"
msgstr "Choíche ar an suíomh seo"

#. # Geolocation UI
