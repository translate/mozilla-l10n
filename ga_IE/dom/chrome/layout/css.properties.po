#: MimeNotCss
msgid ""
"The stylesheet %1$S was not loaded because its MIME type, \"%2$S\", is not "
"\"text/css\"."
msgstr ""
"Stílbhileog %1$S gan luchtú, toisc nach ionann a chineál MIME, \"%2$S\", "
"agus \"text/css\"."

#: MimeNotCssWarn
msgid ""
"The stylesheet %1$S was loaded as CSS even though its MIME type, \"%2$S\", "
"is not \"text/css\"."
msgstr ""
"Stílbhileog %1$S arna luchtú, cé nach ionann a chineál MIME, \"%2$S\", agus "
"\"text/css\"."

#: PEUnexpEOF2
msgid "Unexpected end of file while searching for %1$S."
msgstr "Deireadh comhaid gan súil leis agus %1$S á chuardach."

#: PEParseRuleWSOnly
msgid "Whitespace-only string given to be parsed as rule."
msgstr "Tugadh teaghrán gan ach spás bán ann le parsáil mar riail."

#: PEDeclDropped
msgid "Declaration dropped."
msgstr "Ligeadh an fógra ar lár."

#: PEDeclSkipped
msgid "Skipped to next declaration."
msgstr "Imithe ar aghaidh go dtí an chéad fhógra eile."

#: PEUnknownProperty
msgid "Unknown property '%1$S'."
msgstr "Airí anaithnid \"%1$S\"."

#: PEValueParsingError
msgid "Error in parsing value for '%1$S'."
msgstr "Earráid i luach parsála le haghaidh '%1$S'."

#: PEExpectEndValue
msgid "Expected end of value but found '%1$S'."
msgstr "Bhíothas ag súil le deireadh luacha ach fuarthas '%1$S'."

#: PERuleTrailing
msgid "Expected end of rule but found '%1$S'."
msgstr "Bhíothas ag súil le deireadh rialach ach fuarthas '%1$S'."

#: PESkipAtRuleEOF2
msgid "end of at-rule"
msgstr "deireadh le @-riail"

#: PEUnknownAtRule
msgid "Unrecognized at-rule or error parsing at-rule '%1$S'."
msgstr "@-riail anaithnid, nó earráid pharsála san @-riail \"%1$S\"."

#: PECharsetRuleEOF
msgid "charset string in @charset rule"
msgstr "teaghrán tacair charachtar i riail @charset"

#: PECharsetRuleNotString
msgid "Expected charset string but found '%1$S'."
msgstr "Bhíothas ag súil le teaghrán tacair charachtar ach fuarthas \"%1$S\"."

#: PEGatherMediaEOF
msgid "end of media list in @import or @media rule"
msgstr "deireadh le liosta meán i riail @import nó @media"

#: PEGatherMediaNotComma
msgid "Expected ',' in media list but found '%1$S'."
msgstr "Bhíothas ag súil le \",\" i liosta na meán ach fuarthas \"%1$S\"."

#: PEGatherMediaNotIdent
msgid "Expected identifier in media list but found '%1$S'."
msgstr ""
"Bhíothas ag súil le haitheantóir i liosta na meán ach fuarthas \"%1$S\"."

#: PEGatherMediaReservedMediaType
msgid "Found reserved keyword '%1$S' when looking for media type."
msgstr "Fuarthas eochairfhocal coimeádta '%1$S' agus cineál an mheáin á lorg."

#: PEParseSourceSizeListEOF
msgid "length value for matched media condition"
msgstr "luach an fhaid le haghaidh coinníoll meán meaitseáilte"

#: PEParseSourceSizeListNotComma
msgid "Expected ',' after value but found '%1$S'"
msgstr "Bhíothas ag súil le ',' tar éis luacha ach fuarthas '%1$S'"

#: PEImportNotURI
msgid "Expected URI in @import rule but found '%1$S'."
msgstr "Bhíothas ag súil le URI i riail @import ach fuarthas \"%1$S\"."

#: PEImportBadURI
msgid "Invalid URI in @import rule: '%1$S'."
msgstr "URI neamhbhailí i riail @import: '%1$S'."

#: PEImportUnexpected
msgid "Found unexpected '%1$S' within @import."
msgstr "Fuarthas \"%1$S\" taobh istigh de @import gan súil leis."

#: PEGroupRuleEOF2
msgid "end of @media, @supports or @-moz-document rule"
msgstr "deireadh rialach @media, @supports nó @-moz-document"

#: PEGroupRuleNestedAtRule
msgid "%1$S rule not allowed within @media or @-moz-document rule."
msgstr ""
"Ní cheadaítear riail %1$S taobh istigh de riail @media nó @-moz-document."

#: PEMozDocRuleBadFunc2
msgid ""
"Expected url(), url-prefix(), domain() or regexp() in @-moz-document rule "
"but found '%1$S'."
msgstr ""
"Bhíothas ag súil le url(), url-prefix(), domain() nó regexp() i riail @-moz-"
"document, ach fuarthas '%1$S'."

#: PEMozDocRuleNotURI
msgid "Expected URI in @-moz-document rule but found '%1$S'."
msgstr "Bhíothas ag súil le URI i riail @-moz-document ach fuarthas \"%1$S\"."

#: PEMozDocRuleNotString
msgid ""
"Expected string in @-moz-document rule regexp() function but found '%1$S'."
msgstr ""
"Bhíothas ag súil le teaghrán i bhfeidhm regexp() do riail @-moz-document ach "
"fuarthas '%1$S'."

#: PEMozDocRuleEOF
msgid "next URI in @-moz-document rule"
msgstr "an chéad URI eile i riail @-moz-document"

#: PEAtNSPrefixEOF
msgid "namespace prefix in @namespace rule"
msgstr "réimír ainmspáis i riail @namespace"

#: PEAtNSURIEOF
msgid "namespace URI in @namespace rule"
msgstr "URI ainmspáis i riail @namespace"

#: PEAtNSUnexpected
msgid "Unexpected token within @namespace: '%1$S'."
msgstr "Teaghrán comhartha gan súil leis i @namespace: \"%1$S\"."

#: PEKeyframeNameEOF
msgid "name of @keyframes rule."
msgstr "ainm riail @keyframes."

#: PEKeyframeBadName
msgid "Expected identifier for name of @keyframes rule."
msgstr "Bhíothas ag súil le haitheantóir mar ainm ar riail @keyframes."

#: PEKeyframeBrace
msgid "Expected opening { of @keyframes rule."
msgstr "Bhíothas ag súil le { ag tús riail @keyframes."

#: PESkipDeclBraceEOF
msgid "closing } of declaration block"
msgstr "\"}\" dúnta de bhloc fógartha"

#: PESkipRSBraceEOF
msgid "closing } of invalid rule set"
msgstr "\"}\" dúnta de thacar rialacha neamhbhailí "

#: PEBadSelectorRSIgnored
msgid "Ruleset ignored due to bad selector."
msgstr "Tugadh neamhaird ar thacar rialacha de bharr roghnóra mícheart."

#: PEBadSelectorKeyframeRuleIgnored
msgid "Keyframe rule ignored due to bad selector."
msgstr "Tugadh neamhaird ar riail @keyframe de bharr roghnóra mhícheart."

#: PESelectorListExtraEOF
msgid "',' or '{'"
msgstr "\",\" nó \"{\""

#: PESelectorListExtra
msgid "Expected ',' or '{' but found '%1$S'."
msgstr "Bhíothas ag súil le \",\" nó \"{\" ach fuarthas \"%1$S\"."

# Strings for the cert picker dialog
#: PESelectorGroupNoSelector
msgid "Selector expected."
msgstr "Bhíothas ag súil le roghnóir."

#: PESelectorGroupExtraCombinator
msgid "Dangling combinator."
msgstr "Comhcheanglóir ar liobarna."

#: PECounterStyleNotIdent
msgid "Expected identifier for name of @counter-style rule."
msgstr "Bhíothas ag súil le haitheantóir mar ainm ar riail @counter-style."

#: PECounterStyleBadName
msgid "Name of @counter-style rule can't be '%1$S'."
msgstr "Ní cheadaítear '%1$S' mar ainm ar riail @counter-style."

#: PECounterStyleBadBlockStart
msgid "Expected '{' to begin @counter-style rule but found '%1$S'."
msgstr ""
"Bhíothas ag súil '{' ag tús rialach @counter-style ach fuarthas '%1$S'."

#: PECounterStyleEOF
msgid "closing '}' of @counter-style block"
msgstr "'}' deiridh i mbloc @counter-style"

#: PECounterDescExpected
msgid "Expected counter descriptor but found '%1$S'."
msgstr "Bhíothas ag súil le cur síos ar áiritheoir ach fuarthas '%1$S'."

#: PEUnknownCounterDesc
msgid "Unknown descriptor '%1$S' in @counter-style rule."
msgstr "Cur síos anaithnid '%1$S' i riail @counter-style."

#: PECounterExtendsNotIdent
msgid "Expected identifier for extends system but found '%1$S'."
msgstr ""
"Bhíothas ag súil le haitheantóir le haghaidh extend system ach fuarthas "
"'%1$S'."

#: PECounterASWeight
msgid ""
"Each weight in the additive-symbols descriptor must be smaller than the "
"previous weight."
msgstr ""
"Ní mór do gach meáchan sa tuarascálaí additive-symbols a bheith níos lú ná "
"an meáchan roimhe."

#: PEClassSelEOF
msgid "class name"
msgstr "ainm aicme"

#: PEClassSelNotIdent
msgid "Expected identifier for class selector but found '%1$S'."
msgstr ""
"Bhíothas ag súil le haitheantóir le haghaidh roghnóir aicme ach fuarthas "
"\"%1$S\"."

#: PECoordinatePair
msgid "Expected coordinate pair but found '%1$S'."
msgstr "Bhíothas ag súil le péire comhordanáidí ach fuarthas '%1$S'."

#: PETypeSelEOF
msgid "element type"
msgstr "cineál eiliminte"

#: PETypeSelNotType
msgid "Expected element name or '*' but found '%1$S'."
msgstr "Bhíothas ag súil le hainm eiliminte nó \"*\" ach fuarthas \"%1$S\"."

#: PEUnknownNamespacePrefix
msgid "Unknown namespace prefix '%1$S'."
msgstr "Réimír anaithnid ainmspáis \"%1$S\"."

#: PEAttributeNameEOF
msgid "attribute name"
msgstr "ainm tréithe"

#: PEAttributeNameExpected
msgid "Expected identifier for attribute name but found '%1$S'."
msgstr ""
"Bhíothas ag súil le haitheantóir le haghaidh ainm aitreabúide ach fuarthas "
"\"%1$S\"."

#: PEAttributeNameOrNamespaceExpected
msgid "Expected attribute name or namespace but found '%1$S'."
msgstr "Bhíothas ag súil le hainm tréithe nó ainmspás ach fuarthas \"%1$S\"."

#: PEAttSelNoBar
msgid "Expected '|' but found '%1$S'."
msgstr "Bhíothas ag súil le \"|\" ach fuarthas \"%1$S\"."

#: PEAttSelInnerEOF
msgid "part of attribute selector"
msgstr "cuid de roghnóir tréithe"

#: PEAttSelUnexpected
msgid "Unexpected token in attribute selector: '%1$S'."
msgstr "Teaghrán comhartha gan súil leis i roghnóir aitreabúide: \"%1$S\"."

#: PEAttSelValueEOF
msgid "attribute value"
msgstr "luach na haitreabúide"

#: PEAttSelCloseEOF
msgid "']' to end attribute selector"
msgstr "\"]\" chun roghnóir aitreabúide a chríochnú"

#: PEAttSelNoClose
msgid "Expected ']' to terminate attribute selector but found '%1$S'."
msgstr ""
"Bhíothas ag súil le \"]\" mar chríoch le roghnóir aitreabúide ach fuarthas "
"\"%1$S\"."

#: PEAttSelBadValue
msgid ""
"Expected identifier or string for value in attribute selector but found "
"'%1$S'."
msgstr ""
"Bhíothas ag súil le haitheantóir nó teaghrán mar luach i roghnóir "
"aitreabúide ach fuarthas \"%1$S\"."

#: PEPseudoSelEOF
msgid "name of pseudo-class or pseudo-element"
msgstr "ainm bréagaicme nó bréageiliminte"

#: PEPseudoSelBadName
msgid ""
"Expected identifier for pseudo-class or pseudo-element but found '%1$S'."
msgstr ""
"Bhíothas ag súil le haitheantóir bréagaicme nó bréageiliminte ach fuarthas "
"\"%1$S\"."

#: PEPseudoSelNonFunc
msgid ""
"Function token for non-function pseudo-class or pseudo-element, or the other "
"way around, when reading '%1$S'."
msgstr ""
"Fuarthas comhartha feidhme in áit bhréagaicme nó bréageiliminte nach feidhm "
"é, nó é sin droim ar ais, agus \"%1$S\" á léamh."

#: PEPseudoSelNotPE
msgid "Expected pseudo-element but found '%1$S'."
msgstr "Bhíothas ag súil le bréageilimint ach fuarthas \"%1$S\"."

#: PEPseudoSelDoubleNot
msgid "Negation pseudo-class can't be negated '%1$S'."
msgstr "Ní féidir bréagaicme dhiúltaithe a dhiúltú \"%1$S\"."

#: PEPseudoSelPEInNot
msgid "Pseudo-elements can't be negated '%1$S'."
msgstr "Ní féidir bréageilimintí a dhiúltú \"%1$S\"."

#: PEPseudoSelNewStyleOnly
msgid "This pseudo-element must use the \"::\" form: '%1$S'."
msgstr "Caithfidh an bhréageilimint seo an leagan \"::\" a úsáid: \"%1$S\"."

#: PEPseudoSelEndOrUserActionPC
msgid ""
"Expected end of selector or a user action pseudo-class after pseudo-element "
"but found '%1$S'."
msgstr ""
"Bhíothas ag súil le deireadh roghnóra nó bréagaicme ghnímh úsáideora tar éis "
"bréageiliminte, ach fuarthas '%1$S'."

#: PEPseudoSelNoUserActionPC
msgid ""
"Expected end of selector after pseudo-element that does not support user "
"action pseudo-classes but found '%1$S'."
msgstr ""
"Bhíothas ag súil le deireadh roghnóra tar éis bréageiliminte nach dtacaíonn "
"le bréagaicmí gnímh úsáideora, ach fuarthas '%1$S'."

#: PEPseudoSelMultiplePE
msgid "Extra pseudo-element '%1$S'."
msgstr "Bréageilimint bhreise \"%1$S\"."

#: PEPseudoSelUnknown
msgid "Unknown pseudo-class or pseudo-element '%1$S'."
msgstr "Bréagaicme nó bréageilimint anaithnid: \"%1$S\"."

#: PENegationEOF
msgid "selector within negation"
msgstr "roghnóir taobh istigh de shéanadh"

#: PENegationBadInner
msgid "Malformed simple selector as negation pseudo-class argument '%1$S'."
msgstr ""
"Roghnóir simplí míchumtha mar argóint don bhréagaicme shéanta \"%1$S\"."

#: PENegationNoClose
msgid "Missing closing ')' in negation pseudo-class '%1$S'."
msgstr "\")\" dúnta ar iarraidh i mbréagaicme shéanta \"%1$S\"."

#: PENegationBadArg
msgid "Missing argument in negation pseudo-class '%1$S'."
msgstr "Argóint ar iarraidh i mbréagaicme shéanta \"%1$S\"."

#: PEPseudoClassArgEOF
msgid "argument to pseudo-class selector"
msgstr "argóint do roghnóir bréagaicme"

#: PEPseudoClassArgNotIdent
msgid "Expected identifier for pseudo-class parameter but found '%1$S'."
msgstr ""
"Bhíothas ag súil le haitheantóir mar pharaiméadar na bréagaicme ach fuarthas "
"\"%1$S\"."

#: PEPseudoClassArgNotNth
msgid "Expected part of argument to pseudo-class but found '%1$S'."
msgstr ""
"Bhíothas ag súil le cuid de argóint le haghaidh bréagaicme ach fuarthas "
"\"%1$S\"."

#: PEPseudoClassNoClose
msgid "Missing closing ')' in pseudo-class, found '%1$S' instead."
msgstr "\")\" dúnta ar iarraidh i mbréagaicme, fuarthas \"%1$S\" ina ionad."

#: PEPseudoClassNoArg
msgid "Missing argument in pseudo-class '%1$S'."
msgstr "Argóint ar iarraidh i mbréagaicme \"%1$S\"."

#: PEPseudoClassNotUserAction
msgid ""
"Expected end of selector or a user action pseudo-class after pseudo-element "
"but found pseudo-class '%1$S'."
msgstr ""
"Bhíothas ag súil le deireadh roghnóra nó bréagaicme ghnímh úsáideora tar éis "
"bréageiliminte ach fuarthas bréagaicme '%1$S'."

#: PESelectorEOF
msgid "selector"
msgstr "roghnóir"

#: PEBadDeclBlockStart
msgid "Expected '{' to begin declaration block but found '%1$S'."
msgstr ""
"Bhíothas ag súil le \"{\" chun tús a chur le bloc fógra ach fuarthas \"%1$S"
"\"."

#: PEColorEOF
msgid "color"
msgstr "dath"

#: PEColorNotColor
msgid "Expected color but found '%1$S'."
msgstr "Fuarthas \"%1$S\" ach bhíothas ag súil le dath."

#: PEColorComponentEOF
msgid "color component"
msgstr "comhpháirt datha"

#: PEExpectedPercent
msgid "Expected a percentage but found '%1$S'."
msgstr "Fuarthas \"%1$S\" ach bhíothas ag súil le céatadán."

#: PEExpectedInt
msgid "Expected an integer but found '%1$S'."
msgstr "Bhíothas ag súil le slánuimhir ach fuarthas \"%1$S\"."

#: PEColorBadRGBContents
msgid "Expected number or percentage in rgb() but found '%1$S'."
msgstr "Bhíothas ag súil le huimhir nó céatadán i rgb() ach fuarthas \"%1$S\"."

#: PEColorComponentBadTerm
msgid "Expected '%2$S' but found '%1$S'."
msgstr "Bhíothas ag súil le \"%2$S\" ach fuarthas \"%1$S\"."

#: PEColorHueEOF
msgid "hue"
msgstr "lí"

#: PEExpectedComma
msgid "Expected ',' but found '%1$S'."
msgstr "Bhíothas ag súil le \",\" ach fuarthas \"%1$S\"."

#: PEColorSaturationEOF
msgid "saturation"
msgstr "sáithiú"

#: PEColorLightnessEOF
msgid "lightness"
msgstr "gile"

#: PEColorOpacityEOF
msgid "opacity in color value"
msgstr "teimhneacht i ndath"

#: PEExpectedNumber
msgid "Expected a number but found '%1$S'."
msgstr "Bhíothas ag súil le huimhir ach fuarthas \"%1$S\"."

#: PEPositionEOF
msgid "<position>"
msgstr "<position>"

#: PEExpectedPosition
msgid "Expected <position> but found '%1$S'."
msgstr "Bhíothas ag súil le <position> ach fuarthas '%1$S'."

#: PEExpectedRadius
msgid "Expected radius but found '%1$S'."
msgstr "Bhíothas ag súil le ga ach fuarthas '%1$S'."

#: PEExpectedCloseParen
msgid "Expected ')' but found '%1$S'."
msgstr "Bhíothas ag súil le \")\" ach fuarthas \"%1$S\"."

#: PEDeclEndEOF
msgid "';' or '}' to end declaration"
msgstr "\";\" nó \"}\" chun fógra a chríochnú"

#: PEParseDeclarationNoColon
msgid "Expected ':' but found '%1$S'."
msgstr "Bhíothas ag súil le \":\" ach fuarthas \"%1$S\"."

#: PEParseDeclarationDeclExpected
msgid "Expected declaration but found '%1$S'."
msgstr "Bhíothas ag súil le fógra ach fuarthas \"%1$S\"."

#: PEEndOfDeclEOF
msgid "end of declaration"
msgstr "deireadh fógra"

#: PEImportantEOF
msgid "important"
msgstr "tábhachtach"

#: PEExpectedImportant
msgid "Expected 'important' but found '%1$S'."
msgstr "Bhíothas ag súil le \"important\" ach fuarthas \"%1$S\"."

#: PEBadDeclEnd
msgid "Expected ';' to terminate declaration but found '%1$S'."
msgstr ""
"Bhíothas ag súil le \";\" chun fógra a chríochnú ach fuarthas \"%1$S\"."

#: PEBadDeclOrRuleEnd2
msgid "Expected ';' or '}' to terminate declaration but found '%1$S'."
msgstr ""
"Bhíothas ag súil le \";\" nó \"}\" chun fógra a chríochnú ach fuarthas \"%1$S"
"\"."

#: PEInaccessibleProperty2
msgid "Cannot specify value for internal property."
msgstr "Ní féidir luach a shonrú d'airí inmheánach."

#: PECommentEOF
msgid "end of comment"
msgstr "deireadh nóta tráchta"

#: SEUnterminatedString
msgid "Found unclosed string '%1$S'."
msgstr "Fuarthas teaghrán gan dúnadh: \"%1$S\"."

#: PEFontDescExpected
msgid "Expected font descriptor but found '%1$S'."
msgstr "Bhíothas ag súil le tuarascálaí cló ach fuarthas '%1$S'."

#: PEUnknownFontDesc
msgid "Unknown descriptor '%1$S' in @font-face rule."
msgstr "Tuarascálaí anaithnid '%1$S' i riail @font-face."

#: PEMQExpectedExpressionStart
msgid "Expected '(' to start media query expression but found '%1$S'."
msgstr ""
"Bhíothas ag súil le '(' mar thús sloinn iarratais mheáin ach fuarthas '%1$S'."

#: PEMQExpressionEOF
msgid "contents of media query expression"
msgstr "inneachar sloinn iarratais mheáin"

#: PEMQExpectedFeatureName
msgid "Expected media feature name but found '%1$S'."
msgstr "Bhíothas ag súil le hainm gné meáin ach fuarthas '%1$S'."

#: PEMQExpectedFeatureNameEnd
msgid "Expected ':' or ')' after media feature name but found '%1$S'."
msgstr ""
"Bhíothas ag súil le ':' nó ')' i ndiaidh ainm gné meáin ach fuarthas '%1$S'."

#: PEMQNoMinMaxWithoutValue
msgid "Media features with min- or max- must have a value."
msgstr "Caithfear luach a bheith ag gnéithe meán le min- nó max-."

#: PEMQExpectedFeatureValue
msgid "Found invalid value for media feature."
msgstr "Fuarthas luach neamhbhailí le haghaidh gné meáin."

#: PEBadFontBlockStart
msgid "Expected '{' to begin @font-face rule but found '%1$S'."
msgstr "Bhíothas ag súil le '{' ag tús riail @font-face ach fuarthas '%1$S'."

#: PEBadFontBlockEnd
msgid "Expected '}' to end @font-face rule but found '%1$S'."
msgstr ""
"Bhíothas ag súil le '}' ag deireadh riail @font-face ach fuarthas '%1$S'."

#: PEAnonBoxNotAlone
msgid "Did not expect anonymous box."
msgstr "Ní rabhthas ag súil le bosca gan ainm."

#: PEFFVUnexpectedEOF
msgid "Unexpected end of @font-feature-values rule."
msgstr "Deireadh rialach @font-feature-values gan súil leis."

#: PEFFVBlockStart
msgid "Expected opening { of @font-feature-values rule but found '%1$S'."
msgstr ""
"Bhíothas ag súil le { ag tús na rialach @font-feature-values, ach fuarthas "
"'%1$S'."

#: PEFFVValueSetStart
msgid "Expected opening { of feature value set but found '%1$S'."
msgstr "Bhíothas ag súil le { ag tús tacar feature-value, ach fuarthas '%1$S'."

#: PEFFVNoFamily
msgid ""
"Expected font family list for @font-feature-values rule but found '%1$S'."
msgstr ""
"Bhíothas ag súil le liosta finte clófhoirne i riail @font-feature-values ach "
"fuarthas '%1$S'."

#: PEFFVUnexpectedBlockEnd
msgid "Expected '}' to end @font-feature-values rule but found '%1$S'."
msgstr ""
"Bhíothas ag súil le '}' ag deireadh na rialach @font-feature-values, ach "
"fuarthas '%1$S'."

#: PEFFVUnknownFontVariantPropValue
msgid "Unknown font-variant property value '%1$S'."
msgstr "Luach airí font-variant anaithnid '%1$S'."

#: PEFFVExpectedIdent
msgid "Expected identifier but found '%1$S'."
msgstr "Bhíothas ag súil le haitheantóir, ach fuarthas '%1$S'."

#: PEFFVExpectedValue
msgid "Expected non-negative integer value but found '%1$S'."
msgstr "Bhíothas ag súil le slánuimhir neamhdhiúltach, ach fuarthas '%1$S'."

#: PEFFVTooManyValues
msgid "Too many values for feature type '%1$S'."
msgstr "An iomarca luachanna ar cineál airí '%1$S'."

#: PEFFVGenericInFamilyList
msgid "Family list cannot contain generic font family name."
msgstr ""
"Ní cheadaítear fine clófhoirne chineálach i liosta na bhfinte clófhoirne."

#: PEFFVValueDefinitionTrailing
msgid "Expected end of value definition but found '%1$S'."
msgstr ""
"Bhíothas ag súil le deireadh shainmhíniú an luacha ach fuarthas '%1$S'."

#: PEBadDirValue
msgid "Expected 'ltr' or 'rtl' in direction selector but found '%1$S'."
msgstr ""
"Bhíothas ag súil le 'ltr' nó 'rtl' sa roghnóir treo, ach fuarthas '%1$S'."

#: PESupportsConditionStartEOF2
msgid "'not', '(', or function"
msgstr "'not', '(', nó feidhm"

#: PESupportsConditionInParensEOF
msgid "')'"
msgstr "')'"

