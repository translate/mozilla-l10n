#. #LOCALIZATION NOTE (ImageTitleWithDimensions2AndFile): first %S is filename, second %S is type, third %S is width and fourth %S is height
#. #LOCALIZATION NOTE (ImageTitleWithoutDimensions): first %S is filename, second %S is type
#. #LOCALIZATION NOTE (ImageTitleWithDimensions2): first %S is type, second %S is width and third %S is height
#. #LOCALIZATION NOTE (ImageTitleWithNeitherDimensionsNorFile): first %S is type
#. #LOCALIZATION NOTE (MediaTitleWithFile): first %S is filename, second %S is type
#. #LOCALIZATION NOTE (MediaTitleWithNoInfo): first %S is type
#: ImageTitleWithDimensions2AndFile
msgid "%S (%S Image, %S × %S pixels)"
msgstr "%S (Íomhá %S, %S × %S picteilín)"

#: ImageTitleWithoutDimensions
msgid "%S (%S Image)"
msgstr "%S (Íomhá %S)"

#: ImageTitleWithDimensions2
msgid "(%S Image, %S × %S pixels)"
msgstr "(Íomhá %S, %S × %S picteilín)"

# variable will be something like PNG, JPG
#: ImageTitleWithNeitherDimensionsNorFile
msgid "(%S Image)"
msgstr "(Íomhá %S)"

#: MediaTitleWithFile
msgid "%S (%S Object)"
msgstr "%S (Réad den chineál %S)"

#: MediaTitleWithNoInfo
msgid "(%S Object)"
msgstr "(Réad den chineál %S)"

#: InvalidImage
#, fuzzy
msgid "The image “%S” cannot be displayed because it contains errors."
msgstr "Ní féidir íomhá \"%S\" a thaispeáint toisc go bhfuil earráidí inti."

#: ScaledImage
msgid "Scaled (%S%%)"
msgstr "Scálaithe (%S%%)"

#: TitleWithStatus
msgid "%S - %S"
msgstr "%S - %S"
