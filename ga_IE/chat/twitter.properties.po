#. # LOCALIZATION NOTE (twitter.protocolName)
#. #  This name is used whenever the name of the protocol is shown.
#: twitter.protocolName
msgid "Twitter"
msgstr "Twitter"

#. # LOCALIZATION NOTE (error.*):
#. #   These are errors that will be shown to the user in conversation.
#: error.tooLong
msgid "Status is over 140 characters."
msgstr "Tá an stádas níos faide ná 140 carachtar."

#. # LOCALIZATION NOTE (error.general, error.retweet, error.delete):
#. #   %1$S will be either the error string returned by the twitter server,
#. #   in English, inside parenthesis, or the empty string if we have no specific
#. #   message for the error.
#. #   %2$S is the message that caused the error.
#: error.general
msgid "An error %1$S occurred while sending: %2$S"
msgstr "Tharla earráid %1$S agus an teachtaireacht á seoladh: %2$S"

#: error.retweet
msgid "An error %1$S occurred while retweeting: %2$S"
msgstr "Tharla earráid %1$S le linn atuíteála: %2$S"

#: error.delete
msgid "An error %1$S occurred while deleting: %2$S"
msgstr "Tharla earráid %1$S le linn scriosadh: %2$S"

#. # LOCALIZATION NOTE (error.descriptionTooLong)
#. #   %S is the truncated string that was sent to the server.
#: error.descriptionTooLong
msgid ""
"Description is over the maximum length (160 characters), it was "
"automatically truncated to: %S."
msgstr ""
"Tá an cur siós níos faide ná an t-uasmhéid (160 carachtar); bhí sé "
"giorraithe go huathoibríoch: %S."

#. # LOCALIZATION NOTE (timeline):
#. #   This is the title of the conversation tab, %S will be replaced by
#. #   @<username>.
#: timeline
msgid "%S timeline"
msgstr "Amlíne %S"

#. # LOCALIZATION NOTE (action.*):
#. #  This will be an action in the context menu of displayed tweets.
#: action.copyLink
msgid "Copy Link to Tweet"
msgstr "Cóipeáil Nasc le Tuít"

#: action.retweet
msgid "Retweet"
msgstr "Atuíteáil"

#: action.reply
msgid "Reply"
msgstr "Freagair"

#: action.delete
msgid "Delete"
msgstr "Scrios"

#. # LOCALIZATION NOTE (action.follow, action.stopFollowing):
#. #  %S will be replaced by the screen name of a twitter user.
#: action.follow
msgid "Follow %S"
msgstr "Lean %S"

#: action.stopFollowing
msgid "Stop following %S"
msgstr "Ná lean %S a thuilleadh"

#. # LOCALIZATION NOTE (event.follow, event.unfollow, event.followed):
#. #  This will be displayed in system messages inside the timeline conversation.
#. #  %S will be replaced by the screen name of a twitter user.
#: event.follow
msgid "You are now following %S."
msgstr "Tá tú ag leanúint %S anois."

#: event.unfollow
msgid "You are no longer following %S."
msgstr "Níl tú ag leanúint %S a thuilleadh."

#: event.followed
msgid "%S is now following you."
msgstr "Tá %S do do leanúint anois."

#. # LOCALIZATION NOTE (event.deleted):
#. #  %S will be replaced by the text of the deleted tweet.
#: event.deleted
msgid "You have deleted this tweet: \"%S\"."
msgstr "Scrios tú an tuít seo: \"%S\"."

#. # LOCALIZATION NOTE (replyingToStatusText):
#. #  This will be visible in the status bar of the conversation window
#. #  while the user is typing a reply to a tweet.
#. #  %S will be replaced by the text of the tweet the user is replying to.
#: replyingToStatusText
msgid "Replying to: %S"
msgstr "Freagra ar: %S"

#. # LOCALIZATION NOTE (connection.*):
#. #   These will be displayed in the account manager in order to show the progress
#. #   of the connection.
#. #   (These will be displayed in account.connection.progress from
#. #    accounts.properties, which adds … at the end, so do not include
#. #    periods at the end of these messages.)
#: connection.initAuth
msgid "Initiating authentication process"
msgstr "Próiseas fíordheimhnithe á thosú"

#: connection.requestAuth
msgid "Waiting for your authorization"
msgstr "Ag fanacht le húdarú uait"

#: connection.requestAccess
msgid "Finalizing authentication"
msgstr "Fíordheimhniú á chur i gcrích"

#: connection.requestTimelines
msgid "Requesting user timelines"
msgstr "Amlínte úsáideora á bhfáil"

#. # LOCALIZATION NOTE (connection.error.*):
#. #   These will show in the account manager if an error occurs during the
#. #   connection attempt.
#: connection.error.userMismatch
msgid "Username mismatch."
msgstr "Ainmneacha mí-oiriúnaithe."

#: connection.error.failedToken
msgid "Failed to get request token."
msgstr "Ní bhfuarthas an ceadchomhartha iarratais."

#: connection.error.authCancelled
msgid "You cancelled the authorization process."
msgstr "Chealaigh tú an próiseas údaraithe."

#: connection.error.authFailed
msgid "Failed to get authorization."
msgstr "Theip ar údarú."

#: connection.error.noNetwork
msgid "There is no network connection available."
msgstr "Níl ceangal leis an líonra ar fáil."

#. # LOCALIZATION NOTE (authPrompt):
#. #   This is the prompt in the browser window that pops up to authorize us
#. #   to use a Twitter account. It is shown in the title bar of the authorization
#. #   window.
#: authPrompt
msgid "Give permission to use your Twitter account"
msgstr "Tabhair cead dúinn do chuntas Twitter a úsáid"

#. # LOCALIZATION NOTE (options.*):
#. #   These are the protocol specific options shown in the account manager and
#. #   account wizard windows.
#: options.track
msgid "Tracked keywords"
msgstr "Eochairfhocail lorgtha"

#. # LOCALIZATION NOTE (tooltip.*):
#. #   These are the Twitter information that will appear in the tooltip
#. #   for each participant on the home timeline.
#. # LOCALIZATION NOTE (tooltip.created_at): the date the user joined.
#: tooltip.created_at
msgid "User Since"
msgstr "Úsáideoir ó"

#: tooltip.location
msgid "Location"
msgstr "Suíomh"

#: tooltip.lang
msgid "Language"
msgstr "Teanga"

#: tooltip.time_zone
msgid "Timezone"
msgstr "Crios Ama"

#: tooltip.url
msgid "Homepage"
msgstr "Leathanach Baile"

#. # LOCALIZATION NOTE (tooltip.protected):
#. #  whether the user's tweets are publicly visible.
#: tooltip.protected
msgid "Protects Tweets"
msgstr "Tuíteanna Faoi Rún"

#. # LOCALIZATION NOTE (tooltip.following):
#. #  whether you are subscribed to the user's tweets.
#: tooltip.following
msgid "Currently Following"
msgstr "Á Leanúint Faoi Láthair"

#: tooltip.name
msgid "Name"
msgstr "Ainm"

#: tooltip.description
msgid "Description"
msgstr "Cur Síos"

#. # LOCALIZATION NOTE (tooltip.*_count):
#. #  Please see the right side of the official Twitter website UI.
#: tooltip.friends_count
msgid "Following"
msgstr "Á Leanúint"

#: tooltip.statuses_count
msgid "Tweets"
msgstr "Tuíteanna"

#: tooltip.followers_count
msgid "Followers"
msgstr "Leantóirí"

#: tooltip.listed_count
msgid "Listed"
msgstr "Liostaithe"

#. # LOCALIZATION NOTE (yes, no):
#. #  These are used to turn true/false values into a yes/no response.
#: yes
msgid "Yes"
msgstr "Tá"

#: no
msgid "No"
msgstr "Níl"

#: command.follow
msgid ""
"%S &lt;username&gt;[ &lt;username&gt;]*: Start following a user / users."
msgstr ""
"%S &lt;ainm úsáideora&gt;[ &lt;ainm úsáideora&gt;]*: Lean an t-úsáideoir/na "
"húsáideoirí seo."

#: command.unfollow
msgid "%S &lt;username&gt;[ &lt;username&gt;]*: Stop following a user / users."
msgstr ""
"%S &lt;ainm úsáideora&gt;[ &lt;ainm úsáideora&gt;]*: Ná lean an t-úsáideoir/"
"na húsáideoirí seo a thuilleadh."
