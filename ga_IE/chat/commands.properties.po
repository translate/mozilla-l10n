#. # LOCALIZATION NOTE (commands):
#. #  %S is a comma separated list of command names.
#: commands
msgid ""
"Commands: %S.\n"
"Use /help &lt;command&gt; for more information."
msgstr ""
"Orduithe: %S.\n"
"Úsáid /help &lt;ordú&gt; chun tuilleadh eolais a fháil."

#. # LOCALIZATION NOTE (noCommand, noHelp):
#. #  %S is the command name the user typed.
#: noCommand
msgid "No '%S' command."
msgstr "Níl aon ordú '%S' ann."

#: noHelp
msgid "No help message for the '%S' command, sorry!"
msgstr "Níl cabhair ar fáil le haghadh '%S'"

#: sayHelpString
msgid "say &lt;message&gt;: send a message without processing commands."
msgstr ""
"say &lt;teachtaireacht&gt;: seol teachtaireacht gan orduithe a phróiseáil."

#: rawHelpString
msgid "raw &lt;message&gt;: send a message without escaping HTML entities."
msgstr ""
"raw &lt;teachtaireacht&gt;: seol teachtaireacht gan aonáin HTML a éalú."

#: helpHelpString
msgid ""
"help &lt;name&gt;: show the help message for the &lt;name&gt; command, or "
"the list of possible commands when used without parameter."
msgstr ""
"help &lt;ainm&gt;: taispeáin teachtaireacht chabhrach le haghaidh &lt;"
"ainm&gt;, nó, gan paraiméadar, taispeáin liosta na n-orduithe atá ar fáil."

#. # LOCALIZATION NOTE (statusCommand):
#. #  %1$S is replaced with a status command name
#. #   (one of "back", "away", "busy", "dnd", or "offline").
#. #  %2$S is replaced with the localized version of that status type
#. #   (one of the 5 strings below).
#: statusCommand
msgid ""
"%1$S &lt;status message&gt;: set the status to %2$S with an optional status "
"message."
msgstr ""
"%1$S &lt;stádas&gt;: socraigh %2$S mar stádas, le teachtaireacht roghnach."

#: back
msgid "available"
msgstr "ar fáil"

#: away
msgid "away"
msgstr "amuigh"

#: busy
msgctxt "busy"
msgid "unavailable"
msgstr "gnóthach"

#: dnd
msgctxt "dnd"
msgid "unavailable"
msgstr "gafa"

#: offline
msgid "offline"
msgstr "as líne"
