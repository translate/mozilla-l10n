#: startup.label
msgid "Startup"
msgstr "Tosú"

# don't choose "s" (saveTo) or "t" (manageAddons) or "g" alwaysAsk
#: startupPage.label startupPage.accesskey
msgid "When &brandShortName; &starts:"
msgstr "Ag&us &brandShortName; á thosú:"

#: startupHomePage.label
msgid "Show my home page"
msgstr "Taispeáin mo leathanach baile"

#: startupBlankPage.label
msgid "Show a blank page"
msgstr "Taispeáin leathanach folamh"

#: startupLastSession.label
msgid "Show my windows and tabs from last time"
msgstr "Taispeáin mo chuid fuinneog agus cluaisíní ón seisiún roimhe seo"

#: homepage.label homepage.accesskey
msgid "Home &Page:"
msgstr "Leathanach &Baile:"

#: useCurrentPage.label useCurrentPage.accesskey
msgid "Use &Current Page"
msgstr "Úsáid an Leathanach &Reatha"

#: useMultiple.label
msgid "Use Current Pages"
msgstr "Úsáid na Leathanaigh Reatha"

# don't use "B" - Leathanach &Baile
#: chooseBookmark.label chooseBookmark.accesskey
msgid "Use &Bookmark…"
msgstr "Úsáid &Leabharmharc…"

# don't use "R" - "Reatha" above
#: restoreDefault.label restoreDefault.accesskey
msgid "&Restore to Default"
msgstr "Cuir na réamhshocruithe ar &ais"

#: downloads.label
msgid "Downloads"
msgstr "Íosluchtuithe"

# Don't choose "B" - "Bainistigh Breiseáin" below
#: saveTo.label saveTo.accesskey
msgid "Sa&ve files to"
msgstr "&Sábháil comhaid i"

# don't choose "a" for access key, nor "b" (Leathanach &Baile),
# nor "l" Úsáid &Leabharmharc, nor "r" (useCurrentPage)
# safe to choose same as chooseFolderMac since #ifndef'd in
#: chooseFolderWin.label chooseFolderWin.accesskey
msgid "Br&owse…"
msgstr "Brabhsá&il…"

#: chooseFolderMac.label chooseFolderMac.accesskey
msgid "Choos&e…"
msgstr "Roghna&igh…"

# "a" conflicts with restoreDefault.label
#: alwaysAsk.label alwaysAsk.accesskey
msgid "&Always ask me where to save files"
msgstr "Fiafraigh díom cá sábhálfar an comhad i &gcónaí"

#: alwaysCheckDefault2.label alwaysCheckDefault2.accesskey
msgid "Al&ways check if &brandShortName; is your default browser"
msgstr "Seiceáil i gcónaí an é &brandShortName; an brabhsálaí réamhshocraithe"

#: setAsMyDefaultBrowser2.label setAsMyDefaultBrowser2.accesskey
msgid "Make &Default"
msgstr "Úsái&d mar Réamhshocrú"

#: isDefault.label
msgid "&brandShortName; is currently your default browser"
msgstr "Is é &brandShortName; do bhrabhsálaí réamhshocraithe faoi láthair"

#: isNotDefault.label
msgid "&brandShortName; is not your default browser"
msgstr "Ní hé &brandShortName; do bhrabhsálaí réamhshocraithe faoi láthair"

#: separateProfileMode.label
msgid "Allow &brandShortName; and Firefox to run at the same time"
msgstr "Lig do &brandShortName; agus Firefox a bheith ar siúl san am céanna"

#: useFirefoxSync.label
msgid "Tip: This uses separate profiles. Use Sync to share data between them."
msgstr ""
"Leid: Úsáideann sé seo dhá phróifíl ar leith. Úsáid Sync le sonraí a "
"chomhroinnt eatarthu."

#: getStarted.label
msgid "Start using Sync…"
msgstr "Tosaigh le Sync…"
