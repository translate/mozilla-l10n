msgstr "Eisceacht:"

#: typeCssParser
msgid "CSS Parser:"
msgstr "Parsálaí CSS:"

#: typeStrict
msgid "Strict Warning:"
msgstr "Dianrabhadh:"

#: msgCategory
msgid "Category:"
msgstr "Catagóir:"

#: errLine
msgid "Line: %S"
msgstr "Líne: %S"

#: btnHide
