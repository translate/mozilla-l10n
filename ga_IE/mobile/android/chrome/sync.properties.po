#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Sioncrónú is déanaí: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "Sioncrónú is déanaí: faoi shiúl…"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Cuntas: %S"

#: notconnected.label
msgid "Not connected"
msgstr "Gan cheangal"

#: connecting.label
msgid "Connecting…"
msgstr "Ag ceangal…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "Baineadh do chuntas Firefox Sync"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Cealaigh"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"Níl #1 #2 comhoiriúnach leis an leagan is déanaí de Firefox Sync. "
"Nuashonraigh go dtí an leagan is déanaí."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"Níl #1 #2 comhoiriúnach leis na leaganacha níos sine de Firefox Sync. "
"Nuashonraigh Firefox ar do riomhairí eile."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox Sync"

#: sync.update.button
msgid "Learn More"
msgstr "Tuilleadh Eolais"

#: sync.update.close
msgid "Close"
msgstr "Dún"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "Ní féidir Sync a shocrú"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "Níl ceangal leis an Idirlíon ar fáil"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr ""
"Níorbh fhéidir le %S ceangal le Sync.  An bhfuil fonn ort triail eile a "
"bhaint as?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Bain triail eile as"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Socrú de láimh"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "Níl aon chluaisíní ar ríomhairí eile."

#. # LOCALIZATION NOTE (promoBanner.message.text): First part of the message displayed in a
#. # banner on about:home. The final space separates this text from the link.
#: promoBanner.message.text
msgid "Sync is brand new and easier than ever. "
msgstr "Tá Sync úrnua agus níos éasca ná a bhí riamh. "

#. # LOCALIZATION NOTE (promoBanner.message.link): Second part of the message, styled as a link.
#: promoBanner.message.link
msgid "Tap here to learn more"
msgstr "Tapáil anseo chun tuilleadh eolais a fháil"
