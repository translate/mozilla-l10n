#: hostColon
msgid "Host:"
msgstr "Óstach:"

#: domainColon
msgid "Domain:"
msgstr "Fearann:"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "Ceangail chriptithe amháin"

#: forAnyConnection
msgid "Any type of connection"
msgstr "Gach cineál ceangail"

#: expireAtEndOfSession
msgid "At end of session"
msgstr "Ag deireadh an tseisiúin"

#: showDetails
msgid "Show Details"
msgstr "Taispeáin Sonraí"

#: hideDetails
msgid "Hide Details"
msgstr "Folaigh Sonraí"

#: detailsAccessKey
msgid "T"
msgstr "T"

#: permissionToSetACookie
msgid "The site %S wants to set a cookie."
msgstr "Ba mhaith leis an suíomh %S fianán a shocrú."

#: permissionToSetSecondCookie
msgid "The site %S wants to set a second cookie."
msgstr "Ba mhaith leis an suíomh %S fianán eile a shocrú."

# LOCALIZATION NOTE (PermissionToSetAnotherCookie): First %S: sitename, second %S: number of cookies already present for that site
#. #  LOCALIZATION NOTE (PermissionToSetAnotherCookie): First %S: sitename, second %S: number of cookies already present for that site
#: permissionToSetAnotherCookie
msgid ""
"The site %S wants to set another cookie.\n"
"You already have %S cookies from this site."
msgstr ""
"Ba mhaith leis an suíomh %S fianán eile a shocrú.\n"
"Tá %S fianán agat cheana ón suíomh seo."

#: permissionToModifyCookie
msgid "The site %S wants to modify an existing cookie."
msgstr "Ba mhaith leis an suíomh %S fianán atá ann cheana a athrú."
