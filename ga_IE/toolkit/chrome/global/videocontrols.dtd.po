#: playButton.pauseLabel
msgid "Pause"
msgstr "Cuir ar Shos"

#: muteButton.muteLabel
msgid "Mute"
msgstr "Gan fuaim"

#: muteButton.unmuteLabel
msgid "Unmute"
msgstr "Le fuaim"

#: fullscreenButton.enterfullscreenlabel
msgid "Full Screen"
msgstr "Lánscáileán"

#: fullscreenButton.exitfullscreenlabel
msgid "Exit Full Screen"
msgstr "Fág Mód Lánscáileáin"

#: castingButton.castingLabel
msgid "Cast to Screen"
msgstr "Craol ar an Scáileán"

#: stats.media
msgid "Media"
msgstr "Meáin"

#. Do not translate this.  Only change the numeric values if you need this dialogue box to appear bigger
#: stats.size
msgid "Size"
msgstr "Méid"

#: stats.activity
msgid "Activity"
msgstr "Gníomhaíocht"

# Status Text
#: stats.activityPaused
msgid "Paused"
msgstr "Curtha ar Shos"

#: stats.activityPlaying
msgid "Playing"
msgstr "Ag Seinm"

#: stats.activityEnded
msgid "Ended"
msgstr "Críochnaithe"

#: stats.activitySeeking
msgid "(seeking)"
msgstr "(cuardach)"

#: stats.volume
msgid "Volume"
msgstr "Airde"

#: stats.framesParsed
msgid "Frames parsed"
msgstr "Frámaí parsáilte"

#: stats.framesDecoded
msgid "Frames decoded"
msgstr "Frámaí díchódaithe"

#: stats.framesPresented
msgid "Frames presented"
msgstr "Frámaí curtha ar taispeáint"

#: stats.framesPainted
msgid "Frames painted"
msgstr "Frámaí péinteáilte"

#: error.aborted
msgid "Video loading stopped."
msgstr "Stopadh luchtú an fhíseáin."

#: error.network
msgid "Video playback aborted due to a network error."
msgstr "Stopadh athsheinm an fhíseáin mar gheall ar earráid líonra."

#: error.decode
msgid "Video can't be played because the file is corrupt."
msgstr "Ní féidir an físeán a sheinm toisc go bhfuil an comhad truaillithe."

#: error.srcNotSupported
msgid "Video format or MIME type is not supported."
msgstr "Ní thacaítear leis an bhformáid fhíse nó leis an gcineál MIME."

#: error.noSource2
msgid "No video with supported format and MIME type found."
msgstr ""
"Níor aimsíodh aon fhíseán a dtacaítear lena fhormáid agus lena chineál MIME."

#: error.generic
msgid "Video playback aborted due to an unknown error."
msgstr "Stopadh athsheinm an fhíseáin mar gheall ar earráid anaithnid."

#. the #1 string is the current
#. media position, and the #2 string is the total duration. For example, when at
#. the 5 minute mark in a 6 hour long video, #1 would be "5:00" and #2 would be
#. "6:00:00", result string would be "5:00 of 6:00:00 elapsed".
#: scrubberScale.nameFormat
msgid "#1 of #2 elapsed"
msgstr "#1 as #2 imithe"
