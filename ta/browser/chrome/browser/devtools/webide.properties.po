msgid "Validation status: %1$S"
msgstr "சரிபார்த்தல் நிலவரம்: %1$S"

#: status_valid
msgid "VALID"
msgstr "செல்லும்"

#: status_warning
msgid "WARNINGS"
msgstr "எச்சரிக்கைகள்"

#: status_error
msgid "ERRORS"
msgstr "பிழைகள்"

#: status_unknown
msgid "UNKNOWN"
msgstr "அறியாத"

