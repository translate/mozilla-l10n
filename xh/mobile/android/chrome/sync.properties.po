#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Ungqamaniso lokugqibela: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "Ungqamaniso lokugqibela: luyaqhuba…"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Iakhawunti: %S"

#: notconnected.label
msgid "Not connected"
msgstr "Ayinxityelelwanga"

#: connecting.label
msgid "Connecting…"
msgstr "Iyanxibelelana…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "Iakhawunti yakho yongqamaniso lwefirefox isusiwe"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Qhaqha okwenzileyo"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 ayihambelani nenguqulelo yakutshanje yongqamaniso lweFirefox. Nceda "
"uhlaziye inguqulelo yakutshanje."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 ayihambelani neenguqulelo ezindala zongqamaniso lweFirefox. Nceda "
"uhlaziye iFirefox kwi(i)khompyutha y(z)akho."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Ungqamaniso lweFirefox"

#: sync.update.button
msgid "Learn More"
msgstr "Funda ngaphezulu"

#: sync.update.close
msgid "Close"
msgstr "Vala"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "Ayinakho ukuseka ungqamaniso"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "Akukho lunxibelelwano lweintanethi lufumanekayo"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr ""
"I%S ayibanga nakunxibeleleka kungqamaniso. Ungathanda ukuzama kwakhona?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Zama kwakhona"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Useto ngesandla"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "Akukho zithebhu kwezinye iikhompyutha zakho."

#. # LOCALIZATION NOTE (promoBanner.message.text): First part of the message displayed in a
#. # banner on about:home. The final space separates this text from the link.
#: promoBanner.message.text
msgid "Sync is brand new and easier than ever. "
msgstr "Ukungqamanisa kutsha kraca yaye kulula kunangaphambili. "

#. # LOCALIZATION NOTE (promoBanner.message.link): Second part of the message, styled as a link.
#: promoBanner.message.link
msgid "Tap here to learn more"
msgstr "Cofa apha ukuze ufunde okungakumbi"
