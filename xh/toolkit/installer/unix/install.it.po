#------------------------------------------------------------------------------
#: -601
msgid "Out of memory"
msgstr "Ayikho kuvimba wolwazi"

#: -602
msgid "Invalid param"
msgstr "I-param engasebenziyo"

#: -603
msgid "Invalid member variable"
msgstr "Ixabiso lotshintshatshintsho lwelungu elingasebenziyo"

#: -604
msgid "Invalid pointer"
msgstr "Isalathi esingasebenziyo"

#: -605
msgid "Parse key has no value"
msgstr "Iqhosha le-parse alinaxabiso"

#: -606
msgid "Failed to read readme"
msgstr "Ukusilela ekufundeni u-ndifunde"

#: -607
msgid "Failed to read license"
msgstr "Ukusilela ukufunda ilayisensi"

#: -608
msgid "Out of bounds in Component/Setup Type list"
msgstr "Kungaphaya kwemida Kwinxalenye/Kuludwe Lwesimbo Somiselo"

#: -609
msgid "Mismatched ref counts"
msgstr "Kusilelwe kubalo lwesalathiso"

#: -610
msgid "No components in the INI file"
msgstr "Akukho zinxalenye kwifayili ye-INI"

#: -611
msgid "No setup types in the INI file"
msgstr "Akukho zimbo zomiselo kwifayili ye-INI"

#: -612
msgid "URL at this index already exists"
msgstr "I-URL kwesi salathisi seyikhona"

#: -613
msgid "Couldn't create the directory"
msgstr "Ayikwazanga kuyila uvimba weefayili"

#: -614
msgid "FTP URL malformed"
msgstr "I-FTP URL ayibumbekanga kakuhle"

#: -615
msgid "Download failed"
msgstr "Kusilele ukuthoba umthwalo"

#: -616
msgid "Extraction of XPCOM failed"
msgstr "Ukucatshulwa kwe-XPCOM kusilele"

#: -617
msgid "Failed to fork a process"
msgstr "Ayiphumelelanga ukwenza inkqubo ngendlela entsha"

#: -618
msgid "Couldn't open xpistub library"
msgstr "Aykwazanga kuvuleka ilayibrari ye-xpistub"

#: -619
msgid "Couldn't get symbol in library"
msgstr "Awukwazanga kufumaneka umqondiso elayibrari"

#: -620
msgid "A xpistub call failed"
msgstr "Ubizo lwe-xpistub call lusilele"

#: -621
msgid "An Installer module %s (.xpi) failed to install"
msgstr "Imodyuli yokuhlohla i-%s (.xpi) ayiphumelelanga ukuhlohla"

#: -622
msgid "Copy of an installer module (.xpi) failed"
msgstr "Ikopi yemodyuli yokuhlohla i-(.xpi) ayiphumelelanga"

#: -623
msgid "Destination directory doesn't exist"
msgstr "Uvimba weefayili ekuyiwa kuye akakho kwayena"

#: -624
msgid "Can't make destination directory.  Please try another directory."
msgstr "Akakwazi kwenzeka uvimba weefayili ekuyiwa kuye.  Nceda zama omnye uvimba weefayili."

#: -625
msgid "A previous installation exists."
msgstr "Kukho ukuhlohla kwangaphambili."

#: -626
msgid "Insufficient permission"
msgstr "Imvume enganelanga"

#: -627
msgid "Insufficient disk space"
msgstr "Isithuba sedisk esinganelanga"

#: -628
