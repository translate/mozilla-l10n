# Token Manager
#: password_not_set
msgid "(not set)"
msgstr "(ayicwangciswanga)"

#: failed_pw_change
msgid "Unable to change Master Password."
msgstr "Ayikwazi ukutshintsha Umphathi Wegama lokugqitha."

#: incorrect_pw
msgid ""
"You did not enter the correct current Master Password. Please try again."
msgstr ""
"Awungenisanga igama elilungileyo Lomphathi Wegama lokugqitha. Nceda zama "
"kwakhona."

#: pw_change_ok
msgid "Master Password successfully changed."
msgstr "Elona Gama lokugqitha litshintshwe ngokuphumeleleyo."

#: pw_erased_ok
msgid "You have deleted your Master Password."
msgstr ""

#: pw_not_wanted
msgid "Warning! You have decided not to use a Master Password."
msgstr "Isilumkiso! Ukhethe ukungasebenzisi Elona Gama lokugqtha."

#: pw_empty_warning
msgid ""
"Your stored web and email passwords, form data, and private keys will not be "
"protected."
msgstr ""
"Ezigciniweyo idilesi ze web namagama agqithisiweyo e email, ifomu ze data, "
"nezitshixo zekhusi azizukukhuselwa."

#: pw_change2empty_in_fips_mode
msgid ""
"You are currently in FIPS mode. FIPS requires a non-empty Master Password."
msgstr ""
"Okwangoku ukwindlela ye FIPS. FIPS ifuna Igama lokugqitha Lomphathi omkhulu "
"onento."

#: pw_change_success_title
msgid "Password Change Succeeded"
msgstr "Utshintsho lwesingenisi luphumelele"

#: pw_change_failed_title
msgid "Password Change Failed"
msgstr "Utshintsho lwesingenisi aluphumelelanga"

