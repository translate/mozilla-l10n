msgid "#1 of #2 KB"
msgstr "i-#1 ye-#2 KB"

#: statusFormatKBMB
msgid "#1 KB of #2 MB"
msgstr "i-#1 KB ye-#2 MB"

#: statusFormatMBMB
msgid "#1 of #2 MB"
msgstr "i-#1 ye-#2 MB"

#: disabledObsoleteTitle
msgid "Old Extensions"
msgstr "Izolulo Ezidala"

#: disabledObsoleteMessage
msgid "Any old extensions that you have installed have been disabled."
msgstr "Naziphi izolulo ezidala ozihlohlileyo zaphulwe isakhono."

#: type-32
msgid "Multiple Extension Package"
msgstr "Umqulu Opheleleyo Wesolulo Seziphindaphindwa"

#: type-4
msgid "Theme"
msgstr "Umxholo"

#: type-2
msgid "Extension"
msgstr "Isolulo"

#: incompatibleTitle
msgid "Incompatible %S"
msgstr "I-%S engahambelaniyo"

#: incompatibleMsg
msgid "%S %S could not be installed because it is not compatible with %S %S. (%S %S will only work with %S versions from %S to %S)"
msgstr "I-%S %S ayikwazanga kuhlohlwa kuba ayihambelani ne-%S %S. (I-%S %S iya kusebenza kuphela neentlobo zoguqulelo lwe-%S ukusuka kwi-%S ukuya kwi-%S)"

#: incompatibleMsgSingleAppVersion
msgid "%S %S could not be installed because it is not compatible with %S %S. (%S %S will only work with %S %S)"
msgstr "I-%S %S ayikwazanga kuhlohlwa kuba ayihambelani ne-%S %S. (I-%S %S iya kusebenza kuphela ne-%S %S)"

#: incompatibleMessageNoApp
msgid "%S %S could not be installed because it is not compatible with %S."
msgstr "I-%S %S ayikwazanga kuhlohleka kuba ayihambelani ne-%S."

#: incompatibleOlder
msgid "versions 0.8 or older."
msgstr "iintlobo zoguqulelo lwe-0.8 okanye ezindala kunoko."

#: incompatibleThemeName
msgid "this Theme"
msgstr "lo Mxholo"

#: incompatibleExtension
msgid "Disabled - not compatible with %S %S"
msgstr "Yaphulwe isakhono - ayihambelani ne-%S %S"

#: invalidGUIDMessage
msgid "\"%S\" could not be installed because of an error in its Install Manifest (\"%S\" is not a valid GUID). Please contact the author of this item about the problem."
msgstr "I-\"%S\" ayikwazanga kuhlohleka ngenxa yemposiso kwiSibonakaliso Sokuhlohla i-(\"%S\" aiyinyanisekanga GUID). Nceda qhagamshelana nombhali weli nqaku ngale ngxaki."

#: invalidVersionMessage
msgid "\"%S\" could not be installed because of an error in its Install Manifest (\"%S\" is not a valid Version String). Please contact the author of this item about the problem."
msgstr "I-\"%S\" ayikwazanga kuhlohleka ngenxa yemposiso Kwisibonakalisi Sokuhlohla i-(\"%S\" asingomtya Woguqulelo onyanisekileyo). Nceda qhagamshelana nombhali weli nqaku malunga neli nale ngxaki."

#: incompatiblePlatformMessage
msgid "\"%S\" could not be installed because it is not compatible with your %S build type (%S). Please contact the author of this item about the problem."
msgstr "I-\"%S\" ayikwazanga kuhlohleka kuba ayihambelani nesimbo skho sokwakha se-%S (%S). Nceda qhagamshelana nombhali weli nqaku malunga nale ngxaki."

#: missingFileTitle
msgid "Missing File"
msgstr "Ifayili Elahlekileyo"

#: missingFileMessage
msgid "%S could not load this item because the file %S was missing."
msgstr "I-%S ayikwazanga kulayisha eli nqaku kuba ifayili ye-%S ibilahlekile."

#: missingFileConsoleMessage
msgid "Failed to install from %S because %S was not provided at the top level of the jar/xpi file."
msgstr "Ayiphumelelanga ukuhlohla ukusuka kwi-%S kuba i-%S ibinganikwanga kumgangatho ophezulu wefayili ye-jar/xpi."

#: malformedMessage
msgid "%S could not install this item because \"%S\" (provided by the item) is not well-formed or does not exist. Please contact the author about this problem."
msgstr "I-%S ayikwazanga kulihlohla eli nqaku kuba i-\"%S\" (enikwa leli nqaku) alakhiwanga kakuhle okanye alikho kwalona. Nceda qhagamshelana nombhali ngali ngxaki."

#: malformedTitle
msgid "Malformed File"
msgstr "Ifayili Engakhiwanga Kakuhle"

#: malformedRegistrationTitle
msgid "Chrome Registration Failed"
msgstr "Aluphumelelanga Ubhaliso lwe-Chrome"

#: malformedRegistrationMessage
msgid "%S could not install this item because of a failure in Chrome Registration. Please contact the author about this problem."
msgstr "I-%S ayikwazanga kuhlohla eli nqaku ngenxa yokungaphumeleli kuBhaliso lweChrome. Nceda qhagamshelana nombhali malunga nale ngxaki."

#: invalidFileExtTitle
msgid "Invalid File Extension"
msgstr "Isolulo Sefayili Asinyanisekanga"

#: invalidFileExtMessage
msgid "\"%S\" could not be installed because this item has an invalid file extension (%S is not a valid file extension for a %S). Please contact the author about this problem."
msgstr "I-\"%S\" ayikwazanga kuhlohlwa ngenxa yokuba eli nqaku linesolulo sefayili esinganyanisekanga (i-%S ayisosolulo sefayili esinganyanisekanga se-%S). Nceda qhagamshelana nombhali malunga nale ngxaki."

#: missingPackageFilesTitle
msgid "Missing Installation Files"
msgstr "Iifayili Zokuhlohla Ezilahlekileyo"

#: missingPackageFilesMessage
msgid "\"%S\" could not be installed because it does not contain a valid package (a %S must contain at least one extension or theme). Please contact the author about this problem."
msgstr "I-\"%S\" ayikwazanga kuhlohleka kuba ayiqulethanga mqulu opheleleyo unyanisekileyo i-(a %S mayiqulathe obona buncinci isolulo esinye okanye umxholo). Nceda qhagamshelana nombhali malunga nale ngxaki."

#: errorInstallTitle
msgid "Error"
msgstr "Imposiso"

#: errorInstallMsg
msgid ""
"%S could not install the file at \n"
"\n"
"%S\n"
"\n"
"because: %S"
msgstr ""
"I-%S ayikwazanga kuhlohla ifayili kwi-\n"
"\n"
"%S\n"
"\n"
"ngenxa ye-: %S"

