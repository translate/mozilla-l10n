msgstr "Iqwalasela Iziganeko Zohlaziyo Kwimixholo yakho"

#: cmdInstallTooltipTheme
msgid "Install a Theme"
msgstr "Ihlohla Umxholo"

#: dssSwitchAfterRestart
msgid "Restart %S to use."
msgstr "Phinda uqalise i-%S ukuze isetyenziswe."

#: updateFailedMsg
msgid "Update failed for this item."
msgstr "Uhlaziyo aluphumelelanga malunga neli nqaku."

#: updateDisabledMsg
msgid "Update is disabled for this item."
msgstr "Uhlaziyo lwaphulwe isakhono malunga neli nqaku."

