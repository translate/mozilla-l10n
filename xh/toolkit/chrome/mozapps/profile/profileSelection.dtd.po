#: windowtitle.label
msgid "&brandShortName; - Choose User Profile"
msgstr "i-&brandShortName; khetha imbali yobomi bakho"

#: profilename.label
msgid "Profile Name:"
msgstr "Igama Lemboniselo yabucala:"

#: start.label
msgid "Start &brandShortName;"
msgstr "Qala &brandShortName;"

#: exit.label
msgid "Exit"
msgstr "Phuma"

#: availprofiles.label
msgid "Available Profiles"
msgstr "Iimboniselo zabucala Ezifumanekayo"

#: newButton.label newButton.accesskey
msgid "&Create Profile..."
msgstr "&Dala imbali yobomi..."

#: renameButton.label renameButton.accesskey
msgid "&Rename Profile..."
msgstr "&Nika igama kwakhona Imboniselo yabucala..."

#: deleteButton.label deleteButton.accesskey
msgid "&Delete Profile..."
msgstr "&lahla imbali yobom..."

# <!-- manager entities -->
#: pmDescription.label
msgid ""
"&brandShortName; stores information about your settings, preferences, and "
"other user items in your user profile."
msgstr ""
"i-&brandShortName; igcina ulwazi malunga nezixhobo zokuseta, izinto othanda "
"ukuzenza, kunye nezinye izinto ezisetyenziswa ngumsebenzisi kwimbali yobomi "
"bakho."

#: offlineState.label offlineState.accesskey
msgid "W&ork offline"
msgstr "S&ebenza ngaphandle konxibelelwano"

#: autoSelect.label autoSelect.accesskey
msgid "Don't a&sk at startup"
msgstr "Sukubuza kwi &startup"

