# <!-- These strings are used on the download progress dialog.
#      See: http://lxr.mozilla.org/seamonkey/source/embedding/components/ui/progressDlg/nsProgressDialog.xul
# -->
# <!-- This is used prior to initializing with a proper title -->
#: defaultTitle
msgid "Saving"
msgstr "Iyagcina"

# <!-- These are used when saving -->
#: savingTitle
msgid "#2&#037; of #1 Saved"
msgstr "#2&#037; ye #1 Igciniwe"

#: savingSource
msgid "Saving From:"
msgstr "Igcina Ukusuka:"

#: savingTarget
msgid "To:"
msgstr "Ku:"

# <!-- These are used when opening (with an application) -->
#: openingTitle
msgid "#2&#037; of #1 Completed"
msgstr "#2&#037; ye #1 Igqibekile"

#: openingSource
msgid "Opening From:"
msgstr "Ivula Ukusuka ku:"

#: openingTarget
msgid "With:"
msgstr "Nge:"

# <!-- These strings are used on the download progress dialog.
#      See: http://lxr.mozilla.org/seamonkey/source/embedding/components/ui/progressDlg/nsProgressDialog.xul
# -->
# <!-- This is used prior to initializing with a proper title -->
#: unknownSavingTitle
msgid "Saving #1"
msgstr "Igcina #1"

#: unknownOpeningTitle
msgid "Opening #1"
msgstr "Iyavula #1"

# <!-- Labels on other fields -->
#: status
msgid "Status:"
msgstr "Isimo:"

#: timeElapsed
msgid "Time Elapsed:"
msgstr "Ixesha Elidlulileyo:"

#: timeLeft
msgid "Time Left:"
msgstr "Ixesha Elishiyekileyo:"

#: progress
msgid "Progress:"
msgstr "Inkqubela:"

# <!-- Label for the "keep dialog" checkbox -->
#: keep
msgid "Keep this window open after the download is complete."
msgstr "Gcina le window ivulekile emva kokuba ulayisho ezantsi lugqityiwe."

# <!-- Button labels -->
#: cancel
msgid "Cancel"
msgstr "Rhoxisa"

#: pause
msgid "Pause"
msgstr "Yima"

#: resume
msgid "Resume"
msgstr "Qhubeka"

#: close
msgid "Close"
msgstr "Vala"

#: reveal
msgid "Show File Location"
msgstr "Bonisa Indawo ekuyo Ifayile"

#: launch
msgid "Launch File"
msgstr "Qalisa Ifayile"

# #-#-#-#-#  nsProgressDialog.po  #-#-#-#-#
# <!-- Status/progress messages -->
# <!-- LOCALIZATION NOTE (pausedMsg):
#     This string is used to indicate that the download is paused -->
# #-#-#-#-#  downloadProgress.po  #-#-#-#-#
# <!-- LOCALIZATION NOTE (pausedMsg):
#     This string is used to indicate that the download is paused -->
#: pausedMsg
msgid ""
"_: \n"
"Download Paused"
msgstr "Ulayisho ezantsi Luphumlisiwe"

# <!-- LOCALIZATION NOTE (progressMsg):
#     This string is used to generate the status message used during
#     file download.#
#     #1 will be replaced by the number of bytes downloaded thus far
#     #2 will be replaced by the total number of bytes to be downloaded
#     #3 will be replaced by the download rate (in KB per second) -->
#: progressMsg
msgid ""
"_: \n"
"#1KB of #2KB (at #3KB/sec)"
msgstr "#1KB ye #2KB (e #3KB/sec)"

# #-#-#-#-#  nsProgressDialog.po  #-#-#-#-#
# <!-- LOCALIZATION NOTE (unknownTime):
#     This string is used when the time remaining is unknown. -->
# #-#-#-#-#  downloadProgress.po  #-#-#-#-#
# <!-- LOCALIZATION NOTE (unknownTime):#
#     This string is used when the time remaining is unknown. -->
#: unknownTime
msgid ""
"_: \n"
"Unknown"
msgstr "Engaziwayo"

# <!-- LOCALIZATION NOTE (longTimeFormat):
#     This string is used to format elapsed/remaining times when the
#     time exceeds 1 hour in length.#
#     #1 will be replaced by the number of hours
#     #2 will be replaced by the number of minutes
#     #3 will be replaced by the number of seconds -->
#: longTimeFormat
msgid ""
"_: \n"
"#1:#2:#3"
msgstr "#1:#2:#3"

# <!-- LOCALIZATION NOTE (shortTimeFormat):
#     This string is used to format elapsed/remaining times when the
#     time is less than 1 hour in length.  Note that you can leave
#     "#1" out of this string to omit the leading "00:" if you so choose.#
#     #1 will be replaced by the number of hours
#     #2 will be replaced by the number of minutes
#     #3 will be replaced by the number of seconds -->
#: shortTimeFormat
msgid ""
"_: \n"
"#2:#3"
msgstr "#2:#3"

# <!-- LOCALIZATION NOTE (completeMsg):
#     This string is displayed when the download is complete.  The insert
#     "#1" is replaced by the elapsed time, formatted according to the
#     xxxTimeFormat strings defined above).  You can omit "#1" if you don't
#     want the elapsed time in the message.#
#     #1 will be replaced by the elapsed time -->
#: completeMsg
#, fuzzy
msgid ""
"_: \n"
"Finished, #2 KB downloaded (elapsed time was #1)"
msgstr "Ulayisho ezantsi lugqityiwe (ixesha elidlulileya libe ngu #1)"

# #-#-#-#-#  sendProgress.po  #-#-#-#-#
# <!-- LOCALIZATION NOTE (percentMsg):
#     This string is used to format the text to the right of the progress
#     meter.#
#     #1 will be replaced by the percentage of the file that has been saved -->
# #-#-#-#-#  nsProgressDialog.po  #-#-#-#-#
# <!-- LOCALIZATION NOTE (percentMsg):#
#     This string is used to format the text to the right of the progress
#     meter.#
#     #1 will be replaced by the percentage of the file that has been saved -->
# #-#-#-#-#  printProgress.po  #-#-#-#-#
# <!-- LOCALIZATION NOTE (percentPrint):#
#     This string is used to format the text to the right of the progress
#     meter.#
#     #1 will be replaced by the percentage of the file that has been saved -->
# #-#-#-#-#  downloadProgress.po  #-#-#-#-#
# <!-- LOCALIZATION NOTE (percentMsg):#
#     This string is used to format the text to the right of the progress
#     meter.#
#     #1 will be replaced by the percentage of the file that has been saved -->
#: percentMsg
msgid ""
"_: \n"
"#1&#037;"
msgstr "#1&#037;"

