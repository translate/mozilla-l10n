#--------------------------------------------------------------------
#   Install Actions
#--------------------------------------------------------------------
#: InstallFile
msgid "Installing: %s"
msgstr "Kuhlohlwa: i-%s"

#: InstallSharedFile
msgid "Installing Shared File: %s"
msgstr "Kuhlohlwa Ifayili Ekwabelwana Ngayo: %s"

#: ReplaceFile
msgid "Replacing: %s"
msgstr "Kususwa ngokubeka okunye: i-%s"

#: ReplaceSharedFile
msgid "Replacing Shared File: %s"
msgstr "Kususwa Ngokubeka Okunye Ifayili Ekwabelwana Ngayo: %s"

#: SkipFile
msgid "Skipping: %s"
msgstr "Kutsitywa: %s"

#: SkipSharedFile
msgid "Skipping Shared File: %s"
msgstr "Kutsitywa Ifayili Ekwabelwana Ngayo: %s"

#: DeleteFile
msgid "Deleting file: %s"
msgstr "Kucinywa ifayili: %s"

#: DeleteComponent
msgid "Deleting component: %s"
msgstr "Kucinywa inxalenye: %s"

#: Execute
msgid "Executing: %s"
msgstr "Kusetyenziswa: %s"

#: ExecuteWithArgs
msgid "Executing: %s with argument: %s"
msgstr "Kusetyenziswa: i-%s nesiphumezi somyalelo: %s"

#: CopyFile
msgid "Copy File: %s to %s"
msgstr "Kopa Ifayili: i-%s ukuya kwi-%s"

#: ExecuteFile
msgid "Execute File: %s"
msgstr "Kusetyenziswa Ifayili: %s"

#: ExecuteFileWithArgs
msgid "Execute File: %s with argument: %s"
msgstr "Kusetyenziswa Ifayili: i-%s nesiphumezi somyalelo: %s"

#: MoveFile
msgid "Move File: %s to %s"
msgstr "Shenxisa Ifayili: i-%s ukuya kwi-%s"

#: RenameFile
msgid "Rename File: %s to %s"
msgstr "Phinda Unike Igama Kwifayili: i-%s ukuya kwi-%s"

#: CreateFolder
msgid "Create Folder: %s"
msgstr "Yila Isiqulathi Seefayili: %s"

#: RemoveFolder
msgid "Remove Folder: %s"
msgstr "Susa Isiqulathi Seefayili: %s"

#: RenameFolder
msgid "Rename Folder: %s to %s"
msgstr "Phinda Unike Igama Kwisiqulathi Seefayili: i-%s ukuya kwi-%s"

#: WindowsShortcut
msgid "Windows Shortcut: %s"
msgstr "Indlela Enqumlayo ye-Windows Shortcut: %s"

#: MacAlias
msgid "Mac Alias: %s"
msgstr "Elinye Igama le-Mac: i-%s"

#: WindowsRegisterServer
msgid "Windows Register Server: %s"
msgstr "Iseva Yobhaliso ye-Windows: %s"

#: UnknownFileOpCommand
msgid "Unknown file operation command!"
msgstr "Umyalelo wokusebenza kwefayili engaziwayo!"

#: Patch
msgid "Patching: %s"
msgstr "Kuyakhuselwa: %s"

#: Uninstall
msgid "Uninstalling: %s"
msgstr "Kususwa okuhlohliweyo: %s"

#: RegSkin
msgid "Register Skin: %s"
msgstr "Isikhumba Sobhaliso: %s"

#: RegLocale
msgid "Register Locale: %s"
msgstr "Bhalisa Indawo Yokuthile: %s"

#: RegContent
msgid "Register Content: %s"
msgstr "Bhalisa Isiqulatho: %s"

#: RegPackage
msgid "Register Package: %s"
msgstr "Bhalisa Umqulu Opheleleyo: %s"

#--------------------------------------------------------------------
#   Dialog Messages
#--------------------------------------------------------------------
#: ApplyNowSkin
msgid "Use this theme"
msgstr "Sebenzisa lo mxholo"

#: ApplyNowLocale
msgid "Use this locale"
msgstr "Sebenzisa le ndawo yokuthile"

#: ConfirmSkin
msgid "Install the theme \"%1$S\" from %2$S?"
msgstr "Hlohla umxholo i-\"%1$S\" ukusuka kwi-%2$S?"

#: ConfirmLocale
msgid "Install the locale \"%1$S\" from %2$S?"
msgstr "Hlohla indawo yokuthile i-\"%1$S\" ukusuka kwi-%2$S?"

#: OK
msgid "Install"
msgstr "Hlohla"

#: progress.queued
msgid "Queued"
msgstr "Ukroziso"

#: progress.downloading
msgid "Downloading..."
msgstr "Kuthotywa umthwalo..."

#: progress.downloaded
msgid "Downloaded"
msgstr "Uthotyiwe umthwalo"

#: progress.installing
msgid "Installing..."
msgstr "Kuyahlohlwa..."

#: Unsigned
msgid "Unsigned"
msgstr "Akusayinwanga"

#--------------------------------------------------------------------
#   Miscellaneous
#--------------------------------------------------------------------
#: ERROR
msgid "ERROR"
msgstr "IMPOSISO"

#: error0
msgid "Success"
msgstr "Impumelelo"

#: error999
msgid "Restart to complete"
msgstr "Kuphinda kuqaliswe ukuze kugqitywe"

#: error-202
msgid "Access denied"
msgstr "Ufikelelo lukhatyiwe"

#: error-203
msgid ""
"Unexpected installation error\n"
"Review the JavaScript Console log for more details."
msgstr ""
"Imposiso yokuhlohla engalindelekanga\n"
"Phengulula i-Console ye-JavaScript Console uze ungene ngeenkcukacha ezingaphaya kwezinikiweyo."

#: error-204
msgid "Install script not found"
msgstr "I-script sokuhlohla asifunyenwanga"

#: error-207
msgid "Not a valid install package"
msgstr "Asingomqulu upheleleyo wokuhlohla ongasebenziyo"

#: error-208
msgid "Invalid argument"
msgstr "Isiphumezi sokuyalela asinyanisekiswanga"

#: error-210
msgid "User cancelled"
msgstr "Umsebenzisi urhoxisiwe"

#: error-214
msgid "Required file does not exist"
msgstr "Ifayili efunwayo ayikho kwayona"

#: error-215
msgid "Read only"
msgstr "Funda kuphela"

#: error-218
msgid "AppleSingle extraction error"
msgstr "Imposiso yokucaphula yeAppleSingle"

#: error-219
msgid "Invalid path"
msgstr "Indlela yothungelwano engasebenziyo"

#: error-225
msgid "EXTRACTION_FAILED"
msgstr "UKUCAPHULA_AKUPHUMELELANGA"

#: error-227
msgid "Cancelled"
msgstr "Kurhoxisiwe"

#: error-228
msgid "Download error"
msgstr "Imposiso xa kuthotywa umthwalo"

#: error-229
msgid "Script error"
msgstr "Imposiso ye-script"

#: error-230
msgid "Already exists"
msgstr "Seyikhona"

#: error-235
msgid "Out of space"
msgstr "Siphelile isithuba"

#: error-239
msgid "Chrome registration failed"
msgstr "Aluphumelelanga ubhaliso lwe-Chrome"

#: error-240
msgid "Unfinished install"
msgstr "Ukuhlohla akugqitywanga"

#: error-260
msgid "Signing could not be verified."
msgstr "Ukusayina akukwazanga kuqinisekiswa."

#: error-261
msgid "Invalid file hash (possible download corruption)"
msgstr "Ukonakala kwefayili engasebenziyo (ukonakala ekuthotyweni komthwalo okunokwenzeka)"

#: error-262
msgid "Unknown or invalid file hash type"
msgstr "Isimbo sokonakala kwefayili engaziwayo okanye engasebenziyo"

#: error-299
msgid "Out of memory"
msgstr "Akukho kuvimba wolwazi"

# there are other error codes, either rare or obsolete,
# that are not worth translating at this time.
#: unknown.error
msgid "Unexpected error %S"
msgstr "Imposiso engalindelekanga i-%S"

