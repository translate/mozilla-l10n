msgid "ERROR"
msgstr "IMPOSISO"

#: error0
msgid "Success"
msgstr "Impumelelo"

#: error999
msgid "Restart to complete"
msgstr "Qala kwakhona ukugqiba"

#: error-202
msgid "Access denied"
msgstr "Unikezelo alwamkelwanga"

#: error-204
msgid "Install script not found"
msgstr "Ufakelelo lwegama elishicilelwe phantsi alifumanekanga"

#: error-207
msgid "Not a valid install package"
msgstr "Ayompahla yofakelelo esebenzayo"

#: error-208
msgid "Invalid argument"
msgstr "Engasebenziyo ingxabano"

#: error-210
msgid "User cancelled"
msgstr "Umsebenzisi urhoxisiwe"

#: error-214
msgid "Required file does not exist"
msgstr "Ifayile efunekayo ayikho"

#: error-215
msgid "Read only"
msgstr "Funda kuphela"

#: error-218
