msgstr "AppleSingle imposiso kutsalelo"

#: error-219
msgid "Invalid path"
msgstr "Ongasebenziyo umendo"

#: error-225
msgid "EXTRACTION_FAILED"
msgstr "UTSALELO_ALUPHUMELELANGA"

#: error-227
msgid "Cancelled"
msgstr "Irhoxisiwe"

#: error-228
msgid "Download error"
msgstr "Imposiso yolayisho ezantsi"

#: error-229
msgid "Script error"
msgstr "Imposiso kwigama loshicilelo elisezantsi"

#: error-230
msgid "Already exists"
msgstr "Sele ikhona"

#: error-235
msgid "Out of space"
msgstr "Iphelelwe sisithuba"

#: error-239
msgid "Chrome registration failed"
msgstr "Chrome ubhaliso aluphumelelanga"

#: error-240
msgid "Unfinished install"
msgstr "Ukupho lofakelo olungagqibekanga"

#: error-260
msgid "Signing could not be verified."
msgstr "Isiphawulisi asikwazanga kuqinisekiswa."

#: error-299
msgid "Out of memory"
msgstr "Iphelelwe yinkumbulo"

