msgid "Register Skin: %s"
msgstr "Bhalisa Isikhumba: %s"

#: RegLocale
msgid "Register Locale: %s"
msgstr "Bhalisa Indawo: %s"

#: RegContent
msgid "Register Content: %s"
msgstr "Bhalisa Umxholo: %s"

#: RegPackage
msgid "Register Package: %s"
msgstr "Bhalisa Impahla: %s"

# --------------------------------------------------------------------
#   Dialog Messages
# --------------------------------------------------------------------
#: ApplyNowSkin
msgid "Use this theme"
msgstr "Sebenzisa lomxholo"

#: ApplyNowLocale
msgid "Use this locale"
msgstr "Sebenzisa lendawo"

#: ConfirmSkin
msgid "Install the theme \"%1$S\" from %2$S?"
msgstr "Faka lomxholo \"%1$S\" ukusuka ku %2$S?"

#: ConfirmLocale
msgid "Install the locale \"%1$S\" from %2$S?"
msgstr "Faka lendawo \"%1$S\" ukusuka ku %2$S?"

#: OK
msgid "Install"
msgstr "Faka"

#: progress.queued
msgid "Queued"
msgstr "Ifolisiwe"

#: progress.downloading
msgid "Downloading..."
msgstr "Ilayisha ezantsi..."

#: progress.downloaded
msgid "Downloaded"
msgstr "Ilayishiwe"

#: progress.installing
msgid "Installing..."
msgstr "Iyafaka..."

#: Unsigned
msgid "Unsigned"
msgstr "Ayisayinwanga"

