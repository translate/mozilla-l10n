msgid "HTML Files"
msgstr "Iifayili ze-HTML"

#: htmlFilter
msgid "*.html; *.htm; *.shtml; *.xhtml"
msgstr "*.html; *.htm; *.shtml; *.xhtml"

#: textTitle
msgid "Text Files"
msgstr "Iifayili Zesiqendu"

#: textFilter
msgid "*.txt; *.text"
msgstr "*.txt; *.text"

