msgid "XML Files"
msgstr "Iifayili ze-XML"

#: xmlFilter
msgid "*.xml"
msgstr "*.xml"

#: xulTitle
msgid "XUL Files"
msgstr "Iifayili ze-XUL"

#: xulFilter
msgid "*.xul"
msgstr "*.xul"

#: appsTitle
msgid "Applications"
msgstr "Iinkqubo"

#: dirTextInputLabel
msgid ""
"_: dirTextInputLabel\n"
"Directory name:"
msgstr ""
"Igama lovimba weefayili:"

#: confirmTitle
msgid "Confirm"
msgstr "Qinisekisa"

#: confirmFileReplacing
msgid ""
"%S already exists.\n"
"Do you want to replace it?"
msgstr ""
"I-%S seyikhona.\n"
"Ingaba ufuna ukubeka okunye endaweni yayo?"

#: openButtonLabel
msgid "Open"
msgstr "Vula"

#: saveButtonLabel
msgid "Save"
msgstr "Gcina"

#: selectFolderButtonLabel
msgid "Select"
msgstr "Khetha"

#: noButtonLabel
msgid "No"
msgstr "Hayi"

#: errorOpenFileDoesntExistTitle
msgid ""
"_: errorOpenFileDoesntExistTitle\n"
"Error opening %S"
msgstr ""
"Kuvulwa imposiso ye-%S"

#: errorOpenFileDoesntExistMessage
msgid "File %S doesn't exist"
msgstr "Ifayili i-%S ayikho kwayona"

#: errorDirDoesntExistTitle
msgid "Error accessing %S"
msgstr "Imposiso ifikelela ku-%S"

#: errorDirDoesntExistMessage
msgid "Directory %S doesn't exist"
msgstr "Uvimba weefayili i-%S akakho kwayena"

#: errorDirNotReadableMessage
msgid "Directory %S is not readable or does not exist."
msgstr "Uvimba weefayili i-%S akafundeki okanye akakho kwayena."

#: errorOpeningFileTitle
msgid ""
"_: errorOpeningFileTitle\n"
"Error opening %S"
msgstr ""
"Kuvulwa Imposiso i-%S"

#: openWithoutPermissionMessage_file
msgid "File %S is not readable"
msgstr "Ifayili i-%S ayifundeki"

#: errorSavingFileTitle
msgid "Error saving %S"
msgstr "I-%S Egcina imposiso"

#: saveParentIsFileMessage
msgid "%S is a file, can't save %S"
msgstr "I-%S yifayili, ayikwazi kugcina i-%S"

#: saveParentDoesntExistMessage
msgid "Path %S doesn't exist, can't save %S"
msgstr "Indlela yothungelwano i-%S ayikho kwayona, ayikwazi kugcina i-%S"

#: saveWithoutPermissionMessage_file
msgid "File %S is not writable."
msgstr "Ifayili engu-%S ayibhaleki."

#: saveWithoutPermissionMessage_dir
msgid "Cannot create file. Directory %S is not writable."
msgstr "Ayikwazi kuyileka ifayili. Uvimba weefayili i-%S akabhaleki."

#: errorNewDirDoesExistTitle
msgid ""
"_: errorNewDirDoesExistTitle\n"
"Error creating %S"
msgstr ""
"Error creating %S"

#: errorNewDirDoesExistMessage
msgid "A file named %S already exists, directory cannot be created."
msgstr "Ifayili eyaziwa ngokuba yi-%S seyikhona, uvimba weefayili akanakuyileka."

#: errorCreateNewDirTitle
msgid ""
"_: errorCreateNewDirTitle\n"
"Error creating %S"
msgstr ""
"Imposiso eyila Imposiso engu-%S"

#: errorCreateNewDirMessage
msgid "Directory %S could not be created"
msgstr "Uvimba weefayili i-%S ayikwazanga kuyileka"

#: errorCreateNewDirIsFileMessage
msgid "Directory cannot be created, %S is a file"
msgstr "Uvimba weefayili akakwazi kuyileka, i-%S yifayili"

#: errorCreateNewDirPermissionMessage
msgid "Directory cannot be created, %S not writable"
msgstr "Uvimba weefayili akakwazi kuyileka, i-%S ayibhaleki"

#: promptNewDirTitle
msgid "Create new directory"
msgstr "Yila uvimba weefayili omtsha"

#: promptNewDirMessage
msgid ""
"_: promptNewDirMessage\n"
"Directory name:"
msgstr ""
"Igama likavimba weefayili:"

#: errorPathProblemTitle
msgid "Unknown Error"
msgstr "Imposiso Engaziwayo"

#: errorPathProblemMessage
msgid "An unknown error occured (path %S)"
msgstr "Kwenzeke imposiso engaziwayo (indlela yothungelwano i-%S)"

