#: hostColon
msgid "Host:"
msgstr "Umamkeli:"

#: domainColon
msgid "Domain:"
msgstr "Ummandla wolwazi:"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "Imidibaniso entsonkothileyo kuphela"

#: forAnyConnection
msgid "Any type of connection"
msgstr "Nasiphi isimbo somdibaniso"

#: atEndOfSession
msgid "at end of session"
msgstr "ekupheleni kweseshoni"

#: showDetails
msgid "Show Details"
msgstr "Bonisa Iinkcukacha"

#: hideDetails
msgid "Hide Details"
msgstr "Fihla Iinkcukacha"

#: detailsAccessKey
msgid "T"
msgstr "T"

#: permissionToSetACookie
msgid "The site %S wants to set a cookie."
msgstr "Isiza %S sifuna ukumisela i-cookie."

#: permissionToSetSecondCookie
msgid "The site %S wants to set a second cookie."
msgstr "Isiza %S sifuna ukumisela i-cookie yesibini."

#  LOCALIZATION NOTE (PermissionToSetAnotherCookie): First %S: sitename, second %S: number of cookies already present for that site
#: permissionToSetAnotherCookie
msgid ""
"The site %S wants to set another cookie.\n"
"You already have %S cookies from this site."
msgstr ""
"Isiza %S sifuna ukumisela enye i-cookie.\n"
"Sowune-%S zee-cookie ezivela kwesi siza."

#: permissionToModifyCookie
msgid "The site %S wants to modify an existing cookie."
msgstr "Isiza %S sifuna ukuhlengahlengisa i-cookie eseyikho."

#: permissionToAcceptImage
msgid "The site %S wants to load an image."
msgstr "Isiza %S sifuna ukulayisha umfuziselo."

#: rememberThisDecision
msgid "Remember this decision"
msgstr "Khumbula esi sigqibo"

