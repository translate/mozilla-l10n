#: loadError.label
msgid "Problem loading page"
msgstr "Kukho ingxaki ngokulayisha ikhasi"

#: retry.label
msgid "Try Again"
msgstr "Zama Kwakhona"

# Specific error messages
#: connectionFailure.title
msgid "Unable to connect"
msgstr "Ayikwazi kudibanisa"

#: connectionFailure.longDesc
msgid ""
"_: connectionFailure.longDesc\n"
"&sharedLongDesc;"
msgstr ""
"&sharedLongDesc;"

#: deniedPortAccess.title
msgid "This address is restricted"
msgstr "Le dilesi inemida"

#: dnsNotFound.title
msgid "Server not found"
msgstr "Iseva ayifunyenwanga"

#: dnsNotFound.longDesc
msgid "<ul>  <li>Check the address for typing errors such as    <strong>ww</strong>.example.com instead of    <strong>www</strong>.example.com</li>  <li>If you are unable to load any pages, check your computer's network    connection.</li>  <li>If your computer or network is protected by a firewall or proxy, make sure    that &brandShortName; is permitted to access the Web.</li></ul>"
msgstr "<ul>  <li>Qwalasela idilesi yeemposiso zokuchwetheza ezifana nezi    <strong>ww</strong>.umzekelo.com endaweni ka    <strong>www</strong>.umzekelo.com</li>  <li>Ukuba akukwazi kulayisha nawaphi amakhasi, qwalasela umdibaniso womsebenzi wekhompyutha yakho.</li>  <li>Ukuba ikhompyutha yakho okanye umsebenzi wothungelwano ukhuselwe ngodonga lomlilo okanye ngommeli, qiniseka ukuba i-&brandShortName; ivunyelwe ukufikelela kwi-Web.</li></ul>"

#: fileNotFound.title
msgid "File not found"
msgstr "Ifayili ayifunyenwanga"

#: fileNotFound.longDesc
msgid "<ul>  <li>Check the file name for capitalization or other typing errors.</li>  <li>Check to see if the file was moved, renamed or deleted.</li></ul>"
msgstr "<ul>  <li>Qwalasela igama lefayili ukwenzela ukufakwa kukanobumba omkhulu okanye ezinye iimposiso zokuchwetheza.</li>  <li>Qwalasela ukuze ubone ukuba ingaba ifayili yashenxiswa na, yanikwa elinye igama okanye yacinywa na.</li></ul>"

#: generic.title
msgid "Oops."
msgstr "Yhoo."

#: generic.longDesc
msgid "<p>&brandShortName; can't load this page for some reason.</p>"
msgstr "<p>i-&brandShortName; ayikwazi kulayisha eli khasi ngenxa yesizathu esithile.</p>"

#: malformedURI.title
msgid "The address isn't valid"
msgstr "Idilesi ayisebenzi"

#: malformedURI.longDesc
msgid "<ul>  <li>Web addresses are usually written like    <strong>http://www.example.com/</strong></li>  <li>Make sure that you're using forward slashes (i.e.    <strong>/</strong>).</li></ul>"
msgstr "<ul>  <li>Iidilesi zothungelwano zikholisa ukubhalwa kanje    <strong>http://www.example.com/</strong></li>  <li>Qiniseka ukuba usebenzisa imikrwelo ekekelele phambili (o.k.t.    <strong>/</strong>).</li></ul>"

#: netInterrupt.title
msgid "The connection was interrupted"
msgstr "Umdibaniso uphazamisekile"

#: netInterrupt.longDesc
msgid ""
"_: netInterrupt.longDesc\n"
"&sharedLongDesc;"
msgstr ""
"&sharedLongDesc;"

#: netOffline.title
msgid "Offline mode"
msgstr "Imo engeyo yakusebenza"

#: netOffline.longDesc
msgid "<ul>  <li>Uncheck &quot;Work Offline&quot; in the File menu, then try again.</li></ul>"
msgstr "<ul>  <li>Susa uqwalaselo &quot;Work Offline&quot; kwimenyu Yeefayili, uze uzame kwakhona.</li></ul>"

#: netReset.title
msgid "The connection was reset"
msgstr "Umdibaniso uphinde wamiselwa"

#: netReset.longDesc
msgid ""
"_: netReset.longDesc\n"
"&sharedLongDesc;"
msgstr ""
"&sharedLongDesc;"

#: netTimeout.title
msgid "The connection has timed out"
msgstr "Umdibaniso uphelelwe lixesha"

#: netTimeout.longDesc
msgid ""
"_: netTimeout.longDesc\n"
"&sharedLongDesc;"
msgstr ""
"&sharedLongDesc;"

#: protocolNotFound.title
msgid "The address wasn't understood"
msgstr "Idilesi ayiqondwanga"

#: protocolNotFound.longDesc
msgid "<ul>  <li>You might need to install other software to open this address.</li></ul>"
msgstr "<ul>  <li>Kusenokufuneka ukuba uzame ukuhlohla enye inkqubo ukuze uvule le dilesi.</li></ul>"

#: proxyConnectFailure.title
msgid "The proxy server is refusing connections"
msgstr "Iseva engummeli yala imidibaniso"

#: proxyConnectFailure.longDesc
msgid "<ul>  <li>Check the proxy settings to make sure that they are correct.</li>  <li>Contact your network administrator to make sure the proxy server is    working.</li></ul>"
msgstr "<ul>  <li>Qwalasela imimiselo yommeli ukuze uqiniseke ukuba ilungile.</li>  <li>Zidibanise nomlawuli wakho womsebenzi wothungelwano ukuze uqiniseke ukuba ingaba iseva yommeli iyasebenza na.</li></ul>"

#: proxyResolveFailure.title
msgid "Unable to find the proxy server"
msgstr "Akuphumelelanga ukufumana iseva yommeli"

#: proxyResolveFailure.longDesc
msgid "<ul>  <li>Check the proxy settings to make sure that they are correct.</li>  <li>Check to make sure your computer has a working network connection.</li>  <li>If your computer or network is protected by a firewall or proxy, make sure    that &brandShortName; is permitted to access the Web.</li></ul>"
msgstr "<ul>  <li>Qwalasela imimiselo yommeli ukuqinisekisa ukuba ilungile.</li>  <li>Qwalasela ukuqinisekisa ukuba ikhompyutha yakho inomdibaniso wokusebenza womsebenzi wothungelwano.</li>  <li>Ukuba ikhompyutha yakho okanye umsebenzi wothungelwano ukhuselwe ngodonga lomlilo okanye ngommeli, qiniseka ukuba i-&brandShortName; ivunyelwe ukufikelela Kuthungelwano.</li></ul>"

#: redirectLoop.title
msgid "The page isn't redirecting properly"
msgstr "Eli khasi aliyiguquli intsingiselo ngokufanelekileyo"

#: redirectLoop.longDesc
msgid "<ul>  <li>This problem can sometimes be caused by disabling or refusing to accept    cookies.</li></ul>"
msgstr "<ul>  <li>Le ngxaki maxa wambi inokubangwa kukwaphula isakhono okanye ukwala ukwamkela ii-cookies.</li></ul>"

#: unknownSocketType.title
msgid "Unexpected response from server"
msgstr "Impendulo engalindelekanga evela kwiseva"

#: unknownSocketType.longDesc
msgid "<ul>  <li>Check to make sure your system has the Personal Security Manager    installed.</li>  <li>This might be due to a non-standard configuration on the server.</li></ul>"
msgstr "<ul>  <li>Qwalasela ukuqinisekisa ukuba isixokelelwano sakho sihlohlwe Umphathi Wokhuseleko Lobuqu Bomntu.</li>  <li>Oku kunokwenziwa lubumbeko olungeso siqhelo kwiseva.</li></ul>"

#: sharedLongDesc
msgid "<ul>  <li>The site could be temporarily unavailable or too busy. Try again in a few    moments.</li>  <li>If you are unable to load any pages, check your computer's network    connection.</li>  <li>If your computer or network is protected by a firewall or proxy, make sure    that &brandShortName; is permitted to access the Web.</li></ul>"
msgstr "<ul>  <li>Isiza sisenokungabikho okwethutyana okanye sixakeke kakhulu. Zama kwakhona kwimizuzu embalwa.</li>  <li>Ukuba akukwazi kulayisha nawaphi amakhasi, qwalasela umdibaniso womsebenzi wothungelwano wekhompyutha yakho.</li>  <li>Ukuba ikhompyutha yakho okanye umsebenzi wothungelwano ukhuselwe ngodonga lomlilo okanye ngommeli, qiniseka ukuba i-&brandShortName; ivunyelwe ukufikelela Kuthungelwano.</li></ul>"

