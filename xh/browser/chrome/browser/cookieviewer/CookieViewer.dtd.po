#: div.cookiesonsystem.label
msgid "View and remove cookies that are stored on your computer."
msgstr "Jonga ususe ii-cookies ezilondolozwe ekhompyutheni yakho."

#: treehead.cookiename.label
msgid "Cookie Name"
msgstr "Igama le-Cookie"

#: treehead.cookiedomain.label
msgid "Site"
msgstr "Isiza"

#: treehead.infoselected.label
msgid "Information about the selected Cookie"
msgstr "Ulwazi malunga ne-Cookie ekhethiweyo"

#: button.removecookie.label
msgid "Remove Cookie"
msgstr "Susa i-Cookie"

#: button.removeallcookies.label
msgid "Remove All Cookies"
msgstr "Susa Zonke ii-Cookies"

#: props.name.label
msgid "Name:"
msgstr "Igama:"

#: props.value.label
msgid "Content:"
msgstr "Isiqulatho:"

#: props.domain.label
msgid "Host:"
msgstr "Umamkeli:"

#: props.path.label
msgid "Path:"
msgstr "Indlela yothungelwano:"

#: props.secure.label
msgid "Send For:"
msgstr "Thumela Apho:"

#: props.expires.label
msgid "Expires:"
msgstr "Kuphelelwe lixesha:"

#: window.title
msgid "Stored Cookies"
msgstr "Ii-Cookies Ezilondoloziweyo"

#: checkbox.label
msgid "Don't allow sites that set removed cookies to set future cookies"
msgstr "Musa ukuvumela iziza ezimisela ii-cookies ezisusiweyo ukuba zimisele ii-cookies zekamva"

