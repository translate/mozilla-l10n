#: askOnSave.label
msgid "Download Folder"
msgstr "Thoba Umthwalo Wesiqulathi Seefayili"

#: alwaysAsk.label
#: alwaysAsk.accesskey
msgid "&Ask me where to save every file"
msgstr "&Ndibuze apho ifayili nganye ifanele ukugcinwa khona"

#: neverAsk.label
#: neverAsk.accesskey
msgid "&Save all files to this folder:"
msgstr "&Gcina zonke iifayili kwesi siqulathi seefayili:"

#: desktop.label
msgid "Desktop"
msgstr "Ulwazi oluseskrinini"

#: downloads.label
msgid "My Downloads"
msgstr "Iziganeko Zam Zokuthotywa Kwemithwalo"

#: chooseFolderWin.label
#: chooseFolderWin.accesskey
msgid "&Browse..."
msgstr "&Khangela..."

#: chooseFolderMac.label
#: chooseFolderMac.accesskey
msgid "C&hoose..."
msgstr "Kh&etha..."

#: downloadManagerWindow.label
msgid "Download Manager"
msgstr "Thoba Umthwalo Womphathi"

#: showWhenStarting.label
#: showWhenStarting.accesskey
msgid "Sh&ow Download Manager when a download begins"
msgstr "Bo&nisa Umphathi Wokuthoba Umthwalo ixesha lokuqalisa ukuthoba umthwalo"

#: closeWhenDone.label
#: closeWhenDone.accesskey
msgid "&Close the Download Manager when all downloads are complete"
msgstr "&Vala Umphathi Wokuthoba Umthwalo xa zonke iziganeko zokuthoba umthwalo ziphelile"

#: fileTypes.label
msgid "Download Actions"
msgstr "Amanyathelo Okuthoba Umthwalo"

#: configureActions.label
#: configureActions.accesskey
msgid "&View &amp; Edit Actions..."
msgstr "&Jonga &amp; Hlela Amanyathelo..."

#: fileTypesDescription.label
msgid "&brandShortName; can automatically download or open files of certain types."
msgstr "I-&brandShortName; inokuthoba umthwalo ngokuzenzekela okanye ivule iifayili zezimbo ezithile."

