#: tabsTab.label
msgid "Tabs"
msgstr ""

#: openExternalLinksIn.label
msgid "Open links from other applications in:"
msgstr "Vula amakhonco kwezinye izixhobo ezise:"

#: newWindow.label
msgid "a new window"
msgstr "festileni entsha"

#: newTabInRecentWindow.label
msgid "a new tab in the most recent window"
msgstr "kwiphepha elitsha kwifestile esandula kuhlaziywa"

#: recentWindowOrTab.label
msgid "the most recent tab/window"
msgstr "phepheni okanye efestileni esandula kuhlaziywa"

#: forceNewWindowLinks.label
msgid "Force links that open new windows to open in:"
msgstr "Nyanzelisa amakhonco avula ifestile entsha ukuba avule kwi: "

#: sameTabOrWindow.label
msgid "the same tab/window as the link"
msgstr "phepha/festile enye njenge khonco"

#: newTab.label
msgid "a new tab"
msgstr "phepha elitsha"

#: hideTabBar.label
msgid "Hide the tab bar when only one web site is open"
msgstr ""
"Fihla ibha yeqhosha lokucofa uvule entsha xa kuvuleke isayithi yewebhu enye "
"kuphela"

#: loadInBackground.label
msgid "Select new tabs opened from links"
msgstr "Khetha amaphepha amtsha avulwe kumakhonco"

#: loadBookmarksInBackground.label
msgid "Select new tabs opened from bookmarks or history"
msgstr "Khetha amaphepha amatsha avulwe kumaphawu endawo okanye embali"

#: warnOnClose.label
msgid "Warn when closing multiple tabs"
msgstr "Nika isilumkiso xa uvala amaphepha amaninzi"
