#: tabsTab.label
msgid "Tabs"
msgstr "Izilungelelanisi zemigca"

#: openExternalLinksIn.label
msgid "Open links from other applications in:"
msgstr "Vula iindawo zonxulumaniso ezivela kwezinye iinkqubo apha:"

#: newWindow.label
#: newWindow.accesskey
msgid "a ne&w window"
msgstr "ifestile en&ntsha"

#: newTabInRecentWindow.label
#: newTabInRecentWindow.accesskey
msgid "a new &tab in the most recent window"
msgstr "isilungelelanisi &semigca esitsha kweyona festile intsha"

#: recentWindowOrTab.label
#: recentWindowOrTab.accesskey
msgid "the most &recent tab/window"
msgstr "esona silungelelanisi semigca &sitsha /ifestile"

#: forceNewWindowLinks.label
#: forceNewWindowLinks.accesskey
msgid "&Force links that open new windows to open in:"
msgstr "&Nyanzelisa iindawo zonxulumaniso ezivula iifestile ezintsha zokuvula ngaphakathi:"

#: sameTabOrWindow.label
#: sameTabOrWindow.accesskey
msgid "the &same tab/window as the link"
msgstr "&kwasona isilungelelanisi semigca/ifestile njengesinxulumanisi"

#: newTab.label
#: newTab.accesskey
msgid "a &new tab"
msgstr "isilungelelanisi semigca &esitsha"

#: hideTabBar.label
#: hideTabBar.accesskey
msgid "Hi&de the tab bar when only one web site is open"
msgstr "Fi&hla isixwexwe sesilungelelanisi semigca kuphela xa sisinye isiza sothungelwano esivulekileyo"

#: loadInBackground.label
#: loadInBackground.accesskey
msgid "Se&lect new tabs opened from links"
msgstr "Khe&tha izilungelelanisi zemigca ezitsha ezivulekileyo"

#: loadBookmarksInBackground.label
#: loadBookmarksInBackground.accesskey
msgid "Select new ta&bs opened from bookmarks or history"
msgstr "Khetha izilu&ngelelanisi ezitsha ezivulekileyo ezivela kwizalathisi eziphawulayo okanye kwimbali"

#: warnOnClose.label
#: warnOnClose.accesskey
msgid "Warn when closing &multiple tabs"
msgstr "Lumkisa xa uvala izilungelelanisi zemigca &ezininzi"

