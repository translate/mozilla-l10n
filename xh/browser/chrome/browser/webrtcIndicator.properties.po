#: webrtcIndicator.sharingBrowserWithNTabs.menuitem
#, fuzzy
msgid "Sharing a Tab with #1 tab;Sharing Tabs with #1 tabs"
msgstr "Ukwabelana ngekhamera ne-#1 tab;Ukwabelana ngekhamera ne-#1 tabs"

#. # LOCALIZATION NOTE (webrtcIndicator.controlSharingOn.menuitem):
#. # %S is the title of the tab using the share.
#: webrtcIndicator.controlSharingOn.menuitem
msgid "Control Sharing on \"%S\""
msgstr "Lawula ukwabelana kwi-\"%S\""
