msgstr "Iphepha:"

#: page_of
msgid "of {{pageCount}}"
msgstr "kwali- {{pageCount}}"

#: zoom_out.title
msgctxt "zoom_out.title"
msgid "Zoom Out"
msgstr "Bhekelisela Kudana"

#: zoom_out_label
