#. # Main toolbar buttons (tooltips and alt text for images)
#: previous.title
msgid "Previous Page"
msgstr "Iphepha langaphambili"

#: previous_label
msgctxt "previous_label"
msgid "Previous"
msgstr "Okwangaphambili"

#: next.title
msgid "Next Page"
msgstr "Iphepha elilandelayo"

#: next_label
msgctxt "next_label"
msgid "Next"
msgstr "Okulandelayo"

