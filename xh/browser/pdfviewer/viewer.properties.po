msgstr "Bhekelisela Kudana"

#: zoom_in.title
msgctxt "zoom_in.title"
msgid "Zoom In"
msgstr "Sondeza Kufuphi"

#: zoom_in_label
msgctxt "zoom_in_label"
msgid "Zoom In"
msgstr "Sondeza Kufuphi"

#: zoom.title
msgid "Zoom"
msgstr "Yandisa Unciphise"

#: print.title
msgctxt "print.title"
msgid "Print"
msgstr "Printa"

#: print_label
msgctxt "print_label"
msgid "Print"
msgstr "Printa"

#: presentation_mode.title
msgid "Switch to Presentation Mode"
msgstr "Tshintshela kwimo yonikezelo"

#: presentation_mode_label
msgid "Presentation Mode"
msgstr "Imo yonikezelo"

#: open_file.title
msgid "Open File"
msgstr "Vula Ifayile"

#: open_file_label
msgid "Open"
msgstr "Vula"

#: download.title
msgctxt "download.title"
msgid "Download"
msgstr "Khuphela"

#: download_label
msgctxt "download_label"
msgid "Download"
msgstr "Khuphela"

#: bookmark.title
msgid "Current view (copy or open in new window)"
msgstr "Imbonakalo ekhoyo (kopa okanye vula kwifestile entsha)"

#: bookmark_label
msgid "Current View"
msgstr "Imbonakalo ekhoyo"

#. # Tooltips and alt text for side panel toolbar buttons
#. # (the _label strings are alt text for the buttons, the .title strings are
#. # tooltips)
#: toggle_sidebar.title
msgctxt "toggle_sidebar.title"
msgid "Toggle Sidebar"
msgstr "Togola ngebha eseCaleni"

#: toggle_sidebar_label
msgctxt "toggle_sidebar_label"
msgid "Toggle Sidebar"
msgstr "Togola ngebha eseCaleni"

#: outline.title
msgid "Show Document Outline"
msgstr "Bonisa isishwankathelo soxwebhu"

#: outline_label
msgid "Document Outline"
msgstr "Isishwankathelo soxwebhu"

#: thumbs.title
msgid "Show Thumbnails"
msgstr "Bonisa ukrobiso kumfanekiso"

#: thumbs_label
msgid "Thumbnails"
msgstr "Ukrobiso kumfanekiso"

#: findbar.title
msgid "Find in Document"
msgstr "Fumana kuXwebhu"

#: findbar_label
msgid "Find"
msgstr "Fumana"

#. # Thumbnails panel item (tooltip and alt text for images)
#. # LOCALIZATION NOTE (thumb_page_title): "{{page}}" will be replaced by the page
#. # number.
#: thumb_page_title
msgid "Page {{page}}"
msgstr "Iphepha {{page}}"

#. # LOCALIZATION NOTE (thumb_page_canvas): "{{page}}" will be replaced by the page
#. # number.
#: thumb_page_canvas
msgid "Thumbnail of Page {{page}}"
msgstr "Ukrobiso kumfanekiso wephepha {{page}}"

#. # Context menu
#: first_page.label
msgid "Go to First Page"
msgstr "Yiya kwiphepha lokuqala"

#: last_page.label
msgid "Go to Last Page"
msgstr "Yiya kwiphepha lokugqibela"

#: page_rotate_cw.label
msgid "Rotate Clockwise"
msgstr "Jikelisa ngasekunene"

#: page_rotate_ccw.label
msgid "Rotate Counterclockwise"
msgstr "Jikelisa ngasekhohlo"

#. # Find panel button title and messages
#: find_label
msgid "Find:"
msgstr "Fumanisa:"

#: find_previous.title
msgid "Find the previous occurrence of the phrase"
msgstr "Fumanisa isenzeko sangaphambili sebinzana lamagama"

#: find_previous_label
msgctxt "find_previous_label"
msgid "Previous"
msgstr "Okwangaphambili"

#: find_next.title
msgid "Find the next occurrence of the phrase"
msgstr "Fumanisa isenzeko esilandelayo sebinzana lamagama"

#: find_next_label
msgctxt "find_next_label"
msgid "Next"
msgstr "Okulandelayo"

#: find_highlight
msgid "Highlight all"
msgstr "Qaqambisa konke"

#: find_match_case_label
msgid "Match case"
msgstr "Tshatisa ngobukhulu bukanobumba"

#: find_reached_top
msgid "Reached top of document, continued from bottom"
msgstr "Ufike ngaphezulu ephepheni, kusukwa ngezantsi"

#: find_reached_bottom
msgid "Reached end of document, continued from top"
msgstr "Ufike ekupheleni kwephepha, kusukwa ngaphezulu"

#: find_not_found
msgid "Phrase not found"
msgstr "Ibinzana alifunyenwanga"

#. # Error panel labels
#: error_more_info
msgid "More Information"
msgstr "Inkcazelo Engakumbi"

#: error_less_info
msgid "Less Information"
msgstr "Inkcazelo Encinane"

#: error_close
msgid "Close"
msgstr "Vala"

#. # LOCALIZATION NOTE (error_version_info): "{{version}}" and "{{build}}" will be
#. # replaced by the PDF.JS version and build ID.
#: error_version_info
msgid "PDF.js v{{version}} (build: {{build}})"
msgstr "I-PDF.js v{{version}} (ulwakhiwo: {{build}})"

#. # LOCALIZATION NOTE (error_message): "{{message}}" will be replaced by an
#. # english string describing the error.
#: error_message
msgid "Message: {{message}}"
msgstr "Umyalezo: {{message}}"

#. # LOCALIZATION NOTE (error_stack): "{{stack}}" will be replaced with a stack
#. # trace.
#: error_stack
msgid "Stack: {{stack}}"
msgstr "Imfumba: {{stack}}"

#. # LOCALIZATION NOTE (error_file): "{{file}}" will be replaced with a filename
#: error_file
msgid "File: {{file}}"
msgstr "Ifayile: {{file}}"

#. # LOCALIZATION NOTE (error_line): "{{line}}" will be replaced with a line number
#: error_line
msgid "Line: {{line}}"
msgstr "Umgca: {{line}}"

#: rendering_error
msgid "An error occurred while rendering the page."
msgstr "Imposiso yenzekile xa bekunikezelwa iphepha."

#. # Predefined zoom values
#: page_scale_width
msgid "Page Width"
msgstr "Ububanzi bephepha"

#: page_scale_fit
msgid "Page Fit"
msgstr "Ukulinganiswa kwephepha"

#: page_scale_auto
msgid "Automatic Zoom"
msgstr "Ukwandisa/Ukunciphisa Ngokwayo"

#: page_scale_actual
msgid "Actual Size"
msgstr "Ubungakanani bokwenene"

#. # Loading indicator messages
#: loading_error_indicator
msgid "Error"
msgstr "Imposiso"

#: loading_error
msgid "An error occurred while loading the PDF."
msgstr "Imposiso yenzekile xa kulayishwa i-PDF."

#: invalid_file_error
msgid "Invalid or corrupted PDF file."
msgstr "Ifayile ye-PDF engeyiyo okanye eyonakalisiweyo."

#: missing_file_error
msgid "Missing PDF file."
msgstr "Ifayile ye-PDF edukileyo."

#. # LOCALIZATION NOTE (text_annotation_type.alt): This is used as a tooltip.
#. # "{{type}}" will be replaced with an annotation type from a list defined in
#. # the PDF spec (32000-1:2008 Table 169 â€“ Annotation types).
#. # Some common types are e.g.: "Check", "Text", "Comment", "Note"
#: text_annotation_type.alt
msgid "[{{type}} Annotation]"
msgstr "[{{type}} Ubhalo-nqaku]"

#: request_password
msgid "PDF is protected by a password:"
msgstr "I-PDF ikhuselwe yipasiwedi:"

#: invalid_password
msgid "Invalid Password."
msgstr "Ipasiwedi engasebenziyo."

#: printing_not_supported
msgid "Warning: Printing is not fully supported by this browser."
msgstr "Isilumkiso: Ukuprinta akuxhaswa ngokupheleleyo yile bhrawuza."

#: printing_not_ready
msgid "Warning: The PDF is not fully loaded for printing."
msgstr "Isilumkiso: IPDF ayihlohlwanga ngokupheleleyo ukwenzela ukuprinta."

#: web_fonts_disabled
msgid "Web fonts are disabled: unable to use embedded PDF fonts."
msgstr ""
"Iifonti zewebhu ziqhwalelisiwe: ayikwazi ukusebenzisa iifonti ze-PDF "
"ezincanyathelisiweyo."

#: document_colors_disabled
msgid ""
"PDF documents are not allowed to use their own colors: 'Allow pages to "
"choose their own colors' is deactivated in the browser."
msgstr ""
"Amaxwebhu ePDF akavumelekanga ukuba asebenzise imibala yawo: 'Ukuvumela "
"amaphepha ukuba asebenzise eyawo imibala' kuvaliwe ukuba kungasebenzi "
"kwibhrawuza."

#~ msgid "Web colors are disabled."
#~ msgstr "Imibala yewebhu yenziwe ayasebenza."

#~ msgid "No Outline Available"
#~ msgstr "Akukho sishwankathelo sifumanekayo"
