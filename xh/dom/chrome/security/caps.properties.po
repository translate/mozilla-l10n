#: Yes
msgid "Allow"
msgstr "Vumela"

#: No
msgid "Deny"
msgstr "Yalela"

#: Titleline
msgid "Internet Security"
msgstr "Ukhuseleko lwe Internet"

#: CheckMessage
msgid "Remember this decision"
msgstr "Khumbula esi sigqibo"

#: EnableCapabilityQuery
msgid ""
"A script from \"%S\" is requesting enhanced abilities that are UNSAFE and "
"could be used to compromise your machine or data:\n"
"\n"
"%S\n"
"\n"
"Allow these abilities only if you trust this source to be free of viruses or "
"malicious programs."
msgstr ""
"Okubhaliweyo kusuka kwi-\"%S\" kucela imvume yokukhulisa amendu "
"angakhuselekanga kwaye ungaphulukana nolwazi nokucima komatshini wakho:\n"
"\n"
"%S\n"
"\n"
". Vumela lamendu xa uqinisekile ukuba lomthombo umsulwa kwiivarasi okanye "
"kwiinkqubo ezingalunganga zekhompyutha "

#: EnableCapabilityDenied
msgid "A script from \"%S\" was denied %S privileges."
msgstr "Okubhaliweyo kusuka kwi \"%S\" walelwe %S amalungelo."

#: CheckLoadURIError
msgid "Security Error: Content at %S may not load or link to %S."
msgstr ""
"Imposiso Yokhuseleko: Umxholo kwi %S ayinokwazi ukulayisha okanye ikhonknco "
"kwi %S."

#: CheckSameOriginError
msgid "Security Error: Content at %S may not load data from %S."
msgstr ""
"Imposiso Yokhuseleko: Umxholo kwi %S ayinokwazi ukulayisha okanye ikhonknco "
"kwi %S."

#: GetPropertyDenied
msgid "Permission denied to get property %S.%S"
msgstr "Walelwe imvume yokufumana ipropati %S.%S"

#: SetPropertyDenied
msgid "Permission denied to set property %S.%S"
msgstr "Walelwe imvume sebenzisa ipropati %S.%S"

#: CallMethodDenied
msgid "Permission denied to call method %S.%S"
msgstr "Walelwe imvume yokugqithela kwizicwangciso %S.%S"

#: CreateWrapperDenied
msgid "Permission denied to create wrapper for object of class %S"
msgstr "Walelwe imvume yokwenza irepha kwinto yeklasi:%S"

# #-#-#-#-#  imapMsgs.properties.po (imapMsgs.prop)  #-#-#-#-#
# # @name IMAP_UNKNOWN_USER
# # @loc None
# 5083
#: ExtensionCapability
msgid "Unknown: %S"
msgstr "Engaziwayo: %S"

#
# The following descriptions are shown in the EnableCapabilityQuery dialog
#
#: capdesc.UniversalBrowserRead
msgid "Read private data from any site or window"
msgstr "Funda ulwazi oluyimfihlo kuyo nayophina isayithi okanye ifestile"

#: capdesc.UniversalBrowserWrite
msgid "Modify any open window"
msgstr "Lungisa nokuba yeyi ifestile evuliweyo"

#: capdesc.UniversalXPConnect
msgid "Run or install software on your machine"
msgstr "Qhuba okanye faka izizhobo zekhompyutha kumashini wakho"

#: capdesc.UniversalFileRead
msgid "Read and upload local files"
msgstr "Funda ubuyisele kumaxwebhu angasentla"

#: capdesc.CapabilityPreferencesAccess
msgid "By-pass core security settings"
msgstr "Zidlule izicwangciso zokhuseleko ezingundoqo"

# Description
# # @name EUDORAIMPORT_ACCOUNTNAME
# # @loc None
# LOCALIZATION NOTE (2006): Do not translate "Eudora" below.
# 2006
#: capdesc.UniversalPreferencesRead
msgid "Read program settings"
msgstr "Funda inkqubo yokuseta"

# Description
# # @name EUDORAIMPORT_ACCOUNTNAME
# # @loc None
# LOCALIZATION NOTE (2006): Do not translate "Eudora" below.
# 2006
#: capdesc.UniversalPreferencesWrite
msgid "Modify program settings"
msgstr "Lungisa inkqubo yokuseta"

