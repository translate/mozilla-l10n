#: 1
msgid "Parsing an XSLT stylesheet failed."
msgstr "Ucazululo lwephepha lesimbo se-XSLT aluphumelelanga."

#: 2
msgid "Parsing an XPath expression failed."
msgstr "Ucazululo lwembonakalo ye-XPath aluphumelelanga."

#: 4
msgid "XSLT transformation failed."
msgstr "Ukuguqulwa kwe-XSLT akuphumelelanga."

#: 5
msgid "Invalid XSLT/XPath function."
msgstr "Umsebenzi we-XSLT/XPath awuchanekanga."

#: 6
msgid "XSLT Stylesheet (possibly) contains a recursion."
msgstr "Iphepha lesimbo leXSLT (mhlawumbi) linophinda-phindo."

#: 7
msgid "Attribute value illegal in XSLT 1.0."
msgstr "Beka uphawu kwixabiso olungekho mthethweni kwi-XSLT 1.0."

#: 8
msgid "An XPath expression was expected to return a NodeSet."
msgstr "Imbonakalo ye-XPath ibilindelwe ukuba ibuyele kwiNodeSet."

#: 9
msgid "XSLT transformation was terminated by <xsl:message>."
msgstr "Inguqulo ye-XSLT ipheliswe yi<xsl:message>."

#: 10
msgid "A network error occured loading an XSLT stylesheet:"
msgstr ""
"Kwenzeke impazamo yothungelwano xa bekudontswa iphepha lesimbo se-XSLT:"

#: 11
msgid "An XSLT stylesheet does not have an XML mimetype:"
msgstr "Iphepha lesimbo se-XSLT alina-mimetype yeXML"

#: 12
msgid "An XSLT stylesheet directly or indirectly imports or includes itself:"
msgstr ""
"Iphepha lesimbo se-XSLT lithatha kwenye indawo ngokungqalileyo okanye "
"ngokungangqalanga okanye liyazifaka:"

#: 13
msgid "An XPath function was called with the wrong number of arguments."
msgstr "Umsebenzi we-XPath ubizwe nenani elingalunganga lengxoxo."

#: 14
msgid "An unknown XPath extension function was called."
msgstr "Kubizwe umsebenzi wolwandiso lwe-XPath olungaziwayo"

#: 15
msgid "XPath parse failure: ')' expected:"
msgstr "Ukungaphumeleli kocazululo lwe-XPath: ')' kulindelekile:"

#: 16
msgid "XPath parse failure: invalid axis:"
msgstr "Ukungaphumeleli kocazululo lwe-XPath: i-aksisi engachanekanga:"

#: 17
msgid "XPath parse failure: Name or Nodetype test expected:"
msgstr ""
"Ukungaphumeleli kocazululo lwe-XPath: Igama okanye uvavanyo lweNodetype "
"lulindelekile:"

#: 18
msgid "XPath parse failure: ']' expected:"
msgstr "Ukungaphumeleli kocazululo lwe-XPath: ']' kulindelekile:"

#: 19
msgid "XPath parse failure: invalid variable name:"
msgstr ""
"Ukungaphumeleli kocazululo lwe-XPath: igama elahlukileyo elingachanekanga:"

#: 20
msgid "XPath parse failure: unexpected end of expression:"
msgstr ""
"Ukungaphumeleli kocazululo lwe-XPath: isiphelo ezingalindelekanga sentetha:"

#: 21
msgid "XPath parse failure: operator expected:"
msgstr "Ukungaphumeleli kocazululo lwe-XPath: Umlungisi ulindelekile:"

#: 22
msgid "XPath parse failure: unclosed literal:"
msgstr "Ukungaphumeleli kocazululo lwe-XPath: unclosed literal"

#: 23
msgid "XPath parse failure: ':' unexpected:"
msgstr "Ukungaphumeleli kocazululo lwe-XPath: ':' akulindelekanga:"

#: 24
msgid "XPath parse failure: '!' unexpected, negation is not():"
msgstr ""
"Ukungaphumeleli kocazululo lwe-XPath: '!' akulindelekanga, ukukhanyela ():"

#: 25
msgid "XPath parse failure: illegal character found:"
msgstr ""
"Ukungaphumeleli kocazululo lwe-XPath: kufunyenwe umbhalo ongachanekanga:"

#: 26
msgid "XPath parse failure: binary operator expected:"
msgstr "Ukungaphumeleli kocazululo lwe-XPath: umlungisiwesibini ulindelekile:"

#: 27
msgid "An XSLT stylesheet load was blocked for security reasons."
msgstr ""

#: LoadingError
msgid "Error loading stylesheet: %S"
msgstr "Imposiso xa bekudontswa iphepha lesimbo: %S"

#: TransformError
msgid "Error during XSLT transformation: %S"
msgstr "Imposiso ngexesha loguqulelo lwe-XSLT: %S"
