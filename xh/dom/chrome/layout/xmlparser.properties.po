# Map Expat error codes to error strings
#: 1
msgid "out of memory"
msgstr "ayikho kuvimba wolwazi"

#: 2
msgid "syntax error"
msgstr "imposiso kwizivakalisi"

#: 3
msgid "no element found"
msgstr "akukho siqalelo sofunyenweyo"

#: 4
msgid "not well-formed"
msgstr "ayibumbekanga kakuhle"

#: 5
msgid "unclosed token"
msgstr "umqondiso ongavalwanga"

#: 6
msgid "partial character"
msgstr "umbhalo ongaphelelanga"

#: 7
msgid "mismatched tag"
msgstr "ilebhile engangqanyaniswanga"

#: 8
msgid "duplicate attribute"
msgstr "uphawu loyelelwano oluphindwe kabini"

#: 9
msgid "junk after document element"
msgstr "ingxubevange emva kwesiqalelo soxwebhu"

#: 10
msgid "illegal parameter entity reference"
msgstr "isalathiso esingekho mthethweni somda wenkcazelo iphelele"

#: 11
msgid "undefined entity"
msgstr "okukhoyo okungachazwanga"

#: 12
msgid "recursive entity reference"
msgstr "isalathiso esiphindaphinda okukhoyo"

#: 13
msgid "asynchronous entity"
msgstr "okukhoyo okungenzeki ngaxeshanye"

#: 14
msgid "reference to invalid character number"
msgstr "isalathiso esiya kumbhalo olinani ongasebenziyo"

#: 15
msgid "reference to binary entity"
msgstr "isalathiso esiya kokukhoyo okuhamba kubini"

#: 16
msgid "reference to external entity in attribute"
msgstr "isalathiso esiya kokukhoyo koyelelwano lwangaphandle"

#: 17
msgid "xml declaration not at start of external entity"
msgstr "isibhengezo se-xml esingekho ekuqaleni kokukhoyo kwangaphandle"

#: 18
msgid "unknown encoding"
msgstr "ukufakwa kweekhowudi okungaziwayo"

#: 19
msgid "encoding specified in XML declaration is incorrect"
msgstr "ukufakwa kweekhowudi okuxelwe kwisibhengezo se-XML akuchanekanga"

#: 20
msgid "unclosed CDATA section"
msgstr "icandelo elingavalwanga le-CDATA"

#: 21
msgid "error in processing external entity reference"
msgstr "imposiso ekuqhubeleni phambili isalathiso sokukhoyo kwangaphandle"

#: 22
msgid "document is not standalone"
msgstr "uxwebhu alumanga lodwa"

#: 23
msgid "unexpected parser state"
msgstr "imeko yokwahlulahlula engalindelekanga"

#: 24
msgid "entity declared in parameter entity"
msgstr "okukhoyo kubhengezwa kokukhoyo komda wenkcazelo"

#: 27
msgid "prefix not bound to a namespace"
msgstr "isimaphambili asinyanzelekanga kwisithuba samagama"

#: 29
msgid "reserved prefix (xml) must not be undeclared or bound to another namespace URI"
msgstr "isimaphambili esibuyiseka emva i-(xml) masingabi sesingabhengezwayo okanye sibopheleleke kwesinye isithuba samagama URI"

#: 30
msgid "reserved prefix (xmlns) must not be declared or undeclared"
msgstr "isimaphambili esibuyiseka emva i-(xmlns) masingabi sesibhengezwayo okanye esingabhengezwayo"

#: 31
msgid "prefix must not be bound to one of the reserved namespace URIs"
msgstr "isimaphambili masingabopheleleki kwisithuba segama esinye esibekiweyo see-URI"

# %1$S is replaced by the Expat error string, may be followed by Expected (see below)
# %2$S is replaced by URL
# %3$d is replaced by line number
# %4$d is replaced by column number
#: XMLParsingError
msgid ""
"XML Parsing Error: %1$S\n"
"Location: %2$S\n"
"Line Number %3$d, Column %4$d:"
msgstr ""
"Imposiso Yokwahlulahlula ye-XML: %1$S\n"
"Indawo ekuyo: %2$S\n"
"Inombolo Yomgca %3$d, Umqolo owehlayo %4$d:"

# %S is replaced by a tag name.
# This gets appended to the error string if the error is mismatched tag.
#: Expected
msgid ". Expected: </%S>."
msgstr ". Okulindelekileyo: </%S>."

