#: MimeNotCss
msgid "The stylesheet %1$S was not loaded because its MIME type, \"%2$S\", is not \"text/css\"."
msgstr "Icwecwe lesimbo i-%1$S alilayishwanga kuba isimbo sayo se-MIME, i-\"%2$S\", asisiso \"text/css\"."

#: MimeNotCssWarn
msgid "The stylesheet %1$S was loaded as CSS even though its MIME type, \"%2$S\", is not \"text/css\"."
msgstr "Icwecwe lesimbo i-%1$S belilayishwe njenge-CSS nangona isimbo sayo se-MIME, i-\"%2$S\", ibesingeso \"text/css\"."

#: PEUnexpEOF
msgid "Unexpected end of file while searching for %1$S."
msgstr "Isiphelo sefayili ebesingalindelekanga xa bekuzingelwa i-%1$S."

#: PEParseRuleWSOnly
msgid "Whitespace-only string given to be parsed as rule."
msgstr "Umtya wesikhewu esimhlophe-kuphela sinikelwe ukwahlulahlula njengomthetho."

#: PEDeclDropped
msgid "Declaration dropped."
msgstr "Isibhengezo silahliwe."

#: PEDeclSkipped
msgid "Skipped to next declaration."
msgstr "Kutsityelwe kwisibhengezo esilandelayo."

#: PEUnknownProperty
msgid "Unknown property '%1$S'."
msgstr "Yinkcazelo ngophawu engaziwyo i-'%1$S'."

#: PEPropertyParsingError
msgid "Error in parsing value for property '%1$S'."
msgstr "Imposiso kwixabiso elahlulahlulayo ukwenzela inkcazelo yophawu i-'%1$S'."

#: PEExpectEndProperty
msgid "Expected end of value for property but found '%1$S'."
msgstr "Isiphelo ebesilindelekile sexabiso lenkcazelo yophawu kodwa kufunyenwe i-'%1$S'."

#: PESkipAtRuleEOF
msgid "end of unknown at-rule"
msgstr "isiphelo somthetho ongaziwayo"

#: PEUnknownAtRule
msgid "Unrecognized at-rule or error parsing at-rule '%1$S'."
msgstr "Asichongeki ku-mthetho okanye imposiso yokwahlulahlula ku-mthetho i-'%1$S'."

#: PECharsetRuleEOF
msgid "charset string in @charset rule"
msgstr "umtya we-charset kumthetho we--@charset"

#: PECharsetRuleNotString
msgid "Expected charset string but found '%1$S'."
msgstr "Umtya olindelekileyo we-charset kodwa kufumaneke i-'%1$S'."

#: PEGatherMediaEOF
msgid "end of media list in @import or @media rule"
msgstr "isiphelo soludwe lwesilondolozi kumthetho we-@import okanye i-@media"

#: PEGatherMediaNotComma
msgid "Expected ',' in media list but found '%1$S'."
msgstr "Okulindelekileyo ',' kuludwe lwesilondolozi kodwa kufunyenwe i-'%1$S'."

#: PEGatherMediaNotIdent
msgid "Expected identifier in media list but found '%1$S'."
msgstr "Isichongi esilindelekileyo kuludwe lwesilondolozi kodwa kufunyenwe i-'%1$S'."

#: PEImportNotURI
msgid "Expected URI in @import rule but found '%1$S'."
msgstr "Bekulindeleke i-URI kumthetho we-@import kodwa kufunyenwe i-'%1$S'."

#: PEImportUnexpected
msgid "Found unexpected '%1$S' within @import."
msgstr "Ifunyenwe ingalindelekanga i-'%1$S' kwi-@import."

#: PEGroupRuleEOF
msgid "end of @media or @-moz-document rule"
msgstr "isiphelo se-@media okanye somthetho we-@-moz-document"

#: PEMozDocRuleBadFunc
msgid "Expected url(), url-prefix(), or domain() in @-moz-document rule but found '%1$S'."
msgstr "Bekulindeleke i- url(), url-prefix(), okanye i- domain() ikumthetho we-@-moz-document kodwa kufunyenwe i-'%1$S'."

#: PEMozDocRuleNotURI
msgid "Expected URI in @-moz-document rule but found '%1$S'."
msgstr "Bekulindeleke i-URI kumthetho we-@-moz-document kodwa kufunyenwe i-'%1$S'."

#: PEAtNSPrefixEOF
msgid "namespace prefix in @namespace rule"
msgstr "isimaphambili se-namespace kumthetho we-@namespace"

#: PEAtNSURIEOF
msgid "namespace URI in @namespace rule"
msgstr "i-namespace se-URI kumthetho we-@amespace"

#: PEAtNSUnexpected
msgid "Unexpected token within @namespace: '%1$S'."
msgstr "Umqondiso obungalindelekanga kwi-@namespace: '%1$S'."

#: PESkipDeclBraceEOF
msgid "closing } of declaration block"
msgstr "kuvalwa } ibloko yesibhengezo"

#: PESkipRSBraceEOF
msgid "closing } of invalid rule set"
msgstr "kuvalwa } iseti yemithetho engasebenziyo"

#: PEBadSelectorRSIgnored
msgid "Ruleset ignored due to bad selector."
msgstr "Ayihoywa iseti yemithetho ngenxa yokukhetha okungalunganga."

#: PESelectorListExtraEOF
msgid "',' or '{'"
msgstr "',' okanye '{'"

#: PESelectorListExtra
msgid "Expected ',' or '{' but found '%1$S'."
msgstr "Obekulindelekile ',' okanye '{' kodwa kufunyenwe i-'%1$S'."

#: PESelectorGroupNoSelector
msgid "Selector expected."
msgstr "Kulindeleke umkhthi."

#: PESelectorGroupExtraCombinator
msgid "Dangling combinator."
msgstr "Isidibanisi esilengalengayo."

#: PEClassSelEOF
msgid "class name"
msgstr "igama lodidi"

#: PEClassSelNotIdent
msgid "Expected identifier for class selector but found '%1$S'."
msgstr "Isichongi esilindelekileyo ukwenzela umkhethi wodidi kodwa kufunyenwe i-'%1$S'."

#: PETypeSelEOF
msgid "element type"
msgstr "isimbo sesiqalelo"

#: PETypeSelNotType
msgid "Expected element name or '*' but found '%1$S'."
msgstr "Igama lesiqalelo esilindelekileyo okanye '*' kodwa kufunyewe i-'%1$S'."

#: PEUnknownNamespacePrefix
msgid "Unknown namespace prefix '%1$S'."
msgstr "Isimaphambiki sesithuba segama esingaziwayo '%1$S'."

#: PEAttributeNameEOF
msgid "attribute name"
msgstr "igama lophawu loyelelwano"

#: PEAttributeNameExpected
msgid "Expected identifier for attribute name but found '%1$S'."
msgstr "Isichongi esilindelekileyo segama lophawu loyelelwano kodwa kufunyenwe i-'%1$S'."

#: PEAttributeNameOrNamespaceExpected
msgid "Expected attribute name or namespace but found '%1$S'."
msgstr "Igama lophawu loyelelwano elilindelekileyo okanye isithuba segama kodwa kufunyenwe i-'%1$S'."

#: PEAttSelNoBar
msgid "Expected '|' but found '%1$S'."
msgstr "Bekulindeleke i-'|' kodwa kufunyenwe i-'%1$S'."

#: PEAttSelInnerEOF
msgid "part of attribute selector"
msgstr "inxalenye yomkhethi wophawu loyelelwano"

#: PEAttSelUnexpected
msgid "Unexpected token in attribute selector: '%1$S'."
msgstr "Umqondiso obungalindelekanga womkhethi wophawu loyelelwano: i-'%1$S'."

#: PEAttSelValueEOF
msgid "attribute value"
msgstr "ixabiso lophawu loyelelwano"

#: PEAttSelCloseEOF
msgid "']' to end attribute selector"
msgstr "I-']' ukuze kupheliswe umkhethi wophawu loyelelwano"

#: PEAttSelNoClose
msgid "Expected ']' to terminate attribute selector but found '%1$S'."
msgstr "Bekulindelwe i-']' ukuphelisa umkhethi kodwa kufunyenwe i-'%1$S'."

#: PEAttSelBadValue
msgid "Expected identifier or string for value in attribute selector but found '%1$S'."
msgstr "Umchongi Olindelekileyo okanye umtya wexabiso kumkhethi wophawu loyelelwano kodwa kufunyenwe i-'%1$S'."

#: PEPseudoSelEOF
msgid "name of pseudo-class or pseudo-element"
msgstr "igama likasinga-ludidi okanye usinga-siqalelo"

#: PEPseudoSelBadName
msgid "Expected identifier for pseudo-class or pseudo-element but found '%1$S'."
msgstr "Umchongi obelindelekile kasinga-ludidi okanye usinga-siqalelo kodwa kufunyenwe i-'%1$S'."

#: PEPseudoSelNonFunc
msgid "Function token for non-function pseudo-class or pseudo-element, or the other way around, when reading '%1$S'."
msgstr "Umqondiso wokusebenza ukwenzela usinga-ludidi okanye usinga-siqalelo, okanye xa kugqwethiwe, ngethuba lokufunda i-'%1$S'."

#: PEPseudoSelNotPE
msgid "Expected pseudo-element but found '%1$S'."
msgstr "Usinga-siqalelo obelindelekile kodwa kufunyenwe i-'%1$S'."

#: PEPseudoSelDoubleNot
msgid "Negation pseudo-class can't be negated '%1$S'."
msgstr "Usinga-ludidi wochasiso akakwazi kuchaswa '%1$S'."

#: PEPseudoSelPEInNot
msgid "Pseudo-elements can't be negated '%1$S'."
msgstr "Oosinga-ziqalelo abakwazi kuchaswa '%1$S'."

#: PEPseudoSelNewStyleOnly
msgid "This pseudo-element must use the \"::\" form: '%1$S'."
msgstr "Lo singa-siqalelo makasebenzise i-\"::\" ifom: '%1$S'."

#: PEPseudoSelTrailing
msgid "Found trailing token after pseudo-element, which must be the last part of a selector:  '%1$S'."
msgstr "Kufunyenwe umqondiso obhijelayo emva kukasinga-siqalelo, ofanele ukuba yinxalenye yokugqibela yomkhethi::  '%1$S'."

#: PEPseudoSelMultiplePE
msgid "Extra pseudo-element '%1$S'."
msgstr "Usinga-siqalelo owongezelekileyo i-'%1$S'."

#: PEPseudoSelUnknown
msgid "Unknown pseudo-class or pseudo-element '%1$S'."
msgstr "Usinga-ludidi ongaziwayo okanye usinga-siqalelo i-'%1$S'."

#: PENegationEOF
msgid "selector within negation"
msgstr "umkhethi onochasiso"

#: PENegationBadInner
msgid "Malformed simple selector as negation pseudo-class argument '%1$S'."
msgstr "Umkhethi ongenabugocigoci ongabumbekanga kakuhle njengochasiso lwesiyaleli sikasinga-ludidi i-'%1$S'."

#: PENegationNoClose
msgid "Missing closing ')' in negation pseudo-class '%1$S'."
msgstr "Kuduke ukuvala i-')' kuchasiso lukasinga-ludidi we-'%1$S'."

#: PENegationBadArg
msgid "Missing argument in negation pseudo-class '%1$S'."
msgstr "Isiyaleli esidukileyo kuchasiso lukasinga-ludidi we-'%1$S'."

# (review) do not translate: :lang
#: PELangArgEOF
msgid "argument to :lang selector"
msgstr "isiyaleli siya ku:mkhethi wolwimi"

#: PELangArgNotIdent
msgid "Expected identifier for lang pseudo-class parameter but found '%1$S'."
msgstr "Umchongi olindelekileyo womda wenkcazelo ngosinga-ludidi wolwimi kodwa kufunyenwe i-'%1$S'."

#: PELangNoClose
msgid "Missing closing ')' in lang pseudo-class '%1$S'."
msgstr "Ukuvala okudukileyo ')' kusinga-ludidi wolwimi i-'%1$S'."

#: PELangNoArg
msgid "Missing argument in lang pseudo-class '%1$S'."
msgstr "Isiyaleli esidukileyo kusinga-ludidi wolwimi i-'%1$S'."

#: PESelectorEOF
msgid "selector"
msgstr "umkhethi"

#: PEBadDeclBlockStart
msgid "Expected '{' to begin declaration block but found '%1$S'."
msgstr "Okulindelekileyo '{' ukuqalisa ibloko yesibhengezo kodwa kufunyenwe i-'%1$S'."

#: PEColorEOF
msgid "color"
msgstr "umbala"

#: PEColorNotColor
msgid "Expected color but found '%1$S'."
msgstr "Bekulindeleke umbala kodwa kufunyenwe i-'%1$S'."

#: PEColorComponentEOF
msgid "color component"
msgstr "inxalenye yombala"

#: PEExpectedPercent
msgid "Expected a percentage but found '%1$S'."
msgstr "Bekulindeleke ipesenti ethile kodwa kufunyenwe i-'%1$S'."

#: PEExpectedInt
msgid "Expected an integer but found '%1$S'."
msgstr "Bekulindeleke inani elipheleleyo kodwa kufunyenwe i-'%1$S'."

#: PEColorBadRGBContents
msgid "Expected number or percentage in rgb() but found '%1$S'."
msgstr "Bekulindeleke inani okanye ipesenti ye- rgb() kodwa kufunyenwe i-'%1$S'."

#: PEColorComponentBadTerm
msgid "Expected '%2$S' but found '%1$S'."
msgstr "Bekulindeleke i-'%2$S' kodwa kufunyenwe i-'%1$S'."

#: PEColorHueEOF
msgid "hue"
msgstr "ibala"

#: PEExpectedComma
msgid "Expected ',' but found '%1$S'."
msgstr "Obekulindelekile ',' kodwa kufunyenwe i-'%1$S'."

#: PEColorSaturationEOF
msgid "saturation"
msgstr "ukuhluthisa"

#: PEColorLightnessEOF
msgid "lightness"
msgstr "ubukhaphukhaphu"

#: PEColorOpacityEOF
msgid "opacity in color value"
msgstr "ukungaboni ngaphaya ngokwexabiso lombala"

#: PEExpectedNumber
msgid "Expected a number but found '%1$S'."
msgstr "Bekulindeleke inani kodwa kufunyenwe i-'%1$S'."

#: PEExpectedCloseParen
msgid "Expected ')' but found '%1$S'."
msgstr "Bekulindeleke i-')' kodwa kufunyenwe i-'%1$S'."

#: PEDeclEndEOF
msgid "';' or '}' to end declaration"
msgstr "I-';' okanye '}' ukuphetha isibhengezo"

#: PEParseDeclarationNoColon
msgid "Expected ':' but found '%1$S'."
msgstr "Bekulindeleke i- ':' kodwa kufunyenwe i-'%1$S'."

#: PEParseDeclarationDeclExpected
msgid "Expected declaration but found '%1$S'."
msgstr "Kulindeleke isibhengezo kodwa kufunyenwe i-'%1$S'."

#: PEEndOfDeclEOF
msgid "end of declaration"
msgstr "isiphelo sesibhengezo"

#: PEImportantEOF
msgid "important"
msgstr "okubalulekileyo"

#: PEExpectedImportant
msgid "Expected 'important' but found '%1$S'."
msgstr "Bekulindeleke u-'okubalulekileyo' kodwa kufunyenwe i-'%1$S'."

#: PEBadDeclEnd
msgid "Expected ';' to terminate declaration but found '%1$S'."
msgstr "Bekulindeleke i-';' ukuphetha isibhengezo kodwa kufunyenwe i-'%1$S'."

#: PEBadDeclOrRuleEnd
msgid "Expected ';' or '}' to terminate declaration but found '%1$S'."
msgstr "Bekulindeleke i-';' okanye '}' ukuphetha isibhengezo kodwa kufumaneke i-'%1$S'."

#: PEInaccessibleProperty
msgid "Attempt to use inaccessible property"
msgstr "Ilinge lokusebenzisa inkcazelo ngophawu engafikelelekiyo"

#: PECommentEOF
msgid "end of comment"
msgstr "isiphelo sokuphawula"

#: SEUnterminatedString
msgid "Found unclosed string '%1$S'."
msgstr "Kufunyenwe umtya ongavalwanga i-'%1$S'."

