msgstr "%S = %S"

#: SSLDisabled
msgid "You cannot connect to %S because SSL is disabled."
msgstr "Awukwazi kudibaniselana ne %S kuba iSSL ikhubaziwe."

#: SSL2Disabled
msgid "You cannot connect to %S because SSL version 2 is disabled."
msgstr ""
"Awukwazi kudibaniselana ne %S ngokuba SSL uguqulelo lwesi2 likhubaziwe."

#: SSLNoMatchingCiphers
msgid ""
"%S and %S cannot communicate securely because they have no common encryption "
"algorithms."
msgstr ""
"%S no %S azikwazi kuqhagamshelana ngokukhuselekileyo ngenxa abanamfihlo "
"ifanayo."

#: UsersCertRevoked
msgid ""
"Could not establish an encrypted connection with %S because your certificate "
"has been revoked."
msgstr ""
"Ayikwazanga kuvula indibaniselwano efihliweyo nge %S kuba isiqinisekiso "
"sakho sibuyiselwe umva."

#: UsersCertExpired
msgid ""
"Could not establish an encrypted connection with %S because your certificate "
"is expired."
msgstr ""
"Ayikwazanga ukufumana udibaniselwano olufihlakaleyo ne %S ngokuba "
"isiqinisekiso sakho siphelelwe lixesha."

#: UsersCertRejected
msgid ""
"Could not establish an encrypted connection because your certificate was "
"rejected by %S. Error Code: %S."
msgstr ""
"Ayikwazanga kufumana indibaniselwano efihliweyo kuba isiqinisekiso sakho "
"saliwe yi %S. Ikhowudi Yempazamo: %S."

#: BadMac
msgid ""
"%S received a message with incorrect Message Authentication Code. If the "
"error occurs frequently, contact the website administrator."
msgstr ""

#: PeerResetConnection
msgid "%S has closed the connection. Error Code: %S"
msgstr "%S ivale udibaniselwano. Imposiso Yekhowudi: %S"

#: HostResetConnection
msgid "The current transfer has been cancelled."
msgstr "Uhambiso lwangoku lirhoxisiwe."

#: BadPassword
msgid "An incorrect password was provided."
msgstr "Kunikezelwe igama eligqithisiweyo elingalunganga."

#: BadDatabase
msgid "There is a problem with your certificate database [Error Code: %S]."
msgstr ""
"Kukho ingxaki nesiseko sedata sesiqinisekiso sakho [Ikhowudi Yemposiso: %S]."

#: BadServer
msgid "%S has sent an incorrect or unexpected message. Error Code: %S"
msgstr ""
"%S ithumele umyalezo ongalunganga okanye umyalezo obenga lindelwanga. "
"Ikhowudi Yemposiso: %S"

#: BadClient
msgid "%S has received an incorrect or unexpected message. Error Code: %S"
msgstr ""
"%S ifumane umyalezo ongalunganga okanye ongalindelekanga. Ikhowudi "
"Yemposiso: %S"

#: HostReusedIssuerSerial
msgid ""
"You have received an invalid certificate.  Please contact the server "
"administrator or email correspondent and give them the following "
"information:\n"
"\n"
"Your certificate contains the same serial number as another certificate "
"issued by the certificate authority.  Please get a new certificate "
"containing a unique serial number."
msgstr ""
"Ufumene isiqinisekiso esingachanekanga. Nceda uqhagamshelane nomlawuli "
"weseva okanye thumela imbalelwano yekhompyutha kwaye unike olu lwazi "
"lulandelayo:\n"
"\n"
"IsiQinisekiso sakho siqulathe kwa inombolo efana neyesinye isiqinisekiso "
"esikhutshwe ziziPhatha-mandla zeziQinisekiso. Nceda ufumane isiqinisekiso "
"esitsha esiqulathe inombolo eyodwa.   "

#: SSLGenericError
msgid "Error establishing an encrypted connection to %S. Error Code: %S."
msgstr ""
"Imposiso ekusekweni kodibaniso olufihliweyo kwi %S. Ikhowudi Yemposiso: %S."

#: TimeOut
msgid "Connection request timed out."
msgstr "Isicelo sendibaniselwano siphelelwe lixesha."

#: PeersCertUntrusted
msgid ""
"Could not establish an encrypted connection because certificate presented by "
"%S is not trusted."
msgstr ""
"Ayikwazanga ukuseka udinabiso olufihliweyo ngokuba isiqinisekiso esiboniswe "
"ngu %S asithembwanga."

#: PeersCertRevoked
msgid ""
"Could not establish an encrypted connection because certificate presented by "
"%S has been revoked."
msgstr ""
"Ayikwazanga ukuseka udibaniso olufihliweyo ngokuba isiqinisekiso esiboniswe "
"ngu %S sibuyiselwe umva."

#: PeersCertExpired
msgid ""
"Could not establish an encrypted connection because certificate presented by "
"%S has expired."
msgstr ""
"Ayikwazanga ukuseka udibaniso elifihliweyo ngokuba isiqinisekiso esiboniswe "
"ngu %S siphelelwe lixesha."

#: PeersCertWrongDomain
msgid ""
"Could not establish an encrypted connection because certificate presented by "
"%S is for a different domain."
msgstr ""
"Ayikwazanga ukuseka udibaniso elifihliweyo ngokuba isiqinisekiso esiboniswe "
"ngu %S lithambeka elahlukileyo."

#: PeersCertBadSignature
msgid ""
"Could not establish an encrypted connection because certificate presented by "
"%S has an invalid signature."
msgstr ""
"Ayikwazanga ukuseka indibaniso elifihliweyo kuba isiqinisekiso esiboniswe yi "
"%S sinesotyikityo esiphelelwe lixesha."

#: PeersCertNoGood
msgid ""
"Could not establish an encrypted connection because certificate presented by "
"%S is invalid or corrupted. Error Code: %S"
msgstr ""
"Ayikwazanga ukuseka indibaniso elifihliweyo kuba isiqinisekiso esiboniswe yi "
"%S asisebenzi okanye yonakalisiwe. Ikhowudi Yempazamo: %S"

#: CRLExpired
msgid ""
"Certificate Revocation List (CRL) from the CA certifying %S is past its Next "
"Update date. Please update the CRL."
msgstr ""
"Umqulu Wokususwa Kweziqinisekiso (CRL) kwiCA eqinisekisa i-%S udlulile "
"kumhla wohlaziyo olandelayo. Nceda uhlaziyer iCRL."

#: CRLNotYetValid
msgid ""
"Certificate Revocation List (CRL) from the CA certifying %S is not yet "
"valid. Please check your system clock."
msgstr ""
"Uluhlu Lweziqinisekiso ezibuyeliswa emva (CRL) ukusuka kuqunisekiso lwe CA %"
"S ayikasebenzi. Nceda khangela ikloko yendlela yakho yokusebenza."

#: CRLSigNotValid
msgid ""
"Certificate Revocation List (CRL) from the CA certifying %S has an invalid "
"digital signature."
msgstr ""
"Uluhlu Lweziqinisekiso ezibuyeliswa emva (CRL) ukusuka kuqunisekiso lwe CA %"
"S inotyikityo lwamanani elingasebenziyo."

#: CRLSNotValid
msgid ""
"Certificate Revocation List (CRL) from the CA certifying %S is not valid."
msgstr ""
"Uluhlu Lweziqinisekiso ezibuyeliswa emva (CRL) ukusuka kuqunisekiso lwe CA %"
"S alisebenzi."

#: OCSPMalformedRequest
msgid ""
"Error trying to validate certificate from %S using OCSP - malformed request."
msgstr ""
"Imposiso ngexesha uzama isiqinisekisi sisebenze esisuka %S usebenzisa i OCSP "
"z - yesicelo esingakhiwanga kakuhle."

#: OCSPRequestNeedsSig
msgid ""
"Error trying to validate certificate from %S using OCSP - request needs "
"signature."
msgstr ""
"Imposiso ngexesha kuzama ukuqinisekisa ukusuka kwi %S usebenzisa i OCSP - "
"yesicelo esifuna utyikityo."

#: OCSPUnauthorizedReq
msgid ""
"Error trying to validate certificate from %S using OCSP - unauthorized "
"request."
msgstr ""
"Imposiso ngexesha kuzama isiqinisekiso sisebenze esisuka %S usebenzisa i "
"OCSP - isicelo ezingavumelekanga."

#: OCSPServerError
msgid "Error trying to validate certificate from %S using OCSP - server error."
msgstr ""
"Imposiso ngexesha kuzama isiqinisekiso sisebenze kwi %S usebenzisa i OCSP - "
"imposiso yomncedisi."

#: OCSPTryServerLater
msgid ""
"Error trying to validate certificate from %S using OCSP - server is busy. "
"Please try again later."
msgstr ""
"Imposiso ngexesha kuzama isiqinisekiso sisebenze kwi %S usebenzisa i OSCP - "
"umncedisi usa xakekile. Nceda zama kwakhona kamva."

#: OCSPFutureResponse
msgid ""
"Error trying to validate certificate from %S using OCSP - response contains "
"a date which is in the future."
msgstr ""
"Imposiso ngexesha kuzama isiqinisekiso sisebenze ukusuka kwi %S usebenzisa i "
"OSCP - impendulo iqulathe umhla okwixesha elizayo."

#: OCSPOldResponse
msgid "Error trying to validate certificate from %S using OCSP - old response."
msgstr ""
"Imposiso ngexesha kuzama isiqinisekiso sisebenze ukusuka kwi %S usebenzisa "
"iOCSCP - impendulo yakudala."

#: OCSPCorruptedResponse
msgid ""
"Error trying to validate certificate from %S using OCSP - corrupted or "
"unknown response. Error Code: %S."
msgstr ""
"Imposiso ngexesha kuzama isiqinisekiso sisebenze ukusuka kwi %S usebenzisa "
"iOSCP - yonakele okanye impendulo ayaziwa. Ikhowudi ye Mposiso: %S."

#: OCSPUnauthorizedResponse
msgid ""
"Error trying to validate certificate from %S using OCSP - unauthorized "
"response."
msgstr ""
"Imposiso ngexesha kuzama isiqinisekiso sisebenze ukusuka kwi %S usebenzisa "
"iOSCP - impendulo engagunyaziswanga."

#: OCSPUnknownCert
msgid ""
"Error trying to validate certificate from %S using OCSP - unknown "
"certificate."
msgstr ""
"Imposiso ngexesha kuzama isiqinisekiso sisebenze esuka kwi %S usebenzisa "
"iOSCP - isiqinisekiso esingaziwayo."

#: OCSPNoDefaultResponder
msgid ""
"Error trying to validate certificate from %S using OCSP - no default "
"responder specified."
msgstr ""
"Imposiso ngexesha kuzama isiqinisekiso sisebenze esuka kwi %S usebenzisa "
"iOSCP - akhukho mphenduli okhankanyiweyo."

#: OCSPDirLookup
msgid ""
"Error trying to validate certificate from %S using OCSP - directory lookup "
"error."
msgstr ""
"Imposiso ngexesha kuzama isiqinisekiso sisebenze ukusuka kwi %S usebenzisa "
"iOSCP - isilawuli sokukhangela imposiso."

