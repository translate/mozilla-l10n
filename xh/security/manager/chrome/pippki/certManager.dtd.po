#: certmgr.title
msgid "Certificate Manager"
msgstr "Umphathi Wesatifikethi"

#: certmgr.tab.mine
msgid "Your Certificates"
msgstr "Izatifikethi Zakho"

#: certmgr.tab.others
msgid "Other People's"
msgstr "Ezabanye abantu"

#: certmgr.tab.websites
msgid "Web Sites"
msgstr "Iziza Zothungelwano"

#: certmgr.tab.ca
msgid "Authorities"
msgstr "Oogunyaziwe"

#: certmgr.ca.builtins
msgid "Built-in CAs"
msgstr "Ii-CA ezakhelwe ngaphakathi"

#: certmgr.ca.mycas
msgid "CAs Managed By You"
msgstr "Ii-CA Ezilawulwa Nguwe"

#: certmgr.mine
msgid "You have certificates from these organizations that identify you:"
msgstr "Unezatifikethi ezivela kule mibutho ekuchongileyo:"

#: certmgr.others
msgid "You have certificates on file that identify these people:"
msgstr "Unezatifikethi efayilini echonga aba bantu:"

#: certmgr.websites
msgid "You have certificates on file that identify these web sites:"
msgstr "Unezatifikethi efayilini echonga ezi ziza zothungelwano:"

#: certmgr.cas
msgid "You have certificates on file that identify these certificate authorities:"
msgstr "Unezatifikethi efayilini echonga aba gunyaziwe bezatifikethi:"

#: certmgr.detail.general_tab.title
msgid "General"
msgstr "Jikelele"

#: certmgr.detail.prettyprint_tab.title
msgid "Details"
msgstr "Iinkcukacha"

#: certmgr.subjectinfo.label
msgid "Issued To"
msgstr "Zinikelwa Ku"

#: certmgr.issuerinfo.label
msgid "Issued By"
msgstr "Zinikelwa Ngu"

#: certmgr.validity.label
msgid "Validity"
msgstr "Ukunyaniseka"

#: certmgr.fingerprints.label
msgid "Fingerprints"
msgstr "Izishicilelo zeminwe"

#: certmgr.certdetail.title
msgid "Certificate Detail"
msgstr "Iinkcukacha Ngesatifikethi"

#: certmgr.certdetail.cn
msgid "Common Name (CN)"
msgstr "Igama Eliqhelekileyo (CN)"

#: certmgr.certdetail.o
msgid "Organization (O)"
msgstr "Umbutho (O)"

#: certmgr.certdetail.ou
msgid "Organizational Unit (OU)"
msgstr "Icandelo Lombutho (OU)"

#: certmgr.certdetail.serialnumber
msgid ""
"_: certmgr.certdetail.serialnumber\n"
"Serial Number"
msgstr ""
"Inombolo Yolandelelwano"

#: certmgr.certdetail.sha1fingerprint
msgid "SHA1 Fingerprint"
msgstr "Ushicileleo lweminwe lwe-SHA1"

#: certmgr.certdetail.md5fingerprint
msgid "MD5 Fingerprint"
msgstr "Ushicilelo lweminwe lwe-MD5"

#: certmgr.editcert.title
msgid "Edit Security Certificate Settings"
msgstr "Hlela Imimiselo Yezatifikethi Yokhuseleko"

#: certmgr.editcacert.title
msgid "Edit CA certificate trust settings"
msgstr "Hlela Imimiselo yentembeko yesatifikethi se-CA"

#: certmgr.editsslcert.title
msgid "Edit web site certificate trust settings"
msgstr "Hlela imimiselo yentembeko yesatifikethi sothungelwano lwesiza"

#: certmgr.editcert.edittrust
msgid "Edit trust settings:"
msgstr "Hlela imimiselo yentembeko:"

#: certmgr.editcert.trustssl
msgid "This certificate can identify web sites."
msgstr "Esi satifikethi sinako ukuchonga iziza zothungelwano."

#: certmgr.editcert.trustemail
msgid "This certificate can identify mail users."
msgstr "Esi satifikethi sinako ukuchonga abasebenzisi bemeyile."

#: certmgr.editcert.trustobjsign
msgid "This certificate can identify software makers."
msgstr "Esi satifikethi sinako ukuchonga abenzi benkqubo ekhohliweyo."

#: certmgr.editsslcert.edittrust
msgid ""
"_: certmgr.editsslcert.edittrust\n"
"Edit certificate trust settings:"
msgstr ""
"Hlela imimiselo yentembeko yesatifikethi:"

#: certmgr.editsslcert.dotrust
msgid ""
"_: certmgr.editsslcert.dotrust\n"
"Trust the authenticity of this certificate."
msgstr ""
"Themba ukunyaniseka kwesi satifikethi."

#: certmgr.editsslcert.donttrust
msgid ""
"_: certmgr.editsslcert.donttrust\n"
"Do not trust the authenticity of this certificate."
msgstr ""
"Musa ukukuthemba ukunyaniseka kwesi satifikethi."

#: certmgr.editemailcert.title
msgid "Edit email certificate trust settings"
msgstr "Hlela imimiselo yentembeko yesatifikethi semeyile"

#: certmgr.editemailcert.edittrust
msgid ""
"_: certmgr.editemailcert.edittrust\n"
"Edit certificate trust settings:"
msgstr ""
"Hlela imimiselo yentembeko yesatifikethi:"

#: certmgr.editemailcert.dotrust
msgid ""
"_: certmgr.editemailcert.dotrust\n"
"Trust the authenticity of this certificate."
msgstr ""
"Themba ukunyaniseka kwesi satifikethi."

#: certmgr.editemailcert.donttrust
msgid ""
"_: certmgr.editemailcert.donttrust\n"
"Do not trust the authenticity of this certificate."
msgstr ""
"Musa ukukuthemba ukunyaniseka kwesi satifikethi."

#: certmgr.deletecert.title
msgid "Delete Certificate"
msgstr "Cima Isatifikethi"

#: certmgr.deletecert.beforename
msgid "You have requested to delete this certificate:"
msgstr "Ucele ukucima esi satifikethi:"

#: certmgr.deletecert.aftername
msgid "Are you sure you want to delete this certificate?"
msgstr "Uqinisekile ufuna ukucima esi satifikethi?"

#: certmgr.deleteusercert.title
msgid "Delete Your Certificate"
msgstr "Cima Isatifikethi Sakho"

#: certmgr.deleteusercert.beforename
msgid "You have requested to delete the certificate:"
msgstr "Ucele ukucima isatifikethi:"

#: certmgr.deleteusercert.aftername
msgid "Once you have deleted this certificate, you will not be able to read mail that has been encrypted with it."
msgstr "Wakuba ucime esi satifikethi, akusayi kukwazi ukufunda imeyile enoguqulelo oluntsonkothileyo ehamba nayo."

#: certmgr.certname
msgid "Certificate Name"
msgstr "Igama Lesatifikethi"

#: certmgr.tokenname
msgid "Security Device"
msgstr "Isixhobo Sokhuseleko"

#: certmgr.purpose
msgid "Purposes"
msgstr "Iinjongo"

#: certmgr.issued
msgid "Issued On"
msgstr "Inikelwe Ngeli Xesha"

#: certmgr.expires
msgid "Expires On"
msgstr "Iphelelwa Ngeli Xesha"

#: certmgr.email
msgid "E-Mail Address"
msgstr "Idilesi Yemeyile"

#: certmgr.serial
msgid ""
"_: certmgr.serial\n"
"Serial Number"
msgstr ""
"Inombolo Yolandelelwano"

#: certmgr.ocsp_info
msgid "Certificates have not been validated with OCSP. Click View to do so."
msgstr "Izatifikethi azikanyanisekiwa nge-OCSP. Nqomfa ku-Jonga ukuze wenze njalo."

#: certmgr.close.label
msgid "Close"
msgstr "Vala"

#: certmgr.view.label
msgid "View"
msgstr "Jonga"

#: certmgr.edit.label
msgid "Edit"
msgstr "Hlela"

#: certmgr.editca.label
msgid "Edit CA Trust"
msgstr "Hlela Intembeko ye-CA"

#: certmgr.add.label
msgid "Add"
msgstr "Fakela"

#: certmgr.delete.label
msgid "Delete"
msgstr "Cima"

#: certmgr.backup.label
msgid "Backup"
msgstr "Gcina ikopi"

#: certmgr.backupall.label
msgid "Backup All"
msgstr "Gcina Iikopi Zako Konke"

#: certmgr.restore.label
msgid "Import"
msgstr "Thatha ngaphandle"

#: certmgr.details.label
msgid "Certificate Fields"
msgstr "Imimandla Yezifundo Zezatifikethi"

#: certmgr.fields.label
msgid "Field Value"
msgstr "Ixabiso Lommandla Wezifundo"

#: certmgr.hierarchy.label
msgid "Certificate Hierarchy"
msgstr "Amabakala Ezatifikethi"

