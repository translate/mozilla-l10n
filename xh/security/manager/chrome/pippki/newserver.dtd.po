# <!-- Values for newserver.xul -->
#: newserver.title
msgid "Website Certified by an Unknown Authority"
msgstr "Uqinisekiso Lwenxuwa le web Engumntu Umgunyazisi Ongaziwayo"

#: newserver.reasons
msgid "Possible reasons for this error:"
msgstr "Izizathu ezinokubakho zale mposiso:"

#: newserver.reason1
msgid ""
"- Your browser does not recognize the Certificate Authority that issued the "
"site's certificate."
msgstr ""
"- Umkhangeli zincwadi wakho akalqondi Isiqinisekiso Sogunyaziso esinikezele "
"isiqinisekiso senxuwa."

#: newserver.reason2
msgid ""
"- The site's certificate is incomplete due to a server misconfiguration."
msgstr ""
"- Isiqinisekiso senxuwa asigqityiwanga ngenxa yoqwalaselo kakubi lomncedisi."

#: newserver.notify
msgid "Please notify the site's webmaster about this problem."
msgstr "Nceda qwalasela eli nxuwa lomphathi we web malunga nale ngxaki."

#: newserver.examine
msgid "Examine Certificate..."
msgstr "Vavanya Isiqinisekisi..."

#: newserver.remember
msgid "Accept this certificate permanently"
msgstr "Yamkela esi siqinisekiso unomphelo"

#: newserver.session
msgid "Accept this certificate temporarily for this session"
msgstr "Yamkela esi siqinisekiso sexeshana salo mhlangano"

#: newserver.refuse
msgid "Do not accept this certificate and do not connect to this Web site"
msgstr ""

