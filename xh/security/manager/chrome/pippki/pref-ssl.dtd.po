msgstr "I-SSL3/TLS eyongezelelweyo"

# SSL2 Ciphers
#: cipher.ssl2.rc4_128
msgid ""
"_: cipher.ssl2.rc4_128\n"
"128-bit RC4 encryption with RSA and an MD5 MAC"
msgstr ""
"_: cipher.ssl2.rc4_128\n"
"I-128-bit RC4 yoguqulelo oluntsonkothileyo ne-RSA kunye ne-MD5 MAC"

#: cipher.ssl2.rc2_128
msgid "128-bit RC2 encryption with RSA and an MD5 MAC"
msgstr "I-128-bit RC2 yoguqulelo oluntsonkothileyo ne-RSA kunye ne-MD5 MAC"

#: cipher.ssl2.des_ede3_192
msgid "168-bit Triple DES encryption with RSA and MD5 MAC "
msgstr "I-168-bit yoguqulelo oluntsonkothileyo ye-DES ephindaphindwe kathathu ne-RSA kunye ne-MD5 MAC "

#: cipher.ssl2.des_64
msgid "56-bit DES encryption with RSA and an MD5 MAC"
msgstr "I-56-bit yoguqulelo oluntsonkothileyo ye-DES ne-RSA kunye ne-MD5 MAC"

#: cipher.ssl2.rc4_40
msgid ""
"_: cipher.ssl2.rc4_40\n"
"40-bit RC4 encryption with RSA and an MD5 MAC (export)"
msgstr ""
"_: cipher.ssl2.rc4_40\n"
"I-40-bit yoguqulelo oluntshonkothileyo lwe-RC4 ne-RSA kunye ne-MD5 MAC (ethunyelwa ngaphandle)"

#: cipher.ssl2.rc2_40
msgid ""
"_: cipher.ssl2.rc2_40\n"
"40-bit RC2 encryption with RSA and an MD5 MAC (export)"
msgstr ""
"_: cipher.ssl2.rc2_40\n"
"I-40-bit yoguqulelo oluntsonkothileyo lwe-RC2 ne-RSA akunye ne-MD5 MAC (ethunyelwa ngaphandle)"

# SSL3/TLS ciphers
#: cipher.ssl3.rsa_rc4_128_md5
msgid ""
"_: cipher.ssl3.rsa_rc4_128_md5\n"
"128-bit RC4 encryption with RSA and an MD5 MAC"
msgstr ""
"_: cipher.ssl3.rsa_rc4_128_md5\n"
"I-128-bit yoguqulelo oluntsonkothileyo lwe-RC4 ne-RSA kunye ne-MD5 MAC"

#: cipher.ssl3.rsa_rc4_128_sha
msgid "128-bit RC4 encryption with RSA and a SHA1 MAC"
msgstr "I-128-bit yoguqulelo oluntsonkothileyo lwe-RC4 ne-RSA kunye ne-SHA1 MAC"

#: cipher.ssl3.rsa_fips_des_ede3_sha
msgid "168-bit Triple DES with RSA and a SHA1 MAC (FIPS)"
msgstr "I-168-bit ye-DES Ephindaphindwe kathathu ne-RSA kunye ne-SHA1 MAC (FIPS)"

#: cipher.ssl3.rsa_des_ede3_sha
msgid "168-bit Triple DES with RSA and a SHA1 MAC"
msgstr "I-168-bit yeDES Ephindaphindwe kathathu ne-RSA kunye ne-SHA1 MAC"

#: cipher.ssl3.rsa_fips_des_sha
msgid "56-bit DES encryption with RSA and a SHA1 MAC (FIPS)"
msgstr "I-56-bit yoguqulelo oluntsonkothileyo lwe-DES ne-RSA kunye ne-SHA1 MAC (FIPS)"

#: cipher.ssl3.rsa_des_sha
msgid "56-bit DES encryption with RSA and a SHA1 MAC"
msgstr "I-56-bit yoguqulelo oluntsonkothileyo lwe-DES ne-RSA kunye ne-SHA1 MAC"

#: cipher.ssl3.rsa_1024_rc4_56_sha
msgid "56-bit RC4 encryption with RSA and a SHA1 MAC (export)"
msgstr "I-56-bit yoguqulelo oluntsonkothileyo lwe-RC4 ne-RSA kunye ne-SHA1 MAC (ethunyelwa ngaphandle)"

#: cipher.ssl3.rsa_1024_des_cbc_sha
msgid "56-bit DES encryption with RSA and a SHA1 MAC (export)"
msgstr "I-56-bit yoguqulelo oluntsonkothileyo lwe-DES ne-RSA kunye ne-SHA1 MAC (ethunyelwa ngaphandle)"

#: cipher.ssl3.rsa_rc4_40_md5
msgid ""
"_: cipher.ssl3.rsa_rc4_40_md5\n"
"40-bit RC4 encryption with RSA and an MD5 MAC (export)"
msgstr ""
"_: cipher.ssl3.rsa_rc4_40_md5\n"
"I-40-bit yoguqulelo oluntsonkothileyo lwe-RC4 ne-RSA kunye ne-MD5 MAC (ethunyelwa ngaphandle)"

#: cipher.ssl3.rsa_rc2_40_md5
msgid ""
"_: cipher.ssl3.rsa_rc2_40_md5\n"
"40-bit RC2 encryption with RSA and an MD5 MAC (export)"
msgstr ""
"_: cipher.ssl3.rsa_rc2_40_md5\n"
"I-40-bit yoguqulelo oluntsonkothileyo lwe-RC2  ne-RSA kunye ne-MD5 MAC (ethunyelwa ngaphandle)"

# Extra SSL3/TLS ciphers
#: cipher.ssl3.dhe_rsa_aes_256_sha
msgid "256-bit AES encryption with RSA, DHE, and a SHA1 MAC"
msgstr "I-256-bit yoguqulelo oluntsonkothileyo lwe-AES ne-RSA, DHE, kunye ne-SHA1 MAC"

#: cipher.ssl3.dhe_dss_aes_256_sha
msgid "256-bit AES encryption with DSA, DHE, and a SHA1 MAC"
msgstr "I-256-bit yoguqulelo oluntsonkothileyo lwe-AES ne-DSA, DHE, kunye ne-SHA1 MAC"

#: cipher.ssl3.rsa_aes_256_sha
msgid "256-bit AES encryption with RSA and a SHA1 MAC"
msgstr "I-256-bit yoguqulelo oluntsonkothileyo lwe-AES ne-RSA kunye ne-SHA1 MAC"

#: cipher.ssl3.dhe_rsa_aes_128_sha
msgid "128-bit AES encryption with RSA, DHE, and a SHA1 MAC"
msgstr "I-128-bit yoguqulelo lwe-AES ne-RSA, DHE, kunye ne-SHA1 MAC"

#: cipher.ssl3.dhe_dss_aes_128_sha
msgid "128-bit AES encryption with DSA, DHE, and a SHA1 MAC"
msgstr "I-128-bit yoguqulelo oluntsonkothileyo lwe-AES ne-DSA, DHE, kunye ne-A1 MAC"

#: cipher.ssl3.rsa_aes_128_sha
msgid "128-bit AES encryption with RSA and a SHA1 MAC"
msgstr "I-128-bit yoguqulelo oluntsonkothileyo lwe-AES ne-RSA kunye ne-SHA1 MAC"

#: cipher.ssl3.dhe_rsa_des_ede3_sha
msgid "168-bit Triple DES with RSA, DHE, and a SHA1 MAC"
msgstr "I-168-bit yeDES Ephindaphindwe kathathu ne-RSA, DHE, kunye ne-SHA1 MAC"

#: cipher.ssl3.dhe_dss_des_ede3_sha
msgid "168-bit Triple DES with DSA, DHE, and a SHA1 MAC"
msgstr "I-168-bit ye-DES Ephindaphindwe Kathathu ne-DSA, DHE, kunye ne-SHA1 MAC"

#: cipher.ssl3.dhe_rsa_des_sha
msgid "56-bit DES encryption with RSA, DHE, and a SHA1 MAC"
msgstr "I-56-bit yoguqulelo oluntsonkothileyo lwe-DES ne-RSA, DHE, kunye ne-SHA1 MAC"

#: cipher.ssl3.dhe_dss_des_sha
msgid "56-bit DES encryption with DSA, DHE, and a SHA1 MAC"
msgstr "I-56-bit yoguqulelo oluntsonkothileyo lwe-DES ne-DSA, DHE, kunye ne-SHA1 MAC"

#: cipher.ssl3.rsa_null_sha
msgid "No encryption with RSA authentication and a SHA1 MAC"
msgstr "Akukho nguqulelo intsonkothileyo nokunyaniseka kwe-RSA kunye ne-SHA1 MAC"

#: cipher.ssl3.rsa_null_md5
msgid "No encryption with RSA authentication and an MD5 MAC"
msgstr "Akukho nguqulelo intsonkothileyo nokunyaniseka kwe-RSA kunye ne-MD5MAC"

