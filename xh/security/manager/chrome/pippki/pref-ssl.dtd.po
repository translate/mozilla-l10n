#: SSLProtocolVersions
msgid "SSL Protocol Versions"
msgstr "Iinguqulelo Zomthetho Olandelwayo we-SSL"

#: SSLWarnings
msgid "SSL Warnings"
msgstr "Izilumkiso ze-SSL"

#: SSLClientAuthMethod
msgid "Client Certificate Selection"
msgstr "Ukhetho Lwesiqinisekiso Somxhasi"

#: prefheader.title
msgid "Secure Sockets Layer (SSL)"
msgstr "Khusela Ulwaleko Lwemingxuma (SSL)"

#: enable.ssl2
msgid "Enable SSL version 2"
msgstr "Yenza inguqulelo yesi-2 ye-SSL isebenze"

#: enable.ssl3
msgid "Enable SSL version 3"
msgstr "Yenza inguqulelo yesi-3 ye-SSL isebenze"

#: enable.tls
msgid "Enable TLS"
msgstr "Yenza i-TLS isebenze"

#: edit.sslciphers
msgid "Edit Ciphers..."
msgstr "Hlela i-Ciphers..."

#: edit.sslciphers.accesskey
msgid "E"
msgstr "E"

#: warn.description
msgid ""
"&brandShortName; can alert you to the security status of the web page you "
"are viewing. Set &brandShortName; to show a warning and ask permission "
"before:"
msgstr ""
"&brandShortName; ingakulumkisa ngemo yokhuseleko yephephe leweb "
"olijongisisayo. Faka i-&brandShortName; ukubonisa isilumkiso futhi ucele "
"imvume kwangaphambili:"

#: warn.enteringsecure
msgid "Loading a page that supports encryption"
msgstr "Ilayisha iphepha elixhasa imfihlo"

#: warn.enteringweak
msgid "Loading a page that uses low-grade encryption"
msgstr "Ilayisha iphepha elisebenzisa ufihlo olukumgangatho osezantsi"

#: warn.insecurepost
msgid "Sending form data from an unencrypted page to an unencrypted page"
msgstr ""
"Ithumela iinkcukacha zefomu ukusuka kwiphepha elifihliweyo ukuya kwiphepha "
"elingafihlwanga"

#: warn.leavingsecure
msgid "Leaving a page that supports encryption"
msgstr "Ishiya iphepha elixhasa ufihlo"

#: warn.secureredirect
msgid "Redirecting from one encrypted page to another"
msgstr "Isusa kwelinye iphepha elifihliweyo ibhekisa kwelinye"

#: warn.secureredirecttoinsecure
msgid "Redirecting from an encrypted page to an unencrypted page"
msgstr "Isusa kwiphepha elifihliweyo isisa kwelingafihlwanga"

#: warn.viewmixed
msgid "Viewing a page with an encrypted/unencrypted mix"
msgstr "Ijonga iphepha elinomxube ofihliweyo/ongafihlwanga"

#: certselect.description
msgid ""
"Decide how &brandShortName; selects a security certificate to present to web "
"sites that require one:"
msgstr ""
"Gqiba ngendlela i- &brandShortName; ekhetha ngayo isiqinisekiso sokhuseleko "
"ukuya kumanxuwa asifunayo:"

#: certselect.auto
msgid "Select Automatically"
msgstr "Khetha Ngokuzenzekelayo"

#: certselect.ask
msgid "Ask Every Time"
msgstr "Buza Ngalolonke Ixesha"

# <!-- Cipher pref window -->
#: cipher.title
msgid "SSL: Edit Ciphers"
msgstr "SSL: Hlela iCiphers"

#: cipher.ssl2.label
msgid "SSL2 Cipher Suites"
msgstr "SSL2 Cipher ezihambelanayo"

#: cipher.ssl3.label
msgid "SSL3/TLS Cipher Suites"
msgstr "SSL3/TLS Cipher ezihambelanayo"

#: cipher.ssl3_extra.label
msgid "Extra SSL3/TLS Cipher Suites"
msgstr "SSL3/TLS Cipher Suites Ezongezelelweyo"

#: cipher.tab.ssl2
msgid "SSL2"
msgstr "SSL2"

#: cipher.tab.ssl3tls
msgid "SSL3/TLS"
msgstr "SSL3/TLS"

#: cipher.tab.ssl3tls_extra
msgid "Extra SSL3/TLS"
msgstr "SSL3/TLS Ezongezelelweyo"

# SSL2 Ciphers
#: cipher.ssl2.rc4_128
msgid ""
"_: cipher.ssl2.rc4_128\n"
"128-bit RC4 encryption with RSA and an MD5 MAC"
msgstr ""

#: cipher.ssl2.rc2_128
msgid "128-bit RC2 encryption with RSA and an MD5 MAC"
msgstr "Ufihlo lwe 128-bit RC2 olune RSA kunye ne MD5 MAC"

#: cipher.ssl2.des_ede3_192
msgid "168-bit Triple DES encryption with RSA and MD5 MAC "
msgstr "ufihlo lwe 168-bit DES ephindwe kathathu olune RSA ne MD5 MAC "

#: cipher.ssl2.des_64
msgid "56-bit DES encryption with RSA and an MD5 MAC"
msgstr "ufihlo lwe 56-bit DES olune RSA kunye ne MD5 MAC"

#: cipher.ssl2.rc4_40
msgid ""
"_: cipher.ssl2.rc4_40\n"
"40-bit RC4 encryption with RSA and an MD5 MAC (export)"
msgstr ""

#: cipher.ssl2.rc2_40
msgid ""
"_: cipher.ssl2.rc2_40\n"
"40-bit RC2 encryption with RSA and an MD5 MAC (export)"
msgstr ""

# SSL3/TLS ciphers
#: cipher.ssl3.rsa_rc4_128_md5
msgid ""
"_: cipher.ssl3.rsa_rc4_128_md5\n"
"128-bit RC4 encryption with RSA and an MD5 MAC"
msgstr ""

#: cipher.ssl3.rsa_rc4_128_sha
msgid "128-bit RC4 encryption with RSA and a SHA1 MAC"
msgstr "Ufihlo lwe 128-bit RC4 olune RSA kunye ne SHA1 MAC"

#: cipher.ssl3.rsa_fips_des_ede3_sha
msgid "168-bit Triple DES with RSA and a SHA1 MAC (FIPS)"
msgstr "168-bit DES ephindwe kathathu ene RSA kunye ne SHA1 MAC (FIPS)"

#: cipher.ssl3.rsa_des_ede3_sha
msgid "168-bit Triple DES with RSA and a SHA1 MAC"
msgstr ""
"Ufihlo lwe 168-BIT DES oluphinda-phindwe kathathu olune RSA kunye ne SHA-1 "
"MAC"

#: cipher.ssl3.rsa_fips_des_sha
msgid "56-bit DES encryption with RSA and a SHA1 MAC (FIPS)"
msgstr "Ufuhlo lwe 56-bit DES olune RSA kunye ne SHA-1 MAC(FIPS)"

#: cipher.ssl3.rsa_des_sha
msgid "56-bit DES encryption with RSA and a SHA1 MAC"
msgstr "Ufuhlo lwe 56-bit DES olune RSA kunye neSHA-1 MAC"

#: cipher.ssl3.rsa_1024_rc4_56_sha
msgid "56-bit RC4 encryption with RSA and a SHA1 MAC (export)"
msgstr ""
"Ufihlo lwe 56-bit RC4 olune RSA kunye neSHA-1 MAC (urhwebo lwangaphandle)"

#: cipher.ssl3.rsa_1024_des_cbc_sha
msgid "56-bit DES encryption with RSA and a SHA1 MAC (export)"
msgstr ""
"Ufuhlo lwe 56-bit DES olune RSA kunye neSHA-1 MAC(urhwebo lwangaphandle)"

#: cipher.ssl3.rsa_rc4_40_md5
msgid ""
"_: cipher.ssl3.rsa_rc4_40_md5\n"
"40-bit RC4 encryption with RSA and an MD5 MAC (export)"
msgstr ""

#: cipher.ssl3.rsa_rc2_40_md5
msgid ""
"_: cipher.ssl3.rsa_rc2_40_md5\n"
"40-bit RC2 encryption with RSA and an MD5 MAC (export)"
msgstr ""

#: cipher.ssl3.dhe_rsa_aes_256_sha
msgid "256-bit AES encryption with RSA, DHE, and a SHA1 MAC"
msgstr "Ufuhlo lwe 256-bit DES olune RSA kunye neSHA1 MAC"

#: cipher.ssl3.dhe_dss_aes_256_sha
msgid "256-bit AES encryption with DSA, DHE, and a SHA1 MAC"
msgstr "Ufuhlo lwe 256-bit AES olune DSA, DHE, kunye neSHA-1 MAC"

#: cipher.ssl3.rsa_aes_256_sha
msgid "256-bit AES encryption with RSA and a SHA1 MAC"
msgstr "Ufuhlo lwe 256-bit AES olune RSA kunye ne SHA-1 MAC"

# _ numbers: checks whether numbers of various forms are consistent between the two strings
#: cipher.ssl3.dhe_rsa_aes_128_sha
msgid "128-bit AES encryption with RSA, DHE, and a SHA1 MAC"
msgstr "Ufuhlo lwe 128-bit AES olune RSA, DHE, kunye ne SHA1 MAC"

#: cipher.ssl3.dhe_dss_aes_128_sha
msgid "128-bit AES encryption with DSA, DHE, and a SHA1 MAC"
msgstr "Ufuhlo lwe 128 AES olune DSA, DHE, kunye ne SHA-1 MAC"

#: cipher.ssl3.rsa_aes_128_sha
msgid "128-bit AES encryption with RSA and a SHA1 MAC"
msgstr "Ufuhlo lwe 128-bit AES olune RSA kunye neSHA-1 MAC"

#: cipher.ssl3.dhe_rsa_des_ede3_sha
msgid "168-bit Triple DES with RSA, DHE, and a SHA1 MAC"
msgstr ""
"Ufihlo lwe 168-bit DES ephindwe kathathu olune RSA, DHE, kunye ne SHA-1 MAC"

#: cipher.ssl3.dhe_dss_des_ede3_sha
msgid "168-bit Triple DES with DSA, DHE, and a SHA1 MAC"
msgstr ""
"Ufihlo lwe 168-bit DES ephindwe kathathu olune DSA, DHE, kunye ne SHA-1 MAC"

#: cipher.ssl3.dhe_rsa_des_sha
msgid "56-bit DES encryption with RSA, DHE, and a SHA1 MAC"
msgstr "Ufuhlo lwe 56-bit DES olune RSA, DHE, kunye ne SHA-1 MAC"

#: cipher.ssl3.dhe_dss_des_sha
msgid "56-bit DES encryption with DSA, DHE, and a SHA1 MAC"
msgstr "Ufuhlo lwe 56-bit DES olune DSA, DHE, kunye ne SHA-1 MAC"

#: cipher.ssl3.rsa_null_sha
msgid "No encryption with RSA authentication and a SHA1 MAC"
msgstr "Akukho fihlo kwi RSA enyanisekileyo kunye ne SHA-1 MAC"

#: cipher.ssl3.rsa_null_md5
msgid "No encryption with RSA authentication and an MD5 MAC"
msgstr "Akukho fihlo kwi RSA enyanisekileyo kunye ne MD5 MAC"

