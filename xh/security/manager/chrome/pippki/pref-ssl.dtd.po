#: SSLProtocolVersions
msgid "SSL Protocol Versions"
msgstr "Iintlobo Zoguqulelo Zenkqubo ye-SSL"

#: SSLWarnings
msgid "SSL Warnings"
msgstr "Izilumkiso ze-SSL"

#: SSLClientAuthMethod
msgid "Client Certificate Selection"
msgstr "Okukhethiweyo Kwesatifikethi Somxumi"

#: prefheader.title
msgid "Secure Sockets Layer (SSL)"
msgstr "Umaleko Wesokethi Yokhuseleko (SSL)"

#: enable.ssl2
msgid "Enable SSL version 2"
msgstr "Nika isakhono kuguqulelo lwesi-2 lwe-SSL"

#: enable.ssl3
msgid "Enable SSL version 3"
msgstr "Nika isakhono kuguqulelo lwesi-3 lwe-SSL"

#: enable.tls
msgid "Enable TLS"
msgstr "Nika isakhono kwi-TLS"

#: edit.sslciphers
msgid "Edit Ciphers..."
msgstr "Hlela ii-ciphers..."

#: edit.sslciphers.accesskey
msgid "E"
msgstr "E"

#: warn.description
msgid "&brandShortName; can alert you to the security status of the web page you are viewing. Set &brandShortName; to show a warning and ask permission before:"
msgstr "I-&brandShortName; inako ukukulumkisa ukuba ube kwisimo sokhuseleko sekhasi lothungelwano olijongileyo. Misela i-&brandShortName; ukuba ibonise isilumkiso kwaye icele imvume kwangaphambili:"

#: warn.enteringsecure
msgid "Loading a page that supports encryption"
msgstr "Kulayishwa ikhasi elixhasa uguqulelo oluntsonkothileyo"

#: warn.enteringweak
msgid "Loading a page that uses low-grade encryption"
msgstr "Kulayishwa ikhasi elisebenzisa uguqulelo oluntsonkothileyo lwenqanaba elisezantsi"

#: warn.insecurepost
msgid "Sending form data from an unencrypted page to an unencrypted page"
msgstr "Kuthunyelwa iinkcukacha ezibunjiweyo ukusuka kwikhasi elinoguqulelo olungantsonkothanga ukuya kwikhasi elinoguqulelo olungantsonkothanga"

#: warn.leavingsecure
msgid "Leaving a page that supports encryption"
msgstr "Kushiywa ikhasi elixhasa uguqulelo oluntsonkothileyo"

#: warn.secureredirect
msgid "Redirecting from one encrypted page to another"
msgstr "Kuguqulwa intsingiselo ukusuka kwikhasi elinye elinoguqulelo oluntsonkothileyo ukuya kwelinye"

#: warn.secureredirecttoinsecure
msgid "Redirecting from an encrypted page to an unencrypted page"
msgstr "Kuguqulwa intsingiselo ukusuka kwikhasi elinoguqulelo oluntsonkothileyo ukuya kwikhasi elinoguqulelo olungantsonkothanga"

#: warn.viewmixed
msgid "Viewing a page with an encrypted/unencrypted mix"
msgstr "Kujongwa ikhasi linomxube ontsonkothileyo/ongantsonkothanga"

#: certselect.description
msgid "Decide how &brandShortName; selects a security certificate to present to web sites that require one:"
msgstr "Yenza isigqibo ngendlela i-&brandShortName; ekhetha ngayo isatifikethi sokhuseleko ukubonisa iziza zothungelwano ezifuna esinye:"

#: certselect.auto
msgid "Select Automatically"
msgstr "Kukhethwa Ngokuzenzekela"

#: certselect.ask
msgid "Ask Every Time"
msgstr "Buza Ngamaxesha Onke"

# Cipher pref window
#: cipher.title
msgid "SSL: Edit Ciphers"
msgstr "I-SSL: Ihlela ii-Ciphers"

#: cipher.ssl2.label
msgid "SSL2 Cipher Suites"
msgstr "I-SSL2 Cipher Suites"

#: cipher.ssl3.label
msgid "SSL3/TLS Cipher Suites"
msgstr "I-SSL3/TLS Cipher Suites"

#: cipher.ssl3_extra.label
msgid "Extra SSL3/TLS Cipher Suites"
msgstr "I-SSL3/TLS Cipher Suites Eyongezelelweyo"

