# Window title
#: windowTitle.label
msgid "Publish Page"
msgstr "Papasha Ikhasi"

#: windowTitleSettings.label
msgid "Publish Settings"
msgstr "Papasha Imimiselo"

#: publishTab.label
msgid ""
"_: publishTab.label\n"
"Publish"
msgstr ""
"Papasha"

#: settingsTab.label
msgid "Settings"
msgstr "Imimiselo"

#: publishButton.label
msgid ""
"_: publishButton.label\n"
"Publish"
msgstr ""
"Papasha"

# Publish Tab Panel
#: siteList.label
msgid ""
"_: siteList.label\n"
"Site Name:"
msgstr ""
"Igama Lesiza:"

#: siteList.tooltip
msgid "Choose the site that you want to publish to"
msgstr "Khetha isiza osifunela ukupapasha"

#: newSiteButton.label
msgid "New Site"
msgstr "Isiza Esitsha"

#: siteDirList.label
msgid "Select or type a directory name in your publishing site:"
msgstr "Khetha okanye chwetheza igama likavimba weefayili kwisiza sakho sokupapasha:"

#: siteDirList.tooltip
msgid "Choose or enter the name of the remote subdirectory to publish to"
msgstr "Khetha okanye ngenisa igama likavimba weefayili owonganyelweyo nokude opapashela kuye"

#: docDirList.label
msgid "Site subdirectory for this page:"
msgstr "Isiza sikavimba weefayili weli khasi:"

#: publishImgCheckbox.label
msgid "Include images and other files"
msgstr "Bandakanya imifuziselo nezinye iifayili"

#: publishImgCheckbox.tooltip
msgid "Publish images and other files referenced by this page"
msgstr "Papasha imifuziselo nezinye iifayili ezalathwe leli khasi"

#: sameLocationRadio.label
msgid "Use same location as page"
msgstr "Sebenzisa kwayona le ndawo njengekhasi"

#: sameLocationRadio.tooltip
msgid "Publish files to the same location as the page"
msgstr "Papasha iifayili kuyo le ndawo njengekhasi"

#: useSubdirRadio.label
msgid "Use this site subdirectory:"
msgstr "Sebenzisa esi siza sikavimba weefayili owonganyelweyo:"

#: useSubdir.tooltip
msgid "Choose or enter name of remote subdirectory where files will be published"
msgstr "Khetha okanye ngenisa igama likavimba weefayili owonganyelweyo nelikude apho iifayili zipapashwa khona"

#: chooseDirButton.label
msgid "Choose..."
msgstr "Khetha..."

#: pageTitle.label
msgid "Page Title:"
msgstr "Isihloko Sekhasi:"

#: pageTitle.tooltip
msgid "Enter a title to identify the page in the window and in bookmarks"
msgstr "Ngenisa isihloko ukuze lichongeke ikhasi kwifestile nakwizalathisi eziphawulayo"

#: pageTitleExample.label
msgid "e.g.: &quot;My Web Page&quot;"
msgstr "umzk.: &quot;Ikhasi Lam Lothungelwano&quot;"

#: filename.label
msgid "Filename:"
msgstr "Igama lefayili:"

#: filename.tooltip
msgid "Enter a name for this file, including '.html' for a web page"
msgstr "Ngenisa igama lale fayili, kubandakanywa i-'.html' ukwenzela ikhasi lothungelwano"

#: filenameExample.label
msgid "e.g,: &quot;mypage.html&quot;"
msgstr "umzk.: &quot;ikhasi-lam.html&quot;"

#: relatedDocs.label
msgid "Other files to publish"
msgstr "Ezinye iifayili zokupapasha"

#: setDefaultButton.label
msgid "Set as Default"
msgstr "Misela Njengokuhlala Kukho"

#: setDefSiteAndDir.label
msgid "Set as Default Site and Directory"
msgstr "Misela Njengesiza Sokuhlala Kukho Novimba Weefayili"

#: removeButton.label
msgid "Remove Site"
msgstr "Susa Isiza"

# Settings Tab Panel
#: publishSites.label
msgid "Publishing Sites"
msgstr "Iziza Zokupapasha"

#: serverInfo.label
msgid "Server Information"
msgstr "Ulwazi Ngeseva"

#: loginInfo.label
msgid "Login Information"
msgstr "Ulwazi Ngokungena"

#: siteName.label
msgid ""
"_: siteName.label\n"
"Site Name:"
msgstr ""
"Igama Lesiza:"

#: siteName.tooltip
msgid "A nickname that identifies this publishing site (e.g.: 'MySite')"
msgstr "Igama lesiqhulo eliphawula esi siza sokupapasha (umzk.: 'Isiza Sam')"

#: siteUrl.label
msgid "Publishing address (e.g.: 'ftp://ftp.myisp.com/myusername'):"
msgstr "Idilesi yokupapasha (umzk.: 'ftp://ftp.myisp.com/igama_lam_lomsebenzisi'):"

#: siteUrl.tooltip
msgid "The FTP:// or HTTP:// address provided by your ISP or web hosting service"
msgstr "Idilesi ye-FTP:// okanye i-HTTP:// enikwe yi-ISP yakho okanye inkonzo yokwamkela kuthungelwano"

#: browseUrl.label
msgid "HTTP address of your homepage (e.g.: 'http://www.myisp.com/myusername'):"
msgstr "Idilesi ye-HTTP yekhasi lasekhaya lakho (umzk.: 'http://www.myisp.com/igama_lam_lomsebenzisi'):"

#: browseUrl.tooltip
msgid "The HTTP:// address of your home directory (don't include filename)"
msgstr "Idilesi ye-HTTP:// kavimba weefayili zakho zasekhaya (ungalibandakanyi igama lefayili)"

#: username.label
msgid "User name:"
msgstr "Igama lomsebenzisi:"

#: username.tooltip
msgid "The user name you use to log in to your ISP or web hosting service"
msgstr "Igama lomsebenzisi olisebenzisela ukungena kwi-ISP yakho okanye kwinkonzo eyamkela uthungelwano"

#: password.label
msgid "Password:"
msgstr "Igama lokugqithisa:"

#: password.tooltip
msgid "The password associated with your user name"
msgstr "Igama lokuthisa elinxulumene negama lakho lomsebenzisi"

#: savePassword.label
msgid "Save Password"
msgstr "Gcina Igama Lokugqithisa"

#: savePassword.tooltip
msgid "Select this to save your password securely using Password Manager"
msgstr "Khetha oku ukuze ugcine igama lakho lokugqithisa ngokhuseleko ngokusebenzisa Umphathi Wegama Lokugqithisa"

