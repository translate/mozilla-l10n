# Window title
#: windowTitle.label
msgid "Page Colors and Background"
msgstr "Imibala Yamakhasi Nokungasemva Eskrinini"

#: pageColors.label
msgid "Page Colors"
msgstr "Imibala Yamakhasi"

#: defaultColorsRadio.label
#: defaultColorsRadio.accessKey
msgid "Reader's default colors (&Don't set colors in page)"
msgstr "Imibala ehlala ikho yomfundi (Musa ukumisela imibala ekhasini)"

#: defaultColorsRadio.tooltip
msgid "Use the color settings from the viewer (reader's) browser only"
msgstr "Sebenzisa imimiselo evela kwisikhangeli sojongileyo (umfundi) kuphela"

#: customColorsRadio.label
#: customColorsRadio.accessKey
msgid "Use &custom colors:"
msgstr "Sebenzisa imibala &yokulungiselelwa:"

#: customColorsRadio.tooltip
msgid "These color settings override the viewer's browser settings"
msgstr "Le mimiselo yemibala iyayogqitha imimiselo yesikhangeli yomjongi"

#: normalText.label
#: normalText.accessKey
msgid "&Normal text"
msgstr "&Isiqendu Esiqhelekileyo"

#: linkText.label
#: linkText.accessKey
msgid "&Link text"
msgstr "&Nxulumanisa isiqendu"

#: activeLinkText.label
#: activeLinkText.accessKey
msgid "&Active link text"
msgstr "&Isiqendu sonxulumaniso esisebenzayo"

#: visitedLinkText.label
#: visitedLinkText.accessKey
msgid "&Visited link text"
msgstr "&Isiqendu sonxulumaniso esityelelweyo"

#: background.label
#: background.accessKey
msgid "&Background:"
msgstr "&Okungasemva eskrinini:"

#: colon.character
msgid ":"
msgstr ":"

