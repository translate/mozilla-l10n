#: lHeader
msgid "Composer"
msgstr "Umhlanganiseli"

#: rHeader
msgid "Settings for authoring of new Web pages"
msgstr "Imimiselo yokuba ngababhali  bamakhasi amatsha Othungelwano"

#: saving
msgid "Saving"
msgstr "Kuyagcinwa"

#: AutoSaveCheck
msgid "Automatically save every"
msgstr "Gcina ngokuzenzekela nganye nganye"

#: minText
msgid "minutes"
msgstr "imizuzu"

#: exterLegend.label
msgid "External Editors"
msgstr "Abahleli Bangaphandle"

#: htmlSource
msgid "HTML Source:"
msgstr "Umthombo we-HTML:"

#: imageeditor
msgid "Images:"
msgstr "Imifuziselo:"

#: chooseButton.label
msgid "Choose"
msgstr "Khetha"

#: recentFiles.title
msgid "Recent Pages Menu"
msgstr "Imenyu Yamakhasi Akutshanje"

#: documentsInMenu.label
#: documentsInMenu.accesskey
msgid "Maximum &number of pages listed:"
msgstr "Elona&nani liphezulu lamakhasi adwelisiweyo:"

#: savingFiles.title
msgid "When Saving or Publishing Pages"
msgstr "Xa Kugcinwa okanye Kupapashwa Amakhasi"

#: preserveExisting.label
#: preserveExisting.accesskey
msgid "&Preserve original source formatting"
msgstr "&Londoloza ulungiselelo olungumthombo oyintsusa"

#: preserveExisting.tooltip
msgid "Preserves line breaks and page's original formatting"
msgstr "Londoloza uqhawuqhawulo lwemigca nolungiselelo oluyintsusa lwekhasi "

#: saveAssociatedFiles.label
#: saveAssociatedFiles.accesskey
msgid "&Save images and other associated files when saving pages"
msgstr "&Gcina imifuziselo nezinye iifayili ezinxulumeneyo xa ugcina amakhasi"

#: showPublishDialog.label
#: showPublishDialog.accesskey
msgid "&Always show Publish dialog when publishing pages"
msgstr "&Yiba soloko ubonisa ingxoxo Yopapasho xa upapasha amakhasi"

#: composerEditing.label
msgid "Editing"
msgstr "Kuyahlelwa"

#: maintainStructure.label
#: maintainStructure.accesskey
msgid "&Maintain table layout when inserting or deleting cells"
msgstr "&Bamba ukumiswa kakuhle kwetheyibhile xa kufakwa okanye kucinywa iiseli"

#: maintainStructure.tooltip
msgid "Preserves table's rectangular shape by automatically adding cells after inserting or deleting cells"
msgstr "Londoloza ubume boxande betheyibhile ngokufakela iiseli ngokuzenzekela emva kokufaka okanye kokucima iiseli"

#: useCSS.label
#: useCSS.accesskey
msgid "&Use CSS styles instead of HTML elements and attributes"
msgstr "&Sebenzisa izimbo ze-CSS endaweni yeziqalelo ze-HTML neempawu zazo zoyelelwano"

#: crInPCreatesNewP.label
#: crInPCreatesNewP.accesskey
msgid "&Return in a paragraph always creates a new paragraph"
msgstr "&Ukubuyela kumhlathi kusoloko kudala umhlathi omtsha"

