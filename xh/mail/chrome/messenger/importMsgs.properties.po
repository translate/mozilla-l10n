#Error strings
#: ImportAlreadyInProgress
msgid "An import operation is currently in progress. Try again when the current import has finished."
msgstr "Umsebenzi wokuthatha ngaphandle uyaqhutywa okwangoku. Zama kwakhona xa ukuthatha ngaphandle okwenzeka ngoku sekugqityiwe."

#Error strings for settings import
#: ImportSettingsBadModule
msgid "Unable to load settings module"
msgstr "Akuphumelelanga ukulayisha imodyuli yemimiselo"

#: ImportSettingsNotFound
msgid "Unable to find settings.  Check to make sure the application is installed on this machine."
msgstr "Akuphumelelanga ukufumana imimiselo. Qwalasela ukuqinisekisa ukuba ihlohliwe inkqubo kulo matshini."

#: ImportSettingsFailed
msgid "An error occurred while importing settings.  Some, or all, of the settings may not have been imported."
msgstr "Kwenzeke imposiso xa bekuthathwa ngaphandle imimiselo. Eminye, okanye yonke, imimiselo isenokuba ayithathanga ngaphandle."

# LOCALIZATION NOTE : Do not translate the word "%S" below.
#: ImportSettingsSuccess
msgid "Settings were imported from %S"
msgstr "Imimiselo ithathwe ngaphandle kwi-%S"

#Error string for mail import
#: ImportMailBadModule
msgid "Unable to load mail import module"
msgstr "Akuphumelelanga ukulayisha imodyuli yokuthatha ngaphandle imeyile"

#: ImportMailNotFound
msgid "Unable to find mail to import.  Check to make sure the mail application is correctly installed on this machine."
msgstr "Akuphumelelanga ukufumana imeyile yokuthatha ngaphandle. Qwalasela ukuqinisekisa ukuba inkqubo yemeyile isoloko ihlohlwa kulo matshini."

#: ImportEmptyAddressBook
msgid "Can't import empty address book %S."
msgstr "Incwadi yeedilesi engenanto i-%S ayikwazi kuthathwa ngaphandle."

# LOCALIZATION NOTE : Do not translate the word "%S" below.
#: ImportMailFailed
msgid "An error occurred importing mail from %S"
msgstr "Kwenzeke imposiso xa bekuthathwa ngaphandle imeyile kwi-%S"

# LOCALIZATION NOTE : Do not translate the word "%S" below.
#: ImportMailSuccess
msgid "Mail was successfully imported from %S"
msgstr "Imeyile ithathwe ngaphandle ngempumelelo kwi-%S"

# Error string for address import
#: ImportAddressBadModule
msgid "Unable to load address book import module."
msgstr "Akuphumelelanga ukulayisha imodyuli yokuthatha ngaphandle incwadi yeedilesi."

#: ImportAddressNotFound
msgid "Unable to find any address books to import.  Check to make sure the selected application or format is correctly installed on this machine."
msgstr "Akuphumelelanga ukufumana naziphi iincwadi zeedilesi zokuthatha ngaphandle. Qwalasela ukuqinisekisa ukuba inkqubo ekhethiweyo okanye ulungiselelo luhlohleke ngokuchanekileyo kulo matshini."

# LOCALIZATION NOTE : Do not translate the word "%S" below.
#: ImportAddressFailed
msgid "An error occurred importing addresses from %S."
msgstr "Kwenzeke imposiso yokuthatha ngaphandle iidilesi kwi-%S."

# LOCALIZATION NOTE : Do not translate the word "%S" below.
#: ImportAddressSuccess
msgid "Addresses successfully imported from %S."
msgstr "Iidilesi zithathe ngaphandle ngempumelelo kwi-%S."

