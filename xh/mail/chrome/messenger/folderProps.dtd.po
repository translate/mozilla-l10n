msgid "Folder Type:"
msgstr "Isimbo Sesiqulathi Seefayili:"

#: folderQuotaTab.label
msgid "Quota"
msgstr "Umlinganiselo"

#: folderQuotaRoot.label
msgid "Quota Root:"
msgstr "Umlinganiselo Onikwe Amandla:"

#: folderQuotaUsage.label
msgid "Usage:"
msgstr "Ukusetyenziswa:"

#: folderQuotaStatus.label
msgid "Status:"
msgstr "Isimo:"

