msgid "Information:"
msgstr "Ulwazi:"

#: folderProps.name.label
#: folderProps.name.accesskey
msgid "&Name:"
msgstr "&Nika igama:"

#: folderSharingTab.label
msgid "Sharing"
msgstr "Ukwabelana"

#: privilegesDesc.label
msgid "Share this and other folders with network users and display and set access privileges"
msgstr "Yabelana ngako oku nangezinye iziqulathi zeefayili nabasebenzisi bomsebenzi wothungelwano uze ubonise kwaye umisele ufikelelo kumalungelo awodwa"

#: privileges.button.label
#: privileges.button.accesskey
msgid "&Privileges..."
msgstr "&Amalungelo awodwa..."

#: permissionsDesc.label
msgid "You have the following permissions:"
msgstr "Uneemvume ezilandelayo:"

