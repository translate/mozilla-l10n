# LOCALIZATION NOTE askSendFormat.dtd UI for dialog that asks the user, which format to use for sending a message
#: windowTitle.label
msgid "HTML Mail Question"
msgstr "Umbuzo Wemeyile ye-HTML"

#: recipient.label
msgid "Some of the recipients are not listed as being able to receive HTML mail."
msgstr "Abanye abamkeli abadweliswanga njengabanako ukufumana imeyile ye-HTML."

#: convertibleYes.label
msgid "Your message can be converted to plain text without losing information."
msgstr "Umyalezo wakho usenokuguqulwa ube sisiqendu esingaxutywanga ngaphandle kokulahlekelwa lulwazi."

#: convertibleAltering.label
msgid "Your message can be converted to plain text without losing important information. However, the plain text version might look different from what you saw in the composer."
msgstr "Umyalezo wakho usenokuguqulwa ube sisiqendu esingaxutywanga ngaphandle kokulahleka kolwazi olubalulekileyo. Nangona kunjalo, uguqulelo lwesiqendu esingaxutywanga lusenokubonakala lwahlukile kwinto oyibone kwisiqambi."

#: convertibleNo.label
msgid "However, you used formatting (e.g. colors) that will not be converted to plain text."
msgstr "Nangona kunjalo, usebenzise ulungiselelo (umzk. imibala) engasayi kuguqulelwa kwisiqendu esingaxutywanga."

#: question.label
msgid "Would you like to convert the message to plain text or send it in HTML anyway?"
msgstr "Ingaba ufuna ukuguqula umyalezo ube sisiqendu esingaxutywanga okanye usithumele nge-HTML nokuba sinjalo?"

#: plainTextAndHtml.label
msgid "Send in Plain Text and HTML"
msgstr "Thumela Ngesiqendu Esingaxutywanga nange-HTML"

#: plainTextOnly.label
msgid "Send in Plain Text Only"
msgstr "Thumela Ngesiqendu Esingaxutywanga Kuphela"

#: htmlOnly.label
msgid "Send in HTML Only"
msgstr "Thumela nge-HTML Kuphela"

#: plainTextAndHtmlRecommended.label
msgid "Send in Plain Text and HTML (recommended)"
msgstr "Thumela Ngesiqendu Esingaxutywanga nange-HTML (kucetyiswa oko)"

#: plainTextOnlyRecommended.label
msgid "Send in Plain Text Only (recommended)"
msgstr "Thumela Ngesiqendu Esingaxutywanga Kuphela (kucetyiswa oko)"

#: htmlOnlyRecommended.label
msgid "Send in HTML Only (recommended)"
msgstr "Thumela nge-HTML Kuphela (kucetyiswa oko)"

#: recommended.label
msgid "(recommended)"
msgstr "(kucetyiswa oko)"

#: send.label
msgid "Send"
msgstr "Thumela"

#: cancel.label
msgid "Cancel"
msgstr "Rhoxisa"

#: recipients.label
msgid "Recipients..."
msgstr "Abamkeli..."

#: help.label
msgid "Help"
msgstr "Uncedo"

