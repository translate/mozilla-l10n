#   LOCALIZATION NOTE :
#   do not localize "\n".  use "\n" to make the text fit nicely in the dialog.
# 
# Download Messages Prompt
#
#: downloadMessagesWindowTitle
msgid "Work Offline"
msgstr "Umsebenzi Kwimo Engeyo Yokusebenza"

#   LOCALIZATION NOTE :
#   do not localize "\n".  use "\n" to make the text fit nicely in the dialog.
#: downloadMessagesLabel
msgid ""
"Do you want to download messages \n"
"for offline use before you go offline?"
msgstr ""
"Ingaba ufuna ukuthoba umthwalo wemiyalezo \n"
"yokusetyenziswa kwimo engeyo yakusebenza phambi kokuya kule mo?"

#: downloadMessagesCheckboxLabel
msgid "Always ask me when I go offline"
msgstr "Yiba soloko undibuza qho xa ndisiya kwimo engeyo yakusebenza"

#: downloadMessagesDownloadButtonLabel
msgid "Download"
msgstr "Thoba umthwalo"

#: downloadMessagesNoDownloadButtonLabel
msgid "Don't Download"
msgstr "Musa Ukuthoba Umthwalo"

#: downloadMessagesCancelButtonLabel
msgid ""
"_: downloadMessagesCancelButtonLabel\n"
"Cancel"
msgstr ""
"Rhoxisa"

# 
# Send Messages Prompt
#
#: sendMessagesWindowTitle
msgid "Work Online"
msgstr "Umsebenzi Kwimo Yokusebenza"

#   LOCALIZATION NOTE :
#   do not localize "\n".  use "\n" to make the text fit nicely in the dialog.
#: sendMessagesLabel
msgid ""
"Would you like to send the messages \n"
"in your Unsent Messages folder?"
msgstr ""
"Ingaba unqwenela ukuthumela imiyalezo \n"
"kwisiqulathi sakho seefayili Semiyalezo Engathunyelwanga?"

#: sendMessagesCheckboxLabel
msgid "Always ask me when I go online"
msgstr "Yiba soloko undibuza qho xa ndisiya kwimo yokusebenza"

#: sendMessagesSendButtonLabel
msgid "Send"
msgstr "Thumela"

#: sendMessagesNoSendButtonLabel
msgid "Don't Send"
msgstr "Musa Ukuthumela"

#: sendMessagesCancelButtonLabel
msgid ""
"_: sendMessagesCancelButtonLabel\n"
"Cancel"
msgstr ""
"Rhoxisa"

#
# GetMessages Offline Prompt 
#
#: getMessagesOfflineWindowTitle
msgid "Get Messages"
msgstr "Fumana Imiyalezo"

#   LOCALIZATION NOTE :
#   do not localize "\n".  use "\n" to make the text fit nicely in the dialog.
#: getMessagesOfflineLabel
msgid ""
"You are currently offline. Would you like \n"
"to go online to get your new messages?"
msgstr ""
"Okwangoku ukwimo engeyo yakusebenza. Ingaba unqwenela \n"
"ukuya kwimo yokusebenza ukuze ufumane imiyalezo yakho emitsha?"

#: getMessagesOfflineGoButtonLabel
msgid ""
"_: getMessagesOfflineGoButtonLabel\n"
"Go online"
msgstr ""
"Yiya kwimo yokusebenza"

#: getMessagesOfflineCancelButtonLabel
msgid ""
"_: getMessagesOfflineCancelButtonLabel\n"
"Cancel"
msgstr ""
"Rhoxisa"

# 
# Send Messages Offline Prompt
#
#: sendMessagesOfflineWindowTitle
msgid "Send Messages"
msgstr "Thumela Imiyalezo"

#   LOCALIZATION NOTE :
#   do not localize "\n".  use "\n" to make the text fit nicely in the dialog.
#: sendMessagesOfflineLabel
msgid ""
"You are currently offline. Would you like \n"
"to go online and send your unsent messages?"
msgstr ""
"Okwangoku ukwimo engeyo yakusebenza. Ingaba unqwenela \n"
"ukuya kwimo yokusebenza ukuze uthumele imiyalezo yakho engathunyelwanga?"

#: sendMessagesOfflineGoButtonLabel
msgid ""
"_: sendMessagesOfflineGoButtonLabel\n"
"Go online"
msgstr ""
"Yiya kwimo yokusebenza"

#: sendMessagesOfflineCancelButtonLabel
msgid ""
"_: sendMessagesOfflineCancelButtonLabel\n"
"Cancel"
msgstr ""
"Rhoxisa"

