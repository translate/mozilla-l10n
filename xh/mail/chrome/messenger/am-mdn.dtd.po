#: pane.title
msgid "Return Receipts"
msgstr "Buyisela Izamkelo"

#: useGlobalPrefs.label
#: useGlobalPrefs.accesskey
msgid "&Use my global return receipt preferences for this account"
msgstr "&Sebenzisa izikhethwa zam zelimiweyo zokubuyisa izamkelo zale akhawunti"

#: useCustomPrefs.label
#: useCustomPrefs.accesskey
msgid "&Customize return receipts for this account"
msgstr "&Lungiselela ukubuyisa izamkelo zale akhawunti"

#: requestReceipt.label
#: requestReceipt.accesskey
msgid "&When sending messages, always request a return receipt"
msgstr "&Xa uthumela imiyalezo, yiba soloko ucela isamkelo sokubuyisa"

#: receiptArrive.label
msgid "When a receipt arrives:"
msgstr "Xa kufika isamkelo:"

#: leaveIt.label
#: leaveIt.accesskey
msgid "Leave it in my &Inbox"
msgstr "Sishiye Kwibhokisi yam &Ngaphakathi"

# LOCALIZATION NOTE moveToSent.label Translate: 'Sent' according to Netscape glossary
#: moveToSent.label
#: moveToSent.accesskey
msgid "&Move it to my &quot;Sent&quot; folder"
msgstr "&Yishenxisele kwi-&quot yam;Ithunyelwe&quot; isiqulathi seefayili"

#: requestMDN.label
msgid "When I receive a request for a return receipt:"
msgstr "Xa ndifumana isicelo sesamkelo sokubuyisa: "

#: returnSome.label
#: returnSome.accesskey
msgid "Allow r&eturn receipts for some messages"
msgstr "Vumela izameko zoku&buyisa ukwenzela imiyalezo ethile"

#: never.label
#: never.accesskey
msgid "&Never send a return receipt"
msgstr "&Ungaze uthumele isamkelo sokubuyisa"

#: notInToCc.label
#: notInToCc.accesskey
msgid "If I'm not in the &To or Cc of the message:"
msgstr "Ukuba andikho kwi-& Iya okanye i-Cc yomyalezo:"

#: outsideDomain.label
#: outsideDomain.accesskey
msgid "If the &sender is outside my domain:"
msgstr "Ukuba &umthumeli ungaphandle kummandla wolwazi lwam:"

#: otherCases.label
#: otherCases.accesskey
msgid "In all &other cases:"
msgstr "Kuzo zonke &ezinye iziganeko:"

#: askMe.label
msgid "Ask me"
msgstr "Ndicela"

#: alwaysSend.label
msgid "Always send"
msgstr "Tumela qho"

#: neverSend.label
msgid "Never send"
msgstr "Ungaze uthumele"

