## @name IMAP_MOVING_MESSAGE_TO
## @loc None
# LOCALIZATION NOTE (Error 5041): Do not translate the word "%S" below.
# Place the word %S in your translation where the name of the folder should appear.
#: 5041
msgid "Moving message to %S..."
msgstr "Kushenxiselwa umyalezo ku-%S..."

## @name IMAP_COPYING_MESSAGES_TO
## @loc None
# LOCALIZATION NOTE (Error 5042): Do not translate the word "%S" below.
# Place the word %S in your translation where the name of the folder should appear.
#: 5042
msgid "Copying messages to %S..."
msgstr "Kukotshelwa imiyalezo ku-%S..."

## @name IMAP_COPYING_MESSAGE_TO
## @loc None
# LOCALIZATION NOTE (Error 5043): Do not translate the word "%S" below.
# Place the word %S in your translation where the name of the folder should appear.
#: 5043
msgid "Copying message to %S..."
msgstr "Kukotshelwa umyalezo ku-%S..."

## @name IMAP_SELECTING_MAILBOX
## @loc None
# LOCALIZATION NOTE (Error 5044): Do not translate the word "%S" below.
# Place the word %S in your translation where the name of the folder should appear.
#: 5044
msgid "Opening folder %S..."
msgstr "Kuvulwa isiqulathi seefayili i-%S..."

## @name IMAP_FOLDER_RECEIVING_MESSAGE_OF
## @loc None
# LOCALIZATION NOTE (Error 5045): Do not translate the word "%S" or "%lu" below.
# Place the word %S in your translation where the name of the folder should appear.
# Place the word %lu where the number of headers should appear.
#: 5045
msgid "%S - Downloading message %lu of %lu"
msgstr "I-%S - Ithoba umthwalo womyalezo i-%lu ye %lu"

## @name IMAP_DISCOVERING_MAILBOX 
## @loc None
# LOCALIZATION NOTE (Error 5046): Do not translate the word "%S" below.
# Place the word %S in your translation where the name of the folder should appear.
#: 5046
msgid "Found folder: %S"
msgstr "Sifunyenwe isiqulathi seefayili: %S"

## @name IMAP_ENTER_PASSWORD_PROMPT
## @loc None
# LOCALIZATION NOTE (Error 5047): Do not translate the word %S below.
# Place the word "%S" in your translation where the email address
# or the username should appear
#: 5047
msgid "Enter your password for %S:"
msgstr "Ngenisa igama lakho lokugqithisa ukwenzela i-%S:"

## @name IMAP_SERVER_NOT_IMAP4
## @loc None
# LOCALIZATION NOTE (Error 5048): Do not translate the word "IMAP4" below.
#: 5048
msgid "Mail server %S is not an IMAP4 mail server."
msgstr "Iseva yemeyile i-%S asiyoseva yemeyile ye-IMAP4."

## @name IMAP_SERVER_SAID
## @loc None
#: 5049
msgid "The current command did not succeed.  The mail server responded:"
msgstr "Umyalelo wangoku awuphumelelanga.  Iseva yemeyile iphendule:"

## @name IMAP_DONE
## @loc None
## @name IMAP_ENTER_PASSWORD_PROMPT_TITLE
## @loc None
#: 5051
msgid "Mail Server Password Required"
msgstr "Kufuneka Igama Lokugqithisa Leseva Yemeyile"

## @name IMAP_UNKNOWN_HOST_ERROR
## @loc None
#: 5052
msgid "Failed to connect to server %S."
msgstr "Akuphumelelanga ukudibanisa kwiseva ye-%S."

## @name IMAP_IMAP_CONNECTION_REFUSED_ERROR
## @loc None
#: 5053
msgid "Could not connect to mail server %S; the connection was refused."
msgstr "Akuphumelelanga ukudibanisa kwiseva yemeyile i-%S; umdibaniso waliwe."

## @name IMAP_NET_TIMEOUT_ERROR
## @loc None
#: 5054
msgid "Connection to server %S timed out."
msgstr "Umdibaniso oya kwiseva i-%S uphelelwe lixesha."

## @name IMAP_MOVE_FOLDER_TO_TRASH
## @loc None
#: 5055
msgid "Are you sure you want to move the selected folder into the Trash?"
msgstr "Uqinisekile ufuna ukushenxisela isiqulathi seefayili esikhethiweyo kwi-Nkunkuma?"

# Status - no messages to download 
## @name IMAP_NO_NEW_MESSAGES
## @loc None
#: 5056
msgid "There are no new messages on the server."
msgstr "Akukho miyalezo mitsha kwiseva."

## @name IMAP_DEFAULT_ACCOUNT_NAME
## @loc None
#: 5057
msgid "Mail for %S"
msgstr "Imeyile ye-%S"

## @name IMAP_DELETE_NO_TRASH
## @loc None
#: 5058
msgid "Deleting this folder is not undoable and will delete all of the messages it contains, and its sub-folders. Are you sure you still want to delete this folder?"
msgstr "Ukucima esi siqulathi seefayili asiyonto engenakuqhaqhwa kwaye kuya kucima yonke imiyalezo esiyiqulathileyo, ngokunjalo neziqulathi zeefayili ezonganyelweyo. Uqinisekile usafuna ukusicima esi siqulathi seefayili?"

## @name IMAP_EMPTY_TRASH_CONFIRM
## @loc None
#: 5061
msgid "Emptying trash will delete %S and all of the messages it contains. Do you want to delete this folder?"
msgstr "Ukuqongqotha inkunkuma kuya kucima i-%S nayo yonke imiyalezo esiyiqulathileyo. Ingaba ufuna ukusicima esi siqulathi seefayili?"

## @name IMAP_PERSONAL_FILING_CABINET
## @loc None
#: 5062
msgid "Personal Filing Cabinet"
msgstr "Ikhabhinethi Yokubeka Iifayili Zobuqu Bomntu"

## @name IMAP_PFC_READ_MAIL
## @loc None
#: 5063
msgid "Read Mail"
msgstr "Funda Imeyile"

## @name IMAP_PFC_SENT_MAIL
## @loc None
#: 5064
msgid "Sent Mail"
msgstr "Ithunyelwe Imeyile"

## @name IMAP_SPECIAL_CHAR
## @loc None
#: 5065
msgid "The %c character is reserved on this imap server. Please choose another name."
msgstr "Umbhalo we-%c ubanjiwe kule seva ye-imap. Nceda khetha elinye igama."

## @name IMAP_PERSONAL_SHARED_FOLDER_TYPE_NAME
## @loc None
#: 5066
msgid "Personal Folder"
msgstr "Isiqulathi Seefayili SObuqu Bomntu"

## @name IMAP_PUBLIC_FOLDER_TYPE_NAME
## @loc None
#: 5067
msgid "Public Folder"
msgstr "Isiqulathi Seefayili Sikawonkewonke"

## @name IMAP_OTHER_USERS_FOLDER_TYPE_NAME
## @loc None
#: 5068
msgid "Other User's Folder"
msgstr "Isiqulathi Seefayili Sabanye Abasebenzisi"

## @name IMAP_PERSONAL_FOLDER_TYPE_DESCRIPTION
## @loc None
#: 5069
msgid "This is a personal mail folder.  It is not shared."
msgstr "Esi sisiqulathi seefayili zemeyile sobuqu bomntu.  Akwabelwana ngaso."

## @name IMAP_PERSONAL_SHARED_FOLDER_TYPE_DESCRIPTION
## @loc None
#: 5070
msgid "This is a personal mail folder.  It has been shared."
msgstr "Esi sisiqulathi seefayili zemeyile sobuqu bomntu.  Bekusabelwana ngaso."

## @name IMAP_PUBLIC_FOLDER_TYPE_DESCRIPTION
## @loc None
#: 5071
msgid "This is a public folder."
msgstr "Esi sisiqulathi seefayili zikawonkewonke."

## @name IMAP_OTHER_USERS_FOLDER_TYPE_DESCRIPTION
## @loc None
#: 5072
msgid "This is a mail folder shared by the user '%S'."
msgstr "Esi sisiqulathi seefayili zemeyile ekwabelwana ngaso ngumsebenzisi i-'%S'."

## @name IMAP_ACL_FULL_RIGHTS
## @loc None
#: 5073
msgid "Full Control"
msgstr "Ulawulo Olupheleleyo"

## @name IMAP_ACL_LOOKUP_RIGHT
## @loc None
#: 5074
msgid "Lookup"
msgstr "Funa"

## @name IMAP_ACL_READ_RIGHT
## @loc None
#: 5075
msgid "Read"
msgstr "Funda"

## @name IMAP_ACL_SEEN_RIGHT
## @loc None
#: 5076
msgid "Set Read/Unread State"
msgstr "Misela Imeko Yokufunda/Yokungafundi"

## @name IMAP_ACL_WRITE_RIGHT
## @loc None
#: 5077
msgid "Write"
msgstr "Bhala"

## @name IMAP_ACL_INSERT_RIGHT
## @loc None
#: 5078
msgid "Insert (Copy Into)"
msgstr "Faka (Kopela Apha)"

## @name IMAP_ACL_POST_RIGHT
## @loc None
#: 5079
msgid "Post"
msgstr "Posa"

## @name IMAP_ACL_CREATE_RIGHT
## @loc None
#: 5080
msgid "Create Subfolder"
msgstr "Yenza Isiqulathi Seefayili Esonganyelweyo"

## @name IMAP_ACL_DELETE_RIGHT
## @loc None
#: 5081
msgid "Delete Messages"
msgstr "Cima Imiyalezo"

## @name IMAP_ACL_ADMINISTER_RIGHT
## @loc None
#: 5082
msgid "Administer Folder"
msgstr "Lawula Isiqulathi Seefayili"

## @name IMAP_UNKNOWN_USER
## @loc None
#: 5083
msgid "Unknown"
msgstr "Akwaziwa"

## @name IMAP_SERVER_DOESNT_SUPPORT_ACL
## @loc None
#: 5084
msgid "This server does not support shared folders."
msgstr "Le seva ayizixhasi iziqulathi zeefayili ekwabelwana ngazo."

## @name IMAP_SERVER_DISCONNECTED
## @loc None
#: 5090
msgid "Server %S has disconnected. The server may have gone down or there may be a network problem."
msgstr "Iseva ye-%S ayisadibenanga. Le seva isenokuba iwile okanye kusenokuba kukho ingxaki yomsebenzi wothungelwano."

## @name IMAP_REDIRECT_LOGIN_FAILED
## @loc None
#: 5091
msgid "Login failed."
msgstr "Akuphumelelanga ukungena."

# LOCALIZATION NOTE (autoSubscribeText): %1$S is the imap folder.
#: 5092
msgid "Would you like to subscribe to %1$S?"
msgstr "Ingaba unomnqweno wokunikela umrhumo kwi-%1$S?"

## @name IMAP_SERVER_DROPPED_CONNECTION
## @loc None
#: 5093
msgid "Unable to connect to your IMAP server. You may have exceeded the maximum number"
msgstr "Akuphumeleli ukudibanisa kwiseva yakho ye-IMAP. Usenokuba ugqithise inani elilelona liphezulu"

## @name IMAP_QUOTA_STATUS_FOLDERNOTOPEN
## @loc None
#: 5095
msgid "Quota information is not available because the folder is not open."
msgstr "Ulwazi ngomlinganiselo alukho kuba isiqulathi seefayili asivulwanga."

## @name IMAP_QUOTA_STATUS_NOTSUPPORTED
## @loc None
#: 5096
msgid "This server does not support quotas."
msgstr "Le seva ayiyixhasi imilinganiselo."

## @name IMAP_QUOTA_STATUS_NOQUOTA
## @loc None
#: 5097
msgid "There are no storage quotas on this folder."
msgstr "Akukho milinganiselo yokulondoloza kwesi siqulathi seefayili."

# Out of memory
## @name IMAP_OUT_OF_MEMORY
## @loc None
#: 5100
msgid "Application is out of memory."
msgstr "Inkqubo ayikho kuvimba wolwazi."

## @name IMAP_AUTH_SECURE_NOTSUPPORTED
## @loc None
#: 5102
msgid ""
"You cannot log in to %S because you have enabled secure authentication and this server does not support it.\n"
"\n"
"To log in, turn off secure authentication for this account."
msgstr ""
"Akunakukwazi ukungena kwi-%S kuba unike isakhono kunyanisekiso lokhuseleko kwaye le seva ayiyixhasi.\n"
"\n"
"Ukungena, valela unyanisekiso lokhuseleko malunga nale akhawunti."

## @name IMAP_COPYING_MESSAGE_OF
## @loc None
# LOCALIZATION NOTE (Error 5103): Do not translate the word "%S" below.
# Place the word %3$S in your translation where the name of the destination folder should appear.
# Place the word %1$S where the currently copying message should appear.
# Place the word %2$S where the total number of messages should appear.
#: 5103
msgid "Copying Message %1$S of %2$S to %3$S"
msgstr "Kukotshelwa Umyalezo i-%1$S ye %2$S kwi %3$S"

## @name IMAP_LOGIN_DISABLED
## @loc None
#: 5104
msgid "You cannot log in to %S because the server has disabled login. You may need to connect via SSL or TLS. Please check the account settings for your mail server."
msgstr "Akunakukwazi ukungena kwi-%S kuba le seva yaphule isakhono sokungena. Kusenokufuneka ukuba wenze umdibaniso ngokugqitha kwi-SSL okanye i-TLS. Nceda qwalasela imimiselo yeeakhawunti ukwenzela iseva yakho yemeyile."

