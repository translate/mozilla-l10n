#
# The following are used by the outlook express import code to display status/error 
# and informational messages
#
# Short name of import module
## @name OEIMPORT_NAME
## @loc None
## LOCALIZATION NOTE (2000): DONT_TRANSLATE
#: 2000
msgid "Outlook Express"
msgstr "I-Outlook Express"

# Description of import module
## @name OEIMPORT_DESCRIPTION
## @loc None
## LOCALIZATION NOTE (2001): In this item, don't translate "Outlook Express"
#: 2001
msgid "Outlook Express mail and address books"
msgstr "Imeyile ye-Outlook Express neencwadi zeedilesi"

# Success message
## @name OEIMPORT_MAILBOX_SUCCESS
## @loc None
## LOCALIZATION NOTE (2002): In this item, don't translate "%S" or "%d"
## The variable %S will contain the name of the Mailbox
## The variable %d will contain the number of messages
#: 2002
msgid "Mailbox %S, imported %d messages"
msgstr "Ibhokisi yemeyile ye-%S, ithathe ngaphandle imiyalezo ye-%d"

# Error message
## @name OEIMPORT_MAILBOX_BADPARAM
## @loc None
#: 2003
msgid "Bad parameter passed to import mailbox."
msgstr "Kugqithiselwe umda wenkcazelo ongalunganga ukuthatha ngaphandle ibhokisi yemeyile."

# Error message
## @name OEIMPORT_MAILBOX_BADSOURCEFILE
## @loc None
## LOCALIZATION NOTE (2004): In this item, don't translate "%S"
## The variable %S will contain the name of the Mailbox
#: 2004
msgid "Error accessing file for mailbox %S."
msgstr "Ifayili yokufikelela kwimposiso yebhokisi yemeyile ye-%S."

# Error message
## @name OEIMPORT_MAILBOX_CONVERTERROR
## @loc None
## LOCALIZATION NOTE (2005): In this item, don't translate "%S"
## The variable %S will contain the name of the Mailbox
#: 2005
msgid "Error importing mailbox %S, all messages may not be imported from this mailbox."
msgstr "Ibhokisi yemeyile ethatha ngaphandle imposiso ye-%S,  yonke imiyalezo  mayingathathwa ngaphandle kule bhokisi yemeyile."

# Default name of imported addressbook
## @name OEIMPORT_DEFAULT_NAME
## @loc None
#: 2006
msgid "Outlook Express Address Book"
msgstr "Incwadi Yeedilesi ze-Outlook Express"

# Autofind description
## @name OEIMPORT_AUTOFIND
## @loc None
#: 2007
msgid "Outlook Express address book (windows address book)"
msgstr "Incwadi Yeedilesi ze-Outlook Express (incwadi yeedilesi ze-windows)"

# Description
## @name OEIMPORT_ADDRESS_SUCCESS
## @loc None
## LOCALIZATION NOTE (2006): In this item, don't translate "%S"
## The variable %S will receive the name of the address book
#: 2008
msgid "Imported address book %S"
msgstr "Incwadi yeedilesi ethathwe ngaphandle ye-%S"

# Error message
## @name OEIMPORT_ADDRESS_CONVERTERROR
## @loc None
## LOCALIZATION NOTE (2009): In this item, don't translate "%S"
## The variable %S will receive the name of the address book
#: 2009
msgid "Error importing address book %S, all addresses may not have been imported."
msgstr "Incwadi yeedilesi yemposiso ethathwe ngaphandle ye-%S, iidilesi zonke mazibe azithathwanga ngaphandle."

# Error message
## @name OEIMPORT_ADDRESS_BADPARAM
## @loc None
#: 2010
msgid "Bad parameter passed to import addressbook."
msgstr "Umda wenkcazelo ongalunganga ugqithele ukuthatha ngaphandle incwadi yeedilesi."

