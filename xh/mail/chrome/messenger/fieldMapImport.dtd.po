#: fieldMapImport.title
msgid "Import Address Book"
msgstr "Thatha Ngaphandle Incwadi Yeedilesi"

#: fieldMapImport.size
msgid ""
"_: Do not translate this.  Only change the numeric values if you need this dialogue box to appear bigger.\n"
"width: 40em; height: 30em;"
msgstr ""
"width: 40em; height: 30em;"

#: fieldMapImport.recordNumber
msgid "Imported data for Record: "
msgstr "Iinkcukacha ezithathwa ngaphandle ukwenzela Ingxelo: "

#: fieldMapImport.next
msgid "Next"
msgstr "Okulandelayo"

#: fieldMapImport.next.accesskey
msgid "N"
msgstr "l"

#: fieldMapImport.previous
msgid "Previous"
msgstr "Okwangaphambili"

#: fieldMapImport.previous.accesskey
msgid "P"
msgstr "w"

#: fieldMapImport.text
msgid "Use Move Up and Move Down to match the address book fields on the left to the correct data for import on the right. Uncheck items you do not want to import."
msgstr "Sebenzisa u-Shenxisela Phezulu no-Shenxisela Ezantsi ukuze imimandla yencwadi yeedilesi esekhohlo ihambelane kulungiswe iinkcukacha ukwenzela ukuthatha ngasekunene. Qwalasela kwakhona amanqaku ongafuni kuwathatha ngaphandle."

#: fieldMapImport.up
msgid "Move Up"
msgstr "Shenxisela Phezulu"

#: fieldMapImport.up.accesskey
msgid "U"
msgstr "P"

#: fieldMapImport.down
msgid "Move Down"
msgstr "Shenxisela Ezantsi"

#: fieldMapImport.down.accesskey
msgid "D"
msgstr "E"

#: fieldMapImport.fieldListTitle
msgid "Address Book fields"
msgstr "Imimandla Yencwadi Yeedilesi"

#: fieldMapImport.dataTitle
msgid "Record data to import"
msgstr "Yenza ingxelo yeenkcukacha zokuthatha ngaphandle"

#: fieldMapImport.skipFirstRecord
msgid "First record contains field names"
msgstr "Ingxelo yokuqala iqulethe amagama emimandla"

#: fieldMapImport.skipFirstRecord.accessKey
msgid "F"
msgstr "I"

