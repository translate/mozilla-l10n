#: serverAdvanced.label
msgid "Advanced Account Settings"
msgstr "Imimiselo Yeeakhawunti Ekwinqanaba Eliphambili"

#: imapAdvanced.label
msgid "Advanced IMAP Server Settings"
msgstr "Imimiselo Yeseva ye-IMAP Ekwinqanaba Eliphambili"

#: serverDirectory.label
#: serverDirectory.accesskey
msgid "IMAP server &directory:"
msgstr "Iseva &uvimba weefayili we-IMAP:"

#: usingSubscription.label
#: usingSubscription.accesskey
msgid "Sho&w only subscribed folders"
msgstr "Bo&nisa kuphela iziqulathi zeefayili ezikhutshelwa imirhumo"

#: dualUseFolders.label
#: dualUseFolders.accesskey
msgid "Server supports &folders that contain sub-folders and messages"
msgstr "Iseva ixhasa &iziqulathi zeefayili eziqulethe iziqulathi zeefayili ezonganyelweyo nemiyalezo"

#: useIdle.label
#: useIdle.accesskey
msgid "Use &IDLE command if the server supports it"
msgstr "Sebenzisa umyalelo we-&IDLE ukuba iseva iyawuxhasa"

#: maximumConnectionsNumber.label
#: maximumConnectionsNumber.accesskey
msgid "&Maximum number of server connections to cache"
msgstr "&Inani elilelona liphezulu lemidibaniso yeseva eya kwisilondolozi esiqukezayo"

#: namespaceDesc.label
msgid ""
"_: DONT_TRANSLATE \"IMAP\"\n"
"These preferences specify the namespaces on your IMAP server"
msgstr ""
"Ezi zikhethwa zixela izithuba zamagama akwiseva yakho ye-IMAP"

#: personalNamespace.label
#: personalNamespace.accesskey
msgid "&Personal namespace:"
msgstr "&Izithuba zamagama obuqu bomntu:"

#: publicNamespace.label
#: publicNamespace.accesskey
msgid "P&ublic (shared):"
msgstr "Kaw&onkewonke (kuyabelwana):"

#: otherUsersNamespace.label
#: otherUsersNamespace.accesskey
msgid "&Other Users:"
msgstr "&Abanye Abasebenzisi:"

#: overrideNamespaces.label
#: overrideNamespaces.accesskey
msgid "&Allow server to override these namespaces"
msgstr "&Vumela iseva ukuba ihambe ngaphezu kwezi zithuba zamagama"

#: pop3Desc.label
msgid "When downloading pop mail for this server, use the following folder for new mail:"
msgstr "Xa uthoba umthwalo wemeyile ezivelelayo yale seva, sebenzisa isiqulathi seefayili esilandelayo ukwenzela imeyile entsha:"

#: folderStorage.label
msgid "Where to store mail"
msgstr "Apho kulondolozwa khona imeyile"

#: globalInbox.label
#: globalInbox.accesskey
msgid "&Global Inbox (Local Folders Account)"
msgstr "&Okuphakathi Ebhokisini Kwelimiweyo (Iakhawunti Yeziqulathi Zeefayili Zendawo Leyo)"

#: accountDirectory.label
#: accountDirectory.accesskey
msgid "Inbox for thi&s server's account"
msgstr "Okuphakathi ebhokisini ukwenzela l&e akhawunti yeseva"

#: deferToServer.label
#: deferToServer.accesskey
msgid "Inbox for &different account"
msgstr "Okuphakathi ebhokisini ukwenzela iakhawunti &eyahlukileyo"

#: deferGetNewMail.label
#: deferGetNewMail.accesskey
msgid "&Include this server when getting new mail"
msgstr "&Bandakanya le seva xa ufumana imeyile entsha"

