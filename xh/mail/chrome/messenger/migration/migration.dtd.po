#: migrationWizard.title
msgid "Import Wizard"
msgstr "Thatha Ngaphandle Umncedi "

#: importFrom.label
msgid "Import Options, Account Settings, Addressbook and other data from:"
msgstr "Thatha Ngaphandle Ekukhethwa Kuko, Imimiselo Yeeakhawunti, Incwadi Yeedilesi nezinye iinkcukacha ezivela:"

#: importFromUnix.label
msgid "Import Preferences, Account Settings, Addressbook and other data from:"
msgstr "Thatha Ngaphandle Izikhethwa, Imimiselo Yeeakhawunti, Incwadi Yeedilesi nezinye iinkcukacha ezivela:"

#: importFromNothing.label
#: importFromNothing.accesskey
msgid "&Don't import anything"
msgstr "&Musa ukuthatha nantoni ngaphandle"

#: importFromSeamonkey.label
#: importFromSeamonkey.accesskey
msgid "&Netscape 6, 7 or Mozilla 1.x"
msgstr "&i-Netscape 6, 7 okanye i-Mozilla 1.x"

#: importFromNetscape4.label
#: importFromNetscape4.accesskey
msgid "Netscape &4.x"
msgstr "i-Netscape &4.x"

#: importFromOExpress.label
#: importFromOExpress.accesskey
msgid "O&utlook Express"
msgstr "i-O&utlook Express"

#: importFromOutlook.label
#: importFromOutlook.accesskey
msgid "&Outlook"
msgstr "&i-Outlook"

#: importFromEudora.label
#: importFromEudora.accesskey
msgid "&Eudora"
msgstr "&i-Eudora"

#: importSource.title
msgid "Import Settings and Mail Folders From"
msgstr "Thatha Ngaphandle Imimiselo Neziqulathi Zeefayili Ezivela"

#: importItems.title
msgid "Items to Import"
msgstr "Amanqaku afanele Ukuthathwa Ngaphandle"

#: importItems.label
msgid "Select which items to import:"
msgstr "Khetha ukuba ngawaphi amanqaku afanele ukuthathwa ngaphandle:"

#: migrating.title
msgid "Importing..."
msgstr "Kuthathwa ngaphandle..."

#: migrating.label
msgid "The following items are currently being imported..."
msgstr "Amanqaku alandelayo athathwa ngaphandle okwangoku..."

#: selectProfile.title
msgid "Select Profile"
msgstr "Khetha Inkangeleko"

#: selectProfile.label
msgid "The following profiles are available to import from:"
msgstr "Iinkangeleko ezilandelayo zikhona kwaye zinokuthathwa ngaphandle zivela:"

#: done.title
msgid "Import Complete"
msgstr "Kugqityiwe Ukuthatha Ngaphandle"

#: done.label
msgid "The following items were successfully imported:"
msgstr "Amanqaku alandelayo athathwe ngaphandle ngempumelelo:"

