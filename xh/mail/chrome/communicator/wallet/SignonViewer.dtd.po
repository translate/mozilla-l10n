#: windowtitle.label
msgid "Password Manager"
msgstr "Umphathi Wegama Lokugqithisa"

#: windowalttitle.label
msgid "Form Manager: Sites"
msgstr "Umphathi Weefom: Iziza"

#: tab.signonsstored.label
msgid "Passwords Saved"
msgstr "Amagama Okugqithisa Agciniwe"

#: tab.signonsnotstored.label
msgid "Passwords Never Saved"
msgstr "Amagama Okugqithisa Zange Agcinwa"

#: tab.nopreview.label
msgid "Forms Never Previewed"
msgstr "Iifom Zange Zajongwa Phambi Koshicilelo"

#: tab.nocapture.label
msgid "Forms Never Saved"
msgstr "Iifom Zange Zagcinwa"

#: spiel.signonsstored.label
msgid "Password Manager has saved login information for the following sites:"
msgstr "Umphathi Wegama Lokugqithisa ulugcinile ulwazi ngokungena kwiziza ezilandelayo:"

#: spiel.signonsnotstored.label
msgid "Password Manager will never save login information for the following sites:"
msgstr "Umphathi Wegama Lokugqithisa akanakuze alugcine ulwazi lokungena kwiziza ezilandelayo:"

#: spiel.nopreview.label
msgid "Form Manager will never preview forms from the following sites before pre-filling them for you:"
msgstr "Umphathi Weefom akasayi kujonga phambi koshicilelo iifom ezivela kwiziza ezilandelayo phambi kokuzizalisela wena kwangaphambili:"

#: spiel.nocapture.label
msgid "Form Manager will never ask to save forms from the following sites:"
msgstr "Umphathi Weefom akasayi kucela ukugcina iifom kwiziza ezilandelayo:"

#: treehead.site.label
msgid "Site"
msgstr "Isiza"

#: treehead.username.label
msgid "Username"
msgstr "Igama lokusebenzisa"

#: treehead.password.label
msgid "Password"
msgstr "Igama lokugqithisa"

#: remove.label
msgid "Remove"
msgstr "Susa"

#: removeall.label
msgid "Remove All"
msgstr "Susa Konke"

