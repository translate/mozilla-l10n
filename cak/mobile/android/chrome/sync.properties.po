#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Ruk'isib'al ximïl: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "Ruk'isib'al ximïl: ri ntajin…"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Retal taqoya'l: %S"

#: notconnected.label
msgid "Not connected"
msgstr "Mani nokisäx"

#: connecting.label
msgid "Connecting…"
msgstr "Tajin nok…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "Xyuj el ri retal ataqoya'l richin Firefox Ximïl"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Titzolin chi rij"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 man nuk'ül ta ri' rik'in ri ruk'isib'äl ruwäch chi Firefox Ximïl. "
"Tab'ana' utzil tak'exa' rik'in ri ruk'isib'al ruwäch."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 man nuk'ül ta ri' kik'in ojer taq ruwäch chi Firefox Ximïl. Tab'ana' "
"utzil, tak'exa' ri Firefox Sync pa ri ch'aqa' chik taq awokisaxel."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox Ximïl"

#: sync.update.button
msgid "Learn More"
msgstr "Tetamäx ch'aqa' chik"

#: sync.update.close
msgid "Close"
msgstr "Titz'apïx"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "Man tikirel ta nib'an runuk'ulem ri Ximïl"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "Man niwachin ta jun okem pa k'amaya'l"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S man nitikïr ta nok pa Ximïl. ¿La nawajo' natojtob'ej jub'ey chik?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Titojtob'ëx chik"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Runuk'ulem chi q'ab'aj"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "Majun taq ruwi' pa ri ch'aqa' chik taq asamajib'al."

#. # LOCALIZATION NOTE (promoBanner.message.text): First part of the message displayed in a
#. # banner on about:home. The final space separates this text from the link.
#: promoBanner.message.text
msgid "Sync is brand new and easier than ever. "
msgstr "Xk'ex ruwäch ri Ximïl, jantape' man k'ayew ta."

#. # LOCALIZATION NOTE (promoBanner.message.link): Second part of the message, styled as a link.
#: promoBanner.message.link
msgid "Tap here to learn more"
msgstr "Tachapa' qa wawe' richin nawetamaj ch'aqa' chik"
