msgid "Sync Options"
msgstr "Taq rucha'oj ximojri'ïl"

#: button.syncOptionsDone.label
msgid "Done"
msgstr "Ruk'isik"

#: button.syncOptionsCancel.label
msgid "Cancel"
msgstr "Tiq'at"

#: invalidEmail.label
msgid "Invalid email address"
msgstr "Man ütz ta ri rachochib'al taqowuj"

#: serverInvalid.label
msgid "Please enter a valid server URL"
msgstr "Tab'ana' utzil tatz'ib'aj jun k'amonel K'AL"

#: usernameNotAvailable.label
msgid "Already in use"
msgstr "Nisamajïx chik"

#: verifying.label
msgid "Verifying…"
msgstr "Tajin nijikib'äx…"

