#: playButton.playLabel
msgid "Play"
msgstr "Titzij"

#: playButton.pauseLabel
msgid "Pause"
msgstr "Titanab'äx"

#: muteButton.muteLabel
msgid "Mute"
msgstr "Timemür"

#: muteButton.unmuteLabel
msgid "Unmute"
msgstr "Tak'axäx"

#: fullscreenButton.enterfullscreenlabel
msgid "Full Screen"
msgstr "Chijun ruwa kematz'ib'"

#: fullscreenButton.exitfullscreenlabel
msgid "Exit Full Screen"
msgstr "Tel pa chijun ruwa kematz'ib'"

#: castingButton.castingLabel
msgid "Cast to Screen"
msgstr "Titaq el pa ruwäch"

#: closedCaption.off
msgid "Off"
msgstr "Chupül"

#: stats.media
msgid "Media"
msgstr "K'ïy k'oxom"

#. Do not translate this.  Only change the numeric values if you need this dialogue box to appear bigger
#: stats.size
msgid "Size"
msgstr "Nimilem"

#: stats.activity
msgid "Activity"
msgstr "Samaj"

#: stats.activityPaused
msgid "Paused"
msgstr "Q'aton"

#: stats.activityPlaying
msgid "Playing"
msgstr "Tajin nitzij"

#: stats.activityEnded
msgid "Ended"
msgstr "Xk'is"

#: stats.activitySeeking
msgid "(seeking)"
msgstr "(tajin nikanöx)"

#: stats.volume
msgid "Volume"
msgstr "Ruch'ab'äl"

#: stats.framesParsed
msgid "Frames parsed"
msgstr "Samajin taq kajtz'ik"

#: stats.framesDecoded
msgid "Frames decoded"
msgstr "Taq kajtz'ik man echolajin ta"

#: stats.framesPresented
msgid "Frames presented"
msgstr "Taq kajtz'ik ek'utun pe"

#: stats.framesPainted
msgid "Frames painted"
msgstr "K'utun taq kajtz'ik"

#: error.aborted
msgid "Video loading stopped."
msgstr "Xpa'e' ri rutzijik silowachib'äl."

#: error.network
msgid "Video playback aborted due to a network error."
msgstr "Xq'at rutzijik silowachib'äl ruma jun sachoj pa ruk'u'x samaj."

#: error.decode
msgid "Video can’t be played because the file is corrupt."
msgstr "Man tikirel ta nitz'et ri silowachib'äl ruma chi itzel ri yakb'äl."

#: error.srcNotSupported
msgid "Video format or MIME type is not supported."
msgstr "Ri rub'anikil o ruwäch"

#: error.noSource2
msgid "No video with supported format and MIME type found."
msgstr ""
"Majun silowachib'äl xelitäj, ri nikixïm ki' pa MIME rub'anikil chuqa' ruwäch."

#: error.generic
msgid "Video playback aborted due to an unknown error."
msgstr "Xpa'e' ri rutzijik silowachib'äl ruma jun sachoj ri man etaman ta."

#. the #1 string is the current
#. media position, and the #2 string is the total duration. For example, when at
#. the 5 minute mark in a 6 hour long video, #1 would be "5:00" and #2 would be
#. "6:00:00", result string would be "5:00 of 6:00:00 elapsed".
#: scrubberScale.nameFormat
msgid "#1 of #2 elapsed"
msgstr "#1 richin #2 k'ulwachitajinäq"
