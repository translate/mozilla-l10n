#: MimeNotCss
msgid ""
"The stylesheet %1$S was not loaded because its MIME type, “%2$S”, is not "
"“text/css”."
msgstr ""
"Ri kixaq b'anikil %1$S man xsamajïx ta ruma chi ri MIME ruwäch, \"%2$S\", "
"man ja ta ri \"text/css\"."

#: MimeNotCssWarn
msgid ""
"The stylesheet %1$S was loaded as CSS even though its MIME type, “%2$S”, is "
"not “text/css”."
msgstr ""
"Ri %1$S ruxaq b'anikil, ke ri' xsamajïx CSS estape' chi ri MIME ruwäch, "
"\"%2$S\", man nuxïm ta ri' rik'in ri \"text/css\"."

#: PEUnexpEOF2
msgid "Unexpected end of file while searching for %1$S."
msgstr "Man oyob'en ta ruk'isib'äl yakb'äl toq tajin nikanöx %1$S."

#: PEParseRuleWSOnly
msgid "Whitespace-only string given to be parsed as rule."
msgstr "Xq'asäx jun cholajil majun rupam achi'el rub'eyal."

#: PEDeclDropped
msgid "Declaration dropped."
msgstr "Xq'at ri q'alajirisanem."

#: PEDeclSkipped
msgid "Skipped to next declaration."
msgstr "Repun kan k'a pa ri jun chik q'alajirisanem."

#: PEUnknownProperty
msgid "Unknown property ‘%1$S’."
msgstr "Man etaman ta ruwäch ri ichinil '%1$S'."

#: PEValueParsingError
msgid "Error in parsing value for ‘%1$S’."
msgstr "Sachoj toq xnik'öx ri rejqalem '%1$S'."

#: PEExpectEndValue
msgid "Expected end of value but found ‘%1$S’."
msgstr "Oyob'en ri ruk'isib'äl rajilab'al xa xe chi xilitäj '%1$S'."

#: PERuleTrailing
msgid "Expected end of rule but found ‘%1$S’."
msgstr "Oyob'en ri ruk'isib'äl b'eyal xa xe chi xilitäj '%1$S'."

#: PESkipAtRuleEOF2
msgid "end of at-rule"
msgstr "ruk'isib'äl at-rule"

#: PEUnknownAtRule
msgid "Unrecognized at-rule or error parsing at-rule ‘%1$S’."
msgstr ""
"Man netamäx ta ruwäch ri b'eyal at o nisach toq nusik'ij ri b'eyal at '%1$S'."

#: PECharsetRuleEOF
msgid "charset string in @charset rule"
msgstr "kicholajil taq tz'ib' pa @charset b'eyal"

#: PECharsetRuleNotString
msgid "Expected charset string but found ‘%1$S’."
msgstr "Oyob'en kicholajil taq tz'ib' xa xe chi xilitäj '%1$S'."

