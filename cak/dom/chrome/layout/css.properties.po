msgstr "ruk'isib'äl @media, @supports o @-moz-document b'eyal"

#: PEGroupRuleNestedAtRule
msgid "%1$S rule not allowed within @media or @-moz-document rule."
msgstr ""
"%1$S b'eyal man ya'on ta q'ij chi re chupam ri @media o @-moz-document "
"b'eyal."

#: PEMozDocRuleBadFunc2
msgid ""
"Expected url(), url-prefix(), domain() or regexp() in @-moz-document rule "
"but found ‘%1$S’."
msgstr ""
"Oyob'en url(), url-prefix(), domain() o regexp() pa ri rub'eyal @-moz-"
"document, xa xe chi xilitäj '%1$S'."

#: PEMozDocRuleNotURI
msgid "Expected URI in @-moz-document rule but found ‘%1$S’."
msgstr "Oyob'en jun URI pa @import b'eyal, xa xe chi xtilitäj '%1$S'."

#: PEMozDocRuleNotString
msgid ""
"Expected string in @-moz-document rule regexp() function but found ‘%1$S’."
msgstr ""
"Oyob'en rucholajil pa @-moz-document rub'eyal samaj regexp() xa xe chi "
"xilitäj '%1$S'."

#: PEMozDocRuleEOF
msgid "next URI in @-moz-document rule"
msgstr "jun chik URI pa @-moz-document rub'eyal"

