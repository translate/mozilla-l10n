#: InvalidChild
msgid "Invalid markup: <%1$S> is not allowed as a child of <%2$S>."
msgstr "Invalid markup: <%1$S> is not allowed as a child of <%2$S>."

#: ChildCountIncorrect
msgid "Invalid markup: Incorrect number of children for <%1$S/> tag."
msgstr "Markup mì-dhligheach: Àireamh chearr a chlann airson an taga <%1$S/>."

#: DuplicateMprescripts
msgid "Invalid markup: More than one <mprescripts/> in <mmultiscripts/>."
msgstr ""
"Markup mì-dhligheach: Barrachd air aon <mprescripts/> ann an <mmultiscripts/"
">."

#. # LOCALIZATION NOTE:  The first child of <mmultiscript/> is the base, that is the element to which scripts are attached.
#: NoBase
msgid ""
"Invalid markup: Expected exactly one Base element in <mmultiscripts/>.  "
"Found none."
msgstr ""
"Markup mì-dhligheach: Bha dùil ri dìreach aon eileamaid Base ann an "
"<mmultiscripts/>.  Cha deach gin a lorg."

#: SubSupMismatch
msgid ""
"Invalid markup: Incomplete subscript/superscript pair in <mmultiscripts/>."
msgstr ""
"Markup mì-dhligheach: Paidhir fho-sgrìobhte/os-sgrìobhte nach eil iomlan ann "
"an <mmultiscripts/>."

#. # LOCALIZATION NOTE:  When localizing the single quotes ('), follow the conventions in css.properties for your target locale.
#: AttributeParsingError
msgid ""
"Error in parsing the value '%1$S' for '%2$S' attribute of <%3$S/>.  "
"Attribute ignored."
msgstr ""
"Mearachd a' parsadh an luach \"%1$S\" airson buadh \"%2$S\" aig <%3$S/>.  "
"Chaidh a bhuadh a leigeil seachad."

#: AttributeParsingErrorNoTag
msgid ""
"Error in parsing the value '%1$S' for '%2$S' attribute.  Attribute ignored."
msgstr ""
"Mearachd a' parsadh an luach \"%1$S\" airson buadh \"%2$S\".  Chaidh a "
"bhuadh a leigeil seachad."

#: LengthParsingError
msgid ""
"Error in parsing MathML attribute value '%1$S' as length.  Attribute ignored."
msgstr ""
"Mearachd a' parsadh luach na buaidh MathML \"%1$S\" mar fhaid.  Chaidh a "
"bhuadh a leigeil seachad."

#: DeprecatedSupersededBy
msgid "'%1$S' is deprecated in MathML 3, superseded by '%2$S'."
msgstr ""
"Cha mholar \"%1$S\" ann am MathML 3 tuilleadh, cleachd \"%2$S\" 'na àite."

#: UnitlessValuesAreDeprecated
msgid "Unitless values are deprecated in MathML 3."
msgstr "Cha mholar luachan gun aonadan tuilleadh ann am MathML 3."

#~ msgid ""
#~ "Invalid markup: Expected at least one subscript/superscript pair in "
#~ "<mmultiscripts/>.  Found none."
#~ msgstr ""
#~ "Markup mì-dhligheach: Bha dùil ri co-dhiù aon phaidhir fho-sgrìobhte/os-"
#~ "sgrìobhte ann an <mmultiscripts/> ach cha do lorg sinn gin."
