#: MimeNotCss
msgid ""
"The stylesheet %1$S was not loaded because its MIME type, \"%2$S\", is not "
"\"text/css\"."
msgstr ""
"Cha deach duilleag nan stoidhle %1$S a luchdadh a chionn 's nach eil a chuir "
"seòrsa MIME, \"%2$S\", agus \"text/css\" co-ionnan."

#: MimeNotCssWarn
msgid ""
"The stylesheet %1$S was loaded as CSS even though its MIME type, \"%2$S\", "
"is not \"text/css\"."
msgstr ""
"Chaidh duilleag nan stoidhle %1$S a luchdadh mar thè CSS ged nach eil a "
"chuid seòrsa MIME, \"%2$S\", agus \"text/css\" co-ionnan."

#: PEUnexpEOF2
msgid "Unexpected end of file while searching for %1$S."
msgstr "Deireadh faidhle ris nach robh dùil fhad 's a bhathar a' lorg %1$S."

#: PEParseRuleWSOnly
msgid "Whitespace-only string given to be parsed as rule."
msgstr ""
"Chaidh sreang anns nach robh ach àite bàn a thoirt seachad airson parsadh "
"mar riaghailt."

#: PEDeclDropped
msgid "Declaration dropped."
msgstr "Chaidh am foirgheall a leagail mu làr."

#: PEDeclSkipped
msgid "Skipped to next declaration."
msgstr "Air leum a thoirt dhan ath-fhoirgheall."

#: PEUnknownProperty
msgid "Unknown property '%1$S'."
msgstr "Feart neo-aithnichte \"%1$S\"."

#: PEValueParsingError
msgid "Error in parsing value for '%1$S'."
msgstr "Mearachd rè parsadh an luach airson '%1$S'."

#: PEExpectEndValue
msgid "Expected end of value but found '%1$S'."
msgstr "Bha dùil ri deireadh an luach ach fhuaradh '%1$S'."

#: PESkipAtRuleEOF
msgid "end of unknown at-rule"
msgstr "deireadh de riaghailt @ neo-aithnichte"

#: PEUnknownAtRule
msgid "Unrecognized at-rule or error parsing at-rule '%1$S'."
msgstr ""
"Riaghailt @ neo-aithnichte no mearachd ann am parsadh riaghailt @ \"%1$S\"."

#: PECharsetRuleEOF
msgid "charset string in @charset rule"
msgstr "sreang charactairean anns an riaghailt @charset"

#: PECharsetRuleNotString
msgid "Expected charset string but found '%1$S'."
msgstr "Bha dùil ri sreang charactairean ach fhuaraidh \"%1$S\"."

#: PEGatherMediaEOF
msgid "end of media list in @import or @media rule"
msgstr "deireadh de liosta meadhain ann an riaghailte @import no @media"

#: PEGatherMediaNotComma
msgid "Expected ',' in media list but found '%1$S'."
msgstr "Bha dùil ri \",\" anns an liosta mheadhain ach fhuaradh \"%1$S\"."

#: PEGatherMediaNotIdent
msgid "Expected identifier in media list but found '%1$S'."
msgstr ""
"Bha dùil ri aithnichear anns an liosta mheadhain ach fhuaradh \"%1$S\"."

#: PEImportNotURI
msgid "Expected URI in @import rule but found '%1$S'."
msgstr "Bha dùil ri URI anns an riaghailt @import ach fhuaradh \"%1$S\"."

#: PEImportBadURI
msgid "Invalid URI in @import rule: '%1$S'."
msgstr "URI mi-dhligheach san riaghailt @import: '%1$S'."

#: PEImportUnexpected
msgid "Found unexpected '%1$S' within @import."
msgstr "Fhuaradh \"%1$S\" am broinn @import gun dùil ris."

#: PEGroupRuleEOF
msgid "end of @media or @-moz-document rule"
msgstr "deireadh na riaghailt @media no @-moz-document"

#: PEGroupRuleNestedAtRule
msgid "%1$S rule not allowed within @media or @-moz-document rule."
msgstr ""
"Chan eil an riaghailt %1$S ceadaichte am broinn riaghailtean @media no @-moz-"
"document."

#: PEMozDocRuleBadFunc
msgid ""
"Expected url(), url-prefix(), or domain() in @-moz-document rule but found "
"'%1$S'."
msgstr ""
"Bha dùil ri url(), url-prefix(), no domain() san riaghailt @-moz-document "
"ach fhuaradh \"%1$S\"."

#: PEMozDocRuleNotURI
msgid "Expected URI in @-moz-document rule but found '%1$S'."
msgstr "Bha dùil ri URI san riaghailt @-moz-document ach fhuaradh \"%1$S\"."

#: PEMozDocRuleNotString
msgid ""
"Expected string in @-moz-document rule regexp() function but found '%1$S'."
msgstr ""
"Bha dùil ri sreang san fhoincsean riaghailt regexp() @-moz-document ach "
"fhuaradh '%1$S'."

#: PEMozDocRuleEOF
msgid "next URI in @-moz-document rule"
msgstr ""

#: PEAtNSPrefixEOF
msgid "namespace prefix in @namespace rule"
msgstr "ro-leasachan namespace anns an riaghailt @namespace"

#: PEAtNSURIEOF
msgid "namespace URI in @namespace rule"
msgstr "URI namespace anns an riaghailt @namespace"

#: PEAtNSUnexpected
msgid "Unexpected token within @namespace: '%1$S'."
msgstr "Tòcan ris nach robh dùil am broinn @namespace: \"%1$S\"."

#: PEKeyframeNameEOF
msgid "name of @keyframes rule."
msgstr "ainm na riaghailt @keyframes."

#: PEKeyframeBadName
msgid "Expected identifier for name of @keyframes rule."
msgstr "Aithnichear ris a bha dùil mar ainm na riaghail @keyframes."

#: PEKeyframeBrace
msgid "Expected opening { of @keyframes rule."
msgstr "\"{\" fosglaidh ris a bha dùil sa riaghailte @keyframes."

#: PESkipDeclBraceEOF
msgid "closing } of declaration block"
msgstr "\"}\" dùnaidh dhen bhloc fhoirghill"

#: PESkipRSBraceEOF
msgid "closing } of invalid rule set"
msgstr "\"}\" dùnaidh de sheata riaghailtean mì-dhligheach"

#: PEBadSelectorRSIgnored
msgid "Ruleset ignored due to bad selector."
msgstr ""
"Chaidh seata de riaghailtean a leigeil seachad air sgàth droch-roghnaicheir."

#: PEBadSelectorKeyframeRuleIgnored
msgid "Keyframe rule ignored due to bad selector."
msgstr ""
"Chaidh riaghailt Keyframe a leigeil seachad air sgàth droch-roghnaicheir."

#: PESelectorListExtraEOF
msgid "',' or '{'"
msgstr "\",\" no \"{\""

#: PESelectorListExtra
msgid "Expected ',' or '{' but found '%1$S'."
msgstr "Bha dùil ri \",\" no \"{\" ach fhuaradh \"%1$S\"."

#: PESelectorGroupNoSelector
msgid "Selector expected."
msgstr "Bha dùil ri roghnaichear."

#: PESelectorGroupExtraCombinator
msgid "Dangling combinator."
msgstr "Co-cheanglaiche air bhogadan."

#: PEClassSelEOF
msgid "class name"
msgstr "ainm clas"

#: PEClassSelNotIdent
msgid "Expected identifier for class selector but found '%1$S'."
msgstr ""
"Bha dùil ri aithnichear airson roghnaichear clas ach fhuaradh \"%1$S\"."

#: PETypeSelEOF
msgid "element type"
msgstr "seòrsa na h-eileamaid"

#: PETypeSelNotType
msgid "Expected element name or '*' but found '%1$S'."
msgstr "Bha dùil ri ainm eileamaid no \"*\" ach fhuaradh \"%1$S\"."

#: PEUnknownNamespacePrefix
msgid "Unknown namespace prefix '%1$S'."
msgstr "Ro-leasachan spàs ainm neo-aithnichte \"%1$S\"."

#: PEAttributeNameEOF
msgid "attribute name"
msgstr "ainm fearta"

#: PEAttributeNameExpected
msgid "Expected identifier for attribute name but found '%1$S'."
msgstr "Bha dùil ri aithnichear airson ainm fearta ach fhuaradh \"%1$S\"."

#: PEAttributeNameOrNamespaceExpected
msgid "Expected attribute name or namespace but found '%1$S'."
msgstr "Bha dùil ri ainm fearta no spàs ainm ach fhuaradh \"%1$S\"."

#: PEAttSelNoBar
msgid "Expected '|' but found '%1$S'."
msgstr "Bha dùil ri \"|\" ach fhuaradh \"%1$S\"."

#: PEAttSelInnerEOF
msgid "part of attribute selector"
msgstr "pàirt de roghnaichear fearta"

#: PEAttSelUnexpected
msgid "Unexpected token in attribute selector: '%1$S'."
msgstr "Tòcan ris nach robh dùil ann an roghnaichear fearta: \"%1$S\"."

#: PEAttSelValueEOF
msgid "attribute value"
msgstr "luach an fhearta"

#: PEAttSelCloseEOF
msgid "']' to end attribute selector"
msgstr "\"]\" gus crìoch achur air an roghnaichear fhearta"

#: PEAttSelNoClose
msgid "Expected ']' to terminate attribute selector but found '%1$S'."
msgstr ""
"Bha dùil ri \"]\" gus crìoch a chur air an roghnaichear fhearta ach fhuaradh "
"\"%1$S\"."

#: PEAttSelBadValue
msgid ""
"Expected identifier or string for value in attribute selector but found "
"'%1$S'."
msgstr ""
"Bha dùil ri aithnichear no sreang airson luach anns an roghnaichear fhearta "
"ach fhuaradh \"%1$S\"."

#: PEPseudoSelEOF
msgid "name of pseudo-class or pseudo-element"
msgstr "ainm clas fudain no eileamaid fhuadain"

#: PEPseudoSelBadName
msgid ""
"Expected identifier for pseudo-class or pseudo-element but found '%1$S'."
msgstr ""
"Bha dùil ri aithnichear airson clas fuadain no eileamaid fhuadain ach "
"fhuaradh \"%1$S\"."

#: PEPseudoSelNonFunc
msgid ""
"Function token for non-function pseudo-class or pseudo-element, or the other "
"way around, when reading '%1$S'."
msgstr ""
"Fhuaradh tòcan feuma an àite clas fuadain no eileamaid fhuadain, no cas mu "
"seach, nuair a bha \"%1$S\" 'ga leughadh."

#: PEPseudoSelNotPE
msgid "Expected pseudo-element but found '%1$S'."
msgstr "Bha dùil ri eileamaid fhuadain ach fhuaradh \"%1$S\"."

#: PEPseudoSelDoubleNot
msgid "Negation pseudo-class can't be negated '%1$S'."
msgstr "Chan urrainnear clas fuadain àicheadh \"%1$S\"."

#: PEPseudoSelPEInNot
msgid "Pseudo-elements can't be negated '%1$S'."
msgstr "Chan urrainnear eileamaid fhuadain àicheadh \"%1$S\"."

#: PEPseudoSelNewStyleOnly
msgid "This pseudo-element must use the \"::\" form: '%1$S'."
msgstr ""
"Feumaidh an eileamaid fhuadain seo a bhith dhen chruth \"::\": \"%1$S\"."

#: PEPseudoSelTrailing
msgid ""
"Found trailing token after pseudo-element, which must be the last part of a "
"selector:  '%1$S'."
msgstr ""
"Fhuaraidh tòcan air dheireadh as dèidh eileamaid fhuadain ach feumaidh e a "
"bhith 'na phàirt dheireannach de roghnaichear:  \"%1$S\"."

#: PEPseudoSelMultiplePE
msgid "Extra pseudo-element '%1$S'."
msgstr "Eileamaid fhuadain a bharrachd \"%1$S\"."

#: PEPseudoSelUnknown
msgid "Unknown pseudo-class or pseudo-element '%1$S'."
msgstr "Clas fuadain no eileamaid fhuadain neo-aithnichte: \"%1$S\"."

#: PENegationEOF
msgid "selector within negation"
msgstr "roghnaichear am broinn an àichidh"

#: PENegationBadInner
msgid "Malformed simple selector as negation pseudo-class argument '%1$S'."
msgstr ""
"Roghnaichear simplidh droch chumte mar argamaid airson a' chlas fhuadain "
"àichidh \"%1$S\"."

#: PENegationNoClose
msgid "Missing closing ')' in negation pseudo-class '%1$S'."
msgstr "\")\" dùnaidh a dhìth anns a' chlas fhuadain àichidh \"%1$S\"."

#: PENegationBadArg
msgid "Missing argument in negation pseudo-class '%1$S'."
msgstr "Argamaid a dhìth anns a' chlas fhuadain àichidh \"%1$S\"."

#: PEPseudoClassArgEOF
msgid "argument to pseudo-class selector"
msgstr "argamaid do roghnaichear clas fuadain"

#: PEPseudoClassArgNotIdent
msgid "Expected identifier for pseudo-class parameter but found '%1$S'."
msgstr ""
"Bha dùil ri aithnichear do pharamadair clas fuadain ach fhuaradh \"%1$S\"."

#: PEPseudoClassArgNotNth
msgid "Expected part of argument to pseudo-class but found '%1$S'."
msgstr ""
"Bha dùil ri pàirt de dh'argamaid do chlas fuadain ach fhuaradh \"%1$S\"."

#: PEPseudoClassNoClose
msgid "Missing closing ')' in pseudo-class, found '%1$S' instead."
msgstr "\")\" dùnaidh a dhìth ann an clas fuadain, fhuaradh \"%1$S\" na àite."

#: PEPseudoClassNoArg
msgid "Missing argument in pseudo-class '%1$S'."
msgstr "Argamaid a dhìth anns a' chlas fhuadain \"%1$S\"."

#: PESelectorEOF
msgid "selector"
msgstr "roghnaichear"

#: PEBadDeclBlockStart
msgid "Expected '{' to begin declaration block but found '%1$S'."
msgstr "Bha dùil ri \"{\" mar thùs bloc an fhoirghill ach fhuaradh \"%1$S\"."

#: PEColorEOF
msgid "color"
msgstr "dath"

#: PEColorNotColor
msgid "Expected color but found '%1$S'."
msgstr "Bha dùil ri dath ach fhuaradh \"%1$S\"."

#: PEColorComponentEOF
msgid "color component"
msgstr "co-phàirt datha"

#: PEExpectedPercent
msgid "Expected a percentage but found '%1$S'."
msgstr "Bha dùil ri ceudad ach fhuaradh \"%1$S\"."

#: PEExpectedInt
msgid "Expected an integer but found '%1$S'."
msgstr "Bha dùil ri àireamh shlàn ach fhuaradh \"%1$S\"."

#: PEColorBadRGBContents
msgid "Expected number or percentage in rgb() but found '%1$S'."
msgstr "Bha dùil ri àireamh no ceudad ann an rgb() ach fhuaradh \"%1$S\"."

#: PEColorComponentBadTerm
msgid "Expected '%2$S' but found '%1$S'."
msgstr "Bha dùil ri \"%2$S\" ach fhuaradh \"%1$S\"."

#: PEColorHueEOF
msgid "hue"
msgstr "tuar"

#: PEExpectedComma
msgid "Expected ',' but found '%1$S'."
msgstr "Bha dùil ri \",\" ach fhuaradh \"%1$S\"."

#: PEColorSaturationEOF
msgid "saturation"
msgstr "sàthachd"

#: PEColorLightnessEOF
msgid "lightness"
msgstr "gile"

#: PEColorOpacityEOF
msgid "opacity in color value"
msgstr "dubharachd ann an luach an datha"

#: PEExpectedNumber
msgid "Expected a number but found '%1$S'."
msgstr "Bha dùil ri àireamh ach fhuaradh \"%1$S\"."

#: PEExpectedCloseParen
msgid "Expected ')' but found '%1$S'."
msgstr "Bha dùil ri \")\" ach fhuaradh \"%1$S\"."

#: PEDeclEndEOF
msgid "';' or '}' to end declaration"
msgstr "\";\" no \"}\" gus crìoch a chur air foirgheall"

#: PEParseDeclarationNoColon
msgid "Expected ':' but found '%1$S'."
msgstr "Bha dùil ri \":\" ach fhuaradh \"%1$S\"."

#: PEParseDeclarationDeclExpected
msgid "Expected declaration but found '%1$S'."
msgstr "Bha dùil ri foirgheall ach fhuaradh \"%1$S\"."

#: PEEndOfDeclEOF
msgid "end of declaration"
msgstr "deireadh an fhoirghill"

#: PEImportantEOF
msgid "important"
msgstr "cudromach"

#: PEExpectedImportant
msgid "Expected 'important' but found '%1$S'."
msgstr "Bha dùil ri \"important\" ach fhuaradh \"%1$S\"."

#: PEBadDeclEnd
msgid "Expected ';' to terminate declaration but found '%1$S'."
msgstr ""
"Bha dùil ri \";\" gus crìoch a chur air an fhoirgheall ach fhuaradh \"%1$S\"."

#: PEBadDeclOrRuleEnd2
msgid "Expected ';' or '}' to terminate declaration but found '%1$S'."
msgstr ""
"Bha dùil ri \";\" no \"}\" gus crìoch a chur air an fhoirgheall ach fhuaradh "
"\"%1$S\"."

#: PEInaccessibleProperty2
msgid "Cannot specify value for internal property."
msgstr "Cha ghabh luach airson in-fheart a shònrachadh."

#: PECommentEOF
msgid "end of comment"
msgstr "deireadh an luaidh"

#: SEUnterminatedString
msgid "Found unclosed string '%1$S'."
msgstr "Fhuaradh sreang gun dùnadh: \"%1$S\"."

#: PEFontDescExpected
msgid "Expected font descriptor but found '%1$S'."
msgstr "Bha dùil ri tuairisgeulaiche crutha-clò ach fhuaradh '%1$S'."

#: PEUnknownFontDesc
msgid "Unknown descriptor '%1$S' in @font-face rule."
msgstr "Tuairisgeulaiche neo-aithnichte '%1$S' anns an righailte @font-face."

#: PEMQExpectedExpressionStart
msgid "Expected '(' to start media query expression but found '%1$S'."
msgstr ""
"Bha dùil ri '(' mar thoiseach abairt an iarrtais mheadhain ach fhuaradh "
"'%1$S'."

#: PEMQExpressionEOF
msgid "contents of media query expression"
msgstr "susbaint abairt an iarrtais mheadhain"

#: PEMQExpectedFeatureName
msgid "Expected media feature name but found '%1$S'."
msgstr "Bha dùil ri ainm de dh'fheart meadhain ach fhuaradh '%1$S'."

#: PEMQExpectedFeatureNameEnd
msgid "Expected ':' or ')' after media feature name but found '%1$S'."
msgstr ""
"Bha dùil ri ':' no ')' as dèidh ainm an fheart mheadhain ach fhuaradh '%1$S'."

#: PEMQNoMinMaxWithoutValue
msgid "Media features with min- or max- must have a value."
msgstr "Feumaidh luach a bhith aig feartean meadhain le min- no max-."

#: PEMQExpectedFeatureValue
msgid "Found invalid value for media feature."
msgstr "Fhuaradh luach mì-dhligheach airson feart meadhain."

#: PEBadFontBlockStart
msgid "Expected '{' to begin @font-face rule but found '%1$S'."
msgstr ""
"Bha dùil ri '{' aig toiseach na riaghailt @font-face ach fhuaradh '%1$S'."

#: PEBadFontBlockEnd
msgid "Expected '}' to end @font-face rule but found '%1$S'."
msgstr ""
"Bha dùil ri '}' aig deireadh na riaghailt @font-face ach fhuaradh '%1$S'."

#: PEAnonBoxNotAlone
msgid "Did not expect anonymous box."
msgstr "Cha robh dùil ri bogsa gun urra."
