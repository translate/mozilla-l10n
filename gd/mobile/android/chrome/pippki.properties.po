#: nssdialogs.ok.label
msgid "OK"
msgstr "Ceart ma-tha"

#: nssdialogs.cancel.label
msgid "Cancel"
msgstr "Sguir dheth"

#. # These strings are stolen from security/manager/locales/en-US/chrome/pippki/pippki.dtd
#: downloadCert.title
msgid "Downloading Certificate"
msgstr "A' luchdadh a-nuas teisteanas"

#: downloadCert.message1
msgid "You have been asked to trust a new Certificate Authority (CA)."
msgstr ""
"Chaidh iarraidh ort earbsa a chur ann an ùghdarras teisteanachaidh (ÙT) ùr."

#: downloadCert.viewCert.label
msgctxt "downloadCert.viewCert.label"
msgid "View"
msgstr "Seall"

#: downloadCert.trustSSL
msgid "Trust to identify websites."
msgstr "Cuir earbsa ann gus làraichean-lìn aithneachadh."

#: downloadCert.trustEmail
msgid "Trust to identify email users."
msgstr "Cuir earbsa ann gus cleachdaichean puist-dhealain aithneachadh."

#: downloadCert.trustObjSign
msgid "Trust to identify software developers."
msgstr "Cuir earbsa ann gus luchd-leasachaidh bathar-bog aithneachadh."

#: caCertExists.title
msgid "Certificate Exists"
msgstr "Tha teisteanas ann"

#: caCertExists.message
msgid "The Certificate already exists."
msgstr "Tha an teisteanas seo ann mu thràth."

#: pkcs12.getpassword.title
msgid "Password Entry Dialog"
msgstr "Còmhradh cur an fhacail-fhaire"

#: pkcs12.getpassword.message
msgid ""
"Please enter the password that was used to encrypt this certificate backup."
msgstr ""
"Cuir a-steach am facal-faire a chleachd thu gus lethbhreac-glèidhidh an "
"teisteanais seo a chrioptachadh."

#: clientAuthAsk.title
msgid "User Identification Request"
msgstr "Iarrtas aithneachadh cleachdaiche"

#: clientAuthAsk.message1
msgid "This site has requested that you identify yourself with a certificate:"
msgstr "Dh'iarr an làrach ort aithne a thoirt ort fhèin le teisteanas:"

#: clientAuthAsk.message2
msgid "Choose a certificate to present as identification:"
msgstr "Tagh teisteanas a nochdas tu mar dhearbh-aithne:"

#: clientAuthAsk.message3
msgid "Details of selected certificate:"
msgstr "Mion-fhiosrachadh an teisteanais a thagh thu:"

#: clientAuthAsk.remember.label
msgid "Remember this decision"
msgstr "Cuimhnich an co-dhùnadh seo"

#: clientAuthAsk.organization
msgid "Organization: \"%S\""
msgstr "Buidheann: \"%S\""

#: clientAuthAsk.issuer
msgid "Issued Under: \"%S\""
msgstr "Air fhoillseachadh fo: \"%S\""

#: clientAuthAsk.viewCert.label
msgctxt "clientAuthAsk.viewCert.label"
msgid "View"
msgstr "Seall"

#: certmgr.title
msgid "Certificate Details"
msgstr "Mion-fhiosrachadh an teisteanais"

#. # These strings are stolen from security/manager/locales/en-US/chrome/pippki/certManager.dtd
#: certmgr.subjectinfo.label
msgid "Issued To"
msgstr "Air fhoillseachadh do"

#: certmgr.issuerinfo.label
msgid "Issued By"
msgstr "Air fhoillseachadh le"

#: certmgr.periodofvalidity.label
msgid "Period of Validity"
msgstr "Dè cho fad 's a bhios e dligheach"

#: certmgr.fingerprints.label
msgid "Fingerprints"
msgstr "Lorgan-meur"

#: certmgr.certdetail.cn
msgid "Common Name (CN)"
msgstr "Ainm Coitcheann (Common Name, CN)"

#: certmgr.certdetail.o
msgid "Organization (O)"
msgstr "Buidheann (Organisation, O)"

#: certmgr.certdetail.ou
msgid "Organizational Unit (OU)"
msgstr "Aonad na buidhne (Organisational Unit, OU)"

#: certmgr.certdetail.serialnumber
msgid "Serial Number"
msgstr "Àireamh shreathach"

#: certmgr.certdetail.sha256fingerprint
msgid "SHA-256 Fingerprint"
msgstr "Lorg-meur SHA-256"

#: certmgr.certdetail.sha1fingerprint
msgid "SHA1 Fingerprint"
msgstr "Lorg-meur SHA1"

#: certmgr.begins
msgid "Begins On"
msgstr "Tòisichidh e"

#: certmgr.expires
msgid "Expires On"
msgstr "Falbhaidh an ùine air"

