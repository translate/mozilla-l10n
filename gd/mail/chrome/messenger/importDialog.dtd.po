msgid "Import"
msgstr "Ion-phortaich"

#: importAll.label importAll.accesskey
msgid "Import &Everything"
msgstr "Ion-phortaich a h-uil&e rud"

#: importMail.label importMail.accesskey
msgid "&Mail"
msgstr "&Post"

#: importFeeds.label importFeeds.accesskey
msgid "Fee&d Subscriptions"
msgstr "Fo-sgrìobhai&dhean inbhirean"

#: importAddressbook.label importAddressbook.accesskey
msgid "&Address Books"
msgstr "Le&abhraichean nan seòladh"

#: importSettings.label importSettings.accesskey
msgid "&Settings"
msgstr "&Roghainnean"

