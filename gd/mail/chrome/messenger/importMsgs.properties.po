#. #Error strings
#: ImportAlreadyInProgress
msgid ""
"An import operation is currently in progress. Try again when the current "
"import has finished."
msgstr ""
"Tha gnìomh cudromach 'ga dhèanamh an-dràsta fhèin. Feuch ris a-rithist nuair "
"a bhios an t-ion-phortadh làithreach deiseil."

#. #Error strings for settings import
#: ImportSettingsBadModule
msgid "Unable to load settings module"
msgstr "Cha ghabh mòideal nan roghainnean a luchdadh"

#: ImportSettingsNotFound
msgid ""
"Unable to find settings.  Check to make sure the application is installed on "
"this machine."
msgstr ""
"Cha ghabh na roghainnean a lorg. Dèan cinnteach gu bheil am prògram air a "
"stàladh air an inneal seo."

#: ImportSettingsFailed
msgid ""
"An error occurred while importing settings.  Some, or all, of the settings "
"may not have been imported."
msgstr ""
"Thachair mearachd fhad 's a chaidh na roghainnean ion-phortadh. Dh'fhaodadh "
"nach deach a h-uile, no tè sam bith, dhe na roghainnean ion-phortadh."

#. # LOCALIZATION NOTE : Do not translate the word "%S" below.
#: ImportSettingsSuccess
msgid "Settings were imported from %S"
msgstr "Chaidh na roghainnean ion-phortadh o %S"

#. #Error string for mail import
#: ImportMailBadModule
msgid "Unable to load mail import module"
msgstr "Cha ghabh mòideal ion-phortadh a' phuist a luchdadh"

#: ImportMailNotFound
msgid ""
"Unable to find mail to import.  Check to make sure the mail application is "
"correctly installed on this machine."
msgstr ""
"Cha deach post a lorg a ghabhas ion-phortadh. Dèan cinnteach gu bheil am "
"prògram puist air a stàladh mar bu chòir air an inneal seo."

#: ImportEmptyAddressBook
msgid "Can't import empty address book %S."
msgstr ""
"Cha ghabh leabhar nan seòladh %S ion-phortadh a chionn 's gu bheil e falamh."

#. # LOCALIZATION NOTE: Do not translate the word "%S" below.
#: ImportMailFailed
msgid "An error occurred importing mail from %S"
msgstr "Thachair mearachd fhad 's a chaidh post ion-phortadh o %S"

#. # LOCALIZATION NOTE: Do not translate the word "%S" below.
#: ImportMailSuccess
msgid "Mail was successfully imported from %S"
msgstr "Chaidh am post ion-phortadh o %S gu soirbheachail"

#. # Error string for address import
#: ImportAddressBadModule
msgid "Unable to load address book import module."
msgstr "Cha ghabh mòideal ion-phortadh leabhar nan seòladh a luchdadh."

#: ImportAddressNotFound
msgid ""
"Unable to find any address books to import.  Check to make sure the selected "
"application or format is correctly installed on this machine."
msgstr ""
"Cha deach leabhar nan seòladh sam bith a lorg a ghabhas ion-phortadh. Dèan "
"cinntach gu bheil am prògram no am fòrmat a thagh thu air a stàladh mar bu "
"chòir air an inneal seo."

#. # LOCALIZATION NOTE : Do not translate the word "%S" below.
#: ImportAddressFailed
msgid "An error occurred importing addresses from %S."
msgstr "Thachair mearachd fhad 's a chaidh na seòlaidhean ion-phortadh o %S."

#. # LOCALIZATION NOTE : Do not translate the word "%S" below.
#: ImportAddressSuccess
msgid "Addresses successfully imported from %S."
msgstr "Chaidh na seòlaidhean ion-phortadh o %S gu soirbheachail."

#. # Error string for filters import
#: ImportFiltersBadModule
msgid "Unable to load filters import module."
msgstr "Cha ghabh mòideal ion-phortadh nan criathragan a luchdadh."

#. # LOCALIZATION NOTE : The %S will get replaced by the name of the import module.
#: ImportFiltersFailed
msgid "An error occurred importing filters from %S."
msgstr "Thachair mearachd fhad 's a chaidh na criathragan ion-phortadh o %S."

#. # LOCALIZATION NOTE : The %S will get replaced by the name of the import module.
#: ImportFiltersSuccess
msgid "Filters successfully imported from %S."
msgstr "Chaidh na criathragan ion-phortadh gu soirbheachail o %S."

#. # LOCALIZATION NOTE : The %S will get replaced by the name of the import module.
#: ImportFiltersPartial
msgid "Filters partially imported from %S.  Warnings below:"
msgstr ""
"Chaidh cuid dhe na criathragan ion-phortadh o %S. Rabhaidhean gu h-ìosal:"

