#: reloadPlugin.middle
msgid "Reload the page"
msgstr "Ath-luchdaich an duilleag"

#: reloadPlugin.post
msgid " to try again."
msgstr " gus feuchainn ris a-rithist."

#. This and the other report.* strings should be as short as possible, ideally 2-3 words.
#: report.please
msgid "Send crash report"
msgstr "Cuir a-steach aithisg tuislidh"

#: report.submitting
msgid "Sending report…"
msgstr "A’ cur a-null na h-aithisge…"

#: report.submitted
msgid "Crash report sent."
msgstr "Chaidh an aithisg tuislidh a chur a-null."

#: report.disabled
msgid "Crash reporting disabled."
msgstr "Tha aithisgean air tuislidhean à comas."

#: report.failed
msgid "Submission failed."
msgstr "Dh’fhàillig a chur a-null."

#: report.unavailable
msgid "No report available."
msgstr "Chan eil aithisg ann."

#: report.comment
msgid "Add a comment (comments are publicly visible)"
msgstr "Cuir beachd ris (chithear na beachdan gu poblach)"

#: report.pageURL
msgid "Include the page's URL"
msgstr "Gabh a-steach URL na duilleige"

#: plugin.file
msgid "File"
msgstr "Faidhle"

#: plugin.mimeTypes
msgid "MIME Types"
msgstr "Seòrsachan MIME"

#: plugin.flashProtectedMode.label
msgid "Enable Adobe Flash protected mode"
msgstr "Cuir an comas modh dìonta Adobe Flash"
