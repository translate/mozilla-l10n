#: playButton.playLabel
msgid "Play"
msgstr "Cluich"

#: playButton.pauseLabel
msgid "Pause"
msgstr "Cuir 'na stad"

#: muteButton.muteLabel
msgid "Mute"
msgstr "Tostaich"

#: muteButton.unmuteLabel
msgid "Unmute"
msgstr "Till an fhuaim"

#: fullscreenButton.enterfullscreenlabel
msgid "Full Screen"
msgstr "Làn-sgrìn"

#: fullscreenButton.exitfullscreenlabel
msgid "Exit Full Screen"
msgstr "Fàg an làn-sgrìn"

#: castingButton.castingLabel
msgid "Cast to Screen"
msgstr "Tilg air an sgrìn"

#: stats.media
msgid "Media"
msgstr "Meadhanan"

#. Do not translate this.  Only change the numeric values if you need this dialogue box to appear bigger
#: stats.size
msgid "Size"
msgstr "Meud"

#: stats.activity
msgid "Activity"
msgstr "Gnìomhachd"

#: stats.activityPaused
msgid "Paused"
msgstr "'Na stad"

#: stats.activityPlaying
msgid "Playing"
msgstr "'Ga chluich"

#: stats.activityEnded
msgid "Ended"
msgstr "Crìochnaichte"

#: stats.activitySeeking
msgid "(seeking)"
msgstr "('ga shireadh)"

#: stats.volume
msgid "Volume"
msgstr "Àirde na fuaime"

#: stats.framesParsed
msgid "Frames parsed"
msgstr "Frèamaichean air am parsadh"

#: stats.framesDecoded
msgid "Frames decoded"
msgstr "Frèamaichean air an dì-chòdachadh"

#: stats.framesPresented
msgid "Frames presented"
msgstr "Frèamaichean air an sealltainn"

#: stats.framesPainted
msgid "Frames painted"
msgstr "Frèamaichean air am peantadh"

#: error.aborted
msgid "Video loading stopped."
msgstr "Sguireadh de luchdadh a' video."

#: error.network
msgid "Video playback aborted due to a network error."
msgstr "Sguireadh cluich a' video air sgàth mearachd lìonraidh."

#: error.decode
msgid "Video can't be played because the file is corrupt."
msgstr "Cha ghabh a' video a chluich a chionn 's gu bheil am faidhle coirbte."

#: error.srcNotSupported
msgid "Video format or MIME type is not supported."
msgstr "Chan eil taic ann ri fòrmat a' video no an seòrsa MIME."

#: error.noSource2
msgid "No video with supported format and MIME type found."
msgstr ""
"Cha deach video a lorg aig a bheil fòrmat no seòrsa MIME a tha taic ris."

#: error.generic
msgid "Video playback aborted due to an unknown error."
msgstr "Sguireadh de chluich a' video air sgàth mearachd neo-aithnichte."

#. the #1 string is the current
#. media position, and the #2 string is the total duration. For example, when at
#. the 5 minute mark in a 6 hour long video, #1 would be "5:00" and #2 would be
#. "6:00:00", result string would be "5:00 of 6:00:00 elapsed".
#: scrubberScale.nameFormat
msgid "#1 of #2 elapsed"
msgstr "#1 seachad de #2"

