#: pane.title
msgid "Return Receipts"
msgstr "Antwoordstrokies"

#: useGlobalPrefs.label
#: useGlobalPrefs.accesskey
msgid "&Use my global return receipt preferences for this account"
msgstr "&Gebruik my algemene antwoordstrokie-voorkeure vir hierdie rekening"

#: useCustomPrefs.label
#: useCustomPrefs.accesskey
msgid "&Customize return receipts for this account"
msgstr "Pasmaak antwoordstrokies vir hierdie rekening"

#: requestReceipt.label
#: requestReceipt.accesskey
msgid "&When sending messages, always request a return receipt"
msgstr "&Wanneer boodskappe versend word, vra altyd 'n antwoordstrokie aan"

#: receiptArrive.label
msgid "When a receipt arrives:"
msgstr "Wanneer 'n antwoordstrokie aankom:"

#: leaveIt.label
#: leaveIt.accesskey
msgid "Leave it in my &Inbox"
msgstr "Los dit in my &inmandjie"

# LOCALIZATION NOTE moveToSent.label Translate: 'Sent' according to Netscape glossary
#: moveToSent.label
#: moveToSent.accesskey
msgid "&Move it to my &quot;Sent&quot; folder"
msgstr "&Verskuif dit na my &quot;Gestuur&quot;-vouer"

#: requestMDN.label
msgid "When I receive a request for a return receipt:"
msgstr "Wanneer ek 'n versoek om antwoordstrokie ontvang:"

#: returnSome.label
#: returnSome.accesskey
msgid "Allow r&eturn receipts for some messages"
msgstr "Laat &antwoordstrokies vir sommige boodskappe toe"

#: never.label
#: never.accesskey
msgid "&Never send a return receipt"
msgstr "&Moet nooit 'n antwoordstrokie stuur nie"

#: notInToCc.label
#: notInToCc.accesskey
msgid "If I'm not in the &To or Cc of the message:"
msgstr "As ek nie in die &Aan- of CC-veld van die boodskap is nie:"

#: outsideDomain.label
#: outsideDomain.accesskey
msgid "If the &sender is outside my domain:"
msgstr "As die &afsender van buite my domein afkomstig is:"

#: otherCases.label
#: otherCases.accesskey
msgid "In all &other cases:"
msgstr "In alle &ander gevalle:"

#: askMe.label
msgid "Ask me"
msgstr "Vra my"

#: alwaysSend.label
msgid "Always send"
msgstr "Stuur altyd"

#: neverSend.label
msgid "Never send"
msgstr "Stuur nooit"

