#. # LOCALIZATION NOTE (downloadsTitleFiles): Semi-colon list of plural forms.
#. # See: http://developer.mozilla.org/en/docs/Localization_and_Plurals
#. # #1 number of relevant days with crash reports
#: crashesTitle
msgid "Crash Reports for the Last #1 Day;Crash Reports for the Last #1 Days"
msgstr ""
"Omvalverslae vir die afgelope #1 dag;Omvalverslae vir die afgelope #1 dae"

#. # LOCALIZATION NOTE (crashesTimeMinutes): Semi-colon list of plural forms.
#. # See: http://developer.mozilla.org/en/docs/Localization_and_Plurals
#. # #1 number of minutes (between 1 and 59) which have passed since the crash
#: crashesTimeMinutes
msgid "#1 minute ago;#1 minutes ago"
msgstr "#1 minuut gelede;#1 minute gelede"

#. # LOCALIZATION NOTE (crashesTimeHours): Semi-colon list of plural forms.
#. # See: http://developer.mozilla.org/en/docs/Localization_and_Plurals
#. # #1 number of hours (between 1 and 23) which have passed since the crash
#: crashesTimeHours
msgid "#1 hour ago;#1 hours ago"
msgstr "#1 uur gelede;#1 ure gelede"

#. # LOCALIZATION NOTE (crashesTimeDays): Semi-colon list of plural forms.
#. # See: http://developer.mozilla.org/en/docs/Localization_and_Plurals
#. # #1 number of days (1 or more) which have passed since the crash
#: crashesTimeDays
msgid "#1 day ago;#1 days ago"
msgstr "#1 dag gelede;#1 dae gelede"

#. # LOCALIZATION NOTE (downloadsTitleFiles): Semi-colon list of plural forms.
#. # See: http://developer.mozilla.org/en/docs/Localization_and_Plurals
#. # #1 number of pending crash reports
#: pendingReports
msgid ""
"All Crash Reports (including #1 pending crash in the given time range);All "
"Crash Reports (including #1 pending crashes in the given time range)"
msgstr ""
"Alle omvalverslae (insluitend #1 uitstaande omval in die gegewe tydperk);"
"Alle omvalverslae (insluitend #1 uitstaande omvalle in die gegewe tydperk)"

