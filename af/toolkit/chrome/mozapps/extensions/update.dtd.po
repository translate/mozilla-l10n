msgid "&brandShortName; can check for compatible versions of these add-ons."
msgstr ""

#: checking.title
msgid "Checking for Updates"
msgstr "Kontroleer vir opdaterings"

#: checking.intro.label
msgid "&brandShortName; is now checking for available updates..."
msgstr "&brandShortName; kontroleer nou vir beskikbare opdaterings..."

#: checking.status
msgid "This may take a few minutes..."
msgstr ""

#: found.title
msgid "Updates Found"
msgstr "Opdaterings gevind"

#: found.intro.label
msgid "&brandShortName; found the following available updates:"
msgstr "&brandShortName; het die volgende opdaterings gevind:"

#: from.label
msgid "from: "
msgstr "van: "

#: updateFound.addons.accesskey
msgid "A"
msgstr "A"

