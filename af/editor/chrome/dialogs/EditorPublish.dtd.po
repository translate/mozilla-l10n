# Window title
#: windowTitle.label
msgid "Publish Page"
msgstr "Publiseer bladsy"

#: windowTitleSettings.label
msgid "Publish Settings"
msgstr "Publiseer-opstelling"

#: publishTab.label
msgid "_: publishTab.label\n"
"Publish"
msgstr ""
"Publiseer"

#: settingsTab.label
msgid "Settings"
msgstr "Opstelling"

#: publishButton.label
msgid "_: publishButton.label\n"
"Publish"
msgstr ""
"Publiseer"

# Publish Tab Panel
#: siteList.label
msgid "_: siteList.label\n"
"Site Name:"
msgstr ""
"Werfnaam:"

#: siteList.tooltip
msgid "Choose the site that you want to publish to"
msgstr "Kies die werf waarheen jy wil publiseer"

#: newSiteButton.label
msgid "New Site"
msgstr "Nuwe werf"

#: siteDirList.label
msgid "Select or type a directory name in your publishing site:"
msgstr "Kies of tik 'n gidsnaam in jou publiseerwerf in:"

#: siteDirList.tooltip
msgid "Choose or enter the name of the remote subdirectory to publish to"
msgstr "Kies of tik die naam van die afgeleë subgids waarin gepubliseer moet word, in"

#: docDirList.label
msgid "Site subdirectory for this page:"
msgstr "Werfsubgids vir hierdie bladsy:"

#: publishImgCheckbox.label
msgid "Include images and other files"
msgstr "Sluit prente en ander lêers in"

#: publishImgCheckbox.tooltip
msgid "Publish images and other files referenced by this page"
msgstr "Publiseer prente en ander lêers waarna hierdie bladsy verwys"

#: sameLocationRadio.label
msgid "Use same location as page"
msgstr "Gebruik dieselfde ligging as bladsy"

#: sameLocationRadio.tooltip
msgid "Publish files to the same location as the page"
msgstr "Publiseer lêers na dieselfde ligging as die bladsy"

#: useSubdirRadio.label
msgid "Use this site subdirectory:"
msgstr "Gebruik hierdie werfsubgids:"

#: useSubdir.tooltip
msgid "Choose or enter name of remote subdirectory where files will be published"
msgstr "Kies of tik naam van afgeleë subgids in waarin lêers gepubliseer gaan word"

#: chooseDirButton.label
msgid "Choose..."
msgstr "Kies..."

#: pageTitle.label
msgid "Page Title:"
msgstr "Bladsytitel:"

#: pageTitle.tooltip
msgid "Enter a title to identify the page in the window and in bookmarks"
msgstr "Tik 'n titel in om die bladsy te identifiseer in die venstertitel en boekmerke."

#: pageTitleExample.label
msgid "e.g.: &quot;My Web Page&quot;"
msgstr "bv: &quot;My webwerf&quot;"

#: filename.label
msgid "Filename:"
msgstr "Lêernaam:"

#: filename.tooltip
msgid "Enter a name for this file, including '.html' for a web page"
msgstr "Tik 'n naam vir hierdie lêer in, insluitend  '.html' vir 'n webwerf"

#: filenameExample.label
msgid "e.g,: &quot;mypage.html&quot;"
msgstr "bv: &quot;mywerf.html&quot;"

#: relatedDocs.label
msgid "Other files to publish"
msgstr "Ander lêers om te publiseer"

#: setDefaultButton.label
msgid "Set as Default"
msgstr "Kies as verstek"

#: setDefSiteAndDir.label
msgid "Set as Default Site and Directory"
msgstr "Stel op as verstek-werf en -subgids"

#: removeButton.label
msgid "Remove Site"
msgstr "Verwyder werf"

# Settings Tab Panel
#: publishSites.label
msgid "Publishing Sites"
msgstr "Publiseer werwe"

#: serverInfo.label
msgid "Server Information"
msgstr "Bedienerinligting"

#: loginInfo.label
msgid "Login Information"
msgstr "Aanmeldinligting"

#: siteName.label
msgid "_: siteName.label\n"
"Site Name:"
msgstr ""
"Werfnaam:"

#: siteName.tooltip
msgid "A nickname that identifies this publishing site (e.g.: 'MySite')"
msgstr "'n Kennaam wat hierdie publiseerwerf identifiseer (bv: 'MyWerf')"

#: siteUrl.label
msgid "Publishing address (e.g.: 'ftp://ftp.myisp.com/myusername'):"
msgstr "Publiseeradres (bv: 'ftp://ftp.myisp.com/mygebruikernaam'):"

#: siteUrl.tooltip
msgid "The FTP:// or HTTP:// address provided by your ISP or web hosting service"
msgstr "Die FTP://- of HTTP://-adres wat deur jou ISP of webgasheer verskaf is"

#: browseUrl.label
msgid "HTTP address of your homepage (e.g.: 'http://www.myisp.com/myusername'):"
msgstr "HTTP-adres van jou tuisblad (bv: 'http://www.myisp.com/mygebruikernaam'):"

#: browseUrl.tooltip
msgid "The HTTP:// address of your home directory (don't include filename)"
msgstr "Die HTTP://-adres van jou tuissubgids (moenie lêernaam insluit nie)"

#: username.label
msgid "User name:"
msgstr "Gebruikernaam:"

#: username.tooltip
msgid "The user name you use to log in to your ISP or web hosting service"
msgstr "Die gebruikernaam waarmee jy by jou ISP of webgasheer aanmeld"

#: password.label
msgid "Password:"
msgstr "Wagwoord:"

#: password.tooltip
msgid "The password associated with your user name"
msgstr "Die wagwoord wat by jou gebruikernaam pas"

#: savePassword.label
msgid "Save Password"
msgstr "Stoor wagwoord"

#: savePassword.tooltip
msgid "Select this to save your password securely using Password Manager"
msgstr "Selekteer dit om jou wagwoord beveilig met die wagwoordbestuurder te stoor"

