#: 1
msgid "Parsing an XSLT stylesheet failed."
msgstr "Kon nie 'n XSLT-stylvel ontleed nie."

#: 2
msgid "Parsing an XPath expression failed."
msgstr "Kon nie 'n XPath-uitdrukking ontleed nie."

#: 4
msgid "XSLT transformation failed."
msgstr "XSLT-transformasie het misluk."

#: 5
msgid "Invalid XSLT/XPath function."
msgstr "Ongeldige XSLT/XPath-funksie."

#: 6
msgid "XSLT Stylesheet (possibly) contains a recursion."
msgstr "XSLT-stylvel bevat (moontlik) 'n terugwerking."

#: 7
msgid "Attribute value illegal in XSLT 1.0."
msgstr "Attribuutwaarde onwettig in XSLT 1.0."

#: 8
msgid "An XPath expression was expected to return a NodeSet."
msgstr "'n Xpath-uitdrukking moes 'n NodeSet opgelewer het."

#: 9
msgid "XSLT transformation was terminated by <xsl:message>."
msgstr "XSLT-transformasie is deur <xsl:message> beëindig."

#: 10
msgid "A network error occured loading an XSLT stylesheet:"
msgstr "'n Netwerkfout het ontstaan toe 'n XSLT-stylvel gelaai het:"

#: 11
msgid "An XSLT stylesheet does not have an XML mimetype:"
msgstr "'n XSLT-stylvel het nie 'n XML-mimesoort nie:"

#: 12
msgid "An XSLT stylesheet directly or indirectly imports or includes itself:"
msgstr "'n XSLT stylvel voer of sluit homself direk of indirek in:"

#: 13
msgid "An XPath function was called with the wrong number of arguments."
msgstr "'n XPath-funksie is geroep met die verkeerde getal argumente."

#: 14
msgid "An unknown XPath extension function was called."
msgstr "'n Onbekende XPath-uitbreidingfunksie is geroep."

#: 15
msgid "XPath parse failure: ')' expected:"
msgstr "XPath-ontleding het misluk: ')' verwag:"

#: 16
msgid "XPath parse failure: invalid axis:"
msgstr "XPath-ontleding het misluk: ongeldige as:"

#: 17
msgid "XPath parse failure: Name or Nodetype test expected:"
msgstr "XPath-ontleding het misluk: Naam- of nodussoorttoets verwag:"

#: 18
msgid "XPath parse failure: ']' expected:"
msgstr "XPath-ontleding het misluk: ']' verwag:"

#: 19
msgid "XPath parse failure: invalid variable name:"
msgstr "XPath-ontleding het misluk: ongeldige veranderlikenaam:"

#: 20
msgid "XPath parse failure: unexpected end of expression:"
msgstr "XPath-ontleding het misluk: onverwagse einde van uitdrukking:"

#: 21
msgid "XPath parse failure: operator expected:"
msgstr "XPath-ontleding het misluk: operateur verwag:"

#: 22
msgid "XPath parse failure: unclosed literal:"
msgstr "XPath-ontleding het misluk: ongeslote letterlike:"

#: 23
msgid "XPath parse failure: ':' unexpected:"
msgstr "XPath-ontleding het misluk: ':' onverwags:"

#: 24
msgid "XPath parse failure: '!' unexpected, negation is not():"
msgstr "XPath-ontleding het misluk: '!' onverwags, ontkenning is not():"

#: 25
msgid "XPath parse failure: illegal character found:"
msgstr "XPath-ontleding het misluk: onwettige karakter gevind:"

#: 26
msgid "XPath parse failure: binary operator expected:"
msgstr "XPath-ontleding het misluk: binêre operateur verwag:"

#: 27
msgid "An XSLT stylesheet load was blocked for security reasons."
msgstr "Laai van 'n XSLT-stylvel is vir sekuriteitsrede geblokkeer."

#: LoadingError
msgid "Error loading stylesheet: %S"
msgstr "Kon nie stylvel laai nie: %S"

#: TransformError
msgid "Error during XSLT transformation: %S"
msgstr "Fout tydens XSLT-transformasie: %S"
