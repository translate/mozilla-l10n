#: certmgr.title
msgid "Certificate Manager"
msgstr "Sertifikaatbestuurder"

#: certmgr.tab.mine
msgid "Your Certificates"
msgstr "U sertifikate"

#: certmgr.tab.others
msgid "Other People's"
msgstr "Ander se"

#: certmgr.tab.websites
msgid "Web Sites"
msgstr "Webwerwe"

#: certmgr.tab.ca
msgid "Authorities"
msgstr "Owerhede"

#: certmgr.ca.builtins
msgid "Built-in CAs"
msgstr "Ingeboude sertifikaatowerhede"

#: certmgr.ca.mycas
msgid "CAs Managed By You"
msgstr "SO's bestuur deur u "

#: certmgr.mine
msgid "You have certificates from these organizations that identify you:"
msgstr "U het sertifikate van hierdie organisasies wat u identifiseer:"

#: certmgr.others
msgid "You have certificates on file that identify these people:"
msgstr "U het sertifikate op rekord wat hierdie mense identifiseer:"

#: certmgr.websites
msgid "You have certificates on file that identify these web sites:"
msgstr "U het sertifikate op rekord wat hierdie webwerwe identifiseer:"

#: certmgr.cas
msgid ""
"You have certificates on file that identify these certificate authorities:"
msgstr ""
"U het sertifikate op rekord wat hierdie sertifikaatowerhede identifiseer:"

#: certmgr.detail.general_tab.title
msgid "General"
msgstr "Algemeen"

#: certmgr.detail.prettyprint_tab.title
msgid "Details"
msgstr "Besonderhede"

#: certmgr.subjectinfo.label
msgid "Issued To"
msgstr "Uitgereik aan"

#: certmgr.issuerinfo.label
msgid "Issued By"
msgstr "Uitgereik deur"

#: certmgr.validity.label
msgid "Validity"
msgstr "Geldigheid"

#: certmgr.fingerprints.label
msgid "Fingerprints"
msgstr "Vingerafdrukke"

#: certmgr.certdetail.title
msgid "Certificate Detail"
msgstr "Sertifikaatbesonderhede"

#: certmgr.certdetail.cn
msgid "Common Name (CN)"
msgstr "Gebruiklike naam (CN)"

#: certmgr.certdetail.o
msgid "Organization (O)"
msgstr "Organisasie (O)"

#: certmgr.certdetail.ou
msgid "Organizational Unit (OU)"
msgstr "Organisasie-eenheid (OU)"

#: certmgr.certdetail.serialnumber
msgid ""
"_: certmgr.certdetail.serialnumber\n"
"Serial Number"
msgstr ""
"Reeksnommer"

#: certmgr.certdetail.sha1fingerprint
msgid "SHA1 Fingerprint"
msgstr "SHA1-vingerafdruk"

#: certmgr.certdetail.md5fingerprint
msgid "MD5 Fingerprint"
msgstr "MD5-vingerafdruk"

#: certmgr.editcert.title
msgid "Edit Security Certificate Settings"
msgstr "Redigeer sekuriteitsertifikaatopstelling"

#: certmgr.editcacert.title
msgid "Edit CA certificate trust settings"
msgstr "Redigeer SO-sertifikaatvertroueopstelling"

#: certmgr.editsslcert.title
msgid "Edit web site certificate trust settings"
msgstr "Redigeer webwerf-sertifikaatvertroueopstelling"

#: certmgr.editcert.edittrust
msgid "Edit trust settings:"
msgstr "Redigeer vertroueopstelling:"

#: certmgr.editcert.trustssl
msgid "This certificate can identify web sites."
msgstr "Hierdie sertifikaat kan webwerwe identifiseer."

#: certmgr.editcert.trustemail
msgid "This certificate can identify mail users."
msgstr "Hierdie sertifikaat kan e-posgebruikers identifiseer."

#: certmgr.editcert.trustobjsign
msgid "This certificate can identify software makers."
msgstr "Hierdie sertifikaat kan sagtewarevervaardigers identifiseer."

#: certmgr.editsslcert.edittrust
msgid ""
"_: certmgr.editsslcert.edittrust\n"
"Edit certificate trust settings:"
msgstr ""
"Redigeer sertifikaatvertroueopstelling:"

#: certmgr.editsslcert.dotrust
msgid ""
"_: certmgr.editsslcert.dotrust\n"
"Trust the authenticity of this certificate."
msgstr ""
"Vertrou die geldigheid van hierdie sertifikaat."

#: certmgr.editsslcert.donttrust
msgid ""
"_: certmgr.editsslcert.donttrust\n"
"Do not trust the authenticity of this certificate."
msgstr ""
"Moenie die geldigheid van hierdie sertifikaat vertrou nie."

#: certmgr.editemailcert.title
msgid "Edit email certificate trust settings"
msgstr "Redigeer e-possertifikaatvertroueopstelling"

#: certmgr.editemailcert.edittrust
msgid ""
"_: certmgr.editemailcert.edittrust\n"
"Edit certificate trust settings:"
msgstr ""
"Redigeer sertifikaatvertroueopstelling:"

#: certmgr.editemailcert.dotrust
msgid ""
"_: certmgr.editemailcert.dotrust\n"
"Trust the authenticity of this certificate."
msgstr ""
"Vertrou die geldigheid van hierdie sertifikaat."

#: certmgr.editemailcert.donttrust
msgid ""
"_: certmgr.editemailcert.donttrust\n"
"Do not trust the authenticity of this certificate."
msgstr ""
"Moenie die geldigheid van hierdie sertifikaat vertrou nie."

#: certmgr.deletecert.title
msgid "Delete Certificate"
msgstr "Skrap sertifikaat"

#: certmgr.deletecert.beforename
msgid "You have requested to delete this certificate:"
msgstr "U het versoek dat hierdie sertifikaat geskrap word:"

#: certmgr.deletecert.aftername
msgid "Are you sure you want to delete this certificate?"
msgstr "Is u seker u wil hierdie sertifikaat skrap?"

#: certmgr.deleteusercert.title
msgid "Delete Your Certificate"
msgstr "Skrap u sertifikaat"

#: certmgr.deleteusercert.beforename
msgid "You have requested to delete the certificate:"
msgstr "U het versoek dat die sertifikaat geskrap word:"

#: certmgr.deleteusercert.aftername
msgid ""
"Once you have deleted this certificate, you will not be able to read mail "
"that has been encrypted with it."
msgstr ""
"Nadat u hierdie sertifikaat geskrap het, sal u nie pos wat daarmee geënkripteer is, "
"kan lees nie."

#: certmgr.certname
msgid "Certificate Name"
msgstr "Naam van sertifikaat"

#: certmgr.tokenname
msgid "Security Device"
msgstr "Sekuriteitstoestel"

#: certmgr.purpose
msgid "Purposes"
msgstr "Doelwitte"

#: certmgr.issued
msgid "Issued On"
msgstr "Uitgereik op"

#: certmgr.expires
msgid "Expires On"
msgstr "Verval op"

#: certmgr.email
msgid "E-Mail Address"
msgstr "E-posadres"

#: certmgr.serial
msgid ""
"_: certmgr.serial\n"
"Serial Number"
msgstr ""
"Reeksnommer"

#: certmgr.ocsp_info
msgid "Certificates have not been validated with OCSP. Click View to do so."
msgstr ""
"Sertifikate is nie met OCSP gestaaf nie. Kliek Bekyk om dit te doen."

#: certmgr.close.label
msgid "Close"
msgstr "Sluit"

#: certmgr.view.label
msgid "View"
msgstr "Bekyk"

#: certmgr.edit.label
msgid "Edit"
msgstr "Redigeer"

#: certmgr.editca.label
msgid "Edit CA Trust"
msgstr "Redigeer SO-vertroue"

#: certmgr.add.label
msgid "Add"
msgstr "Voeg by"

#: certmgr.delete.label
msgid "Delete"
msgstr "Skrap"

#: certmgr.backup.label
msgid "Backup"
msgstr "Rugsteun"

#: certmgr.backupall.label
msgid "Backup All"
msgstr "Rugsteun alles"

