msgstr "#1 anexo;#1 anexos"

#. ## String used by the Initialization Error dialog
#: initErrorDlogTitle
msgid "Message Compose"
msgstr "Edição de mensagem"

#: initErrorDlgMessage
msgid ""
"An error occurred while creating a message compose window. Please try again."
msgstr ""
"Um erro ocorreu ao criar a janela de edição de mensagem. Por favor, tente de "
"novo."

#. ## String used if a file to attach does not exist when passed as
#. ## a command line argument
#: errorFileAttachTitle
msgid "File Attach"
msgstr "Anexar arquivo"

#. ## LOCALIZATION NOTE (errorFileAttachMessage): %1$S will be replaced by the non-existent file name. Do not translate
#: errorFileAttachMessage
msgid "The file %1$S does not exist so could not be attached to the message."
msgstr "O arquivo %1$S não existe e não poderá ser anexado à mensagem."

#. ## Strings used by the Save as Draft/Template dialog
#: SaveDialogTitle
msgctxt "SaveDialogTitle"
msgid "Save Message"
msgstr "Salvar mensagem"

#. ## LOCALIZATION NOTE (SaveDialogMsg): %1$S is the folder name, %2$S is the host name
#: SaveDialogMsg
msgid "Your message has been saved to the folder %1$S under %2$S."
msgstr "Sua mensagem foi salva na pasta %1$S em %2$S."

#: CheckMsg
msgid "Do not show me this dialog box again."
msgstr "Não exibir mais este alerta"

#. ## Strings used by the prompt when Quitting while in progress
#: quitComposeWindowTitle
msgid "Sending Message"
msgstr "Enviando mensagem"

#. ## LOCALIZATION NOTE (quitComposeWindowMessage2): don't translate \n
#: quitComposeWindowMessage2
msgid ""
"%1$S is currently in the process of sending a message.\n"
"Would you like to wait until the message has been sent before quitting or "
"quit now?"
msgstr ""
"Uma mensagem está sendo enviada pelo %1$S.\n"
"Deseja aguardar até que ela seja enviada ou quer sair agora?"

#: quitComposeWindowQuitButtonLabel2
msgid "&Quit"
msgstr "&Sair"

#: quitComposeWindowWaitButtonLabel2
msgid "&Wait"
msgstr "&Aguardar"

#: quitComposeWindowSaveTitle
msgid "Saving Message"
msgstr "Salvando mensagem"

#. ## LOCALIZATION NOTE (quitComposeWindowSaveMessage): don't translate \n
#: quitComposeWindowSaveMessage
msgid ""
"%1$S is currently in the process of saving a message.\n"
"Would you like to wait until the message has been saved before quitting or "
"quit now?"
msgstr ""
"Uma mensagem está sendo salva pelo %1$S.\n"
"Deseja aguardar até que ela seja salva ou quer sair agora?"

#. ## Strings used by the prompt for Ctrl-Enter check before sending message
#: sendMessageCheckWindowTitle
msgid "Send Message"
msgstr "Enviar mensagem"

#: sendMessageCheckLabel
msgid "Are you sure you are ready to send this message?"
msgstr "Tem certeza de que está pronto para enviar esta mensagem?"

#: sendMessageCheckSendButtonLabel
msgid "Send"
msgstr "Enviar"

#: assemblingMessageDone
msgid "Assembling message…Done"
msgstr "Montando mensagem…Concluído"

#: assemblingMessage
msgid "Assembling message…"
msgstr "Montando mensagem…"

#: smtpDeliveringMail
msgid "Delivering mail…"
msgstr "Enviando e-mail…"

#: smtpMailSent
msgid "Mail sent successfully"
msgstr "Mensagem enviada com sucesso"

#: assemblingMailInformation
msgid "Assembling mail information…"
msgstr "Montando informação de e-mail…"

#. ## LOCALIZATION NOTE (gatheringAttachment): argument %S is the file name/URI of attachment
#: gatheringAttachment
msgid "Attaching %S…"
msgstr "Anexando %S…"

#: creatingMailMessage
msgid "Creating mail message…"
msgstr "Criando mensagem de email…"

#. ## LOCALIZATION NOTE (copyMessageStart): argument %S is the folder name
#: copyMessageStart
msgid "Copying message to %S folder…"
msgstr "Copiando mensagem para a pasta %S…"

#: copyMessageComplete
msgid "Copy complete."
msgstr "Cópia concluída."

#: copyMessageFailed
msgid "Copy failed."
msgstr "Falha na cópia."

#: filterMessageComplete
msgid "Filter complete."
msgstr ""

#: filterMessageFailed
msgid "Filter failed."
msgstr ""

#. ## LOCALIZATION NOTE (largeMessageSendWarning):
#. ## Do not translate %S. It is the size of the message in user-friendly notation.
#: largeMessageSendWarning
msgid ""
"Warning! You are about to send a message of size %S. Are you sure you want "
"to do this?"
msgstr ""

#: sendingMessage
msgid "Sending message…"
msgstr "Enviando mensagem…"

#: sendMessageErrorTitle
msgid "Send Message Error"
msgstr "Erro de envio de mensagem"

#: postingMessage
msgid "Posting message…"
msgstr "Enviando mensagem…"

