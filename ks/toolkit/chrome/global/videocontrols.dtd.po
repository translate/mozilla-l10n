msgid "Activity"
msgstr "كٲم"

#: stats.activityPaused
msgid "Paused"
msgstr "روٛكیمُت"

#: stats.activityPlaying
msgid "Playing"
msgstr "چلاوان"

#: stats.activityEnded
msgid "Ended"
msgstr "مكلیمُت"

#: stats.activitySeeking
msgid "(seeking)"
msgstr "(ژھانڈان)"

#: stats.volume
msgid "Volume"
msgstr "آواز"

