#. #   %2$S = unit of measure (bytes, KB, MB, ...)
#: offlineAppUsage
msgid "%1$S %2$S"
msgstr "%1$S %2$S"

#: offlinepermissionstext
msgid "The following websites are not allowed to store data for offline use:"
msgstr "یمن ویب سایٹن چھُ نئ ڈاٹا آفالین جمع كرنُك اجازت"

#: offlinepermissionstitle
msgid "Offline Data"
msgstr "آفلاین ڈاٹا"

#. ####Preferences::Advanced::Network
#. #LOCALIZATION NOTE: The next string is for the disk usage of the web content cache.
#. #   e.g., "Your web content cache is currently using 200 MB"
#. #   %1$S = size
#. #   %2$S = unit (MB, KB, etc.)
#: actualDiskCacheSize
msgid "Your web content cache is currently using %1$S %2$S of disk space"
msgstr "تُہند ویب مواد كیش چھُ حالس  %2$S منزئ %1$S  ڈیسك جاے استیمال كران"

#: actualDiskCacheSizeCalculated
msgid "Calculating web content cache size…"
msgstr "شُمار کران ویب کنٹینٹ کیشے سایزس ۔ ۔ ۔"

#. ####Preferences::Advanced::Network
#. #LOCALIZATION NOTE: The next string is for the disk usage of the application cache.
#. #   e.g., "Your application cache is currently using 200 MB"
#. #   %1$S = size
#. #   %2$S = unit (MB, KB, etc.)
#: actualAppCacheSize
msgid "Your application cache is currently using %1$S %2$S of disk space"
msgstr "تُہنز عمل چھَ حالس  %2$S منزئ %1$S  ڈیسك جاے استیمال كران"

#: syncUnlink.title
msgid "Do you want to unlink your device?"
msgstr "تُہئ چھَوئ پنُن آلئ ان لینك یژھان كرُن؟"

#: syncUnlink.label
msgid ""
"This device will no longer be associated with your Sync account. All of your "
"personal data, both on this device and in your Sync account, will remain "
"intact."
msgstr ""
"یہ ڈیوایس روزِ نْہ وْنی وابستْہ تُہندس سینک ایکاونٹس سْیت۔ تُہند تمام ذأتی ڈاتا،  "
"دوشو۔نئ  یتھ ڈیوایسس پیٹھ تْہ تُہندس سینک ایکاونٹس منز، روزِ صحیح پأٹھ۔"

#: syncUnlinkConfirm.label
msgid "Unlink"
msgstr "ان لینك"

