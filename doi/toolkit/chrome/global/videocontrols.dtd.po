#: stats.size
msgid "Size"
msgstr "नाप"

#: stats.activity
msgid "Activity"
msgstr "गतिविधि"

#: stats.activityPaused
msgid "Paused"
msgstr "विरामशुदा"

#: stats.activityPlaying
msgid "Playing"
msgstr "चला करदा"

#: stats.activityEnded
msgid "Ended"
msgstr "समाप्त"

#: stats.activitySeeking
msgid "(seeking)"
msgstr "(तुप्पा करदा)"

#: stats.volume
msgid "Volume"
msgstr "अवाज़"

#: stats.framesParsed
msgid "Frames parsed"
msgstr "फ्रेमें दी पद-व्याख्या कीती गेई"

#: stats.framesDecoded
msgid "Frames decoded"
msgstr "फ्रेम डीकोड कीते गे"

#: stats.framesPresented
msgid "Frames presented"
msgstr "फ्रेम प्रस्तुत कीते गे"

#: stats.framesPainted
msgid "Frames painted"
msgstr "फ्रेम पेंट कीते गे"

#: error.aborted
msgid "Video loading stopped."
msgstr "वीडियो दी लोडिङ बंद कीती गेई."

#: error.network
msgid "Video playback aborted due to a network error."
msgstr "नैट्टवर्क गल्ती मूजब वीडियो प्लेऽबैक गी खारज कीता गेआ."

#: error.decode
msgid "Video can't be played because the file is corrupt."
msgstr "वीडियो गी चलाया नेईं जाई सकदा की जे फाइल भ्रश्ट ऐ."

#: error.srcNotSupported
msgid "Video format or MIME type is not supported."
msgstr "वीडियो फार्मैट जां MIME किस्म समर्थनशुदा नेईं ऐ. "

#: error.noSource2
msgid "No video with supported format and MIME type found."
msgstr "कोई बी समर्थनशुदा फार्मैट आह्‌ला वीडियो ते MIME किस्म नेईं मिली."

#: error.generic
msgid "Video playback aborted due to an unknown error."
msgstr "अनजांती गल्ती मूजब वीडियो प्लेऽबैक गी खारज कीता गेआ."

#. the #1 string is the current
#. media position, and the #2 string is the total duration. For example, when at
#. the 5 minute mark in a 6 hour long video, #1 would be "5:00" and #2 would be
#. "6:00:00", result string would be "5:00 of 6:00:00 elapsed".
#: scrubberScale.nameFormat
msgid "#1 of #2 elapsed"
msgstr "2# चा 1# बीती गेआ"
