#: startup.label
msgid "Startup"
msgstr "Inici"

#: startupPage.label startupPage.accesskey
msgid "When &brandShortName; &starts:"
msgstr "Quan s'&inicie &brandShortName;:"

#: startupHomePage.label
msgid "Show my home page"
msgstr "Mostra la meua pàgina d'inici"

#: startupBlankPage.label
msgid "Show a blank page"
msgstr "Mostra una pàgina en blanc"

#: startupLastSession.label
msgid "Show my windows and tabs from last time"
msgstr "Mostra les finestres i pestanyes de l'última volta"

#: homepage.label homepage.accesskey
msgid "Home &Page:"
msgstr "&Pàgina d'inici:"

#: useCurrentPage.label useCurrentPage.accesskey
msgid "Use &Current Page"
msgstr "Utilisar la pàgina a&ctual"

#: useMultiple.label
msgid "Use Current Pages"
msgstr "Usar les pàgines actuals"

#: chooseBookmark.label chooseBookmark.accesskey
msgid "Use &Bookmark…"
msgstr "Utilisar &Marcador…"

#: restoreDefault.label restoreDefault.accesskey
msgid "&Restore to Default"
msgstr "&Restaurar les pàgines per defecte"

#: downloads.label
msgid "Downloads"
msgstr "Descàrregues"

#: saveTo.label saveTo.accesskey
msgid "Sa&ve files to"
msgstr "&Guardar el archius en"

#: chooseFolderWin.label chooseFolderWin.accesskey
msgid "Br&owse…"
msgstr "E&xaminar…"

#: chooseFolderMac.label chooseFolderMac.accesskey
msgid "Choos&e…"
msgstr "Tri&e…"

#: alwaysAsk.label alwaysAsk.accesskey
msgid "&Always ask me where to save files"
msgstr "Pregunt&ar-me sempre a on guardar els archius"

#: alwaysCheckDefault2.label alwaysCheckDefault2.accesskey
#, fuzzy
msgid "Alwa&ys check if &brandShortName; is your default browser"
msgstr "C&omprovar sempre si &brandShortName; és el navegador predeterminat"

#: setAsMyDefaultBrowser2.label setAsMyDefaultBrowser2.accesskey
msgid "Make &Default"
msgstr "Convertir en pre&determinat"

#: isDefault.label
msgid "&brandShortName; is currently your default browser"
msgstr "&brandShortName; és el navegador predeterminat"

#: isNotDefault.label
msgid "&brandShortName; is not your default browser"
msgstr "&brandShortName; no és el navegador predeterminat"

#: separateProfileMode.label
msgid "Allow &brandShortName; and Firefox to run at the same time"
msgstr "Permetre a &brandShortName; i Firefox eixecutar-se al mateix temps"

#: useFirefoxSync.label
msgid "Tip: This uses separate profiles. Use Sync to share data between them."
msgstr ""
"Consell: açò usa perfils separats. Use Sync per a compartir senyes entre "
"ells."

#: getStarted.label
msgid "Start using Sync…"
msgstr "Escomence a usar Sync…"

#: e10sEnabled.label
msgid "Enable multi-process &brandShortName;"
msgstr "Activar &brandShortName; multiprocés"
