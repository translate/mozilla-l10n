#. # Tokenizer errors
#: errGarbageAfterLtSlash
msgid "Garbage after “</”."
msgstr "Fem despuix de “</”."

#: errLtSlashGt
msgid ""
"Saw “</>”. Probable causes: Unescaped “<” (escape as “&lt;”) or mistyped end "
"tag."
msgstr ""
"Se ha encontrado “</>”. Causes provables: “<” no escapat (escape'l com "
"“&lt;”) o etiqueta de tancament mal escrita."

#: errCharRefLacksSemicolon
msgid "Character reference was not terminated by a semicolon."
msgstr "Referència a caràcter no està acabat per un punt i coma."

#: errNoDigitsInNCR
msgid "No digits in numeric character reference."
msgstr "No hi ha dígits en la referència numèrica a caràcter."

#: errGtInSystemId
msgid "“>” in system identifier."
msgstr "“>” en un identificador de sistema."

#: errGtInPublicId
msgid "“>” in public identifier."
msgstr "“>” en un identificador públic."

#: errNamelessDoctype
msgid "Nameless doctype."
msgstr "Doctype sense nom."

#: errConsecutiveHyphens
msgid ""
"Consecutive hyphens did not terminate a comment. “--” is not permitted "
"inside a comment, but e.g. “- -” is."
msgstr ""
"Guions consecutius no van acabar un comentari. “-” no es permet dins d'un "
"comentari, pero per eixemple “- -” sí."

#: errPrematureEndOfComment
msgid "Premature end of comment. Use “-->” to end a comment properly."
msgstr ""
"Fi de comentari prematur. Use “->” per a acabar correctament un comentari."

#: errBogusComment
msgid "Bogus comment."
msgstr "Comentari simulat."

#: errUnquotedAttributeLt
msgid ""
"“<” in an unquoted attribute value. Probable cause: Missing “>” immediately "
"before."
msgstr ""
"“<” en un valor d'atribut sense cometes. Causa provable: “>” falta just "
"abans."

#: errUnquotedAttributeGrave
msgid ""
"“`” in an unquoted attribute value. Probable cause: Using the wrong "
"character as a quote."
msgstr ""
"“`” en un valor d'atribut sense cometes. Causa provable: usar el caràcter "
"erròneu com cometa."

#: errUnquotedAttributeQuote
msgid ""
"Quote in an unquoted attribute value. Probable causes: Attributes running "
"together or a URL query string in an unquoted attribute value."
msgstr ""
"Cometa en un valor d'atribut sense cometes. Causes provables: atributs "
"apegats per erro o una cadena de consulta URL en un valor d'atribut sense "
"cometes."

#: errUnquotedAttributeEquals
msgid ""
"“=” in an unquoted attribute value. Probable causes: Attributes running "
"together or a URL query string in an unquoted attribute value."
msgstr ""
"“=” en un valor d'atribut sense cometes. Causes provables: atributs junts "
"per erro o una cadena de consulta URL en un valor d'atribut sense cometes."

#: errSlashNotFollowedByGt
msgid "A slash was not immediately followed by “>”."
msgstr "Una barra no estava immediatament seguida de “>”."

#: errNoSpaceBetweenAttributes
msgid "No space between attributes."
msgstr "Falta espai entre atributs."

#: errUnquotedAttributeStartLt
msgid ""
"“<” at the start of an unquoted attribute value. Probable cause: Missing “>” "
"immediately before."
msgstr ""
"“<” al principi de valor d'atribut sense cometes. Causa provable: “>” falta "
"just abans."

#: errUnquotedAttributeStartGrave
msgid ""
"“`” at the start of an unquoted attribute value. Probable cause: Using the "
"wrong character as a quote."
msgstr ""
"“`” al principi d'un valor d'atribut sense cometes. Causa provable: usar el "
"caràcter erròneu com cometa."

#: errUnquotedAttributeStartEquals
msgid ""
"“=” at the start of an unquoted attribute value. Probable cause: Stray "
"duplicate equals sign."
msgstr ""
"“=” al principi d'un valor d'atribut sense cometes. Causa provable: signe el "
"mateix duplicat/extraviat."

#: errAttributeValueMissing
msgid "Attribute value missing."
msgstr "Falta valor d'atribut."

#: errBadCharBeforeAttributeNameLt
msgid ""
"Saw “<” when expecting an attribute name. Probable cause: Missing “>” "
"immediately before."
msgstr ""
"S'ha trobat “<” quant s'esperava un nom d'atribut. Causa provable: falta un "
"“>” just abans."

#: errEqualsSignBeforeAttributeName
msgid ""
"Saw “=” when expecting an attribute name. Probable cause: Attribute name "
"missing."
msgstr ""
"S'ha trobat “=” quan s'esperava un nom d'atribut. Causa provable: falta el "
"nom de l'atribut."

#: errBadCharAfterLt
msgid ""
"Bad character after “<”. Probable cause: Unescaped “<”. Try escaping it as "
"“&lt;”."
msgstr ""
"Caràcter erròneu despuix de “<”. Causa provable: “<” no escapat. Prove a "
"escapar-ho com “&lt;”."

#: errLtGt
msgid ""
"Saw “<>”. Probable causes: Unescaped “<” (escape as “&lt;”) or mistyped "
"start tag."
msgstr ""
"S'ha trobat “<>”. Causes provables: “<” no escapat (escape'l com “&lt;”) o "
"etiqueta d'inici mal escrita."

#: errProcessingInstruction
msgid ""
"Saw “<?”. Probable cause: Attempt to use an XML processing instruction in "
"HTML. (XML processing instructions are not supported in HTML.)"
msgstr ""
"S'ha trobat “<?”. Causa provable: s'ha intentat usar una instrucció de "
"procés XML en HTML (les instruccions de procés XML no s'admeten en HTML)."

#: errUnescapedAmpersandInterpretedAsCharacterReference
msgid ""
"The string following “&” was interpreted as a character reference. (“&” "
"probably should have been escaped as “&amp;”.)"
msgstr ""
"La cadena que seguix a “&” s'ha interpretat com una referència a caràcter "
"(“&” provablement hauria d'haver segut escapat com “&amp;”)."

#: errNotSemicolonTerminated
msgid ""
"Named character reference was not terminated by a semicolon. (Or “&” should "
"have been escaped as “&amp;”.)"
msgstr ""
"La referència a caràcter en nom no acabava en un punt i coma (o be hauria "
"d'haver-se escapat “&” com “&amp;”)."

#: errNoNamedCharacterMatch
msgid ""
"“&” did not start a character reference. (“&” probably should have been "
"escaped as “&amp;”.)"
msgstr ""
"“&” no ha començat una referència a caràcter (“&” provablement hauria "
"d'haver segut escapat com “&amp;”)."

#: errQuoteBeforeAttributeName
msgid ""
"Saw a quote when expecting an attribute name. Probable cause: “=” missing "
"immediately before."
msgstr ""
"S'ha trobat una cometa quan s'esperava un nom d'atribut. Causa provable: "
"falta un “=” just abans."

#: errLtInAttributeName
msgid "“<” in attribute name. Probable cause: “>” missing immediately before."
msgstr "“<” en un nom d'atribut. Causa provable: “>” falta just abans."

#: errQuoteInAttributeName
msgid ""
"Quote in attribute name. Probable cause: Matching quote missing somewhere "
"earlier."
msgstr ""
"Cometa en nom d'atribut. Causa provable: la cometa corresponent falta en "
"algun lloc anterior."

#: errExpectedPublicId
msgid "Expected a public identifier but the doctype ended."
msgstr "S'esperava un identificador públic pero el doctype ha acabat."

#: errBogusDoctype
msgid "Bogus doctype."
msgstr "Doctype simulat."

#: maybeErrAttributesOnEndTag
msgid "End tag had attributes."
msgstr "L'etiqueta de tancament tenia atributs."

#: maybeErrSlashInEndTag
msgid "Stray “/” at the end of an end tag."
msgstr "“/” extraviada al final d'una etiqueta de tancament."

#: errNcrNonCharacter
msgid "Character reference expands to a non-character."
msgstr "Referència a caràcter s'expandix a no-caràcter."

#: errNcrSurrogate
msgid "Character reference expands to a surrogate."
msgstr "Referència a caràcter s'expandix a un surrogado."

#: errNcrControlChar
msgid "Character reference expands to a control character."
msgstr "Referència a caràcter s'expandix a caràcter de control."

#: errNcrCr
msgid "A numeric character reference expanded to carriage return."
msgstr "Una referència numèrica a caràcter expandida a retorn de carro."

#: errNcrInC1Range
msgid "A numeric character reference expanded to the C1 controls range."
msgstr "Una referència numèrica a caràcter expandida al ranc de controls C1."

#: errEofInPublicId
msgid "End of file inside public identifier."
msgstr "Fi de ficher dins d'un identificador públic."

#: errEofInComment
msgid "End of file inside comment."
msgstr "Fi de ficher dins d'un comentari."

#: errEofInDoctype
msgid "End of file inside doctype."
msgstr "Fi de ficher dins d'un doctype."

#: errEofInAttributeValue
msgid "End of file reached when inside an attribute value. Ignoring tag."
msgstr ""
"Fi de ficher alcançat dins del valor d'un atribut. S'ignora l'etiqueta."

#: errEofInAttributeName
msgid "End of file occurred in an attribute name. Ignoring tag."
msgstr "Fi de ficher trobat en un nom d'atribut. S'ignora l'etiqueta."

#: errEofWithoutGt
msgid "Saw end of file without the previous tag ending with “>”. Ignoring tag."
msgstr ""
"S'ha trobat el fi de ficher sense que l'etiqueta anterior acabe en “>”. "
"S'ignora l'etiqueta."

#: errEofInTagName
msgid "End of file seen when looking for tag name. Ignoring tag."
msgstr ""
"Fi de ficher trobat mentres es busca el nom d'una etiqueta. S'ignora "
"l'etiqueta."

#: errEofInEndTag
msgid "End of file inside end tag. Ignoring tag."
msgstr "Fi de ficher dins d'una etiqueta de tancament. S'ignora l'etiqueta."

#: errEofAfterLt
msgid "End of file after “<”."
msgstr "Fi de ficher despuix de “<”."

#: errNcrOutOfRange
msgid "Character reference outside the permissible Unicode range."
msgstr "Referència a caràcter fora del ranc Unicode admissible."

#: errNcrUnassigned
msgid "Character reference expands to a permanently unassigned code point."
msgstr ""
"Referència a caràcter s'expandix a punt de codi permanentment no assignat."

#: errDuplicateAttribute
msgid "Duplicate attribute."
msgstr "Atribut duplicat."

#: errEofInSystemId
msgid "End of file inside system identifier."
msgstr "Fi de ficher dins d'un identificador de sistema."

#: errExpectedSystemId
msgid "Expected a system identifier but the doctype ended."
msgstr "S'esperava un identificador de sistema pero el doctype ha acabat."

#: errMissingSpaceBeforeDoctypeName
msgid "Missing space before doctype name."
msgstr "Falta espai abans del nom de doctype."

#: errHyphenHyphenBang
msgid "“--!” found in comment."
msgstr "“-!” trobat en comentari."

#: errNcrZero
msgid "Character reference expands to zero."
msgstr "Referència a caràcter s'expandix a zero."

#: errNoSpaceBetweenDoctypeSystemKeywordAndQuote
msgid "No space between the doctype “SYSTEM” keyword and the quote."
msgstr ""
"Falta espai entre la paraula clau “SYSTEM” i l'entrecomillat en el doctype."

#: errNoSpaceBetweenPublicAndSystemIds
msgid "No space between the doctype public and system identifiers."
msgstr ""
"Falta espai entre els identificadors públic i de sistema en el doctype."

#: errNoSpaceBetweenDoctypePublicKeywordAndQuote
msgid "No space between the doctype “PUBLIC” keyword and the quote."
msgstr ""
"Falta espai entre la paraula clau “PUBLIC” i l'entrecomillat en el doctype."

#. # Tree builder errors
#: errStrayStartTag2
msgid "Stray start tag “%1$S”."
msgstr "Etiqueta d'obertura “%1$S” extraviada."

#: errStrayEndTag
msgid "Stray end tag “%1$S”."
msgstr "Etiqueta de tancament “%1$S” extraviada."

#: errUnclosedElements
msgid "End tag “%1$S” seen, but there were open elements."
msgstr "Etiqueta de tancament “%1$S” trobada, pero hi havia elements oberts."

#: errUnclosedElementsImplied
msgid "End tag “%1$S” implied, but there were open elements."
msgstr "Etiqueta de tancament “%1$S” implícita, pero hi havia elements oberts."

#: errUnclosedElementsCell
msgid "A table cell was implicitly closed, but there were open elements."
msgstr ""
"Es va tancar implícitament una cela de taula, pero hi havia elements oberts."

#: errStrayDoctype
msgid "Stray doctype."
msgstr "Doctype extraviat."

#: errAlmostStandardsDoctype
msgid "Almost standards mode doctype. Expected “<!DOCTYPE html>”."
msgstr "Doctype del modo casi estàndart. S'esperava “<!DOCTYPE html>”."

#: errQuirkyDoctype
msgid "Quirky doctype. Expected “<!DOCTYPE html>”."
msgstr ""
"Doctype de compatibilitat en extensions no estàndarts. S'esperava “<!DOCTYPE "
"html>”."

#: errNonSpaceInTrailer
msgid "Non-space character in page trailer."
msgstr "Caràcter diferent d'espai al final de la pàgina."

#: errNonSpaceAfterFrameset
msgid "Non-space after “frameset”."
msgstr "Caràcter diferent d'espai despuix de “frameset”."

#: errNonSpaceInFrameset
msgid "Non-space in “frameset”."
msgstr "Caràcter diferent d'espai en “frameset”."

#: errNonSpaceAfterBody
msgid "Non-space character after body."
msgstr "Caràcter diferent d'espai rere body."

#: errNonSpaceInColgroupInFragment
msgid "Non-space in “colgroup” when parsing fragment."
msgstr "Caràcter diferent d'espai en “colgroup” a l'interpretar fragment."

#: errNonSpaceInNoscriptInHead
msgid "Non-space character inside “noscript” inside “head”."
msgstr "Caràcter diferent d'espai dins de “noscript” dins de “head”."

#: errFooBetweenHeadAndBody
msgid "“%1$S” element between “head” and “body”."
msgstr "Element “%1$S” entre “head” i “body”."

#: errStartTagWithoutDoctype
msgid ""
"Start tag seen without seeing a doctype first. Expected “<!DOCTYPE html>”."
msgstr ""
"Trobada etiqueta d'inici sense vore primer un doctype. S'esperava “<!DOCTYPE "
"html>”."

#: errNoSelectInTableScope
msgid "No “select” in table scope."
msgstr "Falta “select” en l'àmbit de la taula."

#: errStartSelectWhereEndSelectExpected
msgid "“select” start tag where end tag expected."
msgstr "Etiqueta d'inici “select” on s'esperava l'etiqueta de tancament."

#: errStartTagWithSelectOpen
msgid "“%1$S” start tag with “select” open."
msgstr "Etiqueta d'inici “%1$S” en “select“ obert."

#: errBadStartTagInHead2
msgid "Bad start tag “%1$S” in “head”."
msgstr "Etiqueta “%1$S” d'obertura errònea en “head”."

#: errImage
msgid "Saw a start tag “image”."
msgstr "S'ha trobat una etiqueta d'inici “image”."

#: errIsindex
msgid "“isindex” seen."
msgstr "Trobat “isindex”."

#: errFooSeenWhenFooOpen
msgid ""
"An “%1$S” start tag seen but an element of the same type was already open."
msgstr ""
"Trobada una etiqueta d'inici “%1$S”, pero ya havia obert un element del "
"mateix tipo."

#: errHeadingWhenHeadingOpen
msgid "Heading cannot be a child of another heading."
msgstr "Un encapçalament no pot ser fill d'un altre encapçalament."

#: errFramesetStart
msgid "“frameset” start tag seen."
msgstr "Trobada etiqueta d'inici “frameset”."

#: errNoCellToClose
msgid "No cell to close."
msgstr "No hi ha cela que tancar."

#: errStartTagInTable
msgid "Start tag “%1$S” seen in “table”."
msgstr "Etiqueta d'inici “%1$S” vista en “table”."

#: errFormWhenFormOpen
msgid ""
"Saw a “form” start tag, but there was already an active “form” element. "
"Nested forms are not allowed. Ignoring the tag."
msgstr ""
"S'ha trobat una etiqueta d'inici “form”, pero ya hi havia un element “form” "
"actiu. No es permeten formularis niats. S'ignora l'etiqueta."

#: errTableSeenWhileTableOpen
msgid "Start tag for “table” seen but the previous “table” is still open."
msgstr ""
"S'ha trobat una etiqueta d'inici per a “table” pero el “table” anterior "
"encara està obert."

#: errStartTagInTableBody
msgid "“%1$S” start tag in table body."
msgstr "Etiqueta d'inici “%1$S” en cos de taula."

#: errEndTagSeenWithoutDoctype
msgid ""
"End tag seen without seeing a doctype first. Expected “<!DOCTYPE html>”."
msgstr ""
"Etiqueta de tancament trobada sense vore primer un doctype. S'esperava “<!"
"DOCTYPE html>”."

#: errEndTagAfterBody
msgid "Saw an end tag after “body” had been closed."
msgstr ""
"S'ha trobat una etiqueta de tancament despuix que “body” haja segut tancat."

#: errEndTagSeenWithSelectOpen
msgid "“%1$S” end tag with “select” open."
msgstr "Etiqueta de tancament “%1$S” en “select“ obert."

#: errGarbageInColgroup
msgid "Garbage in “colgroup” fragment."
msgstr "Fem en fragment “colgroup”."

#: errEndTagBr
msgid "End tag “br”."
msgstr "Etiqueta de tancament “br”."

#: errNoElementToCloseButEndTagSeen
msgid "No “%1$S” element in scope but a “%1$S” end tag seen."
msgstr ""
"Falta element “%1$S” en l'àmbit pero s'ha vist una etiqueta de tancament "
"“%1$S”."

#: errHtmlStartTagInForeignContext
msgid "HTML start tag “%1$S” in a foreign namespace context."
msgstr "Etiqueta HTML d'inici “%1$S” en un context d'espai de noms alié."

#: errTableClosedWhileCaptionOpen
msgid "“table” closed but “caption” was still open."
msgstr "“table” tancada pero “caption” encara estava obert."

#: errNoTableRowToClose
msgid "No table row to close."
msgstr "No hi ha fila de taula que tancar."

#: errNonSpaceInTable
msgid "Misplaced non-space characters inside a table."
msgstr "Caràcters diferents d'espai descolocats dins d'una taula."

#: errUnclosedChildrenInRuby
msgid "Unclosed children in “ruby”."
msgstr "Fills no tancats en “ruby”."

#: errStartTagSeenWithoutRuby
msgid "Start tag “%1$S” seen without a “ruby” element being open."
msgstr ""
"Etiqueta d'inici “%1$S” vista sense que hi haja un element “ruby” obert."

#: errSelfClosing
msgid ""
"Self-closing syntax (“/>”) used on a non-void HTML element. Ignoring the "
"slash and treating as a start tag."
msgstr ""
"Sintaxis d'autotancament (“/>”) usada en un element HTML no buit. S'ignora "
"la barra i es tracta com una etiqueta d'inici."

#: errNoCheckUnclosedElementsOnStack
msgid "Unclosed elements on stack."
msgstr "Elements no tancats en pila."

#: errEndTagDidNotMatchCurrentOpenElement
msgid ""
"End tag “%1$S” did not match the name of the current open element (“%2$S”)."
msgstr ""
"L'etiqueta de tancament “%1$S” no concordava en el nom de l'element obert "
"actualment (“%2$S”)."

#: errEndTagViolatesNestingRules
msgid "End tag “%1$S” violates nesting rules."
msgstr "L'etiqueta de tancament “%1$S” viola les regles d'implantació."

#: errEndWithUnclosedElements
msgid "End tag for “%1$S” seen, but there were unclosed elements."
msgstr ""
"S'ha trobat l'etiqueta de tancament “%1$S”, pero havia elements sense tancar."
