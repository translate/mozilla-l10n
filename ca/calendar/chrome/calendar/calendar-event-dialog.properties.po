msgid ""
"Occurs %1$S\n"
"effective %2$S for #3 time.;Occurs %1$S\n"
"effective %2$S for #3 times."
msgstr ""
"Té lloc %1$S\n"
"#3 vegada a partir del %2$S.;Té lloc %1$S\n"
"#3 vegades a partir del %2$S."

#. # LOCALIZATION NOTE (repeatDetailsUntil):
