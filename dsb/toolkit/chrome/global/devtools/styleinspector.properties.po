#. # LOCALIZATION NOTE These strings are used inside the Style Inspector.
#. #
#. # The correct localization of this file might be to keep it in
#. # English, or another language commonly spoken among web developers.
#. # You want to make that choice consistent across the developer tools.
#. # A good criteria is the language in which you'd find the best
#. # documentation on web development on the web.
#. # LOCALIZATION NOTE (panelTitle): This is the panel title
#: panelTitle
msgid "Style Inspector"
msgstr "Stilowy inspektor"

#. # LOCALIZATION NOTE (rule.status): For each style property the panel shows
#. # the rules which hold that specific property. For every rule, the rule status
#. # is also displayed: a rule can be the best match, a match, a parent match, or a
#. # rule did not match the element the user has highlighted.
#: rule.status.BEST
msgid "Best Match"
msgstr "Nejlěpjej wótpowědujucy"

#: rule.status.MATCHED
msgid "Matched"
msgstr "Wótpowědujucy"

#: rule.status.PARENT_MATCH
msgid "Parent Match"
msgstr "Nadrědowany wótpowědujucy"

#. # LOCALIZATION NOTE (rule.sourceElement, rule.sourceInline): For each
#. # style property the panel shows the rules which hold that specific property.
#. # For every rule, the rule source is also displayed: a rule can come from a
#. # file, from the same page (inline), or from the element itself (element).
#: rule.sourceInline
msgid "inline"
msgstr "inline"

#: rule.sourceElement
msgid "element"
msgstr "element"

#. # LOCALIZATION NOTE (rule.inheritedFrom): Shown for CSS rules
#. # that were inherited from a parent node. Will be passed a node
#. # identifier of the parent node.
#. # e.g "Inherited from body#bodyID"
#: rule.inheritedFrom
msgid "Inherited from %S"
msgstr "Wot %S zderbnjony"

#. # LOCALIZATION NOTE (rule.keyframe): Shown for CSS Rules keyframe header.
#. # Will be passed an identifier of the keyframe animation name.
#: rule.keyframe
msgid "Keyframes %S"
msgstr "Klucowe wobłuki %S"

#. # LOCALIZATION NOTE (rule.userAgentStyles): Shown next to the style sheet
#. # link for CSS rules that were loaded from a user agent style sheet.
#. # These styles will not be editable, and will only be visible if the
#. # devtools.inspector.showUserAgentStyles pref is true.
#: rule.userAgentStyles
msgid "(user agent)"
msgstr "(wužywarski agent)"

#. # LOCALIZATION NOTE (rule.pseudoElement): Shown for CSS rules
#. # pseudo element header
#: rule.pseudoElement
msgid "Pseudo-elements"
msgstr "Pseudoelementy"

#. # LOCALIZATION NOTE (rule.pseudoElement): Shown for CSS rules
#. # pseudo element header
#: rule.selectedElement
msgid "This Element"
msgstr "Toś ten element"

#. # LOCALIZATION NOTE (helpLinkTitle): For each style property
#. # the user can hover it and get a help link button which allows one to
#. # quickly jump to the documentation from the Mozilla Developer Network site.
#. # This is the link title shown in the hover tooltip.
#: helpLinkTitle
msgid "Read the documentation for this property"
msgstr "Dokumentaciju toś teje kakosći cytaś"

#. # LOCALIZATION NOTE (rule.warning.title): When an invalid property value is
#. # entered into the rule view a warning icon is displayed. This text is used for
#. # the title attribute of the warning icon.
#: rule.warning.title
msgid "Invalid property value"
msgstr "Njepłaśiwa kakostna gódnota"

#. # LOCALIZATION NOTE (ruleView.empty): Text displayed when the highlighter is
#. # first opened and there's no node selected in the rule view.
#: rule.empty
msgid "No element selected."
msgstr "Žeden element wubrany."

#. # LOCALIZATION NOTE (ruleView.selectorHighlighter.tooltip): Text displayed in a
#. # tooltip when the mouse is over a selector highlighter icon in the rule view.
#: rule.selectorHighlighter.tooltip
msgid "Highlight all elements matching this selector"
msgstr "Wšykne elementy wuzwignuś, kótarež wótpowěduju toś tomu selektoroju"

#. # LOCALIZATION NOTE (ruleView.contextmenu.selectAll): Text displayed in the
#. # rule view context menu.
#: ruleView.contextmenu.selectAll
msgid "Select all"
msgstr "Wšykno wubraś"

#. # LOCALIZATION NOTE (ruleView.contextmenu.selectAll.accessKey): Access key for
#. # the rule view context menu "Select all" entry.
#: ruleView.contextmenu.selectAll.accessKey
msgctxt "ruleView.contextmenu.selectAll.accessKey"
msgid "A"
msgstr "u"

#. # LOCALIZATION NOTE (ruleView.contextmenu.copy): Text displayed in the rule view
#. # context menu.
#: ruleView.contextmenu.copy
msgctxt "ruleView.contextmenu.copy"
msgid "Copy"
msgstr "Kopěrowaś"

#. # LOCALIZATION NOTE (ruleView.contextmenu.copy.accessKey): Access key for
#. # the rule view context menu "Copy" entry.
#: ruleView.contextmenu.copy.accessKey
msgctxt "ruleView.contextmenu.copy.accessKey"
msgid "C"
msgstr "K"

#. # LOCALIZATION NOTE (ruleView.contextmenu.copyColor): Text displayed in the rule
#. # and computed view context menu when a color value was clicked.
#: ruleView.contextmenu.copyColor
msgid "Copy Color"
msgstr "Barwu kopěrowaś"

#. # LOCALIZATION NOTE (ruleView.contextmenu.copyColor.accessKey): Access key for
#. # the rule and computed view context menu "Copy Color" entry.
#: ruleView.contextmenu.copyColor.accessKey
msgid "L"
msgstr "B"

#. # LOCALIZATION NOTE (styleinspector.contextmenu.copyImageDataUrl): In rule and computed view :
#. # text displayed in the context menu for an image URL.
#. # Clicking it copies the image as Data-URL to the clipboard of the user.
#: styleinspector.contextmenu.copyImageDataUrl
msgid "Copy Image Data-URL"
msgstr "URL wobrazowych datow kopěrowaś"

#. # LOCALIZATION NOTE (styleinspector.contextmenu.copyDataUri.accessKey): Access key for
#. # the rule and computed view context menu "Copy Image Data-URL" entry.
#: styleinspector.contextmenu.copyImageDataUrl.accessKey
msgid "U"
msgstr "U"

#. # LOCALIZATION NOTE (styleinspector.copyDataUriError): Text set in the clipboard
#. # if an error occurs when using the copyImageDataUrl context menu action
#. # (invalid image link, timeout, etc...)
#: styleinspector.copyImageDataUrlError
msgid "Failed to copy image Data-URL"
msgstr "Kopěrowanje wobrazowego datowego URL njejo se raźiło"

#. # LOCALIZATION NOTE (ruleView.contextmenu.showOrigSources): Text displayed in the rule view
#. # context menu.
#: ruleView.contextmenu.showOrigSources
msgid "Show Original Sources"
msgstr "Originalne žrědła pokazaś"

#. # LOCALIZATION NOTE (ruleView.contextmenu.showOrigSources.accessKey): Access key for
#. # the rule view context menu "Show original sources" entry.
#: ruleView.contextmenu.showOrigSources.accessKey
msgid "O"
msgstr "O"

#. # LOCALIZATION NOTE (ruleView.contextmenu.showMdnDocs): Text displayed in the rule view
#. # context menu to display docs from MDN for an item.
#: ruleView.contextmenu.showMdnDocs
msgid "Show MDN Docs"
msgstr "MDN-dokumenty pokazaś"

#. # LOCALIZATION NOTE (ruleView.contextmenu.showMdnDocs.accessKey): Access key for
#. # the rule view context menu "Show MDN docs" entry.
#: ruleView.contextmenu.showMdnDocs.accessKey
msgid "D"
msgstr "D"

#. # LOCALIZATION NOTE (ruleView.contextmenu.addNewRule): Text displayed in the
#. # rule view context menu for adding a new rule to the element.
#. # This should match addRuleButton.tooltip in styleinspector.dtd
#: ruleView.contextmenu.addNewRule
msgid "Add New Rule"
msgstr "Nowe pšawidło pśidaś"

#. # LOCALIZATION NOTE (ruleView.contextmenu.addRule.accessKey): Access key for
#. # the rule view context menu "Add rule" entry.
#: ruleView.contextmenu.addNewRule.accessKey
msgid "R"
msgstr "N"

#. # LOCALIZATION NOTE (computedView.contextmenu.selectAll): Text displayed in the
#. # computed view context menu.
#: computedView.contextmenu.selectAll
msgid "Select All"
msgstr "Wšykno wubraś"

#. # LOCALIZATION NOTE (computedView.contextmenu.selectAll.accessKey): Access key for
#. # the computed view context menu "Select all" entry.
#: computedView.contextmenu.selectAll.accessKey
msgctxt "computedView.contextmenu.selectAll.accessKey"
msgid "A"
msgstr "b"

#. # LOCALIZATION NOTE (computedView.contextmenu.copy): Text displayed in the
#. # computed view context menu.
#: computedView.contextmenu.copy
msgctxt "computedView.contextmenu.copy"
msgid "Copy"
msgstr "Kopěrowaś"

#. # LOCALIZATION NOTE (computedView.contextmenu.copy.accessKey): Access key for
#. # the computed view context menu "Copy" entry.
#: computedView.contextmenu.copy.accessKey
msgctxt "computedView.contextmenu.copy.accessKey"
msgid "C"
msgstr "C"

#. # LOCALIZATION NOTE (ruleView.contextmenu.copyLocation): Text displayed in the
#. # rule view context menu for copying the source location.
#: ruleView.contextmenu.copyLocation
msgid "Copy Location"
msgstr "Adresu kopěrowaś"

#. # LOCALIZATION NOTE (ruleView.contextmenu.copyPropertyDeclaration): Text
#. # displayed in the rule view context menu for copying the property declaration.
#: ruleView.contextmenu.copyPropertyDeclaration
msgid "Copy Property Declaration"
msgstr "Kakostnu deklaraciju kopěrowaś"

#. # LOCALIZATION NOTE (ruleView.contextmenu.copyPropertyName): Text displayed in
#. # the rule view context menu for copying the property name.
#: ruleView.contextmenu.copyPropertyName
msgid "Copy Property Name"
msgstr "Kakostne mě kopěrowaś"

#. # LOCALIZATION NOTE (ruleView.contextmenu.copyPropertyValue): Text displayed in
#. # the rule view context menu for copying the property value.
#: ruleView.contextmenu.copyPropertyValue
msgid "Copy Property Value"
msgstr "Kakostne gódnotu kopěrowaś"

#. # LOCALIZATION NOTE (ruleView.contextmenu.copyRule): Text displayed in the
#. # rule view context menu for copying the rule.
#: ruleView.contextmenu.copyRule
msgid "Copy Rule"
msgstr "Pšawidło kopěrowaś"

#. # LOCALIZATION NOTE (ruleView.contextmenu.copySelector): Text displayed in the
#. # rule view context menu for copying the selector.
#: ruleView.contextmenu.copySelector
msgid "Copy Selector"
msgstr "Selektor kopěrowaś"

