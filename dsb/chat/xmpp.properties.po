msgstr "Profilowy ID"

#. # LOCALZIATION NOTE (command.*):
#. #  These are the help messages for each command.
#: command.join3
msgid ""
"%S [&lt;room&gt;[@&lt;server&gt;][/&lt;nick&gt;]] [&lt;password&gt;]: Join a "
"room, optionally providing a different server, or nickname, or the room "
"password."
msgstr ""
"%S [&lt;room&gt;[@&lt;server&gt;][/&lt;nick&gt;]] [&lt;password&gt;]: Stupśo "
"do ruma, na žycenje móžośo drugi serwer, pśimě abo rumowe gronidło pódaś."

#: command.part2
msgid "%S [&lt;message&gt;]: Leave the current room with an optional message."
msgstr "%S [&lt;message&gt;]: Aktualny rum z opcionalneju powěsću spušćiś."

#: command.topic
msgid "%S [&lt;new topic&gt;]: Set this room's topic."
msgstr "%S [&lt;nowa tema&gt;]: Temu toś togo ruma nastajiś."

#: command.ban
msgid ""
"%S &lt;nick&gt;[&lt;message&gt;]: Ban someone from the room. You must be a "
"room administrator to do this."
msgstr ""
"%S &lt;nick&gt;[&lt;message&gt;]: Někogo z ruma wugnaś. Musyśo rumowy "
"administrator byś, aby to cynił."

#: command.kick
msgid ""
"%S &lt;nick&gt;[&lt;message&gt;]: Remove someone from the room. You must be "
"a room moderator to do this."
msgstr ""
"%S &lt;nick&gt;[&lt;message&gt;]: Někogo z ruma wótpóraś. Musyśo rumowy "
"moderator byś, aby to cynił."

#: command.invite
msgid ""
"%S &lt;jid&gt;[&lt;message&gt;]: Invite a user to join the current room with "
"an optional message."
msgstr ""
"%S &lt;jid&gt;[&lt;messag&gt;]: Pśepšosćo wužywarja do aktualnego ruma z "
"opcionelneju powěsću stupiś."

#: command.me
msgid "%S &lt;action to perform&gt;: Perform an action."
msgstr "%S &lt;akcija, kótaraž ma se pśewjasć&gt;: Akciju pśewjasć."

#: command.nick
msgid "%S &lt;new nickname&gt;: Change your nickname."
msgstr "%S &lt;new nickname&gt;: Wašo pśimě změniś."

#: command.msg
msgid ""
"%S &lt;nick&gt; &lt;message&gt;: Send a private message to a participant in "
"the room."
msgstr ""
"%S &lt;nick&gt; &lt;message&gt;: Wobdźělnikoju w rumje priwatnu powěsć "
"pósłaś."

