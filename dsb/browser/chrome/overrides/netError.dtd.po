#: loadError.label
msgid "Problem loading page"
msgstr "Zmólka pśi cytanju boka"

#: retry.label
msgid "Try Again"
msgstr "Hyšći raz wopytaś"

#. Specific error messages
#: connectionFailure.title
msgid "Unable to connect"
msgstr "Zwisk njejo móžno"

#: connectionFailure.longDesc
msgctxt "connectionFailure.longDesc"
msgid "&sharedLongDesc;"
msgstr "&sharedLongDesc;"

#: deniedPortAccess.title
msgid "This address is restricted"
msgstr "Toś ta adresa jo wobgranicowana"

#: deniedPortAccess.longDesc
msgctxt "deniedPortAccess.longDesc"
msgid ""
msgstr " "

#: dnsNotFound.title
msgid "Server not found"
msgstr "Serwer njejo se namakał"

#: dnsNotFound.longDesc
msgid ""
"<ul> \n"
"<li>Check the address for typing errors such as \n"
"<strong>ww</strong>.example.com instead of \n"
"<strong>www</strong>.example.com</li> \n"
"<li>If you are unable to load any pages, check your computer's network \n"
"connection.</li> \n"
"<li>If your computer or network is protected by a firewall or proxy, make "
"sure \n"
"that &brandShortName; is permitted to access the Web.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Pśeglědajśo adresu za pisańskimi zmólkami kaž \n"
"<strong>ww</strong>.pśikład.de město \n"
"<strong>www</strong>.pśikład.de</li> \n"
"<li>Jolic njamóžośo někotare boki cytaś, pśekontrolěrujśo seśowy zwisk \n"
"swójogo licadła.</li> \n"
"<li>Jolic wašo licadło abo seś šćita se z wognjoweju murju abo proksy, "
"zawěsććo, \n"
"až &brandShortName; ma pšawo na Web pśistup měś.</li> \n"
"</ul>"

#: fileNotFound.title
msgid "File not found"
msgstr "Dataja njejo se namakała"

#: fileNotFound.longDesc
msgid ""
"<ul> \n"
"<li>Check the file name for capitalization or other typing errors.</li> \n"
"<li>Check to see if the file was moved, renamed or deleted.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Pśeglědajśo datajowe mě za wjelikopisańskimi abo hynakšymi pisańskimi "
"zmólkami.</li> \n"
"<li>Pśespytajśo, lěć dataja jo se pśesunuła, pśemjeniła abo wótpórała.</"
"li> \n"
"</ul>"

#: generic.title
msgid "Oops."
msgstr "Hopla."

#: generic.longDesc
msgid "<p>&brandShortName; can't load this page for some reason.</p>"
msgstr ""
"<p>&brandShortName; njamóžo toś ten bok někakeje pśicyny dla cytaś.</p>"

#: malformedURI.title
msgid "The address isn't valid"
msgstr "Adresa njejo płaśiwa"

#: malformedURI.longDesc
msgid ""
"<ul> \n"
"<li>Web addresses are usually written like \n"
"<strong>http://www.example.com/</strong></li> \n"
"<li>Make sure that you're using forward slashes (i.e. \n"
"<strong>/</strong>).</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Webadrese pišu se zwětšego \n"
"<strong>http://www.pśikład.de/</strong>.</li> \n"
"<li>Zawěsććo, až wužywaśo doprědka schylone nakósne smužki (t.j. \n"
"<strong>/</strong>).</li> \n"
"</ul>"

#: netInterrupt.title
msgid "The connection was interrupted"
msgstr "Zwisk jo se pśetergnuł"

#: netInterrupt.longDesc
msgctxt "netInterrupt.longDesc"
msgid "&sharedLongDesc;"
msgstr "&sharedLongDesc;"

#: notCached.title
msgid "Document Expired"
msgstr "Dokument jo wótběgnuł"

#: notCached.longDesc
msgid ""
"<p>The requested document is not available in &brandShortName;'s cache.</"
"p><ul><li>As a security precaution, &brandShortName; does not automatically "
"re-request sensitive documents.</li><li>Click Try Again to re-request the "
"document from the website.</li></ul>"
msgstr ""
"<p>Póžedany dokument njestoj w pufrowaku &brandShortName; k dispoziciji.</"
"p><ul><li>Ako wěstnotna napšawa &brandShortName; njewótwołujo delikatne "
"dokumenty awtomatiski znowego.</li><li>Klikniśo na Hyšći raz, aby dokument "
"hyšći raz z websedła wótwołał.</li></ul>"

#: netOffline.title
msgid "Offline mode"
msgstr "Offline-modus"

#: netOffline.longDesc2
msgid ""
"<ul> \n"
"<li>Press \"Try Again\" to switch to online mode and reload the page.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Klikniśo na \"Hyšći raz\", aby do online-modusa pśejšeł a bok znowego "
"zacytał.</li> \n"
"</ul>"

#: contentEncodingError.title
msgid "Content Encoding Error"
msgstr "Zmólka pśi koděrowanju wopśimjeśa"

#: contentEncodingError.longDesc
msgctxt "contentEncodingError.longDesc"
msgid ""
"<ul> \n"
"<li>Please contact the website owners to inform them of this problem.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Pšosym stejśo se z wobsejźarjami websedła do zwiska, aby je wó toś tom "
"problemje informěrował.</li> \n"
"</ul>"

#: unsafeContentType.title
msgid "Unsafe File Type"
msgstr "Njewěsty datajowy typ"

#: unsafeContentType.longDesc
msgctxt "unsafeContentType.longDesc"
msgid ""
"<ul> \n"
"<li>Please contact the website owners to inform them of this problem.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Pšosym stejśo se z wobsejźarjami websedła do zwiska, aby je wó toś tom "
"problemje informěrował.</li> \n"
"</ul>"

#: netReset.title
msgid "The connection was reset"
msgstr "Zwisk jo se slědk stajił"

#: netReset.longDesc
msgctxt "netReset.longDesc"
msgid "&sharedLongDesc;"
msgstr "&sharedLongDesc;"

#: netTimeout.title
msgid "The connection has timed out"
msgstr "Zwisk jo cas pśekšocył"

#: netTimeout.longDesc
msgctxt "netTimeout.longDesc"
msgid "&sharedLongDesc;"
msgstr "&sharedLongDesc;"

#: unknownProtocolFound.title
msgid "The address wasn't understood"
msgstr "Adresa njejo se zrozměła"

#: unknownProtocolFound.longDesc
msgid ""
"<ul> \n"
"<li>You might need to install other software to open this address.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Snaź musyśo druge programy instalěrowaś, aby se toś ta adresa wócyniła.</"
"li> \n"
"</ul>"

#: proxyConnectFailure.title
msgid "The proxy server is refusing connections"
msgstr "Proksy-serwer wótpokazujo zwiski"

#: proxyConnectFailure.longDesc
msgid ""
"<ul> \n"
"<li>Check the proxy settings to make sure that they are correct.</li> \n"
"<li>Contact your network administrator to make sure the proxy server is \n"
"working.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Pśekontrolěrujśo proksy-nastajenja, aby se zawěsćiło, až wóne su "
"korektne.</li> \n"
"<li>Stajśo ze swójim seśowym administratorom, aby zawěsćił, až proksy-"
"serwer \n"
"źěła.</li> \n"
"</ul>"

#: proxyResolveFailure.title
msgid "Unable to find the proxy server"
msgstr "Njejo móžno proksy-serwer namakaś"

#: proxyResolveFailure.longDesc
msgid ""
"<ul> \n"
"<li>Check the proxy settings to make sure that they are correct.</li> \n"
"<li>Check to make sure your computer has a working network connection.</"
"li> \n"
"<li>If your computer or network is protected by a firewall or proxy, make "
"sure \n"
"that &brandShortName; is permitted to access the Web.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Pśekontrolěrujśo proksy-nastajenja, aby zawěsćił, až su korektne.</li> \n"
"<li>Skontrolěrujśo, lěc wašo licadło ma funkcioněrujucy seśowy zwisk.</li> \n"
"<li>Jolic wašo licadło abo waša seś šćita se z wognjoweju murju abo proksy, "
"zawěsććo, \n"
"až &brandShortName; ma pšawo na Web pśistup měś.</li> \n"
"</ul>"

#: redirectLoop.title
msgid "The page isn't redirecting properly"
msgstr "BokStrona pšawje njepósrědnja"

#: redirectLoop.longDesc
msgid ""
"<ul> \n"
"<li>This problem can sometimes be caused by disabling or refusing to "
"accept \n"
"cookies.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Toś ten problem zawinujo se wótergi pśez znjemóžnjanje abo "
"wótpokaowanje \n"
"cookiejow.</li> \n"
"</ul>"

#: unknownSocketType.title
msgid "Unexpected response from server"
msgstr "Njewótčakane wótegrono ze serwera"

#: unknownSocketType.longDesc
msgid ""
"<ul> \n"
"<li>Check to make sure your system has the Personal Security Manager \n"
"installed.</li> \n"
"<li>This might be due to a non-standard configuration on the server.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Skontrolěrujśo, lěc Personal Security Manager jo instalěrowany \n"
"na wašom systemje.</li> \n"
"<li>Pśicyna jo snaź njestandardna konfiguracija na serwerje.</li> \n"
"</ul>"

#: nssFailure2.title
msgctxt "nssFailure2.title"
msgid "Secure Connection Failed"
msgstr "Wěsty zwisk njemóžno"

#: nssFailure2.longDesc2
msgid ""
"<ul> \n"
"<li>The page you are trying to view cannot be shown because the authenticity "
"of the received data could not be verified.</li> \n"
"<li>Please contact the website owners to inform them of this problem.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Bok, kótaryž cośo se woglědaś, njedajo se pokazaś, dokulaž awtentiskosć "
"dostanych datow njedajo se pśeglědaś.</li> \n"
"<li>Pšosym stajśo se z wobsejźarjami websedła do zwiska, aby je wó toś tom "
"problemje informěrował.</li> \n"
"</ul>"

#: nssBadCert.title
msgctxt "nssBadCert.title"
msgid "Secure Connection Failed"
msgstr "Wěsty zwisk njemóžno"

#: nssBadCert.longDesc2
msgid ""
"<ul> \n"
"<li>This could be a problem with the server's configuration, or it could "
"be \n"
"someone trying to impersonate the server.</li> \n"
"<li>If you have connected to this server successfully in the past, the error "
"may \n"
"be temporary, and you can try again later.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>To by mógło problem z konfiguraciju serwera byś, abo by mógło byś, \n"
"až něchten wopytujo serwer imitěrowaś.</li> \n"
"<li>Jolic sćo był ze serwerom w zachadnosći wuspěšnje zwězany, by mógł "
"zmólka \n"
"snaź nachylny byś a móžośo pózdźej hyšći raz wopytaś.</li> \n"
"</ul>"

#: sharedLongDesc
msgid ""
"<ul> \n"
"<li>The site could be temporarily unavailable or too busy. Try again in a "
"few \n"
"moments.</li> \n"
"<li>If you are unable to load any pages, check your computer's network \n"
"connection.</li> \n"
"<li>If your computer or network is protected by a firewall or proxy, make "
"sure \n"
"that &brandShortName; is permitted to access the Web.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Sedło njeby mógło nachylu k dispoziciji staś abo by mógło pśeśěžone byś. "
"Wopytajśo za mało wokognuśow \n"
"hyšći raz.</li> \n"
"<li>Jolic njamóžośo někotare boki cytaś, pśespytajśo zwisk wašeje \n"
"licadłoweje seśi.</li> \n"
"<li>Jolic wašo licadło abo waša seś šćita se z wognjoweju murju abo "
"proksy, \n"
"zawěsććo, až &brandShortName; ma pšawo na Web pśistup měś.</li> \n"
"</ul>"

#: malwareBlocked.title
msgid "Suspected Attack Site!"
msgstr "Napadujuce sedło se góda!"

#: malwareBlocked.longDesc
msgid ""
"<p>Attack sites try to install programs that steal private information, use "
"your computer to attack others, or damage your system.</p> \n"
"<p>Website owners who believe their site has been reported as an attack site "
"in error may <a href='http://www.stopbadware.org/home/reviewinfo' >request a "
"review</a>.</p>"
msgstr ""
"<p>Napadujuce sedła wopytuju programy instalěrowaś, kótarež kšadnu priwatne "
"informacije, wašo licadko wužywaś, aby druge napadowali abo wobškóduju waš "
"system.</p> \n"
"<p>Wobsejźarje websedła, kótarež wěrje, až jich sedło jo se zamylnje ako "
"napadujuce sedło wuzjawiło, mógu wó <a href='http://www.stopbadware.org/home/"
"reviewinfo' >pśepytowanje pšosyś</a>.</p>"

#: phishingBlocked.title
msgid "Suspected Web Forgery!"
msgstr "Websfalšowanka se góda!"

#: phishingBlocked.longDesc
msgid ""
"<p>Entering any personal information on this page may result in identity "
"theft or other fraud.</p> \n"
"<p>These types of web forgeries are used in scams known as phishing attacks, "
"in which fraudulent web pages and emails are used to imitate sources you may "
"trust.</p>"
msgstr ""
"<p>Zapódawanje wósobinskich informacijow na toś tom boku móžo ku "
"kšadnjenjeju identity abo drugej wobšuźe wjasć.</p> \n"
"<p>Toś te typy webwobšudow wužywaju se w wobšudnistwach ako snuchlece "
"napady, w kótarychž wobšudne webboki a mejlki se wužywaju, aby žrědła "
"imitěrowali, kótarymž dowěriśo.</p>"

#: cspBlocked.title
msgid "Blocked by Content Security Policy"
msgstr "Pśez wěstotne pšawidła za wopśimjeśe zablokěrowany"

#: cspBlocked.longDesc
msgid ""
"<p>&brandShortName; prevented this page from loading in this way because the "
"page has a content security policy that disallows it.</p>"
msgstr ""
"<p>&brandShortName; jo zacytowanjoju toś togo boka na toś ten nałog "
"zajźował, dokulaž bok ma wěstotne pšawidła za wopśimjeśe, kótarež to "
"njedowóluju.</p>"

#: corruptedContentError.title
msgid "Corrupted Content Error"
msgstr "Zmólka - wobškóźone wopśimjeśe"

#: corruptedContentError.longDesc
msgid ""
"<p>The page you are trying to view cannot be shown because an error in the "
"data transmission was detected.</p><ul><li>Please contact the website owners "
"to inform them of this problem.</li></ul>"
msgstr ""
"<p>Bok, kótaryž cośo se woglědaś, njedajo se pokazaś, dokulaž jo se zmólka "
"pśi pśenosowanju datow namakała.</p><ul><li>Pšosym stajśo se z wobsejźarjami "
"websedła do zwiska, aby je wó toś tom problemje informěrował.</li></ul>"

#: securityOverride.linkText
msgid "Or you can add an exception…"
msgstr "Abo móžośo wuwześe pśidaś…"

#: securityOverride.getMeOutOfHereButton
msgid "Get me out of here!"
msgstr "Cu how wen!"

#: securityOverride.exceptionButtonLabel
msgid "Add Exception…"
msgstr "Wuwześe pśidaś…"

#. <!-- LOCALIZATION NOTE (securityOverride.warningContent) - Do not translate the
#. contents of the <button> tags. It uses strings already defined above. The
#. button is included here (instead of netError.xhtml) because it exposes
#. functionality specific to firefox.
#: securityOverride.warningContent
msgid ""
"<p>You should not add an exception if you are using an internet connection "
"that you do not trust completely or if you are not used to seeing a warning "
"for this server.</p> \n"
" \n"
"<button id='getMeOutOfHereButton'>&securityOverride.getMeOutOfHereButton;</"
"button> \n"
"<button id='exceptionDialogButton'>&securityOverride.exceptionButtonLabel;</"
"button>"
msgstr ""
"<p>Wy njeby měł wuwześe pśidaś, jolic wužywaśo internetny zwisk, kótaremuž "
"cele njedowěriśo abo jolic njewiźiśo warnowanje za toś ten serwer.</p> \n"
" \n"
"<button id='getMeOutOfHereButton'>&securityOverride.getMeOutOfHereButton;</"
"button> \n"
"<button id='exceptionDialogButton'>&securityOverride.exceptionButtonLabel;</"
"button>"

#: errorReporting.title
msgid "Report this error"
msgstr "Toś tu zmólku k wěsći daś"

#: errorReporting.longDesc
msgid ""
"Reporting the address and certificate information for <span id='hostname'></"
"span> will help us identify and block malicious sites. Thanks for helping "
"create a safer web!"
msgstr ""
"Wuzjawjanje adrese a certifikatowych informacijow za <span id='hostname'></"
"span> buźo wam pomagaś, złosne sedła identificěrowaś a blokěrowaś. Wjeliki "
"źěk, až pomagaśo, wěsćejšy web napóraś!"

#: errorReporting.automatic
msgid "Automatically report errors in the future"
msgstr "W pśichoźe zmólki awtomatiski k wěsći daś"

#: errorReporting.learnMore
msgid "Learn more…"
msgstr "Dalšne informacije…"

#: errorReporting.sending
msgid "Sending report"
msgstr "Rozpšawa se sćelo"

#: errorReporting.sent
msgid "Report sent"
msgstr "Rozpšawa jo se pósłała"

#: errorReporting.report
msgid "Report"
msgstr "K wěsći daś"

#: errorReporting.tryAgain
msgid "Try again"
msgstr "Hyšći raz wopytaś"

#: remoteXUL.title
msgid "Remote XUL"
msgstr "Daloki XUL"

#: remoteXUL.longDesc
msgid ""
"<p><ul><li>Please contact the website owners to inform them of this problem."
"</li></ul></p>"
msgstr ""
"<p><ul><li>Pšosym stajśo se z wobsejźarjami websedła do zwiska, aby je wó "
"problemje informěrował.</li></ul></p>"

#: sslv3Used.title
msgid "Unable to Connect Securely"
msgstr "Wěsty zwisk njejo móžno"

#. <!-- LOCALIZATION NOTE (sslv3Used.longDesc) - Do not translate
#. "ssl_error_unsupported_version".
#: sslv3Used.longDesc
msgid "Advanced info: ssl_error_unsupported_version"
msgstr "Rozšyrjona informacija: ssl_error_unsupported_version"

#: sslv3Used.learnMore
msgid "Learn More…"
msgstr "Dalšne informacije…"

#~ msgid ""
#~ "&brandShortName; cannot guarantee the safety of your data on <span "
#~ "id='hostname'></span> because it uses SSLv3, a broken security protocol."
#~ msgstr ""
#~ "&brandShortName; njamóžo wěstotu wašych datow na <span id='hostname'></"
#~ "span> garantěrowaś, dokulaž wužywa SSLv3, wobškóźony wěstotny protokol."

#~ msgid "Advanced info: <span id='errorcode'></span>"
#~ msgstr "Rozšyrjona informacija: <span id='errorcode'></span>"

#~ msgid ""
#~ "<ul> \n"
#~ "<li>The page you are trying to view cannot be shown because the "
#~ "authenticity of the received data could not be verified.</li> \n"
#~ "<li>Please contact the website owners to inform them of this problem. "
#~ "Alternatively, use the command found in the help menu to report this "
#~ "broken site.</li> \n"
#~ "</ul>"
#~ msgstr ""
#~ "<ul> \n"
#~ "<li>Bok, kótaryž cośo se woglědaś, njedajo se pokazaś, dokulaž "
#~ "awtentiskosć pśiwzetych datow njedajo se pśespytaś.</li> \n"
#~ "<li>Pšosym stajśo se z wobsejźarjami websedła do zwiska, aby je wó toś "
