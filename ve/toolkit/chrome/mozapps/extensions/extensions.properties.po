msgstr "Tendelani Nyengedzedzo"

#: restartBeforeDisableTitle
msgid "Disable Extension"
msgstr "Hanelani Nyengedzedzo"

#: restartBeforeEnableMessage
msgid "%S will be enabled the next time you restart %S."
msgstr "%S i ḓo tendeliwa tshifhingani tshi ḓaho musi ni tshi thoma hafhu %S."

#: restartBeforeDisableMessage
msgid "%S will be disabled the next time you restart %S."
msgstr "%S i ḓo haneliwa tshifhingani tshi ḓaho musi ni tshi thoma hafhu %S."

#: restartBeforeUninstallTitle
msgid "Uninstall"
msgstr "Dzhenisululani"

#: restartBeforeUninstallMessage
msgid "%S will be uninstalled the next time you restart %S."
msgstr "%S i ḓo dzhenisululiwa tshifhingani tshi ḓaho musi ni tshi thoma hafhu %S."

#: restartBeforeInstallMessage
msgid "%S will be installed the next time you restart %S."
msgstr "%S i ḓo dzhisiwa tshifhingani tshi ḓaho musi ni tshi thoma hafhu %S."

#: restartBeforeUpgradeMessage
msgid "%S will be upgraded the next time you restart %S."
msgstr "%S i ḓo Khwiniswa tshifhingani tshi ḓaho musi ni tshi thoma hafhu %S."

#: incompatibleUpdateMessage
msgid "%S is checking for a compatibility update to %S."
msgstr "%S i khou ṱoḓa khwinifhadzo dzi elanaho nayo kha %S."

#: queryUninstallExtensionMessage
msgid "If you uninstall %S, the functionality it offers will no longer be available. Do you want to uninstall %S?"
msgstr "Arali na dzhenisulula %S, mushumo une wa ṋetshedza u nga si tsha vha hone. Ni khou toda u dzhenisulula %S?"

#: queryUninstallThemeMessage
msgid "Do you want to uninstall %S?"
msgstr "Ni khou ṱoḓa u dzhenisulula %S?"

#: queryUninstallTitle
msgid "Uninstall %S"
msgstr "Dzhenisululani %S"

#: installSuccess
msgid "Install completed successfully"
msgstr "U dzhenisa ho fhela zwavhuḓi"

#: installWaiting
msgid "Waiting..."
msgstr "Lindela..."

#: installInstalling
msgid "Installing..."
msgstr "Dzhenisa..."

#: droppedInWarning
