msgid "AUS: Update XML File Malformed (200)"
msgstr "AUS: Faela ya XML yo Khwinifhadzwaho a i ngo Vhumbiwa Zwavhudi (200)"

#: checker_error-403
msgid "AUS: Access Denied (403)"
msgstr "AUS: U Dzhena ho Haneliwa (403)"

#: checker_error-404
msgid "AUS: Update XML File Not Found (404)"
msgstr "AUS: Faela ya XML yo Khwinifhadzwaho a i ngo Wanala (404)"

#: checker_error-500
msgid "AUS: Internal Server Error (500)"
msgstr "AUS: Vhukhakhi ha Seva ha Ngomu (500)"

#: checker_error-2152398878
msgid "AUS: Update Server Not Found (Check your internet connection or contact your Administrator)"
msgstr "AUS: Seva ya Khwinifhadzo a i ngo Wanala (Tolani thumanyo yanu ya ithanete kana ni kwame Mulanguli wanu)"

#: checker_error-2152398890
msgid "Proxy Server Not Found (Check your internet connection or contact your Administrator)"
msgstr "Seva Thusedzi a i ngo Wanala (Tolani thumanyo yanu ya ithanete kana ni kwame Mulanguli wanu)"

#: checker_error-2152398861
msgid "AUS: Connection Refused"
msgstr "AUS: Vhutumani ho Haneliwa"

#: checker_error-2152398920
msgid "Proxy Server Connection Refused (Contact your Administrator)"
msgstr "Vhutumani na Seva Thusedzi ho Hanelwa (Kwamani Mulanguli wanu)"

#: checker_error-2152398862
msgid "AUS: Connection timed out"
msgstr "AUS: Vhutumani ho fhelelwa nga tshifhinga"

#: checker_error-2152398864
msgid "Network is Offline (Go online)"
msgstr "Netiweke i Nnda ha Laini (Iyani lainini)"

#: checker_error-2152398849
msgid "Failed (Unknown Reason)"
msgstr "Yo Kundelwa (Ndivho i sa Divhei)"

#: checker_error-2152398868
msgid "AUS: No data was received (Please try again)"
msgstr "AUS: A huna vhutanzi ho wanalaho (Ni khou humbelwa uri ni lingedze hafhu)"

#: checker_error-2152398919
msgid "AUS: Data transfer was interrupted (Please try again)"
msgstr "AUS: U rathisela vhutanzi ho dzhenelelwa (Ni khou humbelwa uri ni lingedze hafhu)"

#: checker_error-2152398867
msgid "AUS: Port not allowed (Contact your Administrator)"
msgstr "AUS: Photho a yo ngo tendelwa (Kwamani Mulanguli wanu)"

