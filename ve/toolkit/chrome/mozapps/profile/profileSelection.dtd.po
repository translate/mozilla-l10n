msgid "Profile Name:"
msgstr "Dzina ḽa Mbonwasia:"

#: start.label
msgid "Start &brandShortName;"
msgstr "Thomani &Dzina ḽipfufhi ḽa pfungavhune;"

#: exit.label
msgid "Exit"
msgstr "Ibvani"

#: availprofiles.label
msgid "Available Profiles"
msgstr "Mbanwasia Dzire Hone"

#: newButton.label
#: newButton.accesskey
msgid "&Create Profile..."
msgstr "&Vhumbani Mbonwasia..."

#: renameButton.label
#: renameButton.accesskey
msgid "&Rename Profile..."
msgstr "&Bvisani Mbonwasia..."

#: deleteButton.label
#: deleteButton.accesskey
msgid "&Delete Profile..."
msgstr "&Thuthani Mbonwasia..."

# manager entities
#: pmDescription.label
msgid "&brandShortName; stores information about your settings, preferences, and other user items in your user profile."
msgstr "&Dzina Lipfufhi la pfungavhune; ḽi vhulunga vhutanzi nga ha nzudzanyo dzanu, zwitakalelwa, na zwitenwa zwa mushumisi kha mbonwasia ya mushumisi yanu."

#: offlineState.label
#: offlineState.accesskey
msgid "W&ork offline"
msgstr "&Shumani nnḓa ha laini"

#: autoSelect.label
#: autoSelect.accesskey
msgid "Don't a&sk at startup"
msgstr "Ni songo &vhudzisa mathomoni"

