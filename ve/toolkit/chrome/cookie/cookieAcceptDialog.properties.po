#: hostColon
msgid "Host:"
msgstr "Muṱanganedzi"

#: domainColon
msgid "Domain:"
msgstr "Fhethu"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "Ṱhumanyo dzo ṅwalwaho nga khoudu fhedzi"

#: forAnyConnection
msgid "Any type of connection"
msgstr "Lushaka luṅwe na luṅwe lwa ṱhumanyo"

#: atEndOfSession
msgid "at end of session"
msgstr "mafhedziseloni a sesheni"

#: showDetails
msgid "Show Details"
msgstr "Sumbedzani Zwidodombedzwa"

#: hideDetails
msgid "Hide Details"
msgstr "Dzumbani Zwidodombedzwa"

#: detailsAccessKey
msgid "T"
msgstr "T"

#: permissionToSetACookie
msgid "The site %S wants to set a cookie."
msgstr "Fhethu  %S i khou ṱoḓa u vhekanya kokisi"

#: permissionToSetSecondCookie
msgid "The site %S wants to set a second cookie."
msgstr "Fhethu  %S i khou ṱoḓa u vhekanya kokisi la vhuvhili "

#  LOCALIZATION NOTE (PermissionToSetAnotherCookie): First %S: sitename, second %S: number of cookies already present for that site
#: permissionToSetAnotherCookie
msgid "The site %S wants to set another cookie.\n"
"You already have %S cookies from this site."
msgstr "Fhethu %S hu khou ṱoḓa u vhekanya kokisi ḽiṅwe. \nNo no ḓi vha na makokisi %S hafha fhethu."

#: permissionToModifyCookie
msgid "The site %S wants to modify an existing cookie."
msgstr "Fhethu %S hu khou ṱoḓa u khwinifhadza kokisi ḽine ḽa vha hone. "

#: permissionToAcceptImage
msgid "The site %S wants to load an image."
msgstr "Fhethu %S ḽi khou ṱoḓa u dzhenis tshivhumbeo."

#: rememberThisDecision
msgid "Remember this decision"
msgstr "Humbulani nga ha hei tsheo"

