msgid "Go to line"
msgstr "Iyani ḽainini"

#: goToLineText
msgid "Enter line number"
msgstr "Dzhenisani nomboro ya ḽaini"

#: invalidInputTitle
msgid "Invalid input"
msgstr "Tshitenwa tshi si ho mulayoni"

#: invalidInputText
msgid "The line number entered is invalid."
msgstr "Nomboro ya ḽaini yo dzheniswaho a i ho mulayoni."

#: outOfRangeTitle
msgid "Line not found"
msgstr "Ḽaini a yo ngo wanala"

#: outOfRangeText
msgid "The specified line was not found."
msgstr "Ḽanini yo bulwaho a yo ngo wanala."

