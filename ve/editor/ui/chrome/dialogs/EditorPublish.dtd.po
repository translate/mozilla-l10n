# Window title
#: windowTitle.label
msgid "Publish Page"
msgstr "Anḓadzani Siaṱari"

#: windowTitleSettings.label
msgid "Publish Settings"
msgstr "Anḓadzani Nzudzanyo"

#: publishTab.label
msgid "_: publishTab.label\n"
"Publish"
msgstr "Anḓadzani"

#: settingsTab.label
msgid "Settings"
msgstr "Nzudzanyo"

#: publishButton.label
msgid "_: publishButton.label\n"
"Publish"
msgstr "Anḓadzani"

# Publish Tab Panel
#: siteList.label
msgid "_: siteList.label\n"
"Site Name:"
msgstr "Dzina ḽa Saithi:"

#: siteList.tooltip
msgid "Choose the site that you want to publish to"
msgstr "Nangani saithi ine na khou ṱoḓa u anḓadza yone"

#: newSiteButton.label
msgid "New Site"
msgstr "Saithi Ntswa"

#: siteDirList.label
msgid "Select or type a directory name in your publishing site:"
msgstr "Nangani kana ni thaiphe dzina la tsumbanḓila ine ya vha kha saithi yaṇu ya u anḓadza:"

#: siteDirList.tooltip
msgid "Choose or enter the name of the remote subdirectory to publish to"
msgstr "Nangani kana ni dzhenise dzina ḽa tsumbanḓila thukhu i songo bvelelaho ine na khou ṱoḓa u i anḓadza kha "

#: docDirList.label
msgid "Site subdirectory for this page:"
msgstr "Tsumbanḓila thukhu ya saithi ya heḽi siaṱari: "

#: publishImgCheckbox.label
msgid "Include images and other files"
msgstr "Katela zwifanyiso na dziṅwe faela"

#: publishImgCheckbox.tooltip
msgid "Publish images and other files referenced by this page"
msgstr "Anḓadzani zwifanyiso na dziṅwe faela dzo laedzwaho nga heḽi siaṱari"

#: sameLocationRadio.label
msgid "Use same location as page"
msgstr "Shumisani fhethu huthihi na siaṱari"

#: sameLocationRadio.tooltip
msgid "Publish files to the same location as the page"
msgstr "Anḓadzani dzifaela fhethu huthihi na siaṱari"

#: useSubdirRadio.label
msgid "Use this site subdirectory:"
msgstr "Shumisani tsumbanḓila ṱhukhu ya hei saithi:"

#: useSubdir.tooltip
msgid "Choose or enter name of remote subdirectory where files will be published"
msgstr "Nangani kana ni nange dzine la tsumbanḓila ṱhukhu i songo bvelelaho hune faela dza ḓo anḓadzwa hone"

#: chooseDirButton.label
msgid "Choose..."
msgstr "Nangani..."

#: pageTitle.label
msgid "Page Title:"
msgstr "Ṱhoho ya Siaṱari: "

#: pageTitle.tooltip
msgid "Enter a title to identify the page in the window and in bookmarks"
msgstr "Dzhenisani ṱhoho u itela u sumba siaṱari kha fasiṱere na kha tswayo dza bugu "

#: pageTitleExample.label
msgid "e.g.: &quot;My Web Page&quot;"
msgstr "tsumbo: &quot;My Web Page&quot;"

#: filename.label
msgid "Filename:"
msgstr "Dzinaḽafaela"

#: filename.tooltip
msgid "Enter a name for this file, including '.html' for a web page"
msgstr "Dzhenisani dzila ḽa hei faela, zwi tshi katela '.html' ya siaṱari ḽa web"

#: filenameExample.label
msgid "e.g,: &quot;mypage.html&quot;"
msgstr "tsumbo: &quot;mypage.html&quot;"

#: relatedDocs.label
msgid "Other files to publish"
msgstr "Dziṅwe faela dza u anḓadza"

#: setDefaultButton.label
msgid "Set as Default"
msgstr "Dzudzanyani sa Tshidzivhisi"

#: setDefSiteAndDir.label
msgid "Set as Default Site and Directory"
msgstr "Dzudzanyani sa Saithi ya u Dzivhisa na Tsumbanḓila"

#: removeButton.label
msgid "Remove Site"
msgstr "Bvisani Saithi"

# Settings Tab Panel
#: publishSites.label
msgid "Publishing Sites"
msgstr "Anḓadzani Dzisaithi"

#: serverInfo.label
msgid "Server Information"
msgstr "Vhuṱanzi ha Seva "

#: loginInfo.label
msgid "Login Information"
msgstr "Vhuṱanzi ha u Loga"

#: siteName.label
msgid "_: siteName.label\n"
"Site Name:"
msgstr "Dzina ḽa Saithi:"

#: siteName.tooltip
msgid "A nickname that identifies this publishing site (e.g.: 'MySite')"
msgstr "Dzina ḽa miswaswo ḽine ḽa sumba hei saithi ya u anḓadza (tsumbo,: 'SaithiYanga')"

#: siteUrl.label
msgid "Publishing address (e.g.: 'ftp://ftp.myisp.com/myusername'):"
msgstr "Diresi ya u anḓadza (tsumbo,: 'ftp://ftp.myisp.com/myusername'):"

#: siteUrl.tooltip
msgid "The FTP:// or HTTP:// address provided by your ISP or web hosting service"
msgstr "Ḓiresi ya FTP:// kana HTTP:// yo ṇetshedzwaho nga ISP yaṇu kana tshumelo ya u kuvhanganya ya web"

#: browseUrl.label
msgid "HTTP address of your homepage (e.g.: 'http://www.myisp.com/myusername'):"
msgstr "Diresi ya HTTP ya siaḽari lanu la hayani (tsumbo,: 'http://www.myisp.com/myusername'):"

#: browseUrl.tooltip
msgid "The HTTP:// address of your home directory (don't include filename)"
msgstr "Ḓiresi ya HTTP:// ya tsumbanḓila ya hayani yaṇu (a i kateli dzina ḽa faela)"

#: username.label
msgid "User name:"
msgstr "Dzina ḽa mushumisi:"

#: username.tooltip
msgid "The user name you use to log in to your ISP or web hosting service"
msgstr "Dzina ḽa mushumisi ḽine na ḽi shumisa u ḽoga kha ISP yaṇu kana tshumelo ya u kuvhanganya ya web"

#: password.label
msgid "Password:"
msgstr "Phasiwede:"

#: password.tooltip
msgid "The password associated with your user name"
msgstr "Phasiwede i elanaho na dzina ḽa mushumisi ḽanu"

#: savePassword.label
msgid "Save Password"
msgstr "Vhulungani Phasiwede"

#: savePassword.tooltip
msgid "Select this to save your password securely using Password Manager"
msgstr "Nangani hezwi u itela u vhulunga phasiwede yanu yo tsireledzea ni tshi khou shumisa Mulanguli wa Phasiwede"

