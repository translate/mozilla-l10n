# These strings are for use specifically in the editor's image and form image dialogs.
# Window title
#: windowTitle.label
msgid "Image Properties"
msgstr "Vhuvha ha Tshivhumbeo"

#: pixelsPopup.value
msgid "pixels"
msgstr "dzipixel"

#: percentPopup.value
msgid "percent"
msgstr "phesenthe"

# These are in the Location tab panel
#: locationEditField.label
#: locationEditField.accessKey
msgid "Image &Location:"
msgstr "Tshumbeo &Fhethu:"

#: locationEditField.tooltip
msgid "Type the image's filename or location"
msgstr "Thaiphani dzina la faela la tshivhumbeo kana fhethu"

#: title.label
#: title.accessKey
msgid "&Tooltip:"
msgstr "&Tsivhudzo ya tshishumiswa:"

#: title.tooltip
msgid "The html 'title' attribute that displays as a tooltip"
msgstr "Ndeme ya 'title' ya html ine ya tanea sa tsivhudzo ya tshishumiswa"

#: altText.label
#: altText.accessKey
msgid "&Alternate text:"
msgstr "&Linwalwa linwe-vho:"

#: altTextEditField.tooltip
msgid "Type text to display in place of the image"
msgstr "Thaiphani linwalwa line na do li tana vhudzuloni ha tshivhumbeo"

#: noAltText.label
#: noAltText.accessKey
msgid "&Don't use alternate text"
msgstr "&Ni songo shumisa linwalwa linwe-vho"

#: previewBox.label
msgid "Image Preview"
msgstr "Mbonalo ya Tshuvhumbeo"

#: MoreFewerButton.tooltip
msgid "Display more or fewer properties to edit"
msgstr "Tanani vhuvha vhunzhi kana vhutuku ha u lulamisa"

# These controls are in the Dimensions tab panel
# actualSize.label should be same as actualSizeRadio.label + ":"
#: actualSize.label
msgid "Actual Size:"
msgstr "Muelo wa Vhukuma:"

#: actualSizeRadio.label
#: actualSizeRadio.accessKey
msgid "&Actual Size"
msgstr "&Muelo wa Vhukuma"

#: actualSizeRadio.tooltip
msgid "Revert to the image's actual size"
msgstr "Humelani kha muvhala wa tshivhumbeo wa vhukuma"

#: customSizeRadio.label
#: customSizeRadio.accessKey
msgid "Custom &Size"
msgstr "Mvelele &Muelo"

#: customSizeRadio.tooltip
msgid "Change the image's size as displayed in the page"
msgstr "Shandukisani muelo wa tshivhumbeo sa zwe wa taniswa zwone kha siatari"

#: heightEditField.label
#: heightEditField.accessKey
msgid "Hei&ght:"
msgstr "&Vhuntha:"

#: widthEditField.label
#: widthEditField.accessKey
msgid "&Width:"
msgstr "&Vhuphara:"

#: constrainCheckbox.label
#: constrainCheckbox.accessKey
msgid "&Constrain"
msgstr "&Fhungudzani"

#: constrainCheckbox.tooltip
msgid "Maintain the image's aspect ratio"
msgstr "Dzudzani vhushaka ha tshipida tsha tshivhumbeo"

# These controls are in the Image Map box of the expanded area
#: imagemapBox.label
msgid "Image Map"
msgstr "Mepe wa Tshivhumbeo"

#: removeImageMapButton.label
#: removeImageMapButton.accessKey
msgid "&Remove"
msgstr "&Bvisani"

#: editImageMapButton.label
msgid "Edit..."
msgstr "Lulamisani..."

#: editImageMapButton.tooltip
msgid "Create clickable hotspots for this image"
msgstr "Vhumbani khombo dzine dza kilikea u itela hetshi tshivhumbeo"

# These are the options for image alignment
#: alignment.label
msgid "Align Text to Image"
msgstr "Vhekanyani Linwalwa kha Tshivhumbeo"

#: bottomPopup.value
msgid "At the bottom"
msgstr "Mafhedziseloni"

#: topPopup.value
msgid "At the top"
msgstr "Ntha"

#: centerPopup.value
msgid "In the center"
msgstr "Vhukati"

#: wrapRightPopup.value
msgid "Wrap to the right"
msgstr "Putelani ni tshi isa kha tsha u ḽa"

#: wrapLeftPopup.value
msgid "Wrap to the left"
msgstr "Putelani ni tshi isa kha tsha monde"

# These controls are in the Spacing Box
#: spacingBox.label
msgid "Spacing"
msgstr "U vula tshikhala"

#: spacingBox.tooltip
msgid "Type a number for the amount of space around image"
msgstr "Thaiphani nomboro u itela tshivhalo tsha tshikhala tsho monaho na tshivhumbeo"

#: leftRightEditField.label
#: leftRightEditField.accessKey
msgid "&Left and Right:"
msgstr "&Tsha Monde na tsha u Ḽa:"

#: topBottomEditField.label
#: topBottomEditField.accessKey
msgid "&Top and Bottom:"
msgstr "&Nṱha na Fhasi:"

#: borderEditField.label
#: borderEditField.accessKey
msgid "Solid &Border:"
msgstr "Tsho Khwaṱhaho &Matungo:"

# These controls are in the Link Box
#: showImageLinkBorder.label
#: showImageLinkBorder.accessKey
msgid "Show &border around linked image"
msgstr "Sumbedzani &Matungo u mona na tshivhumbeo tsha ṱhumano"

# These controls may be added some day; currently not used
#: makePageBackgroundCheckbox.label
msgid "Make Page Background"
msgstr "Itani Siangane ya Siaṱari"

#: makePageBackgroundCheckbox.tooltip
msgid "Use image as background for your page"
msgstr "Shumisani tshivhumbeo sa siangane ya heḽi siaṱari"

#: editImageButton.label
msgid "Edit Image"
msgstr "Lulamisani Tshivhumbeo"

#: editImageButton.tooltip
msgid "Edit image file in another program"
msgstr "Faela ya ndulamiso kha iṅwe mbekanyamushumo"

# These tabs are currently used in the image input dialog
#: imageInputTab.label
msgid "Form"
msgstr "Tshivhumbeo"

#: imageLocationTab.label
msgid "Location"
msgstr "Fhethu"

#: imageDimensionsTab.label
msgid "Dimensions"
msgstr "Daimensheni"

#: imageAppearanceTab.label
msgid "Appearance"
msgstr "Mbonalo"

#: imageLinkTab.label
msgid "Link"
msgstr "Ṱhumanyo"

