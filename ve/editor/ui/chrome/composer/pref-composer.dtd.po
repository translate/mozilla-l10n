#: lHeader
msgid "Composer"
msgstr "Munwali"

#: rHeader
msgid "Settings for authoring of new Web pages"
msgstr "Nzudzanyo ya u nea maanda masiaṱari maswa a Web"

#: saving
msgid "Saving"
msgstr "U vhulunga"

#: AutoSaveCheck
msgid "Automatically save every"
msgstr "Vhulungani nga nothe tshinwe na tshinwe"

#: minText
msgid "minutes"
msgstr "miminete"

#: exterLegend.label
msgid "External Editors"
msgstr "Vhululamisi ha nnḓa"

#: htmlSource
msgid "HTML Source:"
msgstr "Tshiko tsha HTML:"

#: imageeditor
msgid "Images:"
msgstr "Zwiga:"

#: chooseButton.label
msgid "Choose"
msgstr "Nangani"

#: recentFiles.title
msgid "Recent Pages Menu"
msgstr "Menu wa Masiaṱari Ano"

#: documentsInMenu.label
#: documentsInMenu.accesskey
msgid "Maximum &number of pages listed:"
msgstr "Tshihulwanesa &tshivhalo tsha masiaṱari o bulwaho:"

#: savingFiles.title
msgid "When Saving or Publishing Pages"
msgstr "Musi ni tshi khou Vhulunga kana u Andadza Masiatari"

#: preserveExisting.label
#: preserveExisting.accesskey
msgid "&Preserve original source formatting"
msgstr "&Vhulungani nzudzanyo ya tshiko tsha vhukuma"

#: preserveExisting.tooltip
msgid "Preserves line breaks and page's original formatting"
msgstr "I vhulunga u khauwa ha laini na nzudzanyo ya vhukuma ya siaṱari"

#: saveAssociatedFiles.label
#: saveAssociatedFiles.accesskey
msgid "&Save images and other associated files when saving pages"
msgstr "&Vhulungani zwifanyiso na dzinwe faela dzi elanaho musi ni tshi vhulunga masiaṱari"

#: showPublishDialog.label
#: showPublishDialog.accesskey
msgid "&Always show Publish dialog when publishing pages"
msgstr "&Tshifhinga tshothe sumbedzani nyambedzano ya u Andadza musi ni tshi andadza masiaṱari"

#: composerEditing.label
msgid "Editing"
msgstr "U lulamisa"

#: maintainStructure.label
#: maintainStructure.accesskey
msgid "&Maintain table layout when inserting or deleting cells"
msgstr "&Dzudzani mbonalo ya thebulu musi ni tshi dzhenisa kana u thutha dzisele"

#: maintainStructure.tooltip
msgid "Preserves table's rectangular shape by automatically adding cells after inserting or deleting cells"
msgstr "I vhulunga tshivhumbeo tsha rekithengula tsha thebulu nga u engedza dzisele nga yothe nga murahu ha u dzhenisa kana u thutha dzisele"

#: useCSS.label
#: useCSS.accesskey
msgid "&Use CSS styles instead of HTML elements and attributes"
msgstr "&Shumsani tshitaela tsha CSS nga nnda ha zwipida zwa HTML na ndeme"

#: crInPCreatesNewP.label
#: crInPCreatesNewP.accesskey
msgid "&Return in a paragraph always creates a new paragraph"
msgstr "&U humela kha pharagirafu tshifhinga tshothe zwi vhumba pharagirafu ntswa"

