# Map Expat error codes to error strings
#: 1
msgid "out of memory"
msgstr "yo fhelelwa nga muhumbulo"

#: 2
msgid "syntax error"
msgstr "vhukhakhi ha ḓivhaipfi"

#: 3
msgid "no element found"
msgstr "a huna tshipiḓa tsho wanalaho"

#: 4
msgid "not well-formed"
msgstr "a i ngo vhumbiwa zwavhuḓi"

#: 5
msgid "unclosed token"
msgstr "tshiga tshi songo valeaho"

#: 6
msgid "partial character"
msgstr "tshipiḓa tsha tshitenwa"

#: 7
msgid "mismatched tag"
msgstr "thege i sa elani"

#: 8
msgid "duplicate attribute"
msgstr "itani ndeme kavhili"

#: 9
msgid "junk after document element"
msgstr "mashika nga murahu ha tshitenwa tsha linwalwa"

#: 10
msgid "illegal parameter entity reference"
msgstr "vhuimo ha tshipiḓa tsha mukano tshi siho mulayoni"

#: 11
msgid "undefined entity"
msgstr "tshipiḓa tshi songo talutshedzwaho"

#: 12
msgid "recursive entity reference"
msgstr "vhuimo ha tshipida ho humiselwaho murahu"

#: 13
msgid "asynchronous entity"
msgstr "vhuimo ha asynchronous"

#: 14
msgid "reference to invalid character number"
msgstr "vhuimo kha nomboro ya tshitenwa i siho mulayoni"

#: 15
msgid "reference to binary entity"
msgstr "vhuimo kha tshipida tsha kavhili"

#: 16
msgid "reference to external entity in attribute"
msgstr "vhuimo kha vhuimo ha nga nnda kha tshitenwa"

#: 17
msgid "xml declaration not at start of external entity"
msgstr "ndivhadzo ya xml hu si mathomoni a tshipida tsha nga nnda"

#: 18
msgid "unknown encoding"
msgstr "u nwala nga khoudu hu sa divhei"

#: 19
msgid "encoding specified in XML declaration is incorrect"
msgstr "u nwala nga khoudu ho buliwaho kha ndivhadzo ya XML ndi hone"

#: 20
msgid "unclosed CDATA section"
msgstr "tshipida tsha CDATA tshi so ngo valwaho"

#: 21
msgid "error in processing external entity reference"
msgstr "vhukhakhi kha u bveledza vhuimo ha tshipida tsha nnda"

#: 22
msgid "document is not standalone"
msgstr "linwalo a si la muimawoga"

#: 23
msgid "unexpected parser state"
msgstr "maimo a parser a songo lavhelelwaho"

#: 24
msgid "entity declared in parameter entity"
msgstr "tshipida tsho buliwa kha tshipida tsha pharamitha"

#: 27
msgid "prefix not bound to a namespace"
msgstr "thangi a yo ngo vhofhelelwa kha dzina la tshikhala"

#: 29
msgid "reserved prefix (xml) must not be undeclared or bound to another namespace URI"
msgstr "thangi yo humiselwaho murahu (xml) a yo ngo tea u sa buliwa kana u vhofhelelwa kha linwe dzina la tshikhala URI"

#: 30
msgid "reserved prefix (xmlns) must not be declared or undeclared"
msgstr "thangi yo humiselwaho murahu (xmlns) a yo ngo tea u sa buliwa kana u sa buliwa"

#: 31
msgid "prefix must not be bound to one of the reserved namespace URIs"
msgstr "thangi a yo ngo tea u vhofhelelwa kha linwe la madzina a tshikhala o vhewaho a dziURI"

# %1$S is replaced by the Expat error string, may be followed by Expected (see below)
# %2$S is replaced by URL
# %3$d is replaced by line number
# %4$d is replaced by column number
#: XMLParsingError
msgid "XML Parsing Error: %1$S\n"
"Location: %2$S\n"
"Line Number %3$d, Column %4$d:"
msgstr "XML Vhukhakhi ha u Parser: %1$S\nVhupo: %2$S\nNomboro ya Laini %3$d, Kholomu %4$d:"

# %S is replaced by a tag name.
# This gets appended to the error string if the error is mismatched tag.
#: Expected
msgid ". Expected: </%S>."
msgstr ". Yo Lavhelelwaho: </%S>."

