#: MimeNotCss
msgid "The stylesheet %1$S was not loaded because its MIME type, \"%2$S\", is not \"text/css\"."
msgstr "Siaṱari ḽa tshitaela %1$S a ḽi ngo dzhenisiwa ngauri lushaka lwalo lwa MIME, \"%2$S\", a si \"text/css\"."

#: MimeNotCssWarn
msgid "The stylesheet %1$S was loaded as CSS even though its MIME type, \"%2$S\", is not \"text/css\"."
msgstr "Siaṱari ḽa tshitaela %1$S a ḽi ngo dzheniswa sa CSS naho ḽi ḽa lushaka lwa MIME, \"%2$S\", a si \"text/css\"."

#: PEUnexpEOF
msgid "Unexpected end of file while searching for %1$S."
msgstr "Magumo a faela a songo lavhelelwaho musi ni tshi khou ṱoḓa %1$S."

#: PEParseRuleWSOnly
msgid "Whitespace-only string given to be parsed as rule."
msgstr "Metevhe wa Tshikhalatshitshena-fhedzi wo ṋetshedzwa u itela uri u phasiswe sa mulayo."

#: PEDeclDropped
msgid "Declaration dropped."
msgstr "Mbofho yo wisiwa."

#: PEDeclSkipped
msgid "Skipped to next declaration."
msgstr "Yo pfukela kha mbofho i tevhelaho."

#: PEUnknownProperty
msgid "Unknown property '%1$S'."
msgstr "Ndaka i sa ḓivheiho '%1$S'."

#: PEPropertyParsingError
msgid "Error in parsing value for property '%1$S'."
msgstr "Vhukhakhi kha u phasisa ndeme ya ndaka '%1$S'."

#: PEExpectEndProperty
msgid "Expected end of value for property but found '%1$S'."
msgstr "Magumo o lavhelelwaho a ndaka fhedzi wa wana '%1$S'."

#: PESkipAtRuleEOF
msgid "end of unknown at-rule"
msgstr "magumo a zwi sa ḓivhei kha-mulayo"

#: PEUnknownAtRule
msgid "Unrecognized at-rule or error parsing at-rule '%1$S'."
msgstr "Kha-mulayo u songo ḓivheaho kana vhukhakhi musi hu tshi khou phasi kha-mulayo '%1$S'."

# (review) do not translate: @charset
#: PECharsetRuleEOF
msgid "charset string in @charset rule"
msgstr "metevhe wa charset kha @mulayo wa charset"

#: PECharsetRuleNotString
msgid "Expected charset string but found '%1$S'."
msgstr "Mutevhe wa charset wo lavhelelwaho fhedzi wo wana '%1$S'."

# (review) do not translate: @import, @media
#: PEGatherMediaEOF
msgid "end of media list in @import or @media rule"
msgstr "magumo a mutevhe wa midia kha @ṱunḓa kana @mulayo wa midia"

#: PEGatherMediaNotComma
msgid "Expected ',' in media list but found '%1$S'."
msgstr "',' yo lavhelelwaho kha mutevhe wa midia fhedzi yo wana '%1$S'."

#: PEGatherMediaNotIdent
msgid "Expected identifier in media list but found '%1$S'."
msgstr "Musimbi o lavhelwaho kha mutevhe wa midia fhedzi wa wana '%1$S'."

# (review) do not translate: @import
#: PEImportNotURI
msgid "Expected URI in @import rule but found '%1$S'."
msgstr "URI yo lavhelelwaho kha @mulayo wa u ṱunḓa fhedzi wa wana '%1$S'."

# (review) do not translate: @import
#: PEImportUnexpected
msgid "Found unexpected '%1$S' within @import."
msgstr "Ho wanala '%1$S' i songo lavhelelwaho kha @tunda."

# (review) do not translate: @media, @-moz-document
#: PEGroupRuleEOF
msgid "end of @media or @-moz-document rule"
msgstr "magumo a @midia kana mulayo wa @-moz-ḽiṅwalo"

# (review) do not translate: @-moz-document, url(), url-prefix(), domain()
#: PEMozDocRuleBadFunc
msgid "Expected url(), url-prefix(), or domain() in @-moz-document rule but found '%1$S'."
msgstr "Url() yo lavhelelwaho, url-prefix(), kana fhethu() kha mulayo wa @-moz-ḽiṅwalo fhedzi wa wana '%1$S'."

# (review) do not translate: @-moz-document
#: PEMozDocRuleNotURI
msgid "Expected URI in @-moz-document rule but found '%1$S'."
msgstr "URI yo lavhelelwaho kha mulayo wa @-moz-ḽiṅwalo fhedzi ya wanala '%1$S'."

# (review) do not translate: @namespace
#: PEAtNSPrefixEOF
msgid "namespace prefix in @namespace rule"
msgstr "mutshila wa tshikhala tsha dzina kha mulayo wa @tshikhala tsha dzina"

# (review) do not translate: @namespace
#: PEAtNSURIEOF
msgid "namespace URI in @namespace rule"
msgstr "URI ya tshikhala tsha dzina kha mulayo wa @tshikhala tsha dzina"

# (review) do not translate: @namespace
#: PEAtNSUnexpected
msgid "Unexpected token within @namespace: '%1$S'."
msgstr "Tshiga tshi songo lavhelelwaho ngomu kha @tshikhala tsha dzina: '%1$S'."

#: PESkipDeclBraceEOF
msgid "closing } of declaration block"
msgstr "u vala } tshibuḽoko tsha mbofho"

#: PESkipRSBraceEOF
msgid "closing } of invalid rule set"
msgstr "u vala } mutevhe wa milayo i sa shumi"

#: PEBadSelectorRSIgnored
msgid "Ruleset ignored due to bad selector."
msgstr "Mutevhe wa milayo a wo ngo dzhielwa nzhele nga mulandu wa u nanga hu si ha vhuḓi."

# (review) please translate: or
#: PESelectorListExtraEOF
msgid "',' or '{'"
msgstr "',' or '{'"

#: PESelectorListExtra
msgid "Expected ',' or '{' but found '%1$S'."
msgstr "',' yo lavhelelwaho kana '{' fhedzi ya wana '%1$S'."

#: PESelectorGroupNoSelector
msgid "Selector expected."
msgstr "Munangi o lavhelelwa."

#: PESelectorGroupExtraCombinator
msgid "Dangling combinator."
msgstr "Muṱanganyisi ane a khou nembelela."

#: PEIDSelNotIdent
msgid "Expected identifier for ID selector but found '%1$S'."
msgstr "Musumbi o lavhelelwaho wa u nanga ID fhedzi ha wanala '%1$S'."

#: PEIDSelEmpty
msgid "Expected non-empty name for ID selector, but found '%1$S'."
msgstr "Dzina ḽo lavhelelwaho ḽi si na tshithu ḽa munangi wa ID, fhedzi ha wanala '%1$S'."

#: PEClassSelEOF
msgid "class name"
msgstr "dzina ḽa maimo"

#: PEClassSelNotIdent
msgid "Expected identifier for class selector but found '%1$S'."
msgstr "Musumbi o lavhelelwaho wa u nanga maimo fhedzi ha wanala '%1$S'."

#: PETypeSelEOF
msgid "element type"
msgstr "lushaka lwa tshipiḓa"

#: PETypeSelNotType
msgid "Expected element name or '*' but found '%1$S'."
msgstr "Dzina la tshipiḓa ḽo lavhelelwaho kana '*' fhedzi ha wanala '%1$S'."

#: PEUnknownNamespacePrefix
msgid "Unknown namespace prefix '%1$S'."
msgstr "Mutshila wa tshikhala tsha dzina tshi sa ḓivhei '%1$S'."

#: PEAttributeNameEOF
msgid "attribute name"
msgstr "dzina ḽa vhunzani"

#: PEAttributeNameExpected
msgid "Expected identifier for attribute name but found '%1$S'."
msgstr "Musumbi o lavhelelwaho wa dzina ḽa vhunzani fhedzi ha wanala '%1$S'."

#: PEAttributeNameOrNamespaceExpected
msgid "Expected attribute name or namespace but found '%1$S'."
msgstr "Dzinal la vhunzani lo lavhelelwaho kana tshikhala tsha dzina fhedzi ha wanala '%1$S'."

#: PEAttSelNoBar
msgid "Expected '|' but found '%1$S'."
msgstr "'|' yo lavhelelwaho fhedzi ha wanala '%1$S'."

#: PEAttSelInnerEOF
msgid "part of attribute selector"
msgstr "tshipiḓa tsha munangi wa vhunzani"

# (pofilter) variables: do not translate: %1$S
#: PEAttSelUnexpected
#, fuzzy
msgid "Unexpected token in attribute selector: '%1$S'."
msgstr "Tshiga tshi songo lavhelelwaho kha munangi wa vhunzani"

#: PEAttSelValueEOF
msgid "attribute value"
msgstr "ndeme ya vhunzani"

#: PEAttSelCloseEOF
msgid "']' to end attribute selector"
msgstr "']' u fhelisa munangi wa vhunzani"

#: PEAttSelNoClose
msgid "Expected ']' to terminate attribute selector but found '%1$S'."
msgstr "']' yo lavhelelwaho uri i fhelise munangi wa vhunzani fhedzi ha wanala '%1$S'."

#: PEAttSelBadValue
msgid "Expected identifier or string for value in attribute selector but found '%1$S'."
msgstr "Musumbi o lavhelelwaho kana mutevhe wa ndeme kha munangi wa vhunzani fhedzi ha wanala '%1$S'."

#: PEPseudoSelEOF
msgid "name of pseudo-class or pseudo-element"
msgstr "dzina ḽa maimo a khole-khole kana tshipiḓa tsha khole-khole"

#: PEPseudoSelBadName
msgid "Expected identifier for pseudo-class or pseudo-element but found '%1$S'."
msgstr "Musumbi o lavhelelwaho wa maimo a khole-khole kana tshipiḓa tsha khole-khole fhedzi ha wanala '%1$S'."

#: PEPseudoSelNonFunc
msgid "Function token for non-function pseudo-class or pseudo-element, or the other way around, when reading '%1$S'."
msgstr "Tshiga tsha u shuma tsha maimo a khole-khole a sa shumiho, kana nga iṅwe nḓila, musi ni tshi khou vhala '%1$S'."

#: PEPseudoSelNotPE
msgid "Expected pseudo-element but found '%1$S'."
msgstr "Tshipiḓa tsha khole-khole tsho lavhelelwaho fhedzi ha wanala '%1$S'."

#: PEPseudoSelDoubleNot
msgid "Negation pseudo-class can't be negated '%1$S'."
msgstr "Maimo a khole-khole a u phumula ha koni u phumulea '%1$S'."

#: PEPseudoSelPEInNot
msgid "Pseudo-elements can't be negated '%1$S'."
msgstr "Zwipiḓa zwa khole-khole a zwi koni u phumulea '%1$S'."

#: PEPseudoSelNewStyleOnly
msgid "This pseudo-element must use the \"::\" form: '%1$S'."
msgstr "Hezwi zwipiḓa zwa khole-khole zwi tea u shumisa maitevhe a \"::\" : '%1$S'."

#: PEPseudoSelTrailing
msgid "Found trailing token after pseudo-element, which must be the last part of a selector:  '%1$S'."
msgstr "Tshiga tsha u londa tsho wanala nga murahu ha tshipiḓa tsha khole-khole, tshine tsha tea u vha tshipiḓa tsha u fhedzisela tsha munangi:  '%1$S'."

#: PEPseudoSelMultiplePE
msgid "Extra pseudo-element '%1$S'."
msgstr "Tshipiḓa tsha khole-khole tshiṅwe hafhu '%1$S'."

#: PEPseudoSelUnknown
msgid "Unknown pseudo-class or pseudo-element '%1$S'."
msgstr "Kiḽasi ya khole-khole i sa ḓivheiho kana tshipiḓa tsha khole-khole '%1$S'."

#: PENegationEOF
msgid "selector within negation"
msgstr "munangi kha zwine zwa khou phumulwa"

#: PENegationBadInner
msgid "Malformed simple selector as negation pseudo-class argument '%1$S'."
msgstr "Munangi a sa konḓi a songo vhumbiwaho zwavhuḓi sa tsimbe ya maimo a khole-khole a u phumula '%1$S'."

#: PENegationNoClose
msgid "Missing closing ')' in negation pseudo-class '%1$S'."
msgstr "Phendelo ine ya khou ṱahela ')' kha maimo a khole-khole a u phumula '%1$S'."

#: PENegationBadArg
msgid "Missing argument in negation pseudo-class '%1$S'."
msgstr "Tsimbe i khou ṱahela kha maimo a khole-khole a u phumula '%1$S'."

# (review) do not translate: :lang
#: PELangArgEOF
msgid "argument to :lang selector"
msgstr "tsimbe kha: munangi lang"

#: PELangArgNotIdent
msgid "Expected identifier for lang pseudo-class parameter but found '%1$S'."
msgstr "Musumbi o lavhelwaho wa mukano wa maimo a khole-khole a lang fhedzi ha wanala '%1$S'."

#: PELangNoClose
msgid "Missing closing ')' in lang pseudo-class '%1$S'."
msgstr "Phendelo I khou tahela ')' kha kiḽasi ya khole-khole ya lang '%1$S'."

#: PELangNoArg
msgid "Missing argument in lang pseudo-class '%1$S'."
msgstr "Khani i khou tahela kha kiḽasi ya lang ya khole-khole '%1$S'."

#: PESelectorEOF
msgid "selector"
msgstr "munangi"

#: PEBadDeclBlockStart
msgid "Expected '{' to begin declaration block but found '%1$S'."
msgstr "'{' i khou lavhelelwa u thoma tshibuḽoko tsha ndivhadzo fhedzi ha wanala '%1$S'."

#: PEColorEOF
msgid "color"
msgstr "muvhala"

#: PEColorNotColor
msgid "Expected color but found '%1$S'."
msgstr "Muvhala wo lavhelelwaho fhedzi ha wanala '%1$S'."

#: PEColorComponentEOF
msgid "color component"
msgstr "tshipiḓa tsha muvhala"

#: PEExpectedPercent
msgid "Expected a percentage but found '%1$S'."
msgstr "No lavhelela phesenthe fhedzi na wana '%1$S'."

#: PEExpectedInt
msgid "Expected an integer but found '%1$S'."
msgstr "Na lavhelela nomboro fhedza na wana '%1$S'."

#: PEColorBadRGBContents
msgid "Expected number or percentage in rgb() but found '%1$S'."
msgstr "No lavhelela nomboro kan phesenthe kha rgb() fhedzi na wana '%1$S'."

# (pofilter) sentencecount: checks that the number of sentences in both strings match
#: PEColorComponentBadTerm
msgid "Expected '%2$S' but found '%1$S'."
msgstr "No lavhelela '%2$S'. Fhedzi na wana '%1$S'."

#: PEColorHueEOF
msgid "hue"
msgstr "murunzi"

#: PEExpectedComma
msgid "Expected ',' but found '%1$S'."
msgstr "',' yo lavhelelwa fhedzi ha wanala '%1$S'."

#: PEColorSaturationEOF
msgid "saturation"
msgstr "munwelelo"

#: PEColorLightnessEOF
msgid "lightness"
msgstr "leluwa"

#: PEColorOpacityEOF
msgid "opacity in color value"
msgstr "u swifhala kha ndeme ya muvhala"

#: PEExpectedNumber
msgid "Expected a number but found '%1$S'."
msgstr "Ho lavhelela nomboro fhedzi na wana '%1$S'."

#: PEExpectedCloseParen
msgid "Expected ')' but found '%1$S'."
msgstr "Ho lavhelela ')' fhedzi na wana '%1$S'."

#: PEDeclEndEOF
msgid "';' or '}' to end declaration"
msgstr "';' kana '}' u fhedzisa nḓivhadzo"

#: PEParseDeclarationNoColon
msgid "Expected ':' but found '%1$S'."
msgstr "':' yo lavhelelwa fhedzi ha wanala '%1$S'."

#: PEParseDeclarationDeclExpected
msgid "Expected declaration but found '%1$S'."
msgstr "Nḓivhadzo yo lavhelelwa fhedzi ha wanala '%1$S'."

#: PEEndOfDeclEOF
msgid "end of declaration"
msgstr "mafhedziselo a nḓivhadzo"

#: PEImportantEOF
msgid "important"
msgstr "zwa vhuṱhogwa"

# (pofilter) singlequoting: checks whether singlequoting is consistent between the two strings
# (review) missing quotes around: important
#: PEExpectedImportant
msgid "Expected 'important' but found '%1$S'."
msgstr "Ya vhuṱhogwa' yo lavhelelwa fhedzi ha wanala '%1$S'."

#: PEBadDeclEnd
msgid "Expected ';' to terminate declaration but found '%1$S'."
msgstr "';' yo lavhelelwa uri i fhelise nḓivhadzo fhedzi ha wanala '%1$S'."

#: PEBadDeclOrRuleEnd
msgid "Expected ';' or '}' to terminate declaration but found '%1$S'."
msgstr "';' yo lavhelelwa kana '}' uri i fhelise nḓivhadzo fhedzi ha wanala '%1$S'."

#: PEInaccessibleProperty
msgid "Attempt to use inaccessible property"
msgstr "Ndingedzo dza u shumisa zwishumiswa zwi sa swikelelei"

#: PECommentEOF
msgid "end of comment"
msgstr "magumo a mahumbulwa"

#: SEUnterminatedString
msgid "Found unclosed string '%1$S'."
msgstr "Yo wana mutevhe u songo valwaho '%1$S'."

