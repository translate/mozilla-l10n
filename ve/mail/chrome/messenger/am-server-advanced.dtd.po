#: serverAdvanced.label
msgid "Advanced Account Settings"
msgstr "Nzudzanyo ya Mbalelano yo Bvelelaho"

# LOCALIZATION NOTE (serverDirectory.label): DONT_TRANSLATE "IMAP"
#: imapAdvanced.label
msgid "Advanced IMAP Server Settings"
msgstr "Nzudzanyo ya Seva ya IMAP yo Bvelelaho"

#: serverDirectory.label
#: serverDirectory.accesskey
msgid "IMAP server &directory:"
msgstr "Seva ya IMAP &tsumbandila:"

#: usingSubscription.label
#: usingSubscription.accesskey
msgid "Sho&w only subscribed folders"
msgstr "&Sumbedzani fhedzi foḽuda dzo ṅwaliswaho"

#: dualUseFolders.label
#: dualUseFolders.accesskey
msgid "Server supports &folders that contain sub-folders and messages"
msgstr "Thikhedzo ya Seva &foḽuda dzine dza vha na zwifoḽuda zwiṱuku na milaedza"

#: useIdle.label
#: useIdle.accesskey
msgid "Use &IDLE command if the server supports it"
msgstr "Shumisani mulayo wa &IDLE arali seva i tshi u tikedza"

#: maximumConnectionsNumber.label
#: maximumConnectionsNumber.accesskey
msgid "&Maximum number of server connections to cache"
msgstr "&Nomboro khulwanesa ya u dzhena kha seva vhulunga"

#: namespaceDesc.label
msgid "_: DONT_TRANSLATE \"IMAP\"\n"
"These preferences specify the namespaces on your IMAP server"
msgstr "Hezwi zwitakalelwa zwi bula tshivhalo tsha madzina a zwikhala tshine tsha vha hone kha seva ya IMAP yaṇu"

# (review) strange characters: n?e mu?e:
#: personalNamespace.label
#: personalNamespace.accesskey
msgid "&Personal namespace:"
msgstr "&Madzina a zwikhala anga nṇe muṇe:"

#: publicNamespace.label
#: publicNamespace.accesskey
msgid "P&ublic (shared):"
msgstr "&Ya vhoṱhe (kovhiwaho):"

#: otherUsersNamespace.label
#: otherUsersNamespace.accesskey
msgid "&Other Users:"
msgstr "&Vhaṅwe vhashumisi:"

#: overrideNamespaces.label
#: overrideNamespaces.accesskey
msgid "&Allow server to override these namespaces"
msgstr "&Tendelani seva uri i ṅwale ntha ha zwikhala zwa haya madzina"

#: pop3Desc.label
msgid "When downloading pop mail for this server, use the following folder for new mail:"
msgstr "Musi ni tshi khou vula meiḽi une wa thuthuba kha hei seva, shumisani foḽuda dzi tevhelaho u itela meiḽi muswa:"

