#: addressing.label
msgid "Composition &amp; Addressing"
msgstr "U sika &amp; U rumela"

#: addressingGroupTitle.label
msgid "Addressing"
msgstr "U Rumela"

#: addressingText.label
msgid "When looking up addresses:"
msgstr "Musi ni tshi khou ṱoḓa ḓiresi:"

#: useGlobal.label useGlobal.accesskey
msgid "&Use my global LDAP server preferences for this account"
msgstr ""
"&Shumisani seva guṱe yanga ya LDAP ine nda i takalela kha hei mbalelano"

#: editDirectories.label editDirectories.accesskey
msgid "&Edit Directories..."
msgstr "&Lulamisani Tsumbanḓila..."

#: directories.label directories.accesskey
msgid ""
"_: do not translate \"LDAP\" in below line\n"
"Use a different L&DAP server:"
msgstr "Shumisani seva ya L&DAP yo fhambanaho:"

#: compositionGroupTitle.label
msgid "Composition"
msgstr "Zwiitwa"

#: useHtml.label useHtml.accesskey
msgid ""
"_: do not translate \"html\" in below line\n"
"&Compose messages in HTML format"
msgstr "&Itani mulaedza nga maitele a HTML"

#: autoQuote.label autoQuote.accesskey
msgid "Automatically &quote the original message when replying"
msgstr "Vhuḓiiti &topolani mulaedza wa vhukuma musi ni tshi fhindula"

#: then.label then.accesskey
msgid ""
"_: This will concatenate with the 4 strings that follow.\n"
"&Then,"
msgstr "&U bva afho,"

#: aboveQuote.label
msgid "start my reply above the quote"
msgstr "thomani phindulo yanga nga nṱha ha khouthu"

#: belowQuote.label
msgid "start my reply below the quote"
msgstr "thomani phindulo yanga nga fhasi ha khouthu"

#: selectAndQuote.label
msgid "select the quote"
msgstr "nangani khouthu"

#: place.label place.accesskey
msgid "and &place my signature"
msgstr "nahone &vheani tsaino yanga"

#: belowText.label
msgid "below the quote (recommended)"
msgstr "fhasi ha zwo topolwaho (zwi a ṱoḓea)"

#: aboveText.label
msgid "below my reply (above the quote)"
msgstr "fhasi ha phindulo yanga (nṱha ha khouthu)"
