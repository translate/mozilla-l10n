# Success message when no address books are found to import
## @name IMPORT_NO_ADDRBOOKS
## @loc None
#: 2000
msgid "No address books were found to import."
msgstr "A huna bugu dza ḓiresi dzo wanalaho dza u ṱunḓa."

# Error: Address book import not intialized\t\t
## @name IMPORT_ERROR_AB_NOTINITIALIZED
## @loc None
#: 2001
msgid "Unable to import address books: initialization error."
msgstr "A thi koni u ṱunḓa bugu dza ḓiresi: vhukhakhi ha u vhulunga."

# Error: Unable to create the import thread
## @name IMPORT_ERROR_AB_NOTHREAD
## @loc None
#: 2002
msgid "Unable to import address books: cannot create import thread."
msgstr "A thi koni u ṱunḓa bugu dza ḓiresi: a thi koni u ṱunḓa thambo ya u ṱunḓa."

# Error: Unable to create the import thread
## @name IMPORT_ERROR_GETABOOK
## @loc None
# LOCALIZATION NOTE (Error 2003): Do not translate the word "%S" below.
#: 2003
msgid "Error importing %S: unable to create address book."
msgstr "Vhukhakhi hu tshi khou ṱunḓiwa %S: a thi koni u vhumba bugu ya ḓiresi."

# Success message when no mailboxes are found to import
## @name IMPORT_NO_MAILBOXES
## @loc None
#: 2004
msgid "No mailboxes were found to import"
msgstr "A huna mabogisi a meiḽi o wanaho a u ṱunḓa"

# Error: Mailbox import not intialized\t\t
## @name IMPORT_ERROR_MB_NOTINITIALIZED
## @loc None
#: 2005
msgid "Unable to import mailboxes, initialization error"
msgstr "A thi koni u ṱunḓa mabogisi a meiḽi, vhukhakhi ha u vhulunga"

# Error: Unable to create the import thread
## @name IMPORT_ERROR_MB_NOTHREAD
## @loc None
#: 2006
msgid "Unable to import mailboxes, cannot create import thread"
msgstr "A thi koni u ṱunḓa mabogisi a meiḽi, a thi koni u vhumba thambo ya u ṱunḓa"

# Error: Unable to create the proxy object for importing mailboxes
## @name IMPORT_ERROR_MB_NOPROXY
## @loc None
#: 2007
msgid "Unable to import mailboxes, cannot create proxy object for destination mailboxes"
msgstr "A thi koni u ṱunḓa mabogisi a meiḽi, a thi koni u vhumba thambo ya u ṱunḓa zwiitwa zwithusedzi kha vhuyo ha bogisi ḽa meiḽi"

# Error: Error creating destination mailboxes
## @name IMPORT_ERROR_MB_FINDCHILD
## @loc None
# LOCALIZATION NOTE (Error 2008): Do not translate the word "%S" below.
# Place %S in your translation where the name of the mailbox should appear.
#: 2008
msgid "Error creating destination mailboxes, cannot find mailbox %S"
msgstr "Vhukhakhi kha u vhumba vhuyo ha mabogisi a meiḽi, a thi koni u wana bogisi ḽa meiḽi %S"

# Error: Error creating destination mailboxes
## @name IMPORT_ERROR_MB_CREATE
## @loc None
# LOCALIZATION NOTE (Error 2009): Do not translate the word "%S" below.
# Place %S in your translation where the name of the mailbox should appear.
#: 2009
msgid "Error importing mailbox %S, unable to create destination mailbox"
msgstr "Vhukhakhi musi hu tshi khou ṱunḓiwa %S, a thi kona u vhumba vhuyo ha bogisi ḽa meiḽi"

# Error: No destination folder to import mailboxes
## @name IMPORT_ERROR_MB_NODESTFOLDER
## @loc None
#: 2010
msgid "Unable to create folder to import mail into"
msgstr "A thi koni u vhumba foḽuda ya u ṱunḓela meiḽi kha"

