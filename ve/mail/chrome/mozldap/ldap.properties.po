#: 19
msgid "Constraint violation"
msgstr "U tshinyadza ho langiwaho"

## @name TYPE_OR_VALUE_EXISTS
## @loc none
#: 20
msgid "Type or value exists"
msgstr "Lushaka kana ndeme zwi hone"

## @name INVALID_SYNTAX
## @loc none
#: 21
msgid "Invalid syntax"
msgstr "Ḓivhafhungo i si ya vhukuma"

## @name NO_SUCH_OBJECT
## @loc none
#: 32
msgid "No such object"
msgstr "A huna tshiitwa tsho raloho"

## @name ALIAS_PROBLEM
## @loc none
#: 33
msgid "Alias problem"
msgstr "Thaidzo ya dzina ḽihumbulelwa"

## @name INVALID_DN_ SYNTAX
## @loc none
#: 34
msgid "Invalid DN syntax"
msgstr "Ḓivhafhungo ya DN i si ya vhukuma"

## @name IS_LEAF
## @loc none
#: 35
msgid "Object is a leaf"
msgstr "Tshiitwa ndi ṱari"

## @name ALIAS_DEREF_PROBLEM
## @loc none
#: 36
msgid "Alias dereferencing problem"
msgstr "Thaidzo ya u sa tsha rumela dzina ḽihumbulelwa"

## @name INAPPROPRIATE_AUTH
## @loc none
#: 48
msgid "Inappropriate authentication"
msgstr "Vhukwae vhu si ha vhukuma"

## @name INVALID_CREDENTIALS
## @loc none
#: 49
msgid "Invalid credentials"
msgstr "Ndalukanyo dzi si dza vhukuma"

## @name INSUFFICIENT_ACCESS
## @loc none
#: 50
msgid "Insufficient access"
msgstr "U swikelela hu songo eḓanaho"

## @name BUSY
## @loc none
#: 51
msgid "The LDAP server is busy"
msgstr "Seva ya LDAP i khou shuma"

## @name UNAVAILABLE
## @loc none
#: 52
msgid "LDAP server is unavailable"
msgstr "Seva ya LDAP a i khou wanala"

## @name UNWILLING_TO_PERFORM
## @loc none
#: 53
msgid "LDAP server is unwilling to perform"
msgstr "Seva ya LDAP a i funi u shuma"

## @name LOOP_DETECT
## @loc none
#: 54
msgid "Loop detected"
msgstr "Disiki yo waniwa"

## @name SORT_CONTROL_MISSING
## @loc none
#: 60
msgid "Sort Control is missing"
msgstr "Ndango ya Nzudzanyo i khou ṱahela"

## @name INDEX_RANGE_ERROR
## @loc none
#: 61
msgid "Search results exceed the range specified by the offsets"
msgstr "Mvelele dza u ṱoḓa dzo fhirisa mutevhe wo bulwaho nga ndinganyo"

## @name NAMING_VIOLATION
## @loc none
#: 64
msgid "Naming violation"
msgstr "U tshinyadzwa ha u ṋea dzina"

## @name OBJECT_CLASS_VIOLATION
## @loc none
#: 65
msgid "Object class violation"
msgstr "U tshinyadza vhuimo ha tshiitwa"

## @name NOT_ALLOWED_ON_NONLEAF
## @loc none
#: 66
msgid "Operation not allowed on nonleaf"
msgstr "Nyito a i ngo tendelwa kha i si ṱari"

## @name NOT_ALLOWED_ON_RDN
## @loc none
#: 67
msgid "Operation not allowed on RDN"
msgstr "Nyito a i ngo tendelwa kha RDN"

## @name ALREADY_EXISTS
## @loc none
#: 68
msgid "Already exists"
msgstr "Yo no ḓi vha hone"

## @name NO_OBJECT_CLASS_MODS
## @loc none
#: 69
msgid "Cannot modify object class"
msgstr "Ni nga si kone u khwinisa maimo a tshiitwa"

## @name RESULTS_TOO_LARGE
## @loc none
#: 70
msgid "Results too large"
msgstr "Mvelele dzo hulesa"

## @name AFFECTS_MULTIPLE_DSAS
## @loc none
#: 71
msgid "Affects multiple servers"
msgstr "Zwi kwama seva dzi na tshivhalo"

## @name OTHER
## @loc none
#: 80
msgid "Unknown error"
msgstr "Vhukhakhi vhu sa ḓivhei"

## @name SERVER_DOWN
## @loc none
#: 81
msgid "Can't contact the LDAP server"
msgstr "A thi koni u kwama seva ya LDAP"

## @name LOCAL_ERROR
## @loc none
#: 82
msgid "Local error"
msgstr "Vhukhakhi hapo"

## @name ENCODING_ERROR
## @loc none
#: 83
msgid "Encoding error"
msgstr "Vhukhakhi ha u khouda"

## @name DECODING_ERROR
## @loc none
#: 84
msgid "Decoding error"
msgstr "Vhukhakhi ha u khoudulula"

## @name TIMEOUT
## @loc none
#: 85
msgid "The LDAP server timed out"
msgstr "Tshifhinga tsha seva ya LDAP tsho fhela"

## @name AUTH_UNKNOWN
## @loc none
#: 86
msgid "Unknown authentication method"
msgstr "Maitele a u pima vhukwae a sa ḓivhei"

## @name FILTER_ERROR
## @loc none
#: 87
msgid "Invalid search filter"
msgstr "Fiḽitha ya u ṱola i si yone"

## @name USER_CANCELLED
## @loc none
#: 88
msgid "User cancelled operation"
msgstr "Mushumisi o dzivhisa nyito"

## @name PARAM_ERROR
## @loc none
#: 89
msgid "Bad parameter to an LDAP routine"
msgstr "Tshivhalo tshivhi tsha nḓowelo ya LDAP"

## @name NO_MEMORY
## @loc none
#: 90
msgid "Out of memory"
msgstr "Yo fhelelwa nga muhumbulo"

## @name CONNECT_ERROR
## @loc none
#: 91
msgid "Can't connect to the LDAP server"
msgstr "A thi koni u ṱumana na seva ya LDAP"

## @name NOT_SUPPORTED
## @loc none
#: 92
msgid "Not supported by this version of the LDAP protocol"
msgstr "A i tikedzi nga sia ḽa maitele a LDAP"

## @name CONTROL_NOT_FOUND
## @loc none
#: 93
msgid "Requested LDAP control not found"
msgstr "Ndango ya LDAP yo humbeliwaho a i ngo wanala"

## @name NO_RESULTS_RETURNED
## @loc none
#: 94
msgid "No results returned"
msgstr "A huna mvelelo dzo humiswaho"

## @name MORE_RESULTS_TO_RETURN
## @loc none
#: 95
msgid "More results to return"
msgstr "Hu na mvelelo nnzhi dzine dza khou tea humiswa"

## @name CLIENT_LOOP
## @loc none
#: 96
msgid "Client detected loop"
msgstr "Khasiṱama yo wana disiki"

## @name REFERRAL_LIMIT_EXCEEDED
## @loc none
#: 97
msgid "Referral hop limit exceeded"
msgstr "Muelo wa u thamukana wo laedzwaho wo fhela"

