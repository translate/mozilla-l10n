#: certmgr.title
msgid "Certificate Manager"
msgstr "Mulanguli wa Ṱhanziela"

#: certmgr.tab.mine
msgid "Your Certificates"
msgstr "Ṱhanziela Dzaṅu"

#: certmgr.tab.others
msgid "Other People's"
msgstr "Dza Vhaṅwe Vhathu"

#: certmgr.tab.websites
msgid "Web Sites"
msgstr "Dziwebusaithi"

#: certmgr.tab.ca
msgid "Authorities"
msgstr "Vhahulwane"

#: certmgr.ca.builtins
msgid "Built-in CAs"
msgstr "CAS yo fhatelwaho ngomu"

#: certmgr.ca.mycas
msgid "CAs Managed By You"
msgstr "CAS ine ya langwa nga inwi"

#: certmgr.mine
msgid "You have certificates from these organizations that identify you:"
msgstr "Ni na ṱhanziela dzine dza bva kha heḽi dzangano dzine dza sumba inwi:"

#: certmgr.others
msgid "You have certificates on file that identify these people:"
msgstr "Ni na ṱhanziela kha faela dzine dza sumba havha vhathu:"

#: certmgr.websites
msgid "You have certificates on file that identify these web sites:"
msgstr "Ni na ṱhanziela kha faela dzine dza sumba hedzi webusaithi:"

#: certmgr.cas
msgid "You have certificates on file that identify these certificate authorities:"
msgstr "Ni na ṱhanziela kha faela dzine dza sumba vhalangi vha hedzi ṱhanziela:"

#: certmgr.detail.general_tab.title
msgid "General"
msgstr "Ngau angaredza"

#: certmgr.detail.prettyprint_tab.title
msgid "Details"
msgstr "Zwidodombedzwa"

#: certmgr.subjectinfo.label
msgid "Issued To"
msgstr "I Bviselwa"

#: certmgr.issuerinfo.label
msgid "Issued By"
msgstr "Yo Bviswa Nga"

#: certmgr.validity.label
msgid "Validity"
msgstr "U shuma"

#: certmgr.fingerprints.label
msgid "Fingerprints"
msgstr "Khanḓiso dza minwe"

#: certmgr.certdetail.title
msgid "Certificate Detail"
msgstr "Zwidodombedzwa zwa Ṱhanziela"

#: certmgr.certdetail.cn
msgid "Common Name (CN)"
msgstr "Dzina ḽo Ḓoweleaho (CN)"

#: certmgr.certdetail.o
msgid "Organization (O)"
msgstr "Dzangano (O)"

#: certmgr.certdetail.ou
msgid "Organizational Unit (OU)"
msgstr "Tshipiḓa tsha Dzangano (OU)"

#: certmgr.certdetail.serialnumber
msgid "_: certmgr.certdetail.serialnumber\n"
"Serial Number"
msgstr "Nomboro ya nga u londa"

#: certmgr.certdetail.sha1fingerprint
msgid "SHA1 Fingerprint"
msgstr "Khanḓiso dza minwe dza SHA1"

#: certmgr.certdetail.md5fingerprint
msgid "MD5 Fingerprint"
msgstr "Khanḓso dza minwe dza MD5"

#: certmgr.editcert.title
msgid "Edit Security Certificate Settings"
msgstr "Lulamisani Nzudzanyo ya Ṱhanziela dza Tsireledzo"

#: certmgr.editcacert.title
msgid "Edit CA certificate trust settings"
msgstr "Lulamisani nzudzanyo ya ṱhanziela dza fulufhelo kha CA"

#: certmgr.editsslcert.title
msgid "Edit web site certificate trust settings"
msgstr "Lulamisani nzudzanyo dza ṱhanziela dza fulufhelo kha webusaithi"

#: certmgr.editcert.edittrust
msgid "Edit trust settings:"
msgstr "Lulamisani nzudzanyo dza dzangano:"

#: certmgr.editcert.trustssl
msgid "This certificate can identify web sites."
msgstr "Hei ṱhanziela i nga sumba dziwebusaithi."

#: certmgr.editcert.trustemail
msgid "This certificate can identify mail users."
msgstr "Hei ṱhanziela i nga sumba vhashumisi vha meiḽi."

#: certmgr.editcert.trustobjsign
msgid "This certificate can identify software makers."
msgstr "Hei ṱhanziela i nga sumba vhaiti vha sofuthiwee."

#: certmgr.editsslcert.edittrust
msgid "_: certmgr.editsslcert.edittrust\n"
"Edit certificate trust settings:"
msgstr "Lulamisani nzudzanyo dza fulufhelo kha ṱhanziela:"

#: certmgr.editsslcert.dotrust
msgid "_: certmgr.editsslcert.dotrust\n"
"Trust the authenticity of this certificate."
msgstr "Fulufhelani vhukwae ha hei ṱhanziela."

#: certmgr.editsslcert.donttrust
msgid "_: certmgr.editsslcert.donttrust\n"
"Do not trust the authenticity of this certificate."
msgstr "Ni songo fulufhela vhukwae ha hei ṱhanziela."

#: certmgr.editemailcert.title
msgid "Edit email certificate trust settings"
msgstr "Lulamisani nzudzanyo ya fulufhelo ḽa ṱhanziela ya emeiḽi"

#: certmgr.editemailcert.edittrust
msgid "_: certmgr.editemailcert.edittrust\n"
"Edit certificate trust settings:"
msgstr "Lulamisani nzudzanyo ya fulufhelo ḽa ṱhanziela:"

#: certmgr.editemailcert.dotrust
msgid "_: certmgr.editemailcert.dotrust\n"
"Trust the authenticity of this certificate."
msgstr "Fulufhelani vhukwae ha hei ṱhanziela."

#: certmgr.editemailcert.donttrust
msgid "_: certmgr.editemailcert.donttrust\n"
"Do not trust the authenticity of this certificate."
msgstr "Ni songo fulufhela vhukwae ha hei ṱhanziela."

#: certmgr.deletecert.title
msgid "Delete Certificate"
msgstr "Thuthani ṱhanziela"

#: certmgr.deletecert.beforename
msgid "You have requested to delete this certificate:"
msgstr "No humbela u thutha hei ṱhanziela:"

#: certmgr.deletecert.aftername
msgid "Are you sure you want to delete this certificate?"
msgstr "Ni na vhuṱanzi ho fhelelaho ha uri ni khou ṱoḓa u thutha iyi ṱhanziela?"

#: certmgr.deleteusercert.title
msgid "Delete Your Certificate"
msgstr "Thuthani ṱhanziela Yaṇu"

#: certmgr.deleteusercert.beforename
msgid "You have requested to delete the certificate:"
msgstr "No humbela u thutha ṱhanziela:"

#: certmgr.deleteusercert.aftername
msgid "Once you have deleted this certificate, you will not be able to read mail that has been encrypted with it."
msgstr "Musi no thutha heyi milaedza, ni nga si tsha dovha na vhala milaedza yo khoudiwaho khayo."

#: certmgr.certname
msgid "Certificate Name"
msgstr "Dzina ḽa ṱhanziela"

#: certmgr.tokenname
msgid "Security Device"
msgstr "Tshishumswa tsha Tsireledzo"

#: certmgr.purpose
msgid "Purposes"
msgstr "Ndivho"

#: certmgr.issued
msgid "Issued On"
msgstr "Yo Bviswa Nga"

#: certmgr.expires
msgid "Expires On"
msgstr "I Khou Fhelelwa Nga"

#: certmgr.email
msgid "E-Mail Address"
msgstr "Ḓiresi ya Emeiḽi"

#: certmgr.serial
msgid "_: certmgr.serial\n"
"Serial Number"
msgstr "Nomboro ya u Londa"

#: certmgr.ocsp_info
msgid "Certificates have not been validated with OCSP. Click View to do so."
msgstr "Ṱhanziela a yo ngo khwaṱhisedzwa nga OCSP. Kiḽikani Vhona u itela u ita zwenezwo."

#: certmgr.close.label
msgid "Close"
msgstr "Vala"

#: certmgr.view.label
msgid "View"
msgstr "Vhona"

#: certmgr.edit.label
msgid "Edit"
msgstr "Lulamisa"

#: certmgr.editca.label
msgid "Edit CA Trust"
msgstr "Lulamisani Fulufhelo la CA"

#: certmgr.add.label
msgid "Add"
msgstr "Engedzani"

#: certmgr.delete.label
msgid "Delete"
msgstr "Thuthani"

#: certmgr.backup.label
msgid "Backup"
msgstr "Thusedzani"

#: certmgr.backupall.label
msgid "Backup All"
msgstr "Thusedzani Dzoṱhe"

#: certmgr.restore.label
msgid "Import"
msgstr "Tunḓani"

#: certmgr.details.label
msgid "Certificate Fields"
msgstr "Masia a Ṱhanziela"

#: certmgr.fields.label
msgid "Field Value"
msgstr "Ndeme ya Sia"

#: certmgr.hierarchy.label
msgid "Certificate Hierarchy"
msgstr "Mutevhe wa Ṱhanziela"

