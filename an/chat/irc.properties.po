#. # LOCALIZATION NOTE (irc.usernameHint):
#. #  This is displayed inside the accountUsernameInfoWithDescription
#. #  string defined in imAccounts.properties when the user is
#. #  configuring an IRC account.
#: irc.usernameHint
msgid "nick"
msgstr "embotada"

#. # LOCALIZATION NOTE (connection.error.*):
#. #   These will show in the account manager if the account is
#. #   disconnected because of an error.
#: connection.error.lost
msgid "Lost connection with server"
msgstr "S'ha perdiu a connexión con o servidor"

#: connection.error.timeOut
msgid "Connection timed out"
msgstr "O tiempo d'aspera d'a connexión s'ha acotolau"

#: connection.error.invalidUsername
#, fuzzy
msgid "%S is not an allowed username"
msgstr "%S no ye permitida como embotada."

#: connection.error.invalidPassword
msgid "Invalid server password"
msgstr "Clau de servidor incorrecta"

#: connection.error.passwordRequired
msgid "Password required"
msgstr "Se requiere clau"

#. # LOCALIZATION NOTE (joinChat.*):
#. #   These show up on the join chat menu. An underscore is for the access key.
#: joinChat.channel
msgid "_Channel"
msgstr "_Canal"

#: joinChat.password
msgid "_Password"
msgstr "_Clau"

#. # LOCALIZATION NOTE (options.*):
#. #   These are the protocol specific options shown in the account manager and
#. #   account wizard windows.
#: options.server
msgid "Server"
msgstr "Servidor"

#: options.port
msgid "Port"
msgstr "Puerto"

#: options.ssl
msgid "Use SSL"
msgstr "Emplegar SSL"

#: options.encoding
msgid "Character Set"
msgstr "Chuego de caracaters"

#: options.quitMessage
msgid "Quit message"
msgstr "Mensache de salida"

#: options.partMessage
msgid "Part message"
msgstr "Parte d'o mensache"

#: options.showServerTab
msgid "Show messages from the server"
msgstr "Amostrar os mensaches d'o servidor"

#: options.alternateNicks
msgid "Alternate nicks"
msgstr "Embotadas alternativas"

#. # LOCALIZATION NOTE (ctcp.version):
#. #   %1$S is the nickname of the user whose version was requested.
#. #   %2$S is the version response from the client.
#: ctcp.version
msgid "%1$S is using \"%2$S\"."
msgstr "%1$S fa servir \"%2$S\"."

#. # LOCALIZATION NOTE (ctcp.time):
#. #   %1$S is the nickname of the user whose time was requested.
#. #   %2$S is the time response.
#: ctcp.time
msgid "The time for %1$S is %2$S."
msgstr "O tiempo ta %1$S ye de %2$S."

#. # LOCALZIATION NOTE (command.*):
#. #   These are the help messages for each command, the %S is the command name
#. #   Each command first gives the parameter it accepts and then a description of
#. #   the command.
#: command.action
msgid "%S &lt;action to perform&gt;: Perform an action."
msgstr "%S &lt;acción ta realizar&gt;: Realizar una acción."

#: command.ctcp
msgid "%S &lt;nick&gt; &lt;msg&gt;: Sends a CTCP message to the nick."
msgstr ""
"%S &lt;embotada&gt; &lt;msg&gt;: Ninviar un mensache CTCP t'a embotada."

#: command.chanserv
msgid "%S &lt;command&gt;: Send a command to ChanServ."
msgstr "%S &lt;orden&gt;: Ninviar una orden t'o ChanServ."

#: command.deop
msgid ""
"%S &lt;nick1&gt;[,&lt;nick2&gt;]*: Remove channel operator status from "
"someone. You must be a channel operator to do this."
msgstr ""
"%S &lt;embotada1&gt;[,&lt;embotada2&gt;]*: Suprimir o estau d'operador d'a "
"canal de bell un. Ha de estar operador d'a canal ta fer-lo."

#: command.devoice
msgid ""
"%S &lt;nick1&gt;[,&lt;nick2&gt;]*: Remove channel voice status from someone, "
"preventing them from speaking if the channel is moderated (+m). You must be "
"a channel operator to do this."
msgstr ""
"%S &lt;embotada1&gt;[,&lt;embotada2&gt;]*: Suprimir o estau de voz de bell "
"un, empachando que pueda charrar si a canal ye moderada (+m). Ha d'estar "
"operador de canal ta fer-lo."

#: command.invite2
msgid ""
"%S &lt;nick&gt;[ &lt;nick&gt;]* [&lt;channel&gt;]: Invite one or more nicks "
"to join you in the current channel, or to join the specified channel."
msgstr ""
"%S &lt;embotada&gt;[ &lt;embotada&gt;]* [&lt;canal&gt;]: Convidar a mas "
"d'una embotada a unir-se a la canal actual, u a la canal especificada."

#: command.join
msgid ""
"%S &lt;room1&gt;[ &lt;key1&gt;][,&lt;room2&gt;[ &lt;key2&gt;]]*: Enter one "
"or more channels, optionally providing a channel key for each if needed."
msgstr ""
"%S &lt;cambra1&gt;[ &lt;clau1&gt;][,&lt;cambra2&gt;[ &lt;clau2&gt;]]*: "
"Introducir una u mas canals, furnindo opcionalment una clau de canal ta "
"cadaguna si calese."

#: command.kick
msgid ""
"%S &lt;nick&gt; [&lt;message&gt;]: Remove someone from a channel. You must "
"be a channel operator to do this."
msgstr ""
"%S &lt;embotada&gt; [&lt;mensachee&gt;]: Suprimir a bell un d'una canal. Ha "
"d'estar operador d'a canal ta fer-lo."

#: command.list
msgid ""
"%S: Display a list of chat rooms on the network. Warning, some servers may "
"disconnect you upon doing this."
msgstr ""
"%S: Amostrar a lista de cambras de chat en o ret. Aviso: bells servidors "
"pueden desconnectar-le en mirar de fer-lo."

#: command.memoserv
msgid "%S &lt;command&gt;: Send a command to MemoServ."
msgstr "%S &lt;orden&gt;: Ninviar una orden t'o MemoServ."

#: command.modeUser
msgid "%S (+|-)&lt;new mode&gt; [&lt;nick&gt;]: Set or unset a user's mode."
msgstr ""
"%S (+|-)&lt;modo nuevo&gt; [&lt;embotada&gt;]: establir u eliminar un modo "
"d'usuario."

#: command.modeChannel
msgid ""
"%S &lt;channel&gt;[ (+|-)&lt;new mode&gt; [&lt;parameter&gt;][,&lt;"
"parameter&gt;]*]: Get, set or unset a channel mode."
msgstr ""
"%S &lt;canal&gt;[ (+|-)&lt;modo nuevo&gt; [&lt;parametro&gt;][,&lt;"
"parametro&gt;]*]: Obtener, establir u eliminar un modo de canal."

#: command.msg
msgid ""
"%S &lt;nick&gt; &lt;message&gt;: Send a private message to a user (as "
"opposed to a channel)."
msgstr ""
"%S &lt;embotada&gt; &lt;mensache&gt;: Ninviar un mensache privau ta un "
"usuario (en cuenta de fer-lo a una canal)."

#: command.nick
msgid "%S &lt;new nickname&gt;: Change your nickname."
msgstr "%S &lt;nueva embotada&gt;: Cambiar a suya embotada."

#: command.nickserv
msgid "%S &lt;command&gt;: Send a command to NickServ."
msgstr "%S &lt;orden&gt;: Ninviar una orden t'o NickServ."

#: command.notice
msgid "%S &lt;target&gt; &lt;message&gt;: Send a notice to a user or channel."
msgstr ""
"%S &lt;obchectivo&gt; &lt;mensache&gt;: Ninviar un aviso ta un usuario u una "
"canal."

#: command.op
msgid ""
"%S &lt;nick1&gt;[,&lt;nick2&gt;]*: Grant channel operator status to someone. "
"You must be a channel operator to do this."
msgstr ""
"%S &lt;embotada1&gt;[,&lt;embotada2&gt;]*: Conceder o estau d'operador de "
"canal a bell un. Ha d'estar operador de canal ta poder fer-lo."

#: command.operserv
msgid "%S &lt;command&gt;: Send a command to OperServ."
msgstr "%S &lt;orden&gt;: Ninviar una orden t'a l'OperServ."

#: command.part
msgid "%S [message]: Leave the current channel with an optional message."
msgstr "%S [mensache]: Deixar a canal actual con un mensache opcional."

#: command.ping
msgid ""
"%S [&lt;nick&gt;]: Asks how much lag a user (or the server if no user "
"specified) has."
msgstr ""
"%S [&lt;embotada&gt;]: Demandar quánto tiempo de retardo tien un usuario (u "
"o servidor, si no s'especifica garra usuario)."

#: command.quit
msgid ""
"%S &lt;message&gt;: Disconnect from the server, with an optional message."
msgstr ""
"%S &lt;mensache&gt;: Desconnectar d'o servidor, con un mensache opcional."

#: command.quote
msgid "%S &lt;command&gt;: Send a raw command to the server."
msgstr "%S &lt;orden&gt;: Ninviar una orden zancera ta o servidor."

#: command.time
msgid "%S: Displays the current local time at the IRC server."
msgstr "%S: Amostrar a hora local actual a o servidor IRC."

#: command.topic
msgid "%S [&lt;new topic&gt;]: Set this channel's topic."
msgstr "%S [&lt;tema nuevo&gt;]: Amostrar o cambiar o tema d'a canal."

#: command.umode
msgid "%S (+|-)&lt;new mode&gt;: Set or unset a user mode."
msgstr ""
"%S (+|-)&lt;modo nuevo&gt;: Configurar u desconfigurar un modo d'usuario."

#: command.version
msgid "%S &lt;nick&gt;: Request the version of a user's client."
msgstr "%S &lt;embotada&gt;: Sollicitar a versión d'o client d'un usuario."

#: command.voice
msgid ""
"%S &lt;nick1&gt;[,&lt;nick2&gt;]*: Grant channel voice status to someone. "
"You must be a channel operator to do this."
msgstr ""
"%S &lt;embotada1&gt;[,&lt;embotada2&gt;]*: Dar o estau de voz d'a canal ta "
"bell un. Ha d'estar un operador d'a canal ta fer-lo."

#: command.whois2
msgid "%S [&lt;nick&gt;]: Get information on a user."
msgstr "%S [&lt;apodo&gt;]: Obtener información d'un usuario."

#. # LOCALIZATION NOTE (message.*):
#. #    These are shown as system messages in the conversation.
#. #    %1$S is the nick and %2$S is the nick and host of the user who joined.
#: message.join
msgid "%1$S [%2$S] entered the room."
msgstr "%1$S [%2$S] ha dentrau en a cambra."

#: message.rejoined
msgid "You have rejoined the room."
msgstr "Ha tornau a dentrar en a cambra."

#. #    %1$S is the nick of who kicked you.
#. #    %2$S is message.kicked.reason, if a kick message was given.
#: message.kicked.you
msgid "You have been kicked by %1$S%2$S."
msgstr "Ha estau forachitau por %1$S%2$S."

#. #    %1$S is the nick that is kicked, %2$S the nick of the person who kicked
#. #    %1$S. %3$S is message.kicked.reason, if a kick message was given.
#: message.kicked
msgid "%1$S has been kicked by %2$S%3$S."
msgstr "%1$S ha estau forachitau por %2$S%3$S."

#. #    %S is the kick message
#: message.kicked.reason
msgctxt "message.kicked.reason"
msgid ": %S"
msgstr ": %S"

#. #    %1$S is the new mode, %2$S is the nickname of the user whose mode
#. #    was changed, and %3$S is who set the mode.
#: message.usermode
msgid "Mode %1$S for %2$S set by %3$S."
msgstr "Modo %1$S ta %2$S establiu por %3$S."

#. #    %1$S is the new channel mode and %2$S is who set the mode.
#: message.channelmode
msgid "Channel mode %1$S set by %2$S."
msgstr "Modo de canal %1$S establiu por %2$S."

#. #    %S is the user's mode.
#: message.yourmode
msgid "Your mode is %S."
msgstr "O suyo modo ye %S."

#. #    %1$S is the old nick and %2$S is the new nick.
#: message.nick
msgid "%1$S is now known as %2$S."
msgstr "%1$S se conoixe agora como %2$S."

#. #    %S is your new nick.
#: message.nick.you
msgid "You are now known as %S."
msgstr "Soi conoixiu agora como %S."

#. #    Could not change the nickname. %S is the user's nick.
#: message.nick.fail
#, fuzzy
msgid "Could not use the desired nickname. Your nick remains %S."
msgstr ""
"No s'ha puesto fer servir a embotada espedificada. A suya embotada contina "
"estando %S."

#. #    The parameter is the message.parted.reason, if a part message is given.
#: message.parted.you
msgid "You have left the room (Part%1$S)."
msgstr "Ha deixau a cambra (Part%1$S)."

#. #    %1$S is the user's nick, %2$S is message.parted.reason, if a part message is given.
#: message.parted
msgid "%1$S has left the room (Part%2$S)."
msgstr "%1$S ha deixau a cambra (Part%2$S)."

#. #    %S is the part message supplied by the user.
#: message.parted.reason
msgctxt "message.parted.reason"
msgid ": %S"
msgstr ": %S"

#. #    %1$S is the user's nick, %2$S is message.quit2 if a quit message is given.
#: message.quit
msgid "%1$S has left the room (Quit%2$S)."
msgstr "%1$S ha deixau a cambra (Quit%2$S)."

#. #    The parameter is the quit message given by the user.
#: message.quit2
msgctxt "message.quit2"
msgid ": %S"
msgstr ": %S"

#. #    %1$S is the nickname of the user that invited us, %2$S is the conversation
#. #    name.
#: message.inviteReceived
msgid "%1$S has invited you to %2$S."
msgstr "%1$S le ha convidau a %2$S."

#. #    %1$S is the nickname of the invited user, %2$S is the conversation name
#. #    they were invited to.
#: message.invited
msgid "%1$S was successfully invited to %2$S."
msgstr "%1$S estió convidau con exito a %2$S."

#. #    %1$S is the nickname of the invited user, %2$S is the conversation name
#. #    they were invited to but are already in
#: message.alreadyInChannel
msgid "%1$S is already in %2$S."
msgstr "%1$S ya ye en %2$S."

#. #    %S is the nickname of the user who was summoned.
#: message.summoned
msgid "%S was summoned."
msgstr "%S estió convocau."

#. #    %S is the nickname of the user whose WHOIS information follows this message.
#: message.whois
msgid "WHOIS information for %S:"
msgstr "Informacióm WHOIS ta %S:"

#. #    %1$S is the nickname of the (offline) user whose WHOWAS information follows this message.
#: message.whowas
msgid "%1$S is offline. WHOWAS information for %1$S:"
msgstr "%1$S ye desconnectau. Información WHOWAS ta %1$S:"

#. #    %1$S is the entry description (from tooltip.*), %2$S is its value.
#: message.whoisEntry
msgid "    %1$S: %2$S"
msgstr "    %1$S: %2$S"

#. #    %S is the nickname that is not known to the server.
#: message.unknownNick
msgid "%S is an unknown nickname."
msgstr "No s'ha trobau a embotada %S."

#. #    %1$S is the nickname of the user who changed the mode and %2$S is the new
#. #    channel key (password).
#: message.channelKeyAdded
msgid "%1$S changed the channel password to %2$S."
msgstr "%1$S ha cambiau a clau d'a canal ta %2$S."

#: message.channelKeyRemoved
msgid "%S removed the channel password."
msgstr "%S ha suprimiu a clau d'a canal."

#. #    This will be followed by a list of ban masks.
#: message.banMasks
msgid "Users connected from the following locations are banned from %S:"
msgstr "Os usuarios connectaus dende as siguients ubicacions tienen viedau %S:"

#: message.noBanMasks
msgid "There are no banned locations for %S."
msgstr "No i ha ubicacions viedadas ta %S."

#: message.banMaskAdded
msgid "Users connected from locations matching %1$S have been banned by %2$S."
msgstr ""
"Os usuarios connectaus dende as ubicacions que coincidan con %1$S son estaus "
"viedaus por %2$S."

#: message.banMaskRemoved
msgid ""
"Users connected from locations matching %1$S are no longer banned by %2$S."
msgstr ""
"Os usuarios connectaus dende ubicacions que coincidan con %1$S ya no son "
"viedaus por %2$S."

#. # LOCALIZATION NOTE (message.ping): Semi-colon list of plural forms.
#. #  See: http://developer.mozilla.org/en/docs/Localization_and_Plurals
#. #   %1$S is the nickname of the user or the server that was pinged.
#. #   #2 is the delay (in milliseconds).
#: message.ping
msgid ""
"Ping reply from %1$S in #2 millisecond.;Ping reply from %1$S in #2 "
"milliseconds."
msgstr ""
"Respuesta a o ping dende %1$S en #2 milisegundo.;Respuesta a o ping dende "
"%1$S en #2 milisegundos."

#. # LOCALIZATION NOTE (error.*):
#. #    These are shown as error messages in the conversation or server tab.
#. #    %S is the channel name.
#: error.noChannel
msgctxt "error.noChannel"
msgid "There is no channel: %S."
msgstr "A canal %S no existe."

#: error.tooManyChannels
msgid "Cannot join %S; you've joined too many channels."
msgstr "No puede unir-se a %S; s'ha uniu a masiadas canals."

#. #    %1$S is your new nick, %2$S is the kill message from the server.
#: error.nickCollision
msgid "Nick already in use, changing nick to %1$S [%2$S]."
msgstr "A embotada ya ye en uso. Se cambia la embotada ta %1$S [%2$S]."

#: error.erroneousNickname
msgid "%S is not an allowed nickname."
msgstr "%S no ye permitida como embotada."

#: error.banned
msgid "You are banned from this server."
msgstr "Ye forachitau d'iste servidor."

#: error.bannedSoon
msgid "You will soon be banned from this server."
msgstr "Ye en momentos de ser forachitau d'iste servidor."

#: error.mode.wrongUser
msgid "You cannot change modes for other users."
msgstr "No puede cambiar os modos d'os atros usuarios."

#. #    %S is the nickname or channel name that isn't available.
#: error.noSuchNick
msgid "%S is not online."
msgstr "%S no ye connectau."

#: error.wasNoSuchNick
msgid "There was no nickname: %S"
msgstr "A embotada %S no existe"

#: error.noSuchChannel
msgctxt "error.noSuchChannel"
msgid "There is no channel: %S."
msgstr "A canal %S no existe."

#: error.unavailable
msgid "%S is temporarily unavailable."
msgstr "%S no ye disponible temporalment."

#. #    %S is the channel name.
#: error.channelBanned
msgid "You have been banned from %S."
msgstr "Le han vedau en %S."

#: error.cannotSendToChannel
msgid "You cannot send messages to %S."
msgstr "No puede ninviar garra mensache a %S."

#: error.channelFull
msgid "The channel %S is full."
msgstr "A canal %S ye completa."

#: error.inviteOnly
msgid "You must be invited to join %S."
msgstr "Ha d'estar convidau ta unir-se a %S."

#: error.nonUniqueTarget
msgid ""
"%S is not a unique user@host or shortname or you have tried to join too many "
"channels at once."
msgstr ""
"%S no ye un unico usuari@servidor u embotada, u talment ha mirau d'accedir "
"ta masiadas canals a la vegada."

#: error.notChannelOp
msgid "You are not a channel operator on %S."
msgstr "No ye un operador d'a canal %S."

#: error.notChannelOwner
msgid "You are not a channel owner of %S."
msgstr "No ye propietario d'a canal %S."

#: error.wrongKey
msgid "Cannot join %S, invalid channel password."
msgstr "No puede unir-se a %S, a clau de canal no ye valida."

#: error.sendMessageFailed
msgid ""
"An error occurred while sending your last message. Please try again once the "
"connection has been reestablished."
msgstr ""
"Ha ocorriu una error entre que se ninviaba o suyo zaguer mensache. Torne-lo "
"a intentar atra vegada malas que a connexión s'haiga restabliu."

#. #    %1$S is the channel the user tried to join, %2$S is the channel
#. #    he was forwarded to.
#: error.channelForward
msgid "You may not join %1$S, and were automatically redirected to %2$S."
msgstr ""

#. # LOCALIZATION NOTE (tooltip.*):
#. #    These are the descriptions given in a tooltip with information received
#. #    from a whois response.
#. #    The human readable ("realname") description of the user.
#: tooltip.realname
msgid "Name"
msgstr "Nombre"

#: tooltip.server
msgid "Connected to"
msgstr "Connectau a"

#. #    The username and hostname that the user connects from (usually based on the
#. #    reverse DNS of the user's IP, but often mangled by the server to
#. #    protect users).
#: tooltip.connectedFrom
msgid "Connected from"
msgstr "Connectau dende"

#: tooltip.registered
msgid "Registered"
msgstr "Rechistrau"

#: tooltip.registeredAs
msgid "Registered as"
msgstr "Rechistrau como"

#: tooltip.secure
msgid "Using a secure connection"
msgstr "Ye emplegando una connexión segura"

#. # The away message of the user
#: tooltip.away
msgid "Away"
msgstr "Ausent"

#: tooltip.ircOp
msgid "IRC Operator"
msgstr "Operador IRC"

#: tooltip.bot
msgid "Bot"
msgstr "Bot"

#: tooltip.lastActivity
msgid "Last activity"
msgstr "Zaguera actividat"

#. # %S is the timespan elapsed since the last activity.
#: tooltip.timespan
msgid "%S ago"
msgstr "en fa %S"

#: tooltip.channels
msgid "Currently on"
msgstr "Actualment en"

#. #    %1$S is the server name, %2$S is some generic server information (usually a
#. #    location or the date the user was last seen).
#: tooltip.serverValue
msgid "%1$S (%2$S)"
msgstr "%1$S (%2$S)"

#. # LOCALIZATION NOTE (yes, no):
#. #  These are used to turn true/false values into a yes/no response.
#: yes
msgid "Yes"
msgstr "Sí"

#: no
msgid "No"
msgstr "No"
