#~ msgstr "Cop&iar l'HTML interno"

#~ msgid "Copy &Outer HTML"
#~ msgstr "C&opiar l'HTML externo"

#~ msgid "Copy &Unique Selector"
#~ msgstr "Copiar o &selector unico"

#~ msgid "&Paste Outer HTML"
#~ msgstr "A&pegar l'HTML exterior"

#~ msgid "Paste I&nner HTML"
#~ msgstr "Apegar o HTML i&nterior"

#~ msgid "Search with CSS Selectors"
#~ msgstr "Mirar con CSS Selectors"

