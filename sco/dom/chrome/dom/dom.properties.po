msgstr "Uise o setAttributeNode() is deprecatit. Use setAttribute() insteid."

#: GetAttributeNodeNSWarning
msgid ""
"Use of getAttributeNodeNS() is deprecated. Use getAttributeNS() instead."
msgstr ""
"Uise o getAttributeNodeNS() is deprecatit. Use getAttributeNS() insteid."

#: SetAttributeNodeNSWarning
msgid ""
"Use of setAttributeNodeNS() is deprecated. Use setAttributeNS() instead."
msgstr ""
"Uise o setAttributeNodeNS() is deprecatit. Use setAttributeNS() insteid."

#: RemoveAttributeNodeWarning
