#: dropped_frames_label
msgid "Dropped frames"
msgstr "Atlanan kareler"

#: discarded_packets_label
msgid "Discarded packets"
msgstr "Atılan paketler"

#: decoder_label
msgid "Decoder"
msgstr "Çözücü"

#: encoder_label
msgid "Encoder"
msgstr "Kodlayıcı"

#: received_label
msgid "Received"
msgstr "Alınan"

#: packets
msgid "packets"
msgstr "paket"

