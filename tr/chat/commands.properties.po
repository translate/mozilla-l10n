#. # LOCALIZATION NOTE (commands):
#. #  %S is a comma separated list of command names.
#: commands
msgid ""
"Commands: %S.\n"
"Use /help &lt;command&gt; for more information."
msgstr ""
"Komutlar: %S.\n"
"Daha fazla bilgi için /help &lt;komutunu&gt; yazın."

#. # LOCALIZATION NOTE (noCommand, noHelp):
#. #  %S is the command name the user typed.
#: noCommand
msgid "No '%S' command."
msgstr "'%S' komutu yok."

#: noHelp
msgid "No help message for the '%S' command, sorry!"
msgstr "'%S' komutu için yardım iletisi yok. Üzgünüm!"

#: sayHelpString
msgid "say &lt;message&gt;: send a message without processing commands."
msgstr "say &lt;ileti&gt;: Komutları işlemeden bir ileti gönderir."

#: rawHelpString
msgid "raw &lt;message&gt;: send a message without escaping HTML entities."
msgstr ""
"raw &lt;ileti&gt;: HTML etiketlerinden temizlemeden olduğu gibi bir ileti "
"gönderir."

#: helpHelpString
msgid ""
"help &lt;name&gt;: show the help message for the &lt;name&gt; command, or "
"the list of possible commands when used without parameter."
msgstr ""
"help &lt;isim&gt;: İsmi belirtilen komut için yardım iletisi gösterir veya "
"parametre olmadan kullanılabilecek komutları listeler."

#. # LOCALIZATION NOTE (statusCommand):
#. #  %1$S is replaced with a status command name
#. #   (one of "back", "away", "busy", "dnd", or "offline").
#. #  %2$S is replaced with the localized version of that status type
#. #   (one of the 5 strings below).
#: statusCommand
msgid ""
"%1$S &lt;status message&gt;: set the status to %2$S with an optional status "
"message."
msgstr ""
"%1$S &lt;durum iletisi&gt;: İsteğe bağlı durum iletisiyle birlikte durumu "
"\"%2$S\" olarak ayarlar."

#: back
msgid "available"
msgstr "uygun"

#: away
msgid "away"
msgstr "uzakta"

#: busy
msgctxt "busy"
msgid "unavailable"
msgstr "uygun değil"

#: dnd
msgctxt "dnd"
msgid "unavailable"
msgstr "uygun değil"

#: offline
msgid "offline"
msgstr "çevrimdışı"
