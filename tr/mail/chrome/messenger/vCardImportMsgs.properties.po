#. #
#. # The following are used by the vCard import code to display status, error, and
#. # informational messages
#. #
#: vCardImportName
msgid "vCard file (.vcf)"
msgstr "vCard dosyası (.vcf)"

#: vCardImportDescription
msgid "Import an address book from vCard format"
msgstr "vCard biçimindeki adres defterini içeri al"

#: vCardImportAddressName
msgid "vCard Address Book"
msgstr "vCard Adres Defteri"

#. # LOCALIZATION NOTE (vCardImportAddressSuccess): %S is replaced by the
#. # name of the address book being imported.
#: vCardImportAddressSuccess
msgid "Imported address book %S"
msgstr "%S adres defteri içe aktarıldı"

#. # LOCALIZATION NOTE (vCardImportAddressSuccess): %S is replaced by the
#. # name of the address book being imported.
#: vCardImportAddressBadSourceFile
msgid "Error accessing file for address book %S."
msgstr "%S adres defter dosyasına erişilirken hata oluştu."

#. # LOCALIZATION NOTE (vCardImportAddressSuccess): %S is replaced by the
#. # name of the address book being imported.
#: vCardImportAddressConvertError
msgid ""
"Error importing address book %S, all addresses may not have been imported."
msgstr ""
"%S adres defterini içe aktarırken hata oluştu. Tüm adresler aktarılamamış "
"olabilir."
