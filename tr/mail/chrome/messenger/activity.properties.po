#. # Status Text
#: paused2
msgid "Paused"
msgstr "Duraklatıldı"

#: processing
msgid "Processing"
msgstr "İşleniyor"

#: notStarted
msgid "Not Started"
msgstr "Başlatılmadı"

#: failed
msgid "Failed"
msgstr "Başarısız"

#: waitingForInput
msgid "Waiting for input"
msgstr "Girdi bekleniyor"

#: waitingForRetry
msgid "Waiting for retry"
msgstr "Tekrar denemek için bekleniyor"

#: completed
msgid "Completed"
msgstr "Tamamlandı"

#: canceled
msgid "Canceled"
msgstr "Vazgeçildi"

#. # LOCALIZATION NOTE (yesterday): Displayed time for files finished yesterday
#: yesterday
msgid "Yesterday"
msgstr "Dün"

#. # LOCALIZATION NOTE (monthDate): #1 month name; #2 date number; e.g., January 22
#: monthDate
msgid "#1 #2"
msgstr "#2 #1"

#. # LOCALIZATION NOTE (sendingMessages): this is used as a title for grouping processes in the activity manager when sending email.
#: sendingMessages
msgid "Sending Messages"
msgstr "İletiler gönderiliyor"

#: sendingMessage
msgid "Sending Message"
msgstr "İleti gönderiliyor"

#. # LOCALIZATION NOTE (sendingMessageWithSubject): %S will be replaced by the subject of the message being sent.
#: sendingMessageWithSubject
msgid "Sending Message: %S"
msgstr "İletiler gönderiliyor: %S"

#: copyMessage
msgid "Copying message to sent folder"
msgstr "İleti giden dizinine kopyalanıyor"

#: sentMessage
msgid "Sent Message"
msgstr "Gönderilmiş ileti"

#. # LOCALIZATION NOTE (sentMessageWithSubject): %S will be replaced by the subject of the message being sent.
#: sentMessageWithSubject
msgid "Sent Message: %S"
msgstr "Gönderilmiş ileti: %S"

#: failedToSendMessage
msgid "Failed to send message"
msgstr "İleti gönderimi başarısız"

#: failedToCopyMessage
msgid "Failed to copy message"
msgstr "İleti kopyalaması başarısız"

#. # LOCALIZATION NOTE (failedToSendMessageWithSubject): %S will be replaced by the subject of the message being sent.
#: failedToSendMessageWithSubject
msgid "Failed to send message: %S"
msgstr "İleti gönderimi başarısız: %S"

#. # LOCALIZATION NOTE (failedToCopyMessageWithSubject): %S will be replaced by the subject of the message being sent.
#: failedToCopyMessageWithSubject
msgid "Failed to copy message: %S"
msgstr "İleti kopyalaması başarısız: %S"

#. # LOCALIZATION NOTE (autosyncProcessProgress): Do not translate the word "%1$S", "%2$S" and "%3$S" below.
#. # Place the word %1$S in your translation where the number of the message being downloaded should appear.
#. # Place the word %2$S in your translation where the total number of pending messages should appear.
#. # Place the word %3$S in your translation where the name of the folder being processed should appear.
#: autosyncProcessProgress
msgid "Downloading %1$S of %2$S in %3$S"
msgstr "%1$S/%2$S ileti %3$S konumuna indiriliyor"

#. # LOCALIZATION NOTE (autosyncProcessDisplayText): %S will be replaced by the folder name
#: autosyncProcessDisplayText
msgid "Bringing folder %S up to date"
msgstr "%S dizini güncel hâle getiriliyor"

#. # LOCALIZATION NOTE (autosyncEventDisplayText): %S will be replaced by the account name
#: autosyncEventDisplayText
msgctxt "autosyncEventDisplayText"
msgid "%S is up to date"
msgstr "%S güncel"

#. # LOCALIZATION NOTE (autosyncEventStatusText): %S will be replaced by total number of downloaded messages
#: autosyncEventStatusText
msgid "Total number of messages downloaded: %S"
msgstr "Toplam indirilmiş ileti sayısı: %S"

#: autosyncEventStatusTextNoMsgs
msgid "No messages downloaded"
msgstr "Hiçbir ileti indirilmedi"

#. # LOCALIZATION NOTE (autosyncContextDisplayText): %S will be replaced by the account name
#: autosyncContextDisplayText
msgid "Synchronizing: %S"
msgstr "Eşitleniyor: %S"

#. # LOCALIZATION NOTE (pop3ProcessDisplayText): %S will be replaced by the account name
#: pop3EventStartDisplayText
msgid "Checking %S for new messages"
msgstr "Yeni iletiler için %S denetleniyor"

#. # LOCALIZATION NOTE (pop3EventDisplayText): %S will be replaced by the account name
#: pop3EventDisplayText
msgctxt "pop3EventDisplayText"
msgid "%S is up to date"
msgstr "%S güncel"

#. # LOCALIZATION NOTE (pop3EventStatusText): #1 will be replaced by total number of downloaded messages
#: pop3EventStatusText
msgid "#1 message downloaded;#1 messages downloaded"
msgstr "#1 ileti indirildi;#1 ileti indirildi"

#: pop3EventStatusTextNoMsgs
msgid "No messages to download"
msgstr "İndirilecek ileti yok"

#. # Message actions that show up in activity manager
#. # LOCALIZATION NOTE (deletedMessages2): #1 number of messages, #2 folder name
#: deletedMessages2
msgid "Deleted #1 message from #2;Deleted #1 messages from #2"
msgstr "#2 dizininden #1 ileti silindi;#2 dizininden #1 ileti silindi"

#. # LOCALIZATION NOTE (movedMessages): #1 number of messages, #2 and #3: folder names
#: movedMessages
msgid "Moved #1 message from #2 to #3;Moved #1 messages from #2 to #3"
msgstr ""
"#2 dizininden #3 dizinine #1 ileti taşındı;#2 dizininden #3 dizinine #1 "
"ileti taşındı"

#. # LOCALIZATION NOTE (copiedMessages): #1 number of messages, #2 and #3: folder names
#: copiedMessages
msgid "Copied #1 message from #2 to #3;Copied #1 messages from #2 to #3"
msgstr ""
"#2 dizininden #3 dizinine #1 ileti kopyalandı;#2 dizininden #3 dizinine #1 "
"ileti kopyalandı"

#. # LOCALIZATION NOTE (fromServerToServer): #1 source server, #2 destination server
#: fromServerToServer
msgid "from #1 to #2"
msgstr "#1 sunucusundan #2 sunucusuna"

#. # LOCALIZATION NOTE (deletedFolder): #1 folder name
#: deletedFolder
msgid "Deleted folder #1"
msgstr "#1 dizini silindi"

#: emptiedTrash
msgid "Emptied Trash"
msgstr "Çöp boşaltıldı"

#. # LOCALIZATION NOTE (movedFolder): #1 and #2 are folder names
#: movedFolder
msgid "Moved folder #1 into folder #2"
msgstr "#1 dizini #2 dizinine taşındı"

#. # LOCALIZATION NOTE (movedFolderToTrash): #1 is the folder name
#: movedFolderToTrash
msgid "Moved folder #1 to Trash"
msgstr "#1 dizini Çöp'e taşındı"

#. # LOCALIZATION NOTE (copiedFolder): #1 and #2 are folder names
#: copiedFolder
msgid "Copied folder #1 into folder #2"
msgstr "#1 dizini #2 dizinine kopyalandı"

#. # LOCALIZATION NOTE (renamedFolder): #1 and #2 are folder names
#: renamedFolder
msgid "Renamed folder #1 to #2"
msgstr "#1 dizininin ismi #2 olarak değiştirildi"

#: indexing
msgid "Indexing messages"
msgstr "İleti dizini oluşturuluyor"

#. # LOCALIZATION NOTE (indexingFolder): #1 is a folder name
#: indexingFolder
msgid "Indexing messages in #1"
msgstr "#1 dizinindeki iletiler endeksleniyor"

#: indexingStatusVague
msgid "Determining which messages to index"
msgstr "Endekslenecek iletiler tespit ediliyor"

#. # LOCALIZATION NOTE (indexingFolderStatusVague): #1 is a folder name
#: indexingFolderStatusVague
msgid "Determining which messages to index in #1"
msgstr "#1 dizininde endekslenecek iletiler tespit ediliyor"

#. # LOCALIZATION NOTE (indexingStatusExact):
#. #   #1 is the number of the message currently being indexed
#. #   #2 is the total number of messages being indexed
#. #   #3 is the percentage of indexing that is complete
#: indexingStatusExact
msgid "Indexing #1 of #2 message;Indexing #1 of #2 messages (#3% complete)"
msgstr "Endekslenen #1/#2 ileti;Endekslenen #1/#2 ileti (%#3 tamamlandı)"

#. # LOCALIZATION NOTE (indexingFolderStatusExact):
#. #   #1 is the number of the message currently being indexed
#. #   #2 is the total number of messages being indexed
#. #   #3 is the percentage of indexing that is complete
#. #   #4 is a folder name
#: indexingFolderStatusExact
msgid ""
"Indexing #1 of #2 message in #4;Indexing #1 of #2 messages in #4 (#3% "
"complete)"
msgstr ""
"#4 dizininde endekslenen #1/#2 ileti;#4 dizininde endekslenen #1/#2 ileti "
"(%#3 tamamlandı)"

#. # LOCALIZATION NOTE (indexedFolder): #1 number of messages; #2 folder name
#: indexedFolder
msgid "Indexed #1 message in #2;Indexed #1 messages in #2"
msgstr "#2 dizininde #1 ileti endekslendi;#2 dizininde #1 ileti endekslendi"

#. # LOCALIZATION NOTE (indexedFolderStatus): #1 number of seconds spent indexing
#: indexedFolderStatus
msgid "#1 second elapsed;#1 seconds elapsed"
msgstr "#1 saniye geçti;#1 saniye geçti"
