#. # Map Expat error codes to error strings
#: 1
msgid "out of memory"
msgstr "недостатъчно памет"

#: 2
msgid "syntax error"
msgstr "синтактична грешка"

#: 3
msgid "no element found"
msgstr "не е намерен елемент"

#: 4
msgid "not well-formed"
msgstr "не е добре оформен"

#: 5
msgid "unclosed token"
msgstr "незатворен токен"

#: 6
msgid "partial character"
msgstr "частичен знак"

#: 7
msgid "mismatched tag"
msgstr "сбъркан етикет"

#: 8
msgid "duplicate attribute"
msgstr "удвоен атрибут"

#: 9
msgid "junk after document element"
msgstr "боклук след елемента на документа"

#: 10
msgid "illegal parameter entity reference"
msgstr "невалиден указател към параметър"

#: 11
msgid "undefined entity"
msgstr "неопределена стойност"

#: 12
msgid "recursive entity reference"
msgstr "рекурсивен указател"

#: 13
msgid "asynchronous entity"
msgstr "асинхронен обект"

#: 14
msgid "reference to invalid character number"
msgstr "указател към невалиден номер на знак"

#: 15
msgid "reference to binary entity"
msgstr "указател към двоичен обект"

#: 16
msgid "reference to external entity in attribute"
msgstr "указател към външен обект в атрибут"

#: 17
msgid "XML or text declaration not at start of entity"
msgstr "XML или текстовата декларация не са начало на обект"

#: 18
msgid "unknown encoding"
msgstr "неизвестно кодиране"

#: 19
msgid "encoding specified in XML declaration is incorrect"
msgstr "зададеното кодиране в декларация на XML е неправилно"

#: 20
msgid "unclosed CDATA section"
msgstr "незатворена секция CDATA"

#: 21
msgid "error in processing external entity reference"
msgstr "грешка при обработване на указател към външен обект"

#: 22
msgid "document is not standalone"
msgstr "документът не е самостоятелен"

#: 23
msgid "unexpected parser state"
msgstr "неочаквано състояние при разпознаване"

#: 24
msgid "entity declared in parameter entity"
msgstr "обявяване на обект в параметър"

#: 27
msgid "prefix not bound to a namespace"
msgstr "представката не е свързана с пространство от имена"

#: 28
msgid "must not undeclare prefix"
msgstr "не трябва да отхвърляте декларацията на приставката"

#: 29
msgid "incomplete markup in parameter entity"
msgstr "незавършен маркъп в параметър на обект"

#: 30
msgid "XML declaration not well-formed"
msgstr "XML декларацията не е добре оформена"

#: 31
msgid "text declaration not well-formed"
msgstr "текстовата декларация не е добре оформена"

#: 32
msgid "illegal character(s) in public id"
msgstr "illegal character(s) in public id"

#: 38
msgid ""
"reserved prefix (xml) must not be undeclared or bound to another namespace "
"name"
msgstr ""
"запазената представка (xml) не трябва да бъде недекларирана или свързана с "
"друго име"

#: 39
msgid "reserved prefix (xmlns) must not be declared or undeclared"
msgstr "резервирана представка (xmlns) не трябва да бъде обявена или необявена"

#: 40
msgid "prefix must not be bound to one of the reserved namespace names"
msgstr "представката не трябва да бъде свързана с резервираните имена"

#. # %1$S is replaced by the Expat error string, may be followed by Expected (see below)
#. # %2$S is replaced by URL
#. # %3$u is replaced by line number
#. # %4$u is replaced by column number
#: XMLParsingError
msgid ""
"XML Parsing Error: %1$S\n"
"Location: %2$S\n"
"Line Number %3$u, Column %4$u:"
msgstr ""
"Грешка при разбор на XML: %1$S\n"
"Местоположение: %2$S\n"
"Ред %3$u, Колона %4$u:"

#. # %S is replaced by a tag name.
#. # This gets appended to the error string if the error is mismatched tag.
#: Expected
msgid ". Expected: </%S>."
msgstr ". Очаквано: </%S>."
