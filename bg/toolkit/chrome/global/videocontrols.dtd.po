msgid "Full Screen"
msgstr "Цял екран"

#: fullscreenButton.exitfullscreenlabel
msgid "Exit Full Screen"
msgstr "Изход от цял екран"

#: castingButton.castingLabel
msgid "Cast to Screen"
msgstr "Пускане на екрана"

#: stats.media
msgid "Media"
msgstr "Медиа"

#. Do not translate this.  Only change the numeric values if you need this dialogue box to appear bigger
#: stats.size
msgid "Size"
msgstr "Големина"

#: stats.activity
msgid "Activity"
msgstr "Активност"

#: stats.activityPaused
msgid "Paused"
msgstr "Пауза"

#: stats.activityPlaying
msgid "Playing"
msgstr "Възпроизвеждане"

#: stats.activityEnded
msgid "Ended"
msgstr "Край"

#: stats.activitySeeking
msgid "(seeking)"
msgstr "(преместване)"

#: stats.volume
msgid "Volume"
msgstr "Сила на звука"

#: stats.framesParsed
msgid "Frames parsed"
msgstr "Разпознати рамки"

#: stats.framesDecoded
msgid "Frames decoded"
msgstr "Декодирани рамки"

#: stats.framesPresented
msgid "Frames presented"
msgstr "Налични рамки"

#: stats.framesPainted
msgid "Frames painted"
msgstr "Изчертани рамки"

#: error.aborted
msgid "Video loading stopped."
msgstr "Зареждането на видео спря."

#: error.network
msgid "Video playback aborted due to a network error."
msgstr "Зареждането на видео е прекъснато поради мрежова грешка."

#: error.decode
msgid "Video can't be played because the file is corrupt."
msgstr "Видеото не може да бъде възпроизведено, защото файлът е развален."

#: error.srcNotSupported
msgid "Video format or MIME type is not supported."
msgstr "Видеоформатът или този вид MIME не се поддържа."

#: error.noSource2
msgid "No video with supported format and MIME type found."
msgstr "Не е намерено видео с поддържан формат или вид на MIME."

#: error.generic
msgid "Video playback aborted due to an unknown error."
msgstr "Видеовъзпроизвеждането прекъсна поради неизвестна грешка."

#. the #1 string is the current
#. media position, and the #2 string is the total duration. For example, when at
#. the 5 minute mark in a 6 hour long video, #1 would be "5:00" and #2 would be
#. "6:00:00", result string would be "5:00 of 6:00:00 elapsed".
#: scrubberScale.nameFormat
msgid "#1 of #2 elapsed"
msgstr "изтекли са #1 от #2"
