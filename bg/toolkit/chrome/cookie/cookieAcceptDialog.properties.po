#: hostColon
msgid "Host:"
msgstr "Хост:"

#: domainColon
msgid "Domain:"
msgstr "Домейн:"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "Само шифровани връзки"

#: forAnyConnection
msgid "Any type of connection"
msgstr "Всякакъв тип връзка"

#: expireAtEndOfSession
msgid "At end of session"
msgstr "На края на сесията"

#: showDetails
msgid "Show Details"
msgstr "Показване на подробностите"

#: hideDetails
msgid "Hide Details"
msgstr "Скриване на подробностите"

#: detailsAccessKey
msgid "T"
msgstr "д"

#: permissionToSetACookie
msgid "The site %S wants to set a cookie."
msgstr "Сайтът %S иска да зададе бисквитка."

#: permissionToSetSecondCookie
msgid "The site %S wants to set a second cookie."
msgstr "Сайтът %S иска да зададе втора бисквитка."

#. #  LOCALIZATION NOTE (PermissionToSetAnotherCookie): First %S: sitename, second %S: number of cookies already present for that site
#: permissionToSetAnotherCookie
msgid ""
"The site %S wants to set another cookie.\n"
"You already have %S cookies from this site."
msgstr ""
"Сайтът %S иска да постави друга бисквитка.\n"
"Вие вече имате %S бисквитки от този сайт."

#: permissionToModifyCookie
msgid "The site %S wants to modify an existing cookie."
msgstr "Сайтът %S иска да промени съществуваща бисквитка."
